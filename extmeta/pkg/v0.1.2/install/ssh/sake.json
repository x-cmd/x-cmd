{
  "lang": "Go",
  "homepage": "https://sakecli.com/",
  "desc": {
    "cn": "sake 是一个用于本地和远程主机的命令执行工具。",
    "en": "sake is a command runner for local and remote hosts."
  },
  "rule": {
    "linux/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/alajmo/sake/main/install.sh | sh",
      "reference": "https://sakecli.com/installation"
    },
    "darwin/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/alajmo/sake/main/install.sh | sh",
      "reference": "https://sakecli.com/installation"
    },
    "linux/brew": {
      "cmd": "brew tap alajmo/sake && brew install sake",
      "reference": "https://sakecli.com/installation"
    },
    "darwin/brew": {
      "cmd": "brew tap alajmo/sake && brew install sake",
      "reference": "https://sakecli.com/installation"
    },
    "darwin/port": {
      "cmd": "sudo port install sake",
      "reference": "https://sakecli.com/installation/"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S sake",
      "reference": "https://sakecli.com/installation/"
    },
    "freebsd/pkg": {
      "cmd": "pkg install sake",
      "reference": "https://sakecli.com/installation/"
    },
    "/go": {
      "cmd": "go install github.com/alajmo/sake@latest",
      "reference": "https://sakecli.com/installation/",
      "dsnap": "github.com/alajmo/sake@latest"
    }
  },
  "dsnap": {
    "brew": "sake",
    "/go": "github.com/alajmo/sake@latest"
  }
}
