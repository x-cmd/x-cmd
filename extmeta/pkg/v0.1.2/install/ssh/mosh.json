{
  "lang": "C++",
  "homepage": "https://mosh.org/",
  "desc": {
    "cn": "Mosh 是一种替代交互式 SSH 终端的工具。它在 Wi-Fi、蜂窝网络和长距离连接上更加稳定和灵敏。",
    "en": "Mosh is a replacement for interactive SSH terminals. It's more robust and responsive, especially over Wi-Fi, cellular, and long-distance links."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install mosh",
      "reference": "https://mosh.org/#getting"
    },
    "darwin/port": {
      "cmd": "sudo port install mosh",
      "reference": "https://mosh.org/#getting"
    },
    "alpine/apk": {
      "cmd": "apk add mosh",
      "reference": "https://mosh.org/#getting"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mosh",
      "reference": "https://mosh.org/#getting"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install mosh",
      "reference": "https://mosh.org/#getting"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install mosh",
      "reference": "https://mosh.org/#getting"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mosh",
      "reference": "https://mosh.org/#getting"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge net-misc/mosh",
      "reference": "https://mosh.org/#getting"
    },
    "guixsd/guix": {
      "cmd": "sudo guix package -i mosh",
      "reference": "https://mosh.org/#getting"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in mosh",
      "reference": "https://mosh.org/#getting"
    },
    "freebsd/pkg": {
      "cmd": "pkg install net/mosh",
      "reference": "https://mosh.org/#getting"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add mosh",
      "reference": "https://mosh.org/#getting"
    },
    "netbsd/make": {
      "cmd": "cd net/mosh; make install clean",
      "reference": "https://mosh.org/#getting"
    },
    "opencsw/pkgutil": {
      "cmd": "pkgutil -i mosh",
      "reference": "https://mosh.org/#getting"
    }
  }
}
