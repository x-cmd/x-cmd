{
  "lang": "Go",
  "homepage": "https://v1.manfred.life/sshportal/",
  "desc": {
    "cn": "一个简单、有趣且透明的SSH（和Telnet）跳板机",
    "en": "simple, fun and transparent SSH (and telnet) bastion"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install sshportal",
      "reference": "https://formulae.brew.sh/formula/sshportal#default"
    },
    "/go": {
      "cmd": "GO111MODULE=on go get -u moul.io/sshportal",
      "reference": "https://github.com/moul/sshportal?tab=readme-ov-file#manual-install",
      "dsnap": "moul.io/sshportal"
    }
  },
  "dsnap": {
    "brew": "sshportal",
    "/go": "moul.io/sshportal"
  }
}
