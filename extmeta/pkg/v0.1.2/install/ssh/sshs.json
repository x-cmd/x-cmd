{
  "lang": "Rust",
  "homepage": "https://github.com/quantumsheep/sshs",
  "desc": {
    "cn": "用于管理 SSH 连接的 TUI 工具",
    "en": "A TUI tool for managing SSH connections"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install sshs",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#homebrew"
    },
    "linux/brew": {
      "cmd": "brew install sshs",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#homebrew"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install sshs",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#chocolatey"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && \\\nx scoop install extras/sshs\n",
      "reference": "https://scoop.sh/#/apps?q=sshs&id=c56d6b708809daa775746de1590f0622f063641f"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S sshs",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#arch-linux"
    },
    "alpine/apk": {
      "cmd": "apk add sshs",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#alpine-linux"
    },
    "nix/nix": {
      "cmd": "nix profile install 'github:quantumsheep/sshs'",
      "reference": "https://github.com/quantumsheep/sshs?tab=readme-ov-file#nixos--nix"
    }
  },
  "dsnap": {
    "brew": "sshs",
    "/cargo": "sshs"
  }
}
