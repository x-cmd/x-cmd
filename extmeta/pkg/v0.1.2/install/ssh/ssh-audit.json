{
  "lang": "python",
  "desc": {
    "cn": "一个用于 ssh 服务器和客户端配置审核的工具",
    "en": "ssh-audit is a tool for ssh server & client configuration auditing."
  },
  "homepage": "https://github.com/jtesta/ssh-audit",
  "rule": {
    "/pip3": {
      "cmd": "x pip3 install ssh-audit",
      "reference": "https://github.com/jtesta/ssh-audit#pre-built-packages"
    },
    "/snap": {
      "cmd": "snap install ssh-audit",
      "reference": "https://github.com/jtesta/ssh-audit#pre-built-packages"
    },
    "/docker": {
      "cmd": "docker pull positronsecurity/ssh-audit\ndocker run -it -p 2222:2222 positronsecurity/ssh-audit 10.1.1.1\n",
      "reference": "https://github.com/jtesta/ssh-audit#pre-built-packages"
    }
  },
  "dsnap": {
    "/pip": "ssh-audit"
  }
}
