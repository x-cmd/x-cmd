{
  "lang": "c",
  "homepage": "https://www.openssh.com/",
  "desc": {
    "cn": "openssh-server 是 OpenSSH 连接工具的服务端",
    "en": "openssh-server is the server side of the OpenSSH connection tool"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install openssh",
      "reference": "https://formulae.brew.sh/formula/openssh#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install openssh-server",
      "reference": "https://www.cyberciti.biz/faq/ubuntu-linux-install-openssh-server/",
      "dsnap": "openssh-server"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install openssh-server",
      "reference": "https://www.cyberciti.biz/faq/ubuntu-linux-install-openssh-server/",
      "dsnap": "openssh-server"
    },
    "alpine/apk": {
      "cmd": "apk add openssh",
      "reference": "https://www.cyberciti.biz/faq/how-to-install-openssh-server-on-alpine-linux-including-docker/",
      "dsnap": "openssh"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -Sy openssh",
      "reference": "https://www.unixmen.com/how-to-install-openssh-in-archinux/"
    },
    "kali/apt": {
      "cmd": "sudo apt install openssh-server",
      "reference": "https://www.kali.org/tools/openssh/"
    },
    "centos/yum": {
      "cmd": "sudo yum -y install openssh-server openssh-server",
      "reference": "https://phoenixnap.com/kb/how-to-enable-ssh-centos-7"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install openssh-server",
      "reference": "https://linuxconfig.org/how-to-install-start-and-connect-to-ssh-server-on-fedora-linux",
      "dsnap": "openssh-server"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install openssh",
      "reference": "https://linuxways.net/opensuse/how-to-enable-ssh-on-opensuse/"
    }
  },
  "dsnap": {
    "brew": "openssh",
    "yum": "openssh-server",
    "zypper": "openssh",
    "fedora/dnf": "openssh-server",
    "alpine/apk": "openssh",
    "ubuntu/apt": "openssh-server",
    "debian/apt": "openssh-server"
  }
}
