{
  "lang": "Go",
  "homepage": "https://github.com/francoismichel/ssh3",
  "desc": {
    "cn": "SSH3 是对 SSH 协议的彻底重新审视，将其语义映射到 HTTP 机制之上",
    "en": "SSH3 is a complete revisit of the SSH protocol, mapping its semantics on top of the HTTP mechanisms"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/francoismichel/ssh3/cmd/...@latest",
      "reference": "https://github.com/francoismichel/ssh3?tab=readme-ov-file#installing-ssh3-and-ssh3-server-using-go-install",
      "dsnap": "github.com/francoismichel/ssh3/cmd/...@latest"
    },
    "darwin/brew": {
      "cmd": "brew install ssh3",
      "reference": "https://formulae.brew.sh/formula/ssh3#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add main && x scoop install main/ssh3",
      "reference": "https://scoop.sh/#/apps?q=ssh3"
    }
  },
  "dsnap": {
    "brew": "ssh3",
    "/go": "github.com/francoismichel/ssh3/cmd/...@latest"
  }
}
