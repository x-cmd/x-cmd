{
  "lang": "go",
  "homepage": "https://esbuild.github.io/",
  "desc": {
    "en": "Extremely fast JavaScript bundler and minifier",
    "cn": "极快的 JavaScript 打包器和压缩器"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g esbuild",
      "reference": "https://esbuild.github.io/getting-started/#install-esbuild",
      "dsnap": "esbuild"
    },
    "darwin/brew": {
      "cmd": "brew install esbuild",
      "reference": "https://formulae.brew.sh/formula/esbuild#default"
    }
  },
  "dsnap": {
    "brew": "esbuild",
    "/npm": "esbuild"
  }
}
