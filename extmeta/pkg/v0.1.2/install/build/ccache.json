{
  "lang": "c++",
  "homepage": "https://ccache.dev/",
  "desc": {
    "en": "Ccache is a compiler cache.",
    "cn": "CCACHE是编译器缓存"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use ccache",
      "reference": "https://github.com/asdf-community/asdf-ccache"
    },
    "darwin/brew": {
      "cmd": "brew install ccache",
      "reference": "https://formulae.brew.sh/formula/ccache#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install ccache",
      "reference": "https://scoop.sh/#/apps?q=Ccache"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install ccache",
      "reference": "https://community.chocolatey.org/packages/ccache"
    }
  },
  "dsnap": {
    "brew": "ccache"
  }
}
