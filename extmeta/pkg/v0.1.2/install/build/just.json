{
  "lang": "Rust",
  "homepage": "https://just.systems/",
  "desc": {
    "cn": "just 是保存和运行项目特定命令的便捷方法",
    "en": "just is a handy way to save and run project-specific commands"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install just",
      "reference": "https://just.systems/man/zh/chapter_4.html",
      "dsnap": "just"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "darwin/brew": {
      "cmd": "brew install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "darwin/port": {
      "cmd": "sudo port install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "solus/eopkg": {
      "cmd": "sudo eopkg install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "void/xbps": {
      "cmd": "xbps-install -S just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "freebsd/pkg": {
      "cmd": "pkg install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "alpine/apk": {
      "cmd": "apk add just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "gentoo/emerge": {
      "cmd": "eselect repository enable guru && \\\nemerge --sync guru && \\\nemerge sys-devel/just\n",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "/snap": {
      "cmd": "sudo snap install --edge --classic just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "/asdf": {
      "cmd": "x asdf use just",
      "reference": "https://github.com/olofvndrhr/asdf-just"
    },
    "debian/make": {
      "cmd": "git clone 'https://mpr.makedeb.org/just' && \\\ncd just && \\\nmakedeb -si\n",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "ubuntu/make": {
      "cmd": "git clone 'https://mpr.makedeb.org/just' && \\\ncd just && \\\nmakedeb -si\n",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "debian/apt": {
      "cmd": "sudo apt install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install just",
      "reference": "https://just.systems/man/zh/chapter_4.html"
    }
  },
  "dsnap": {
    "/cargo": "just"
  }
}
