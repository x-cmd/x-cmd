{
  "lang": "c",
  "homepage": "https://llvm.org/",
  "desc": {
    "cn": "LLVM 项目是模块化和可重用的编译器和工具链技术的集合",
    "en": "The LLVM Project is a collection of modular and reusable compiler and toolchain technologies"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install llvm",
      "reference": "https://formulae.brew.sh/formula/llvm#default"
    },
    "/apt": {
      "cmd": "sudo apt-get install llvm-dev",
      "reference": "https://apt.llvm.org/"
    },
    "alpine/apk": {
      "cmd": "sudo apk add llvm",
      "reference": "https://command-not-found.com/llvm-extract",
      "dsnap": "llvm"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S llvm",
      "reference": "https://archlinux.org/packages/extra/x86_64/llvm/"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install llvm-dev",
      "reference": "https://apt.llvm.org/"
    },
    "centos/yum": {
      "cmd": "sudo yum install llvm",
      "reference": "https://command-not-found.com/llvm-extract"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install llvm",
      "reference": "https://command-not-found.com/llvm-extract",
      "dsnap": "llvm"
    }
  },
  "dsnap": {
    "brew": "llvm",
    "pacman": "llvm",
    "yum": "llvm",
    "fedora/dnf": "llvm",
    "alpine/apk": "llvm"
  }
}
