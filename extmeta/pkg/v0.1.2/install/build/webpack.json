{
  "lang": "JavaScript",
  "homepage": "https://webpack.js.org/",
  "desc": {
    "en": "A bundler for javascript and friends. Packs many modules into a few bundled assets.",
    "cn": "一个用于 JavaScript 和友好的资源打包工具"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g webpack",
      "reference": "hhttps://github.com/webpack/webpack?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew install webpack",
      "reference": "https://formulae.brew.sh/formula/webpack#default"
    }
  }
}
