{
  "lang": "typescript",
  "homepage": "https://vitejs.dev/",
  "desc": {
    "cn": "Vite 是一种新的前端构建工具，可显着改善前端开发体验",
    "en": "Vite is a new breed of frontend build tooling that significantly improves the frontend development experience"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g vite",
      "reference": "https://vitejs.dev/guide/#scaffolding-your-first-vite-project",
      "dsnap": "vite"
    },
    "darwin/brew": {
      "cmd": "brew install vite",
      "reference": "https://formulae.brew.sh/formula/vite#default"
    }
  },
  "dsnap": {
    "brew": "vite",
    "/npm": "vite"
  }
}
