{
  "lang": "C++",
  "homepage": "https://github.com/Slackadays/Clipboard",
  "desc": {
    "cn": "一个跨平台的命令行剪贴板工具。",
    "en": "A cross-platform command-line clipboard tool."
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sSL https://github.com/Slackadays/Clipboard/raw/main/install.sh | sh",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/powershell": {
      "cmd": "(Invoke-WebRequest -UseBasicParsing https://github.com/Slackadays/Clipboard/raw/main/install.ps1).Content | powershell",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "alpine/apk": {
      "cmd": "apk add clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1",
      "dsnap": "clipboard"
    },
    "arch/yay": {
      "cmd": "yay -S clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/conda-forge": {
      "cmd": "conda install -c conda-forge cb-clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/mamba": {
      "cmd": "mamba install -c conda-forge cb-clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/micromamba": {
      "cmd": "micromamba install -c conda-forge cb-clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/pixi": {
      "cmd": "pixi global install cb-clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/flatpak": {
      "cmd": "flatpak install app.getclipboard.Clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "gentoo/emerge": {
      "cmd": "emerge -av app-misc/clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/brew": {
      "cmd": "brew install clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "nix/nix-env": {
      "cmd": "nix-env -iA nixpkgs.clipboard-jh",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/pacstall": {
      "cmd": "pacstall -I clipboard-bin",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "/snap": {
      "cmd": "sudo snap install clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S Clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install -e --id Slackadays.Clipboard",
      "reference": "https://github.com/Slackadays/Clipboard#-1"
    }
  },
  "dsnap": {
    "brew": "clipboard",
    "alpine/apk": "clipboard"
  }
}
