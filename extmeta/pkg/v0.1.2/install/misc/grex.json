{
  "lang": "rust",
  "homepage": "https://github.com/pemistahl/grex",
  "desc": {
    "cn": "命令行工具和 Rust 库，用于从用户提供的测试用例生成正则表达式",
    "en": "A command-line tool and Rust library for generating regular expressions from user-provided test cases"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use grex",
      "reference": "https://github.com/ouest/asdf-grex"
    },
    "darwin/brew": {
      "cmd": "brew install grex",
      "reference": "https://formulae.brew.sh/formula/grex#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install grex",
      "reference": "https://github.com/pemistahl/grex#41-the-command-line-tool"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install grex",
      "reference": "https://github.com/pemistahl/grex#41-the-command-line-tool"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install grex",
      "reference": "https://github.com/pemistahl/grex#41-the-command-line-tool"
    }
  },
  "dsnap": {
    "brew": "grex",
    "/cargo": "grex"
  }
}
