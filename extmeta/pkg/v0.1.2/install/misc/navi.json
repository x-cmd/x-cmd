{
  "lang": "rust",
  "homepage": "https://github.com/denisidoro/navi",
  "desc": {
    "cn": "用于命令行的交互式备忘单工具",
    "en": "An interactive cheatsheet tool for the command-line"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install navi",
      "reference": "https://formulae.brew.sh/formula/navi#default"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install navi",
      "reference": "https://community.chocolatey.org/packages/navi"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install navi",
      "reference": "https://scoop.sh/#/apps?q=navi"
    }
  },
  "dsnap": {
    "brew": "navi"
  }
}
