{
  "lang": "Python",
  "homepage": "https://github.com/papis/papis",
  "desc": {
    "en": "Papis is a powerful and highly extensible CLI document and bibliography manager",
    "cn": "Papis是一个功能强大且高度可扩展的命令行文档和参考文献管理工具。"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install --user papis",
      "reference": "https://papis.readthedocs.io/en/latest/install.html",
      "dsnap": "papis"
    },
    "arch/yay": {
      "cmd": "yay -S papis",
      "reference": "https://papis.readthedocs.io/en/latest/install.html"
    },
    "/nix-env": {
      "cmd": "nix-env -i papis",
      "reference": "https://papis.readthedocs.io/en/latest/install.html"
    }
  },
  "dsnap": {
    "/pip": "papis"
  }
}
