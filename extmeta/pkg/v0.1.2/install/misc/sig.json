{
  "lang": "Rust",
  "homepage": "https://github.com/ynqa/sig",
  "desc": {
    "cn": "交互式的 grep 工具，用于流式数据。",
    "en": "Interactive grep (for streaming)"
  },
  "rule": {
    "/brew": {
      "cmd": "brew install ynqa/tap/sigrs",
      "reference": "https://github.com/ynqa/sig#homebrew"
    },
    "/cargo": {
      "cmd": "cargo install sigrs",
      "reference": "https://github.com/ynqa/sig#cargo",
      "dsnap": "sigrs"
    },
    "arch/pacman": {
      "cmd": "pacman -S sig",
      "reference": "https://github.com/ynqa/sig#arch-linux"
    }
  },
  "dsnap": {
    "brew": "sigrs",
    "/cargo": "sigrs"
  }
}
