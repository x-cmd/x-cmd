{
  "lang": "Go",
  "homepage": "https://github.com/Lifailon/lazyjournal",
  "desc": {
    "cn": "用于显示来自 journalctl、文件系统、Docker、Podman 和 Kubernetes Pod 的日志的 TUI。",
    "en": "TUI for logs from journalctl, file system, Docker, Podman and Kubernetes pods."
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sS https://raw.githubusercontent.com/Lifailon/lazyjournal/main/install.sh | bash",
      "reference": "https://github.com/Lifailon/lazyjournal#unix-based"
    },
    "arch/paru": {
      "cmd": "paru -S lazyjournal",
      "reference": "https://github.com/Lifailon/lazyjournal#arch-linux"
    },
    "/pixi": {
      "cmd": "pixi global install lazyjournal",
      "reference": "https://github.com/Lifailon/lazyjournal#conda--mamba--pixi-linux--macos--windows"
    },
    "/brew": {
      "cmd": "brew install lazyjournal",
      "reference": "https://github.com/Lifailon/lazyjournal#homebrew-macos--linux"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge lazyjournal",
      "reference": "https://github.com/Lifailon/lazyjournal#conda--mamba--pixi-linux--macos--windows"
    },
    "win/powershell": {
      "cmd": "irm https://raw.githubusercontent.com/Lifailon/lazyjournal/main/install.ps1 | iex",
      "reference": "https://github.com/Lifailon/lazyjournal#windows"
    },
    "/go": {
      "cmd": "go install github.com/Lifailon/lazyjournal@latest",
      "reference": "https://github.com/Lifailon/lazyjournal#go-package"
    },
    "/eget": {
      "cmd": "eget lifailon/lazyjournal --to ~/.local/bin",
      "reference": "https://github.com/Lifailon/lazyjournal#eget"
    }
  }
}
