{
  "lang": "python",
  "homepage": "https://github.com/dylanaraps/pywal",
  "desc": {
    "cn": "Pywal 是一个从图像中的主色调生成调色板的工具",
    "en": "Pywal is a tool that generates a color palette from the dominant colors in an image"
  },
  "rule": {
    "/pip3": {
      "cmd": "pip3 install pywal",
      "reference": "https://github.com/dylanaraps/pywal/wiki/Installation#pip-install"
    },
    "darwin/pip": {
      "cmd": "pip install --user https://github.com/dylanaraps/pywal/archive/master.zip \\\n&& pip show pywal | grep \"Location\" \\\n&& export PATH=\"${PATH}:REPLACE\"\n",
      "reference": "https://github.com/dylanaraps/pywal/wiki/Installation#-macos"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S python-pywal",
      "reference": "https://github.com/dylanaraps/pywal/wiki/Installation#-arch-linux"
    },
    "nix/nixpkgs": {
      "cmd": "sudo nix-env -i pywal",
      "reference": "https://github.com/dylanaraps/pywal/wiki/Installation#-nixpkgsnixos"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install pywal",
      "reference": "https://github.com/dylanaraps/pywal/wiki/Installation#-void-linux"
    }
  },
  "dsnap": {
    "/pip": "pywal"
  }
}
