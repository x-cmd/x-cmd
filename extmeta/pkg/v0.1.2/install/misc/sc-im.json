{
  "lang": "C",
  "homepage": "https://github.com/andmarti1424/sc-im",
  "desc": {
    "cn": "sc-im 是一个使用 ncurses 编写的终端电子表格程序。",
    "en": "sc-im - Spreadsheet Calculator Improvised -- An ncurses spreadsheet program for terminal."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install sc-im",
      "reference": "https://github.com/andmarti1424/sc-im#homebrew-for-osx-users"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    },
    "freebsd/pkg": {
      "cmd": "pkg install sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    },
    "alpine/apk": {
      "cmd": "sudo apk add sc-im",
      "reference": "https://github.com/andmarti1424/sc-im"
    }
  }
}
