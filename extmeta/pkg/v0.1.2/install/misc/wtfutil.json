{
  "lang": "go",
  "homepage": "https://github.com/wtfutil/wtf",
  "desc": {
    "cn": "WTF 是您终端的个人信息仪表板",
    "en": "WTF is the personal information dashboard for your terminal"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use wtfutil",
      "reference": "https://github.com/NeoHsu/asdf-wtfutil"
    },
    "darwin/brew": {
      "cmd": "brew install wtfutil",
      "reference": "https://github.com/wtfutil/wtf?tab=readme-ov-file#installing-via-homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port install wtfutil",
      "reference": "https://github.com/wtfutil/wtf?tab=readme-ov-file#installing-via-macports"
    },
    "/go": {
      "cmd": "export GOPROXY=\"https://proxy.golang.org,direct\"\nexport GOSUMDB=off\nexport GO111MODULE=on\ngo get -u github.com/wtfutil/wtf\ncd $GOPATH/src/github.com/wtfutil/wtf\nmake install\nmake run\n",
      "reference": "https://github.com/wtfutil/wtf?tab=readme-ov-file#installing-from-source"
    }
  }
}
