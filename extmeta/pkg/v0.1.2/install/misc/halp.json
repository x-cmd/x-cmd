{
  "lang": "Rust",
  "homepage": "https://github.com/orhun/halp",
  "desc": {
    "en": "A CLI tool to get help with CLI tools",
    "cn": "一个用于获取命令行工具帮助的命令行工具"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install halp",
      "reference": "https://github.com/orhun/halp?tab=readme-ov-file#cargo"
    },
    "arch/pacman": {
      "cmd": "pacman -S halp",
      "reference": "https://github.com/orhun/halp?tab=readme-ov-file#arch-linux"
    },
    "arch/paru": {
      "cmd": "paru -S halp-git",
      "reference": "https://github.com/orhun/halp?tab=readme-ov-file#arch-linux"
    }
  }
}
