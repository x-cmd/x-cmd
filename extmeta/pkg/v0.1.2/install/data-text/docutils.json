{
  "lang": "python",
  "homepage": "https://docutils.sourceforge.io/",
  "desc": {
    "cn": "Docutils 是一个开源的文本处理系统，用于将明文文档处理成有用的格式",
    "en": "Docutils is an open-source text processing system for processing plaintext documentation into useful formats"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install docutils",
      "reference": "https://formulae.brew.sh/formula/docutils#default"
    },
    "/pip": {
      "cmd": "pip install docutils",
      "reference": "https://pypi.org/project/docutils/#files",
      "dsnap": "docutils"
    }
  },
  "dsnap": {
    "brew": "docutils",
    "/pip": "docutils"
  }
}
