{
  "lang": "go",
  "homepage": "https://github.com/noborus/xlsxsql",
  "desc": {
    "cn": "一个 CLI 工具，可以对包括 xlsx 文件在内的各种文件执行 SQL 查询，并将结果输出到各种文件",
    "en": "A CLI tool that executes SQL queries on various files including xlsx files and outputs the results to various files"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install noborus/tap/xlsxsql",
      "reference": "https://github.com/noborus/xlsxsql?tab=readme-ov-file#homebrew"
    },
    "/go": {
      "cmd": "go install github.com/noborus/xlsxsql/cmd/xlsxsql@latest",
      "reference": "https://github.com/noborus/xlsxsql?tab=readme-ov-file#go-install",
      "dsnap": "github.com/noborus/xlsxsql/cmd/xlsxsql@latest"
    }
  },
  "dsnap": {
    "brew": "noborus/tap/xlsxsql",
    "/go": "github.com/noborus/xlsxsql/cmd/xlsxsql@latest"
  }
}
