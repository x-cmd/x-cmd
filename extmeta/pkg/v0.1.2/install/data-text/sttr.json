{
  "lang": "Go",
  "homepage": "https://github.com/abhimanyu003/sttr",
  "desc": {
    "cn": "sttr 是一款命令行软件，允许你快速对字符串执行各种转换操作。",
    "en": "sttr is command line software that allows you to quickly run various transformation operations on the string."
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/abhimanyu003/sttr/main/install.sh | sh",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "arch/yay": {
      "cmd": "yay -S sttr-bin",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "manjaro/yay": {
      "cmd": "yay -S sttr-bin",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "/nix-env": {
      "cmd": "nix-env -iA sttr",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "linux/snap": {
      "cmd": "sudo snap install sttr",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "darwin/brew": {
      "cmd": "brew install abhimanyu003/sttr/sttr",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install -e --id abhimanyu003.sttr",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add sttr https://github.com/abhimanyu003/scoop-bucket.git\n&& x scoop install sttr\n",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "win/powershell": {
      "cmd": "curl.exe https://webi.ms/sttr | x powershell",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    },
    "/go": {
      "cmd": "go install github.com/abhimanyu003/sttr@latest",
      "reference": "https://github.com/abhimanyu003/sttr#battery-installation"
    }
  }
}
