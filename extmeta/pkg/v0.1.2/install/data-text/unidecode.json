{
  "lang": "python",
  "homepage": "https://github.com/avian2/unidecode#installation",
  "desc": {
    "cn": "用 ASCII 转译 Unicode 文本",
    "en": "ASCII transliterations of Unicode text"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install unidecode",
      "reference": "https://github.com/avian2/unidecode#installation",
      "dsnap": "unidecode"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install python3-unidecode",
      "reference": "https://command-not-found.com/unidecode"
    }
  },
  "dsnap": {
    "/pip": "unidecode"
  }
}
