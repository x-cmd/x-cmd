{
  "lang": "Python",
  "homepage": "https://trzsz.github.io/",
  "desc": {
    "cn": "trzsz 是一种简单的文件传输工具，类似于 lrzsz ( rz / sz )，并且与 tmux 兼容",
    "en": "trzsz is a simple file transfer tools, which similar to lrzsz ( rz / sz ), and compatible with tmux"
  },
  "rule": {
    "/pip": {
      "cmd": "sudo python3 -m pip install --upgrade trzsz",
      "reference": "https://github.com/trzsz/trzsz?tab=readme-ov-file#installation",
      "dsnap": "trzsz"
    },
    "darwin/brew": {
      "cmd": "brew install trzsz",
      "reference": "https://github.com/trzsz/trzsz?tab=readme-ov-file#installation"
    },
    "/npm": {
      "cmd": "sudo npm install -g trzsz",
      "reference": "https://github.com/trzsz/trzsz?tab=readme-ov-file#installation",
      "dsnap": "trzsz"
    }
  },
  "dsnap": {
    "brew": "trzsz",
    "/pip": "trzsz",
    "/npm": "trzsz"
  }
}
