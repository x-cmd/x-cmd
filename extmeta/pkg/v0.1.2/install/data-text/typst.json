{
  "lang": "rust",
  "desc": {
    "cn": "Typst 的 CLI(Typst 是一种新的基于标记的排版系统，其设计与 LaTeX 一样强大，同时更易于学习和使用)",
    "en": "The CLI of Typst (Typst is a new mark-up typesetting system, designed to be as powerful as LaTeX, but easier to learn and use)"
  },
  "homepage": "https://github.com/typst/typst",
  "rule": {
    "darwin/brew": {
      "cmd": "brew install typst",
      "reference": "https://github.com/typst/typst?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "winget install --id Typst.Typst",
      "reference": "https://github.com/typst/typst?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install --git https://github.com/typst/typst typst-cli",
      "reference": "https://github.com/typst/typst?tab=readme-ov-file#installation",
      "dsnap": "typst-cli"
    },
    "/nix": {
      "cmd": "nix run github:typst/typst -- --version",
      "reference": "https://github.com/typst/typst?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "typst",
    "/cargo": "typst-cli"
  }
}
