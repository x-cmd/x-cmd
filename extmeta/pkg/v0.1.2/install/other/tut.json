{
  "lang": "Go",
  "homepage": "https://tut.anv.nu/",
  "desc": {
    "cn": "tut 是一个用于 Mastodon 的 TUI，具有 Vim 风格的键盘映射。",
    "en": "tut is a TUI for Mastodon with vim inspired keys."
  },
  "rule": {
    "netbsd/pkgin": {
      "cmd": "pkgin install tut",
      "reference": "https://tut.anv.nu/install#package-manager"
    },
    "darwim/brew": {
      "cmd": "brew install tut",
      "reference": "https://tut.anv.nu/install#package-manager"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in tut",
      "reference": "https://tut.anv.nu/install#package-manager"
    }
  },
  "dsnap": {
    "brew": "tut"
  }
}
