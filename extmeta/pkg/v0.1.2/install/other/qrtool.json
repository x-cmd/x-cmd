{
  "lang": "Rust",
  "homepage": "https://github.com/sorairolake/qrtool",
  "desc": {
    "cn": "用于编码或解码二维码的实用程序",
    "en": "An utility for encoding or decoding QR code"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install qrtool",
      "reference": "https://github.com/sorairolake/qrtool?tab=readme-ov-file#installation",
      "dsnap": "qrtool"
    },
    "/brew": {
      "cmd": "brew install qrtool",
      "reference": "https://github.com/sorairolake/qrtool?tab=readme-ov-file#installation"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.qrtool",
      "reference": "https://github.com/sorairolake/qrtool?tab=readme-ov-file#installation"
    },
    "arch/pacman": {
      "cmd": "pacman -S qrtool",
      "reference": "https://github.com/sorairolake/qrtool?tab=readme-ov-file#installation"
    },
    "opensuse/zypper": {
      "cmd": "zypper install qrtool",
      "reference": "https://github.com/sorairolake/qrtool?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/cargo": "qrtool"
  }
}
