{
  "lang": "shell",
  "homepage": "https://ohmybash.nntoan.com/",
  "desc": {
    "cn": "Oh-My-BASH 是一个开源的、社区驱动的框架，用于管理您的 BASH 配置",
    "en": "Oh-My-Bash is an open source, community-driven framework for managing your BASH configuration"
  },
  "rule": {
    "/curl": {
      "cmd": "sh -c \"$(curl -fsSL https://raw.github.com/ohmybash/oh-my-bash/master/tools/install.sh)\"",
      "reference": "https://github.com/ohmybash/oh-my-bash"
    },
    "/wget": {
      "cmd": "sh -c \"$(wget https://raw.githubusercontent.com/ohmybash/oh-my-bash/master/tools/install.sh -O -)\"",
      "reference": "https://github.com/ohmybash/oh-my-bash"
    }
  },
  "dsnap": null
}
