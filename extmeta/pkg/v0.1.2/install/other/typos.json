{
  "lang": "rust",
  "homepage": "https://github.com/crate-ci/typos",
  "desc": {
    "cn": "源代码拼写检查器",
    "en": "Source code spell checker"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use typos",
      "reference": "https://github.com/aschiavon91/asdf-typos"
    },
    "darwin/brew": {
      "cmd": "brew install typos-cli",
      "reference": "https://github.com/crate-ci/typos?tab=readme-ov-file#install"
    },
    "/conda": {
      "cmd": "conda install typos",
      "reference": "https://github.com/crate-ci/typos?tab=readme-ov-file#install"
    },
    "/cargo": {
      "cmd": "cargo install typos-cli",
      "reference": "https://github.com/crate-ci/typos?tab=readme-ov-file#install"
    }
  }
}
