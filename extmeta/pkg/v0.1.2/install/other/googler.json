{
  "lang": "python",
  "homepage": "https://github.com/jarun/googler",
  "desc": {
    "cn": "在终端中使用谷歌搜索(已存档)",
    "en": "Google Search, Google Site Search, Google News from the terminal(archived)"
  },
  "rule": {
    "/curl": {
      "cmd": "sudo curl -o /usr/local/bin/googler https://raw.githubusercontent.com/jarun/googler/v4.3.2/googler && sudo chmod +x /usr/local/bin/googler\n",
      "reference": "https://github.com/darrikonn/td-cli?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install googler",
      "reference": "https://community.chocolatey.org/packages/googler"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install googler",
      "reference": "https://command-not-found.com/googler"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install googler",
      "reference": "https://command-not-found.com/googler"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install googler",
      "reference": "https://command-not-found.com/googler"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install googler",
      "reference": "https://command-not-found.com/googler"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install googler",
      "reference": "https://command-not-found.com/googler"
    }
  }
}
