{
  "lang": "Go",
  "homepage": "https://github.com/maaslalani/nap",
  "desc": {
    "cn": "基于终端的代码片段管理器，具有命令行界面 (CLI) 和文本用户界面 (TUI)",
    "en": "A terminal-based code snippet manager with a command-line interface (CLI) and a text-user interface (TUI)"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install nap",
      "reference": "https://formulae.brew.sh/formula/nap#default"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nap",
      "reference": "https://github.com/maaslalani/nap#installation"
    },
    "/eget": {
      "cmd": "eget maaslalani/nap",
      "reference": "https://github.com/maaslalani/nap#installation"
    },
    "/go": {
      "cmd": "go install github.com/maaslalani/nap@main",
      "reference": "https://github.com/maaslalani/nap#installation"
    }
  }
}
