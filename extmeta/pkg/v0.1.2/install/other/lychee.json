{
  "lang": "Rust",
  "homepage": "https://github.com/lycheeverse/lychee",
  "desc": {
    "cn": "一款用 Rust 编写的快速、异步的链接检查器。",
    "en": "A fast, async link checker written in Rust."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S lychee",
      "reference": "https://github.com/lycheeverse/lychee#arch-linux"
    },
    "opensuse/zypper": {
      "cmd": "zypper install lychee",
      "reference": "https://github.com/lycheeverse/lychee#opensuse-tumbleweed"
    },
    "darwin/brew": {
      "cmd": "brew install lychee",
      "reference": "https://github.com/lycheeverse/lychee#macos"
    },
    "darwin/port": {
      "cmd": "sudo port install lychee",
      "reference": "https://github.com/lycheeverse/lychee#macos"
    },
    "alpine/apk": {
      "cmd": "apk add lychee",
      "reference": "https://github.com/lycheeverse/lychee#alpine-linux"
    },
    "freebsd/pkg": {
      "cmd": "pkg install lychee",
      "reference": "https://github.com/lycheeverse/lychee#freebsd"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install lychee",
      "reference": "https://github.com/lycheeverse/lychee#scoop"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install lychee",
      "reference": "https://github.com/lycheeverse/lychee#chocolatey-windows"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install -e --id lycheeverse.lychee",
      "reference": "https://github.com/lycheeverse/lychee#installation"
    },
    "/cargo": {
      "cmd": "cargo install lychee",
      "reference": "https://github.com/lycheeverse/lychee#cargo",
      "dsnap": "lychee"
    },
    "/nix-env": {
      "cmd": "nix-env -iA lychee",
      "reference": "https://github.com/lycheeverse/lychee#nixpkgs"
    }
  },
  "dsnap": {
    "/cargo": "lychee"
  }
}
