{
  "lang": "Go",
  "homepage": "https://github.com/YS-L/flamelens",
  "desc": {
    "cn": "终端中的交互式火焰图查看器。",
    "en": "An interactive flamegraph viewer in the terminal."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S flamelens",
      "reference": "https://github.com/YS-L/flamelens#installation"
    },
    "/cargo": {
      "cmd": "cargo install flamelens --locked",
      "reference": "https://github.com/YS-L/flamelens#installation"
    }
  }
}
