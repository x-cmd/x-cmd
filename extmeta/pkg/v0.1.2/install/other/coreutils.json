{
  "lang": "c",
  "homepage": "https://www.gnu.org/software/coreutils/",
  "desc": {
    "cn": "GNU 核心实用程序是 GNU 操作系统的基本文件、shell 和文本操作实用程序",
    "en": "The GNU Core Utilities are the basic file, shell and text manipulation utilities of the GNU operating system"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install coreutils",
      "reference": "https://formulae.brew.sh/formula/coreutils#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install coreutils",
      "reference": "https://command-not-found.com/true",
      "dsnap": "coreutils"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install coreutils",
      "reference": "https://command-not-found.com/true",
      "dsnap": "coreutils"
    },
    "alpine/apk": {
      "cmd": "sudo apk add coreutils",
      "reference": "https://command-not-found.com/true",
      "dsnap": "coreutils"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S coreutils",
      "reference": "https://command-not-found.com/true"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install coreutils",
      "reference": "https://command-not-found.com/true"
    },
    "centos/yum": {
      "cmd": "sudo yum install coreutils",
      "reference": "https://command-not-found.com/true"
    },
    "fedora/yum": {
      "cmd": "sudo dnf install coreutils",
      "reference": "https://command-not-found.com/true"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install coreutils",
      "reference": "https://command-not-found.com/true"
    }
  },
  "dsnap": {
    "brew": "coreutils",
    "pacman": "coreutils",
    "yum": "coreutils",
    "fedora/dnf": "coreutils",
    "alpine/apk": "coreutils",
    "ubuntu/apt": "coreutils",
    "debian/apt": "coreutils"
  }
}
