{
  "lang": "Python",
  "homepage": "https://toot.bezdomni.net/",
  "desc": {
    "cn": "Toot 是一个命令行（CLI）和终端用户界面（TUI）工具，用于从命令行与 Mastodon（及其他兼容）实例进行交互。",
    "en": "Toot is a CLI and TUI tool for interacting with Mastodon (and other compatible) instances from the command line."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install toot",
      "reference": "https://toot.bezdomni.net/installation.html#homebrew"
    },
    "/pipx": {
      "cmd": "pipx install toot",
      "reference": "https://toot.bezdomni.net/installation.html#using-pipx"
    },
    "debian/brew": {
      "cmd": "sudo apt-get install toot",
      "reference": "https://command-not-found.com/toot"
    },
    "ubuntu/brew": {
      "cmd": "sudo apt-get install toot",
      "reference": "https://command-not-found.com/toot"
    },
    "kali/brew": {
      "cmd": "sudo apt-get install toot",
      "reference": "https://command-not-found.com/toot"
    }
  },
  "dsnap": {
    "brew": "toot"
  }
}
