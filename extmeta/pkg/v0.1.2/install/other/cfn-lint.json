{
  "lang": "Python",
  "homepage": "https://github.com/aws-cloudformation/cfn-lint",
  "desc": {
    "cn": "根据 AWS CloudFormation 资源规范和其他检查验证 AWS CloudFormation yaml/json 模板",
    "en": "Validate AWS CloudFormation yaml/json templates against the AWS CloudFormation Resource Specification and additional checks"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install cfn-lint",
      "reference": "https://github.com/aws-cloudformation/cfn-lint#pip",
      "dsnap": "cfn-lint"
    },
    "darwin/brew": {
      "cmd": "brew install cfn-lint",
      "reference": "https://github.com/aws-cloudformation/cfn-lint#homebrew-macos"
    }
  },
  "dsnap": {
    "brew": "cfn-lint",
    "/pip": "cfn-lint"
  }
}
