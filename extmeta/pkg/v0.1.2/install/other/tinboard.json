{
  "lang": "Python",
  "homepage": "https://github.com/davep/tinboard",
  "desc": {
    "cn": "基于终端的 pinboard.in 客户端",
    "en": "A terminal-based client for pinboard.in"
  },
  "rule": {
    "/pipx": {
      "cmd": "pipx install tinboard",
      "reference": "https://github.com/davep/tinboard#installing"
    },
    "/pip": {
      "cmd": "pip install tinboard",
      "reference": "https://github.com/davep/tinboard#installing",
      "dsnap": "tinboard"
    },
    "darwin/brew": {
      "cmd": "brew install davep/homebrew/tinboard",
      "reference": "https://github.com/davep/tinboard#installing"
    }
  },
  "dsnap": {
    "brew": "tinboard",
    "/pip": "tinboard"
  }
}
