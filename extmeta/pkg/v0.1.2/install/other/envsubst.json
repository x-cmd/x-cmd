{
  "lang": "go",
  "homepage": "https://github.com/a8m/envsubst",
  "desc": {
    "cn": "管理 go 的环境变量",
    "en": "Environment variables substitution for Go"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use envsubst",
      "reference": "https://github.com/dex4er/asdf-envsubst"
    },
    "/curl": {
      "cmd": "curl -L https://github.com/a8m/envsubst/releases/download/v1.2.0/envsubst-`uname -s`-`uname -m` -o envsubst\n&& chmod +x envsubst\n&& sudo mv envsubst /usr/local/bin\n",
      "reference": "https://github.com/a8m/envsubst?tab=readme-ov-file#linux-and-macos"
    },
    "win/powershell": {
      "cmd": "curl -L https://github.com/a8m/envsubst/releases/download/v1.2.0/envsubst.exe",
      "reference": "https://github.com/a8m/envsubst?tab=readme-ov-file#windows"
    },
    "/go": {
      "cmd": "go get github.com/a8m/envsubst/cmd/envsubst",
      "reference": "https://github.com/a8m/envsubst?tab=readme-ov-file#with-go",
      "dsnap": "github.com/a8m/envsubst/cmd/envsubst"
    }
  },
  "dsnap": {
    "/go": "github.com/a8m/envsubst/cmd/envsubst"
  }
}
