{
  "lang": "go",
  "homepage": "https://qrcp.sh/",
  "desc": {
    "cn": "无需离开终端，扫描二维码即可通过 wifi 将文件从计算机传输到移动设备",
    "en": "Scan the QR code to transfer files from your computer to your mobile device over wifi without leaving the terminal"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/claudiodangelis/qrcp@latest",
      "reference": "https://github.com/claudiodangelis/qrcp?tab=readme-ov-file#install-the-latest-development-version-with-go",
      "dsnap": "github.com/claudiodangelis/qrcp@latest"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install qrcp",
      "reference": "https://github.com/claudiodangelis/qrcp?tab=readme-ov-file#scoop"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install qrcp",
      "reference": "https://github.com/claudiodangelis/qrcp?tab=readme-ov-file#chocolatey"
    },
    "darwin/brew": {
      "cmd": "brew install qrcp",
      "reference": "https://github.com/claudiodangelis/qrcp?tab=readme-ov-file#homebrew"
    }
  },
  "dsnap": {
    "brew": "qrcp",
    "/go": "github.com/claudiodangelis/qrcp@latest"
  }
}
