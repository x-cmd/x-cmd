{
  "lang": "C",
  "homepage": "https://weechat.org/",
  "desc": {
    "cn": "WeeChat 是一个快速、轻量且可扩展的聊天客户端，具有基于文本的用户界面。",
    "en": "WeeChat is a fast, light and extensible chat client, with a text-based user interface."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install weechat-curses weechat-plugins",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install weechat-curses weechat-plugins",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install weechat-curses weechat-plugins",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install weechat-curses weechat-plugins",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "mint/apt": {
      "cmd": "sudo apt-get install weechat-curses weechat-plugins",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "freebsd/pkg": {
      "cmd": "sudo pkg install weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    },
    "darwim/brew": {
      "cmd": "brew install weechat",
      "reference": "https://weechat.org/files/doc/weechat/stable/weechat_user.en.html#binary_packages"
    }
  },
  "dsnap": {
    "brew": "weechat"
  }
}
