{
  "lang": "erlang",
  "homepage": "https://github.com/processone/tsung",
  "desc": {
    "en": "Tsung is multi-protocol distributed load testing tool",
    "cn": "Tsung 是一个多协议分布式负载测试工具"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tsung",
      "reference": "https://formulae.brew.sh/formula/tsung#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install tsung",
      "reference": "https://command-not-found.com/tsung",
      "dsnap": "tsung"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install tsung",
      "reference": "https://command-not-found.com/tsung",
      "dsnap": "tsung"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install tsung",
      "reference": "https://command-not-found.com/tsung"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tsung",
      "reference": "https://command-not-found.com/tsung"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install tsung",
      "reference": "https://command-not-found.com/tsung"
    }
  },
  "dsnap": {
    "ubuntu/apt": "tsung",
    "debian/apt": "tsung"
  }
}
