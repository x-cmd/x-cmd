{
  "lang": "Rust",
  "homepage": "https://www.nushell.sh/",
  "desc": {
    "cn": "Nushell 是一种新型的 shell",
    "en": "Nushell is a new type of shell"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install nu",
      "reference": "https://scoop.sh/#/apps?q=nu"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install nushell",
      "reference": "https://community.chocolatey.org/packages/nushell"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install nushell",
      "reference": "https://www.nushell.sh/#windows"
    },
    "/npm": {
      "cmd": "npm install -g nushell",
      "reference": "https://www.nushell.sh/book/installation.html#package-managers",
      "dsnap": "nushell"
    },
    "darwin/brew": {
      "cmd": "brew install nushell",
      "reference": "https://www.nushell.sh/#get-nu"
    },
    "linux/brew": {
      "cmd": "brew install nushell",
      "reference": "https://www.nushell.sh/#get-nu"
    },
    "/nix": {
      "cmd": "nix profile install nixpkgs#nushell",
      "reference": "https://www.nushell.sh/#nix-profile"
    }
  },
  "dsnap": {
    "/npm": "nushell"
  }
}
