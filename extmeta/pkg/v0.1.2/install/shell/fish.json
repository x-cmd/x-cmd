{
  "lang": "rust",
  "homepage": "https://github.com/fish-shell/fish-shell",
  "desc": {
    "cn": "用户友好的命令行 shell",
    "en": "The user-friendly command line shell"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install fish",
      "reference": "https://formulae.brew.sh/formula/fish#default"
    },
    "darwin/port": {
      "cmd": "sudo port install fish",
      "reference": "https://ports.macports.org/port/fish/"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install fish",
      "reference": "https://command-not-found.com/fish",
      "dsnap": "fish"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install fish",
      "reference": "https://command-not-found.com/fish",
      "dsnap": "fish"
    },
    "alpine/apk": {
      "cmd": "sudo apk add fish",
      "reference": "https://command-not-found.com/fish",
      "dsnap": "fish"
    },
    "arch/pacman": {
      "cmd": "sudo ppacman -S fish",
      "reference": "https://command-not-found.com/fish"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install fish",
      "reference": "https://command-not-found.com/fish"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install fish",
      "reference": "https://command-not-found.com/fish",
      "dsnap": "fish"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install fish",
      "reference": "https://command-not-found.com/fish"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge fish",
      "reference": "https://packages.gentoo.org/packages/app-shells/fish"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install fish-shell",
      "reference": "https://fishshell.com/"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixos.fish",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=fish&from=0&size=50&sort=relevance&type=packages&query=fish"
    },
    "/nix-shell": {
      "cmd": "nix-shell -p fish",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=fish&from=0&size=50&sort=relevance&type=packages&query=fish"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper addrepo https://download.opensuse.org/repositories/shells:fish:release:3/openSUSE_Tumbleweed/shells:fish:release:3.repo && \\\nsudo zypper refresh && \\\nsudo zypper install fish\n",
      "reference": "https://software.opensuse.org/download.html?project=shells%3Afish%3Arelease%3A3&package=fish"
    },
    "guixos/guix": {
      "cmd": "guix package -i fish",
      "reference": "https://fishshell.com/"
    },
    "solus/eopkg": {
      "cmd": "sudo eopkg install fish",
      "reference": "https://fishshell.com/"
    },
    "freebsd/pkg": {
      "cmd": "pkg install fish",
      "reference": "https://fishshell.com/"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install fish",
      "reference": "https://fishshell.com/"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add fish",
      "reference": "https://fishshell.com/"
    },
    "win/msys2": {
      "cmd": "sudo pacman -S fish",
      "reference": "https://fishshell.com/"
    }
  },
  "dsnap": {
    "brew": "fish",
    "pacman": "fish",
    "zypper": "fish",
    "guix": "fish",
    "eopkg": "fish",
    "pkg": "fish",
    "pkgin": "fish",
    "pkg_add": "fish",
    "fedora/dnf": "fish",
    "alpine/apk": "fish",
    "ubuntu/apt": "fish",
    "debian/apt": "fish"
  }
}
