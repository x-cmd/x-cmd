{
  "lang": "shell",
  "homepage": "https://github.com/shadow-maint/shadow",
  "desc": {
    "cn": "Shadow-utils软件包包括将UNIX密码文件转换为影子密码格式的必要程序，以及用于管理用户和组帐户的程序",
    "en": "The shadow-utils package includes the necessary programs for converting UNIX password files to the shadow password format, plus programs for managing user and group accounts"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt-get install passwd",
      "reference": "https://command-not-found.com/chsh",
      "dsnap": "passwd"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install passwd",
      "reference": "https://command-not-found.com/chsh",
      "dsnap": "passwd"
    },
    "alpine/apk": {
      "cmd": "sudo apk add util-linux",
      "reference": "https://command-not-found.com/chsh",
      "dsnap": "util-linux"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S util-linux",
      "reference": "https://command-not-found.com/chsh"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install passwd",
      "reference": "https://command-not-found.com/chsh"
    },
    "centos/yum": {
      "cmd": "sudo yum install util-linux-ng",
      "reference": "https://command-not-found.com/chsh"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install util-linux-user",
      "reference": "https://command-not-found.com/chsh",
      "dsnap": "util-linux-user"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install passwd",
      "reference": "https://command-not-found.com/chsh"
    }
  },
  "dsnap": {
    "pacman": "util-linux",
    "yum": "util-linux-ng",
    "fedora/dnf": "util-linux-user",
    "alpine/apk": "util-linux",
    "ubuntu/apt": "passwd",
    "debian/apt": "passwd"
  }
}
