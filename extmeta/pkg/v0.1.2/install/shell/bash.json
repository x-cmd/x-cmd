{
  "lang": "c",
  "homepage": "http://tiswww.case.edu/php/chet/bash/bashtop.html",
  "desc": {
    "cn": "Bash 是 GNU 项目的 Bourne Again SHell",
    "en": "Bash is the GNU Project's Bourne Again SHell"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install bash",
      "reference": "https://formulae.brew.sh/formula/bash#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install bash",
      "reference": "https://command-not-found.com/bash",
      "dsnap": "bash"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install bash",
      "reference": "https://command-not-found.com/bash",
      "dsnap": "bash"
    },
    "alpine/apk": {
      "cmd": "sudo apk add bash",
      "reference": "https://command-not-found.com/bash",
      "dsnap": "bash"
    },
    "arch/pacman": {
      "cmd": "sudo ppacman -S bash",
      "reference": "https://command-not-found.com/bash"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install bash",
      "reference": "https://command-not-found.com/bash"
    },
    "centos/yum": {
      "cmd": "sudo yum install bash",
      "reference": "https://command-not-found.com/bash"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install bash",
      "reference": "https://command-not-found.com/bash",
      "dsnap": "bash"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install bash",
      "reference": "https://command-not-found.com/bash"
    }
  },
  "dsnap": {
    "brew": "bash",
    "fedora/dnf": "bash",
    "alpine/apk": "bash",
    "ubuntu/apt": "bash",
    "debian/apt": "bash"
  }
}
