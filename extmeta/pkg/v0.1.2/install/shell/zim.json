{
  "lang": "Shell",
  "homepage": "https://zimfw.sh/",
  "desc": {
    "cn": "Zim 是一个 Zsh 配置框架，它捆绑了插件管理器、有用的模块和各种主题，而不会影响速度",
    "en": "Zim is a Zsh configuration framework that bundles a plugin manager, useful modules and a wide variety of themes, without compromising on speed"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fsSL https://raw.githubusercontent.com/zimfw/install/master/install.zsh | zsh",
      "reference": "https://github.com/zimfw/zimfw?tab=readme-ov-file#automatic-installation"
    },
    "/wget": {
      "cmd": "wget -nv -O - https://raw.githubusercontent.com/zimfw/install/master/install.zsh | zsh",
      "reference": "https://github.com/zimfw/zimfw?tab=readme-ov-file#automatic-installation"
    }
  }
}
