{
  "lang": "rust",
  "homepage": "https://github.com/bootandy/dust",
  "desc": {
    "cn": "`du` 命令的现代化替代品， 用于查看磁盘空间使用情况",
    "en": "A modern alternative to the `du` command, used to view disk space usage"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use dust",
      "reference": "https://github.com/looztra/asdf-dust"
    },
    "darwin/brew": {
      "cmd": "brew install dust",
      "reference": "https://github.com/bootandy/dust#-homebrew-mac-os"
    },
    "linux/brew": {
      "cmd": "brew tap tgotwig/linux-dust\n&& brew install dust\n",
      "reference": "https://github.com/bootandy/dust#-homebrew-linux"
    },
    "debian/deb-get": {
      "cmd": "sudo deb-get install du-dust",
      "reference": "https://github.com/bootandy/dust"
    },
    "ubuntu/deb-get": {
      "cmd": "sudo deb-get install du-dust",
      "reference": "https://github.com/bootandy/dust"
    },
    "debian/pacstall": {
      "cmd": "sudo pacstall -I dust-bin",
      "reference": "https://github.com/bootandy/dust"
    },
    "ubuntu/pacstall": {
      "cmd": "sudo pacstall -I dust-bin",
      "reference": "https://github.com/bootandy/dust"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install dust",
      "reference": "https://github.com/bootandy/dust"
    }
  },
  "dsnap": {
    "brew": "dust"
  }
}
