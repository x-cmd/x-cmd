{
  "lang": "go",
  "homepage": "https://github.com/muesli/duf",
  "desc": {
    "cn": "`df` 命令的现代化替代品， 用于查看磁盘空间使用情况和磁盘文件系统信息",
    "en": "A modern alternative to the `df` command, used to view disk space usage and disk file system information"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use duf",
      "reference": "https://github.com/NeoHsu/asdf-duf"
    },
    "darwin/brew": {
      "cmd": "brew install duf",
      "reference": "https://github.com/muesli/duf"
    },
    "debian/apt": {
      "cmd": "sudo apt install duf",
      "reference": "https://github.com/muesli/duf",
      "dsnap": "duf"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install duf",
      "reference": "https://github.com/muesli/duf",
      "dsnap": "duf"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S duf",
      "reference": "https://github.com/muesli/duf"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge sys-fs/duf",
      "reference": "https://github.com/muesli/duf"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S duf",
      "reference": "https://github.com/muesli/duf"
    },
    "freebsd/pkg": {
      "cmd": "pkg install duf",
      "reference": "https://github.com/muesli/duf"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add duf",
      "reference": "https://github.com/muesli/duf"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -iA nixpkgs.duf",
      "reference": "https://github.com/muesli/duf"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install duf",
      "reference": "https://github.com/muesli/duf"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install duf",
      "reference": "https://github.com/muesli/duf"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install duf",
      "reference": "https://github.com/muesli/duf"
    }
  },
  "dsnap": {
    "brew": "duf",
    "/cargo": "duf",
    "/go": "duf",
    "fedora/dnf": "duf",
    "alpine/apk": "duf",
    "ubuntu/apt": "duf",
    "debian/apt": "duf"
  }
}
