{
  "lang": "Go",
  "homepage": "https://github.com/viktomas/godu",
  "desc": {
    "cn": "用于发现大文件或文件夹的 golang 程序",
    "en": "Simple golang utility helping to discover large files/folders"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap viktomas/taps && rew install godu",
      "reference": "https://github.com/viktomas/godu?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/viktomas/godu@latest",
      "reference": "https://github.com/viktomas/godu?tab=readme-ov-file#installation",
      "dsnap": "github.com/viktomas/godu"
    }
  },
  "dsnap": {
    "brew": "godu",
    "/go": "github.com/viktomas/godu"
  }
}
