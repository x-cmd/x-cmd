{
  "lang": "rust",
  "homepage": "https://github.com/Byron/dua-cli",
  "desc": {
    "cn": "dua（-> Disk Usage Analyzer）是一个可以方便地了解给定目录的磁盘空间使用情况的工具",
    "en": "dua (-> Disk Usage Analyzer) is a tool to conveniently learn about the usage of disk space of a given directory"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install dua-cli",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "darwin/port": {
      "cmd": "sudo port install dua-cli",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install dua-cli",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S dua-cli",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "darwin/curl": {
      "cmd": "curl -LSfs https://raw.githubusercontent.com/Byron/dua-cli/master/ci/install.sh | sh -s -- --git Byron/dua-cli --crate dua --tag v2.17.4",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "linux/curl": {
      "cmd": "curl -LSfs https://raw.githubusercontent.com/Byron/dua-cli/master/ci/install.sh | sh -s -- --git Byron/dua-cli --target x86_64-unknown-linux-musl --crate dua --tag v2.17.4",
      "reference": "https://lib.rs/install/dua-cli"
    },
    "/cargo": {
      "cmd": "cargo install dua-cli",
      "reference": "https://lib.rs/install/dua-cli",
      "dsnap": "dua-cli"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install dua",
      "reference": "https://lib.rs/install/dua-cli"
    }
  },
  "dsnap": {
    "brew": "dua-cli",
    "/cargo": "dua-cli"
  }
}
