{
  "lang": "Go",
  "homepage": "https://github.com/naggie/dstask",
  "desc": {
    "cn": "Dstask 是一款个人任务追踪器，旨在帮助你集中注意力。它类似于 Taskwarrior 但使用 git 来同步而不是特殊协议。",
    "en": "Dstask is a personal task tracker designed to help you focus. It is similar to Taskwarrior but uses git to synchronise instead of a special protocol."
  },
  "rule": {
    "/brew": {
      "cmd": "brew install dstask",
      "reference": "https://github.com/naggie/dstask?tab=readme-ov-file"
    },
    "arch/pacman": {
      "cmd": "pacman -S dstask",
      "reference": "https://github.com/naggie/dstask?tab=readme-ov-file"
    },
    "alpine/apk": {
      "cmd": "apk add dstask",
      "reference": "https://github.com/naggie/dstask?tab=readme-ov-file",
      "dsnap": "dstask"
    },
    "freebsd/pkg": {
      "cmd": "pkg install dstask",
      "reference": "https://github.com/naggie/dstask?tab=readme-ov-file"
    },
    "/nix-shell": {
      "cmd": "nix-shell -p dstask",
      "reference": "https://search.nixos.org/packages?query=dstask"
    }
  },
  "dsnap": {
    "brew": "dstask",
    "/cargo": "dstask",
    "fedora/dnf": "dstask",
    "alpine/apk": "dstask"
  }
}
