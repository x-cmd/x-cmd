{
  "lang": "Rust",
  "homepage": "https://github.com/kdheepak/taskwarrior-tui",
  "desc": {
    "en": "A terminal user interface for taskwarrior",
    "cn": "Taskwarrior的终端用户界面"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install taskwarrior-tui",
      "reference": "https://kdheepak.com/taskwarrior-tui/installation/"
    },
    "arch/pacman": {
      "cmd": "pacman -S taskwarrior-tui",
      "reference": "https://kdheepak.com/taskwarrior-tui/installation/"
    },
    "arch/yay": {
      "cmd": "yay -S taskwarrior-tui-git",
      "reference": "https://kdheepak.com/taskwarrior-tui/installation/"
    },
    "linux/snap": {
      "cmd": "snap install taskwarrior-tui",
      "reference": "https://kdheepak.com/taskwarrior-tui/installation/"
    }
  }
}
