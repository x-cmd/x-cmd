{
  "lang": "Python",
  "homepage": "https://github.com/insanum/gcalcli",
  "desc": {
    "cn": "Google 日历命令行界面",
    "en": "Google Calendar Command Line Interface"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt-get install gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#debianubuntu"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#debianubuntu"
    },
    "void/xbps": {
      "cmd": "xbps-install gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#void-linux"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -i gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#install-using-nix"
    },
    "darwin/brew": {
      "cmd": "brew install gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#install-using-homebrew-macos"
    },
    "/pip": {
      "cmd": "pip install gcalcli",
      "reference": "https://github.com/insanum/gcalcli?tab=readme-ov-file#install-from-pypi",
      "dsnap": "gcalcli"
    }
  },
  "dsnap": {
    "/pip": "gcalcli"
  }
}
