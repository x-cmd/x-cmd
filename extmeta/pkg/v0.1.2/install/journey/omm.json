{
  "lang": "Go",
  "homepage": "https://github.com/dhth/omm",
  "desc": {
    "cn": "一个基于键盘的终端任务管理器。",
    "en": "A keyboard-driven task manager for the terminal."
  },
  "rule": {
    "/brew": {
      "cmd": "brew install dhth/tap/omm",
      "reference": "https://github.com/dhth/omm#-installation"
    },
    "/go": {
      "cmd": "go install github.com/dhth/omm@latest",
      "reference": "https://github.com/dhth/omm#-installation",
      "dsnap": "omm"
    }
  },
  "dsnap": {
    "brew": "omm",
    "/go": "omm"
  }
}
