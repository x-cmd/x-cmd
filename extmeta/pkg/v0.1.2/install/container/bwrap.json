{
  "lang": "c",
  "homepage": "https://github.com/containers/bubblewrap",
  "desc": {
    "cn": "bwrap 是 Flatpak和类似项目使用的低级非特权沙盒工具",
    "en": "bwrap is low-level unprivileged sandboxing tool used by Flatpak and similar projects"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install bubblewrap",
      "reference": "https://formulae.brew.sh/formula/bubblewrap#default"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S bubblewrap",
      "reference": "https://command-not-found.com/bwrap"
    },
    "alpine/apk": {
      "cmd": "sudo apk add bubblewrap",
      "reference": "https://command-not-found.com/bwrap",
      "dsnap": "bubblewrap"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install bubblewrap",
      "reference": "https://command-not-found.com/bwrap",
      "dsnap": "bubblewrap"
    },
    "ubuntu/apt": {
      "cmd": "apt-get install bubblewrap",
      "reference": "https://command-not-found.com/bwrap",
      "dsnap": "bubblewrap"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install bubblewrap",
      "reference": "https://command-not-found.com/bwrap",
      "dsnap": "bubblewrap"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install bubblewrap",
      "reference": "https://command-not-found.com/bwrap"
    },
    "kali/apt": {
      "cmd": "sudo aapt-get install bubblewrap",
      "reference": "https://command-not-found.com/bwrap"
    }
  },
  "dsnap": {
    "brew": "bubblewrap",
    "fedora/dnf": "bubblewrap",
    "alpine/apk": "bubblewrap",
    "ubuntu/apt": "bubblewrap",
    "debian/apt": "bubblewrap"
  }
}
