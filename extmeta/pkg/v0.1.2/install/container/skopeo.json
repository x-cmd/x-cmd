{
  "lang": "go",
  "homepage": "https://github.com/containers/skopeo",
  "desc": {
    "cn": "skopeo 是一个命令行实用程序，可对容器镜像和镜像存储库执行各种操作",
    "en": "skopeo is a command line utility that performs various operations on container images and image repositories"
  },
  "rule": {
    "fedora/dnf": {
      "cmd": "sudo dnf -y install skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#fedora",
      "dsnap": "skopeo"
    },
    "rhel/dnf": {
      "cmd": "sudo dnf -y install skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#rhel--centos-stream--8"
    },
    "centos/dnf": {
      "cmd": "sudo dnf -y install skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#rhel--centos-stream--8"
    },
    "alpine/apk": {
      "cmd": "sudo apk add skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#alpine",
      "dsnap": "skopeo"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#opensuse"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge app-containers/skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#gentoo"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#arch-linux"
    },
    "darwin/brew": {
      "cmd": "brew install skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#macos"
    },
    "/nix-env": {
      "cmd": "nix-env -i skopeo",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#nix--nixos"
    },
    "debian/apt": {
      "cmd": "sudo apt-get update &&\nsudo apt-get -y install skopeo\n",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#debian",
      "dsnap": "skopeo"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get -y update &&\nsudo apt-get -y install skopeo\n",
      "reference": "https://github.com/containers/skopeo/blob/main/install.md#ubuntu",
      "dsnap": "skopeo"
    }
  },
  "dsnap": {
    "brew": "skopeo",
    "fedora/dnf": "skopeo",
    "alpine/apk": "skopeo",
    "ubuntu/apt": "skopeo",
    "debian/apt": "skopeo"
  }
}
