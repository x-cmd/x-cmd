{
  "lang": "go",
  "homepage": "https://docs.docker.com/machine",
  "desc": {
    "cn": "以容器为中心的机器管理",
    "en": "Machine management for a container-centric world"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -O \"https://gitlab-docker-machine-downloads.s3.amazonaws.com/v0.16.2-gitlab.19/docker-machine-Linux-x86_64\"\ncp docker-machine-Linux-x86_64 /usr/local/bin/docker-machine\nchmod +x /usr/local/bin/docker-machine\n",
      "reference": "https://gitlab.com/gitlab-org/ci-cd/docker-machine/-/blob/main/docs/install-machine.md#install-docker-machine"
    },
    "darwin/brew": {
      "cmd": "brew install docker-machine",
      "reference": "https://formulae.brew.sh/formula/docker-machine#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install docker-machine",
      "reference": "https://scoop.sh/#/apps?q=docker-machine"
    }
  },
  "dsnap": null
}
