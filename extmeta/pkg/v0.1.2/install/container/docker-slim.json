{
  "lang": "go",
  "homepage": "https://slimtoolkit.org/",
  "desc": {
    "cn": "优化您的容器体验。让您的容器更优质、更小巧、更安全，并减少工作量",
    "en": "Optimize Your Experience with Containers. Make Your Containers Better, Smaller, More Secure and Do Less to Get There"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use docker-slim",
      "reference": "https://github.com/xataz/asdf-docker-slim"
    },
    "/curl": {
      "cmd": "curl -sL https://raw.githubusercontent.com/slimtoolkit/slim/master/scripts/install-slim.sh | sudo -E bash -",
      "reference": "https://github.com/slimtoolkit/slim?tab=readme-ov-file#scripted-install"
    },
    "darwin/brew": {
      "cmd": "brew install docker-slim",
      "reference": "https://github.com/slimtoolkit/slim?tab=readme-ov-file#homebrew"
    }
  },
  "dsnap": {
    "/go": "docker-slim"
  }
}
