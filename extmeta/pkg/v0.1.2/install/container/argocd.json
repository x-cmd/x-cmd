{
  "lang": "go",
  "homepage": "https://argoproj.github.io/cd/",
  "desc": {
    "cn": "Argo CD 是一个声明性的、针对库伯内特的 gitOps 持续交付工具",
    "en": "Argo CD is a declarative, GitOps continuous delivery tool for Kubernetes"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use argocd",
      "reference": "https://github.com/beardix/asdf-argocd"
    },
    "darwin/brew": {
      "cmd": "brew install argocd",
      "reference": "https://formulae.brew.sh/formula/argocd#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install argocd",
      "reference": "https://scoop.sh/#/apps?q=argocd"
    }
  },
  "dsnap": {
    "/go": "argocd"
  }
}
