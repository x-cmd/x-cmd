{
  "lang": "go",
  "desc": {
    "en": "A simple terminal UI for both docker and docker-compose",
    "cn": "Docker 和 Docker Compose 的多功能终端看板"
  },
  "homepage": "https://github.com/jesseduffield/lazydocker",
  "rule": {
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/jesseduffield/lazydocker/master/scripts/install_update_linux.sh | bash",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/jesseduffield/lazydocker@latest",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation",
      "dsnap": "github.com/jesseduffield/lazydocker@latest"
    },
    "/asdf": {
      "cmd": "x asdf use lazydocker",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install jesseduffield/lazydocker/lazydocker",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install lazydocker",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install lazydocker",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    },
    "arch/yay": {
      "cmd": "yay -S lazydocker",
      "reference": "https://github.com/jesseduffield/lazydocker?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "lazydocker",
    "/go": "github.com/jesseduffield/lazydocker@latest"
  }
}
