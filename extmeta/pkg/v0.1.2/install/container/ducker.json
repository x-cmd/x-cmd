{
  "lang": "Rust",
  "homepage": "https://github.com/robertpsoane/ducker",
  "desc": {
    "en": "A terminal app for managing docker containers, inspired by K9s",
    "cn": "受 K9s 启发的用于管理 docker 容器的终端应用程序"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --locked ducker",
      "reference": "https://github.com/robertpsoane/ducker?tab=readme-ov-file#installation"
    },
    "arch/pacman": {
      "cmd": "pacman -S ducker",
      "reference": "https://github.com/robertpsoane/ducker?tab=readme-ov-file#arch-linux"
    },
    "darwin/brew": {
      "cmd": "brew install ducker",
      "reference": "https://github.com/robertpsoane/ducker?tab=readme-ov-file#brew"
    }
  }
}
