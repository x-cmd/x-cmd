{
  "lang": "Rust",
  "homepage": "https://github.com/mrjackwills/oxker",
  "desc": {
    "cn": "一个简单的 tui 来查看和控制 docker 容器",
    "en": "A simple tui to view & control docker containers"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install oxker",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#cargo",
      "dsnap": "oxker"
    },
    "nix/run": {
      "cmd": "nix run nixpkgs#oxker",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#nix"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p oxker",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#nix"
    },
    "arch/paru": {
      "cmd": "paru -S oxker",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#aur"
    },
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/mrjackwills/oxker/main/install.sh | bash",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#pre-built"
    },
    "/wget": {
      "cmd": "wget https://www.github.com/mrjackwills/oxker/releases/latest/download/oxker_linux_x86_64.tar.gz && \\\ntar xzvf oxker_linux_x86_64.tar.gz oxker && \\\ninstall -Dm 755 oxker -t \"${HOME}/.local/bin\" && \\\nrm oxker_linux_x86_64.tar.gz oxker\n",
      "reference": "https://github.com/mrjackwills/oxker?tab=readme-ov-file#pre-built"
    }
  },
  "dsnap": {
    "/cargo": "oxker"
  }
}
