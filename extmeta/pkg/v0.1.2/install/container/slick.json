{
  "lang": "Go",
  "homepage": "https://github.com/scmmishra/slick-deploy",
  "desc": {
    "cn": "使用 Caddy 以声明方式部署容器的 CLI 工具",
    "en": "CLI tool to declaratively deploy containers with Caddy"
  },
  "rule": {
    "/build": {
      "cmd": "git clone https://github.com/scmmishra/slick-deploy.git \\\n&& cd slick-deploy \\\n&& make install\n",
      "reference": "https://github.com/scmmishra/slick-deploy?tab=readme-ov-file#installing"
    },
    "/curl": {
      "cmd": "curl -fsSL https://raw.githubusercontent.com/scmmishra/slick-deploy/main/scripts/install.sh | bash",
      "reference": "https://github.com/scmmishra/slick-deploy?tab=readme-ov-file#installing"
    }
  },
  "dsnap": {
    "/go": "scmmishra/slick-deploy"
  }
}
