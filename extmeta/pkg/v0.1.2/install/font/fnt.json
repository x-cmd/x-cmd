{
  "lang": "Shell",
  "homepage": "https://github.com/alexmyczko/fnt",
  "desc": {
    "en": "apt for fonts, the missing font manager for macOS/linux",
    "cn": "适用于字体的缺失字体管理器，适用于macOS和Linux"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install fnt",
      "reference": "https://repology.org/project/fnt/versions"
    },
    "arch/pacman": {
      "cmd": "pacman -S fnt",
      "reference": "https://repology.org/project/fnt/versions"
    },
    "ubuntu/apt": {
      "cmd": "apt install fnt",
      "reference": "https://repology.org/project/fnt/versions",
      "dsnap": "fnt"
    },
    "debian/apt": {
      "cmd": "apt install fnt",
      "reference": "https://repology.org/project/fnt/versions",
      "dsnap": "fnt"
    }
  },
  "dsnap": {
    "brew": "fnt",
    "ubuntu/apt": "fnt",
    "debian/apt": "fnt"
  }
}
