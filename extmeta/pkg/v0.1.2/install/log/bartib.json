{
  "lang": "Rust/shell",
  "homepage": "https://github.com/nikolassv/bartib",
  "desc": {
    "en": "Bartib is an easy to use time tracking tool for the command line",
    "cn": "Bartib 是一个易于使用的命令行时间跟踪工具"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install bartib",
      "reference": "https://github.com/nikolassv/bartib?tab=readme-ov-file#with-cargo",
      "dsnap": "bartib"
    },
    "arch/yay": {
      "cmd": "sudo yay -S bartib",
      "reference": "https://github.com/nikolassv/bartib?tab=readme-ov-file#from-the-aur-arch-linux"
    },
    "darwin/brew": {
      "cmd": "brew install bartib",
      "reference": "https://formulae.brew.sh/formula/bartib#default"
    },
    "alpine/apk": {
      "cmd": "sudo apk add bartib",
      "reference": "https://github.com/nikolassv/bartib?tab=readme-ov-file#via-apk-alpine-linux",
      "dsnap": "bartib"
    }
  },
  "dsnap": {
    "brew": "bartib",
    "/cargo": "bartib",
    "alpine/apk": "bartib"
  }
}
