{
  "lang": "rust",
  "homepage": "https://github.com/bensadeh/tailspin",
  "desc": {
    "cn": "日志文件荧光笔",
    "en": "A log file highlighter"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing"
    },
    "/cargo": {
      "cmd": "cargo install tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing",
      "dsnap": "tailspin"
    },
    "arch/pacman": {
      "cmd": "pacman -S tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing"
    },
    "/nix-shell": {
      "cmd": "nix-shell -p tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing"
    },
    "freebsd/pkg": {
      "cmd": "pkg install tailspin",
      "reference": "https://github.com/bensadeh/tailspin?tab=readme-ov-file#installing"
    }
  },
  "dsnap": {
    "brew": "tailspin",
    "/cargo": "tailspin"
  }
}
