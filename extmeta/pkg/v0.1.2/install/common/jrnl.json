{
  "lang": "python",
  "homepage": "https://jrnl.sh",
  "desc": {
    "cn": "无需离开命令行即可收集您的想法和笔记",
    "en": "Collect your thoughts and notes without leaving the command line"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install jrnl",
      "reference": "https://jrnl.sh/en/stable/installation/",
      "dsnap": "jrnl"
    },
    "darwin/brew": {
      "cmd": "brew install jrnl",
      "reference": "https://jrnl.sh/en/stable/installation/"
    },
    "/pipx": {
      "cmd": "pipx install jrnl",
      "reference": "https://jrnl.sh/en/stable/installation/"
    }
  },
  "dsnap": {
    "brew": "jrnl",
    "/pip": "jrnl"
  }
}
