{
  "lang": "Shell",
  "homepage": "https://github.com/kdabir/has",
  "desc": {
    "cn": "has 检查 PATH 上是否存在各种命令行工具，并报告其安装版本",
    "en": "has checks presence of various command line tools on the PATH and reports their installed version"
  },
  "rule": {
    "/curl": {
      "cmd": "alias has=\"curl -sL https://git.io/_has | bash -s\"",
      "reference": "https://github.com/kdabir/has"
    },
    "/asdf": {
      "cmd": "x asdf use has",
      "reference": "https://github.com/sylvainmetayer/asdf-has"
    },
    "darwin/brew": {
      "cmd": "brew install kdabir/tap/has",
      "reference": "https://github.com/kdabir/has?tab=readme-ov-file#homebrew-macos-"
    }
  },
  "dsnap": {
    "brew": "has"
  }
}
