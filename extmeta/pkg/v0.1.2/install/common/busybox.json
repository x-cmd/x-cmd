{
  "lang": "c",
  "homepage": "https://github.com/mirror/busybox",
  "desc": {
    "cn": "一个集成了多个常用命令和工具的单个可执行文件，旨在为嵌入式系统和资源受限的环境提供常用的命令和实用程序",
    "en": "A streamlined tool set that integrates multiple commonly used commands and tools"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install procs",
      "reference": "https://github.com/dalance/procs"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install busybox",
      "reference": "https://command-not-found.com/busybox",
      "dsnap": "busybox"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install busybox",
      "reference": "https://command-not-found.com/busybox",
      "dsnap": "busybox"
    },
    "alpine/apk": {
      "cmd": "sudo apk add busybox",
      "reference": "https://github.com/dalance/procs",
      "dsnap": "busybox"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S busybox",
      "reference": "https://github.com/dalance/procs"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install busybox",
      "reference": "https://command-not-found.com/busybox"
    },
    "centos/yum": {
      "cmd": "sudo yum install busybox",
      "reference": "https://command-not-found.com/busybox"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install busybox",
      "reference": "https://github.com/dalance/procs",
      "dsnap": "busybox"
    }
  },
  "dsnap": {
    "brew": "procs",
    "pacman": "busybox",
    "yum": "busybox",
    "fedora/dnf": "busybox",
    "alpine/apk": "busybox",
    "ubuntu/apt": "busybox",
    "debian/apt": "busybox"
  }
}
