{
  "lang": "rust",
  "desc": {
    "cn": "用于压缩和解压缩各种格式的 CLI 工具",
    "en": "A CLI tool to compress and decompress various formats"
  },
  "homepage": "https://github.com/ouch-org/ouch",
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S ouch",
      "reference": "https://github.com/ouch-org/ouch?tab=readme-ov-file#on-arch-linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install ouch",
      "reference": "https://github.com/ouch-org/ouch?tab=readme-ov-file#on-windows-via-scoop"
    },
    "/cargo": {
      "cmd": "cargo",
      "reference": "https://github.com/ouch-org/ouch?tab=readme-ov-file#from-cratesio",
      "dsnap": "ouch"
    }
  },
  "dsnap": {
    "/cargo": "ouch"
  }
}
