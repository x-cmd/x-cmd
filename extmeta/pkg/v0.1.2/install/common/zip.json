{
  "lang": "c",
  "homepage": "https://infozip.sourceforge.net/Zip.html",
  "desc": {
    "cn": "Zip 是一个压缩和文件打包/归档实用程序",
    "en": "Zip is a compression and file packaging/archive utility"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install zip",
      "reference": "https://formulae.brew.sh/formula/zip#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install zip",
      "reference": "https://command-not-found.com/zip",
      "dsnap": "zip"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install zip",
      "reference": "https://command-not-found.com/zip",
      "dsnap": "zip"
    },
    "alpine/apk": {
      "cmd": "sudo apk add zip",
      "reference": "https://command-not-found.com/zip",
      "dsnap": "zip"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S zip",
      "reference": "https://command-not-found.com/zip"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install zip",
      "reference": "https://command-not-found.com/zip"
    },
    "centos/yum": {
      "cmd": "sudo yum install zip",
      "reference": "https://command-not-found.com/zip"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install zip",
      "reference": "https://command-not-found.com/zip",
      "dsnap": "zip"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install zip",
      "reference": "https://command-not-found.com/zip"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask app-arch/zip",
      "reference": "https://wiki.gentoo.org/wiki/Zip"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install zip",
      "reference": "https://software.opensuse.org/download/package?package=zip&project=Archiving"
    }
  },
  "dsnap": {
    "brew": "zip",
    "yum": "zip",
    "zypper": "zip",
    "emerge": "app-arch/zip",
    "fedora/dnf": "zip",
    "alpine/apk": "zip",
    "ubuntu/apt": "zip",
    "debian/apt": "zip"
  }
}
