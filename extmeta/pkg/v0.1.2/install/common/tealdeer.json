{
  "lang": "Rust",
  "homepage": "https://github.com/dbrgn/tealdeer",
  "desc": {
    "en": "A very fast implementation of tldr in Rust",
    "cn": "用Rust实现的非常快速的tldr版本"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#through-cargo-install"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    },
    "debian/apt": {
      "cmd": "sudo apt install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    },
    "darwin/brew": {
      "cmd": "brew install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    },
    "darwin/port": {
      "cmd": "sudo port install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tealdeer",
      "reference": "https://tealdeer-rs.github.io/tealdeer/installing.html#package-managers"
    }
  }
}
