{
  "lang": "ruby",
  "homepage": "https://www.vagrantup.com/",
  "desc": {
    "en": "Vagrant is the command line utility for managing the lifecycle of virtual machines",
    "cn": "Vagrant 是用于管理虚拟机生命周期的命令行实用程序"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install main/vagrant",
      "reference": "https://scoop.sh/#/apps?q=vagrant"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install vagrant",
      "reference": "https://community.chocolatey.org/packages/vagrant"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Hashicorp.Vagrant",
      "reference": "https://winget.run/pkg/Hashicorp/Vagrant"
    },
    "darwin/brew": {
      "cmd": "brew install --cask vagrant",
      "reference": "https://formulae.brew.sh/cask/vagrant#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install vagrant",
      "reference": "https://command-not-found.com/vagrant"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install vagrant",
      "reference": "https://command-not-found.com/vagrant"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S vagrant",
      "reference": "https://command-not-found.com/vagrant"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install vagrant",
      "reference": "https://command-not-found.com/vagrant"
    },
    "centos/yum": {
      "cmd": "sudo yum install qemu-kvm",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install vagrant",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install vagrant",
      "reference": "https://command-not-found.com/vagrant"
    }
  },
  "dsnap": null
}
