{
  "lang": "c",
  "homepage": "https://www.virtualbox.org/",
  "desc": {
    "cn": "VirtualBox 是一款开源虚拟机软件",
    "en": "VirtualBox is an open-source virtual machine software"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask virtualbox",
      "reference": "https://formulae.brew.sh/cask/virtualbox#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add nonportable && x scoop install extras/portable-virtualbox",
      "reference": "https://scoop.sh/#/apps?q=virtualbox"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install virtualbox",
      "reference": "https://community.chocolatey.org/packages/virtualbox"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Oracle.VirtualBox",
      "reference": "https://winstall.app/apps/Oracle.VirtualBox"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install virtualbox-qt",
      "reference": "https://command-not-found.com/VirtualBox",
      "dsnap": "virtualbox-qt"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install virtualbox-qt",
      "reference": "https://command-not-found.com/VirtualBox",
      "dsnap": "virtualbox-qt"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S virtualbox",
      "reference": "https://command-not-found.com/VirtualBox"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install virtualbox-qt",
      "reference": "https://command-not-found.com/VirtualBox"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in virtualbox",
      "reference": "https://zh.opensuse.org/Virtualbox"
    }
  },
  "dsnap": {
    "brew": "virtualbox",
    "pacman": "virtualbox",
    "zypper": "virtualbox",
    "fedora/dnf": "virtualbox",
    "ubuntu/apt": "virtualbox-qt",
    "debian/apt": "virtualbox-qt"
  }
}
