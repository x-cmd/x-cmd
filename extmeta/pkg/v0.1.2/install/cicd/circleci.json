{
  "lang": "go",
  "homepage": "https://circleci-public.github.io/circleci-cli/",
  "desc": {
    "cn": "circleci 是 CircleCI 的控制台应用",
    "en": "circleci is CircleCI's command-line application"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fLSs https://raw.githubusercontent.com/CircleCI-Public/circleci-cli/main/install.sh | bash",
      "reference": "https://github.com/CircleCI-Public/circleci-cli?tab=readme-ov-file#install-script"
    },
    "darwin/brew": {
      "cmd": "brew install circleci",
      "reference": "https://github.com/CircleCI-Public/circleci-cli?tab=readme-ov-file#homebrew"
    },
    "/snap": {
      "cmd": "sudo snap install circleci",
      "reference": "https://github.com/CircleCI-Public/circleci-cli?tab=readme-ov-file#snap"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install circleci-cli -y",
      "reference": "https://github.com/CircleCI-Public/circleci-cli?tab=readme-ov-file#chocolatey"
    }
  },
  "dsnap": {
    "/go": "circleci"
  }
}
