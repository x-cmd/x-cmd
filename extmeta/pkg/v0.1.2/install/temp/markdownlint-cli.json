{
  "lang": "JavaScript",
  "homepage": "https://github.com/igorshubovych/markdownlint-cli",
  "desc": {
    "cn": "Markdownlint 命令行接口",
    "en": "MarkdownLint Command Line Interface"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g markdownlint-cli",
      "reference": "https://github.com/igorshubovych/markdownlint-cli?tab=readme-ov-file#installation",
      "dsnap": "markdownlint-cli"
    },
    "darwin/brew": {
      "cmd": "brew install markdownlint-cli",
      "reference": "https://github.com/igorshubovych/markdownlint-cli?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "markdownlint-cli",
    "/npm": "markdownlint-cli"
  }
}
