{
  "lang": "Rust",
  "homepage": "https://tldr.sh/tlrc/",
  "desc": {
    "cn": "tlrc 是一个用 Rust 编写的 tldr 客户端",
    "en": "tlrc is a tldr client written in Rust"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#linuxmacos-using-homebrew"
    },
    "linux/brew": {
      "cmd": "brew install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#linuxmacos-using-homebrew"
    },
    "darwin/nix-shell": {
      "cmd": "nix-shell -p tlrc",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=tlrc&from=0&size=50&sort=relevance&type=packages&query=tlrc"
    },
    "linux/nix-shell": {
      "cmd": "nix-shell -p tlrc",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=tlrc&from=0&size=50&sort=relevance&type=packages&query=tlrc"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixos.tlrc",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=tlrc&from=0&size=50&sort=relevance&type=packages&query=tlrc"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#opensuse"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install tldr-pages.tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#windows"
    },
    "darwin/port": {
      "cmd": "sudo port install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#macos-using-macports"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#netbsd"
    },
    "/cargo": {
      "cmd": "cargo install tlrc",
      "reference": "https://github.com/tldr-pages/tlrc?tab=readme-ov-file#from-cratesio",
      "dsnap": "tlrc"
    }
  },
  "dsnap": {
    "brew": "tlrc",
    "/cargo": "tlrc"
  }
}
