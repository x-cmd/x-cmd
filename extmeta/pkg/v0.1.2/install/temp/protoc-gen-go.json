{
  "lang": "Go",
  "homepage": "https://github.com/protocolbuffers/protobuf-go",
  "desc": {
    "cn": "protoc-gen-go 工具是 protoc（protocol buffer 编译器）的编译器插件",
    "en": "The protoc-gen-go tool is a compiler plugin to protoc, the protocol buffer compiler"
  },
  "rule": {
    "/go": {
      "cmd": "go install google.golang.org/protobuf/cmd/protoc-gen-go@latest",
      "reference": "https://protobuf.dev/getting-started/gotutorial/#compiling-protocol-buffers",
      "dsnap": "google.golang.org/protobuf/cmd/protoc-gen-go"
    }
  },
  "dsnap": {
    "/go": "google.golang.org/protobuf/cmd/protoc-gen-go"
  }
}
