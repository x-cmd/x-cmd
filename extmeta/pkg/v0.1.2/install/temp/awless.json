{
  "lang": "Go",
  "homepage": "https://github.com/wallix/awless",
  "desc": {
    "cn": "awless 是一个功能强大、创新且小型的命令行界面 (CLI)，用于管理 Amazon Web Services",
    "en": "awless is a powerful, innovative and small surface command line interface (CLI) to manage Amazon Web Services"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap wallix/awless && brew install awless",
      "reference": "https://github.com/wallix/awless?tab=readme-ov-file#install"
    },
    "/curl": {
      "cmd": "curl -sS https://raw.githubusercontent.com/wallix/awless/master/getawless.sh | bash",
      "reference": "https://github.com/wallix/awless?tab=readme-ov-file#install"
    },
    "/go": {
      "cmd": "go get -u github.com/wallix/awless",
      "reference": "https://github.com/wallix/awless?tab=readme-ov-file#install",
      "dsnap": "awless"
    }
  },
  "dsnap": {
    "brew": "awless",
    "/go": "awless"
  }
}
