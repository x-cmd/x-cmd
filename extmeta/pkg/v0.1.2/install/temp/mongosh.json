{
  "desc": {
    "cn": "用于与 MongoDB 数据库交互的 CLI",
    "en": "A CLI for interacting with MongoDB databases"
  },
  "homepage": "https://github.com/mongodb-js/mongosh",
  "lang": "node",
  "rule": {
    "darwin/brew": {
      "cmd": "brew install mongosh",
      "reference": "https://www.mongodb.com/docs/mongodb-shell/install"
    },
    "/npm": {
      "cmd": "npm install -g mongosh",
      "reference": "https://www.npmjs.com/package/mongodb",
      "dsnap": "mongosh"
    }
  },
  "dsnap": {
    "brew": "mongosh",
    "/npm": "mongosh"
  }
}
