{
  "lang": "python",
  "homepage": "https://github.com/laixintao/iredis",
  "desc": {
    "cn": "Redis 的终端客户端，具有自动完成和语法功能突出显示",
    "en": "IRedis is a terminal client for redis with auto-completion and syntax highlighting."
  },
  "rule": {
    "/pip": {
      "cmd": "pip install iredis",
      "reference": "https://github.com/laixintao/iredis#pip",
      "dsnap": "iredis"
    },
    "darwin/brew": {
      "cmd": "brew install iredis",
      "reference": "https://formulae.brew.sh/formula/iredis#default"
    },
    "linux/apt": {
      "cmd": "sudo apt install iredis",
      "reference": "https://github.com/laixintao/iredis#linux"
    },
    "/wget": {
      "cmd": "wget  https://github.com/laixintao/iredis/releases/latest/download/iredis.tar.gz \\\n&& tar -xzf iredis.tar.gz \\\n&& ./iredis\n",
      "reference": "https://github.com/laixintao/iredis#download-binary"
    }
  },
  "dsnap": {
    "brew": "iredis",
    "/pip": "iredis"
  }
}
