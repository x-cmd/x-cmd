{
  "lang": "C",
  "homepage": "https://redis.io/",
  "desc": {
    "cn": "Redis 是一个开源(BSD 许可)的内存中数据结构存储，用作数据库、缓存、消息代理和流引擎",
    "en": "Redis is an open source (BSD licensed), in-memory data structure store used as a database, cache, message broker, and streaming engine"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use redis",
      "reference": "https://github.com/smashedtoatoms/asdf-redis"
    },
    "darwin/brew": {
      "cmd": "brew install redis",
      "reference": "https://formulae.brew.sh/formula/redis#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install redis-tools",
      "reference": "https://command-not-found.com/redis-cli",
      "dsnap": "redis-tools"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install redis-tools",
      "reference": "https://command-not-found.com/redis-cli",
      "dsnap": "redis-tools"
    },
    "alpine/apk": {
      "cmd": "sudo apk add redis",
      "reference": "https://command-not-found.com/redis-cli",
      "dsnap": "redis"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S redis",
      "reference": "https://command-not-found.com/redis-cli"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install redis-tools",
      "reference": "https://command-not-found.com/redis-cli"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install redis",
      "reference": "https://command-not-found.com/redis-cli",
      "dsnap": "redis"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install redis-tools",
      "reference": "https://command-not-found.com/redis-cli"
    }
  },
  "dsnap": {
    "brew": "redis",
    "pacman": "redis",
    "fedora/dnf": "redis",
    "alpine/apk": "redis",
    "ubuntu/apt": "redis-tools",
    "debian/apt": "redis-tools"
  }
}
