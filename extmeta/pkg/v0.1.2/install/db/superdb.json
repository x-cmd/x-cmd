{
  "lang": "Go",
  "homepage": "https://superdb.org/",
  "desc": {
    "cn": "SuperDB 是一个新的分析型数据库，支持关系表和 JSON 数据结构，并且在这两者之间提供平等的支持。",
    "en": "SuperDB is a new analytics database that supports relational tables and JSON on an equal footing."
  },
  "X": null,
  "rule": {
    "darwin/brew": {
      "cmd": "brew install brimdata/tap/super",
      "reference": "https://superdb.org/docs/install/#homebrew"
    },
    "linux/brew": {
      "cmd": "brew install brimdata/tap/super",
      "reference": "https://superdb.org/docs/install/#homebrew"
    },
    "/go": {
      "cmd": "go install github.com/brimdata/super/cmd/super@latest",
      "reference": "https://superdb.org/docs/install/#building-from-source"
    }
  }
}
