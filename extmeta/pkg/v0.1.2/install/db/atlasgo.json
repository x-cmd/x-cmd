{
  "lang": "go",
  "homepage": "https://sparanoid.com/lab/7z/",
  "desc": {
    "cn": "Atlas 是使用现代 DevOps 原则管理和迁移数据库模式的语言无关工具",
    "en": "Atlas is a language-agnostic tool for managing and migrating database schemas using modern DevOps principles"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sSf https://atlasgo.sh | sh",
      "reference": "https://github.com/ariga/atlas?tab=readme-ov-file#quick-installation"
    },
    "darwin/brew": {
      "cmd": "brew install ariga/tap/atlas",
      "reference": "https://github.com/ariga/atlas?tab=readme-ov-file#quick-installation"
    },
    "/npm": {
      "cmd": "npx @ariga/atlas",
      "reference": "https://github.com/ariga/atlas?tab=readme-ov-file#quick-installation"
    }
  },
  "dsnap": {
    "/go": "atlas"
  }
}
