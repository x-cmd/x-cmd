{
  "lang": "c++",
  "homepage": "http://www.duckdb.org/",
  "desc": {
    "cn": "DuckDB 是一个 in-process 数据库管理系统",
    "en": "DuckDB is an in-process SQL OLAP database management system"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install duckdb",
      "reference": "https://duckdb.org/docs/installation/?version=latest&environment=cli&installer=binary&platform=macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install duckdb",
      "reference": "https://scoop.sh/#/apps?q=duckdb"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install DuckDB.cli",
      "reference": "https://duckdb.org/docs/installation/?version=latest&environment=cli&installer=binary&platform=winhttps://duckdb.org/docs/installation/?version=latest&environment=cli&installer=binary&platform=win"
    }
  }
}
