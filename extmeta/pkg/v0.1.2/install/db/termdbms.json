{
  "lang": "Go",
  "homepage": "https://github.com/mathaou/termdbms",
  "desc": {
    "en": "A TUI for viewing and editing databases",
    "cn": "用于查看和编辑数据库的图形用户界面（TUI）"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/mathaou/termdbms@latest",
      "reference": "https://github.com/mathaou/termdbms?tab=readme-ov-file#installation-instructions"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S termdbms-git",
      "reference": "https://github.com/mathaou/termdbms?tab=readme-ov-file#installation-instructions"
    },
    "arch/yay": {
      "cmd": "yay -S termdbms-git",
      "reference": "https://github.com/mathaou/termdbms?tab=readme-ov-file#installation-instructions"
    }
  }
}
