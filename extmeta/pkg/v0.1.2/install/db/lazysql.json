{
  "lang": "Go",
  "homepage": "https://github.com/jorgerojas26/lazysql",
  "desc": {
    "cn": "用 Go 编写的跨平台 TUI 数据库管理工具",
    "en": "A cross-platform TUI database management tool written in Go"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap jorgerojas26/lazysql\nbrew install lazysql\n",
      "reference": "https://github.com/jorgerojas26/lazysql?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/jorgerojas26/lazysql@latest",
      "reference": "https://github.com/jorgerojas26/lazysql?tab=readme-ov-file#installation"
    },
    "arch/paru": {
      "cmd": "paru -S lazysql",
      "reference": "https://github.com/jorgerojas26/lazysql?tab=readme-ov-file#installation"
    },
    "arch/yay": {
      "cmd": "yay -S lazysql",
      "reference": "https://github.com/jorgerojas26/lazysql?tab=readme-ov-file#installation"
    }
  }
}
