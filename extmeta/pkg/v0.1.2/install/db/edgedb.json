{
  "lang": "python",
  "homepage": "https://www.edgedb.com/",
  "desc": {
    "cn": "EdgeDB 是一个开源数据库，被设计成 SQL 和关系范式的精神续作",
    "en": "EdgeDB is an open-source database designed as a spiritual successor to SQL and the relational paradigm"
  },
  "rule": {
    "/curl": {
      "cmd": "curl --proto '=https' --tlsv1.2 -sSf https://sh.edgedb.com | sh",
      "reference": "https://www.edgedb.com/docs/cli/index"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install edgedb",
      "reference": "https://scoop.sh/#/apps?q=edgedb"
    },
    "win/powershell": {
      "cmd": "x pwsh iwr https://ps1.edgedb.com -useb | iex",
      "reference": "https://www.edgedb.com/docs/cli/index"
    }
  },
  "dsnap": null
}
