{
  "lang": "Rust",
  "homepage": "https://codeberg.org/lukeflo/bibiman",
  "desc": {
    "cn": "用于处理 BibLaTeX 数据库的简单 TUI",
    "en": "A simple TUI for handling your BibLaTeX database."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install bibiman",
      "reference": "https://codeberg.org/lukeflo/bibiman",
      "dsnap": "bibiman"
    },
    "arch/paru": {
      "cmd": "yay -S bibiman",
      "reference": "https://codeberg.org/lukeflo/bibiman"
    },
    "/nix-env": {
      "cmd": "nix-env -iA bibiman",
      "reference": "https://codeberg.org/lukeflo/bibiman"
    }
  },
  "dsnap": {
    "/cargo": "bibiman"
  }
}
