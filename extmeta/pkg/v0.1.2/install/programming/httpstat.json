{
  "lang": "Python",
  "homepage": "https://github.com/reorx/httpstat",
  "desc": {
    "cn": "httpstat 是一个命令行工具，可在终端中以可视化方式显示 `curl` 请求的统计信息。",
    "en": "httpstat is a CLI tool that displays curl request statistics in a visually in the terminal."
  },
  "rule": {
    "/wet": {
      "cmd": "wget https://raw.githubusercontent.com/reorx/httpstat/master/httpstat.py",
      "reference": "https://github.com/reorx/httpstat?tab=readme-ov-file#installation"
    },
    "/pip": {
      "cmd": "pip install httpstat",
      "reference": "https://github.com/reorx/httpstat?tab=readme-ov-file#installation",
      "dsnap": "httpstat"
    },
    "darwin/brew": {
      "cmd": "brew install httpstat",
      "reference": "https://github.com/reorx/httpstat?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/pip": "httpstat"
  }
}
