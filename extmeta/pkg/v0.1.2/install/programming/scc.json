{
  "lang": "go",
  "desc": {
    "cn": "一个非常快速、准确的代码计数器",
    "en": "A very fast, accurate code counter"
  },
  "homepage": "https://github.com/boyter/scc",
  "rule": {
    "/go": {
      "cmd": "go install github.com/boyter/scc@master",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#go-get"
    },
    "/snap": {
      "cmd": "sudo snap install scc",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#snap"
    },
    "darwin/port": {
      "cmd": "sudo port install scc",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#macports"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install scc",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#scoop"
    },
    "(win|wsl2)/choco": {
      "cmd": "choco install scc",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#chocolatey"
    },
    "freebsd/pkg": {
      "cmd": "pkg install scc",
      "reference": "https://github.com/boyter/scc?tab=readme-ov-file#freebsd"
    }
  }
}
