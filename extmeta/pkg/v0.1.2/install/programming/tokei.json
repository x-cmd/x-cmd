{
  "lang": "rust",
  "homepage": "https://github.com/XAMPPRocky/tokei",
  "desc": {
    "cn": "tokei 是一个用 Rust 编写的代码行数统计工具",
    "en": "Tokei is a line count tool written in Rust"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use tokei",
      "reference": "https://github.com/gasuketsu/asdf-tokei"
    },
    "darwin/brew": {
      "cmd": "brew install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "alpine/apk": {
      "cmd": "sudo apk add tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -i tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install XAMPPRocky.tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    },
    "/cargo": {
      "cmd": "cargo install tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation",
      "dsnap": "tokei"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge tokei",
      "reference": "https://github.com/XAMPPRocky/tokei#installation"
    }
  },
  "dsnap": {
    "/cargo": "tokei"
  }
}
