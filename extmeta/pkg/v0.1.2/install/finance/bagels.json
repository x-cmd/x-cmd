{
  "lang": "Python",
  "homepage": "https://github.com/EnhancedJax/Bagels",
  "desc": {
    "cn": "一个强大的终端费用跟踪器，可在您的终端中使用。",
    "en": "A powerful expense tracker that lives in your terminal."
  },
  "rule": {
    "/uv": {
      "cmd": "uv tool install --python 3.13 bagels",
      "reference": "https://github.com/EnhancedJax/Bagels#unix--macos"
    },
    "darwin/brew": {
      "cmd": "brew install bagels",
      "reference": "https://github.com/EnhancedJax/Bagels?tab=readme-ov-file#-installation"
    },
    "/pipx": {
      "cmd": "pipx install bagels",
      "reference": "https://github.com/EnhancedJax/Bagels#windows"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge bagels",
      "reference": "https://github.com/EnhancedJax/Bagels#windows"
    }
  }
}
