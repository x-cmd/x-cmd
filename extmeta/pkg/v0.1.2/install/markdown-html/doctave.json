{
  "lang": "rust",
  "homepage": "https://github.com/Doctave/doctave",
  "desc": {
    "cn": "Doctave 是一个固执己见的文档站点生成器，可以将 Markdown 文件转换为漂亮的开发人员文档站点",
    "en": "Doctave is an opinionated documentation site generator that converts your Markdown files into a beautiful developer documentation site"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install doctave/doctave/doctave",
      "reference": "https://github.com/Doctave/doctave?tab=readme-ov-file#homebrew"
    },
    "/cargo": {
      "cmd": "cargo install --git https://github.com/Doctave/doctave --tag 0.4.2",
      "reference": "https://github.com/Doctave/doctave?tab=readme-ov-file#cargo-rust-package-manager"
    }
  }
}
