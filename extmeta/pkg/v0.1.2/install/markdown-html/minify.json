{
  "lang": "go",
  "homepage": "https://github.com/tdewolff/minify",
  "desc": {
    "cn": "Minify 是一个用 Go 编写的迷你包。它提供了 HTML5、 CSS3、 JS、 JSON、 SVG 和 XML 小型化程序以及实现任何其他小型化程序的接口",
    "en": "Minify is a minifier package written in Go. It provides HTML5, CSS3, JS, JSON, SVG and XML minifiers and an interface to implement any other minifier"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use minify",
      "reference": "https://github.com/axilleas/asdf-minify"
    },
    "/npm": {
      "cmd": "npm install -g minify",
      "reference": "https://www.npmjs.com/package/minify",
      "dsnap": "minify"
    },
    "/pip": {
      "cmd": "pip install minify",
      "reference": "https://pypi.org/project/minify/",
      "dsnap": "minify"
    },
    "darwin/brew": {
      "cmd": "brew install tdewolff/tap/minify",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#macos"
    },
    "freebsd/pkg": {
      "cmd": "pkg install minify",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#freebsd"
    },
    "alpine/apk": {
      "cmd": "apk add minify",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#alpine-linux",
      "dsnap": "minify"
    },
    "arch/yay": {
      "cmd": "yay -S minify",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#arch-linux"
    },
    "/docker": {
      "cmd": "docker pull tdewolff/minify",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#docker"
    },
    "/go": {
      "cmd": "mkdir $HOME/src\ncd $HOME/src\ngit clone https://github.com/tdewolff/minify.git\ncd minify\ngo install ./cmd/minify\nsource minify_bash_tab_completion\n",
      "reference": "https://github.com/tdewolff/minify?tab=readme-ov-file#installation",
      "dsnap": "minify"
    },
    "/build": {
      "cmd": "mkdir $HOME/src\ncd $HOME/src\ngit clone https://github.com/tdewolff/minify.git\ncd minify\nmake install\n",
      "reference": "https://github.com/tdewolff/minify/tree/master/cmd/minify#installation"
    }
  },
  "dsnap": {
    "brew": "tdewolff/tap/minify",
    "docker": "tdewolff/minify",
    "/pip": "minify",
    "/npm": "minify",
    "/go": "minify",
    "alpine/apk": "minify"
  }
}
