{
  "lang": "rust",
  "desc": {
    "en": "CLI tool for saving complete web pages as a single HTML file",
    "cn": "用于将完整的网页保存为单个 HTML 文件的 CLI 工具"
  },
  "homepage": "https://github.com/y2z/monolith",
  "rule": {
    "/cargo": {
      "cmd": "cargo install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation",
      "dsnap": "monolith"
    },
    "darwin/brew": {
      "cmd": "brew install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "port install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "/snap": {
      "cmd": "snap install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "guixos/guix": {
      "cmd": "guix install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "arch/yay": {
      "cmd": "yay monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "alpine/apk": {
      "cmd": "apk add monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install monolith",
      "reference": "https://github.com/y2z/monolith?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "monolith",
    "/cargo": "monolith"
  }
}
