{
  "lang": "Python",
  "homepage": "https://github.com/Textualize/frogmouth",
  "desc": {
    "cn": "使用 Textual 构建的终端 Markdown 查看器/浏览器",
    "en": "A Markdown viewer/browser for your terminal, built with Textual"
  },
  "rule": {
    "/pipx": {
      "cmd": "pipx install frogmouth",
      "reference": "https://github.com/Textualize/frogmouth?tab=readme-ov-file#installing"
    },
    "/pip": {
      "cmd": "pip install frogmouth",
      "reference": "https://github.com/Textualize/frogmouth?tab=readme-ov-file#installing",
      "dsnap": "frogmouth"
    },
    "darwin/brew": {
      "cmd": "brew install textualize/homebrew/frogmouth",
      "reference": "https://github.com/Textualize/frogmouth?tab=readme-ov-file#installing"
    }
  },
  "dsnap": {
    "brew": "frogmouth",
    "/pip": "frogmouth"
  }
}
