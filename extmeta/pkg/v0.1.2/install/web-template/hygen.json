{
  "homepage": "https://github.com/jondot/hygen#readme",
  "desc": {
    "en": "hygen is the simple, fast, and scalable code generator that lives in your project.",
    "cn": "hygen 是简单、快速、可扩展的代码生成器，它存在于您的项目中。"
  },
  "lang": "node",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use hygen",
      "reference": "https://github.com/brentjanderson/asdf-hygen"
    },
    "/npm": {
      "cmd": "npm i -g hygen",
      "reference": "https://github.com/jondot/hygen?tab=readme-ov-file#quick-start",
      "dsnap": "hygen"
    },
    "darwin/brew": {
      "cmd": "brew tap jondot/tap\n&& brew install hygen\n",
      "reference": "https://github.com/jondot/hygen?tab=readme-ov-file#quick-start"
    }
  },
  "dsnap": {
    "brew": "hygen",
    "/npm": "hygen"
  }
}
