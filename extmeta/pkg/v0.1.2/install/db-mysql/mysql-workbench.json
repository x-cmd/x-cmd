{
  "lang": "swift",
  "homepage": "https://github.com/mxcl/Workbench",
  "desc": {
    "cn": "Workbench 是一个可视化的数据库建模、管理和排队工具",
    "en": "Workbench is a visual database modeling, administration and queuing tool"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask workbench",
      "reference": "https://formulae.brew.sh/cask/workbench#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install mysql-workbench",
      "reference": "https://command-not-found.com/mysql-workbench",
      "dsnap": "mysql-workbench"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install mysql-workbench",
      "reference": "https://command-not-found.com/mysql-workbench",
      "dsnap": "mysql-workbench"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mysql-workbench",
      "reference": "https://command-not-found.com/mysql-workbench"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install mysql-workbench",
      "reference": "https://command-not-found.com/mysql-workbench"
    }
  },
  "dsnap": {
    "brew": "workbench",
    "pacman": "mysql-workbench",
    "fedora/dnf": "mysql-workbench",
    "alpine/apk": "mysql-workbench",
    "ubuntu/apt": "mysql-workbench",
    "debian/apt": "mysql-workbench"
  }
}
