{
  "lang": "c++",
  "homepage": "https://www.csie.ntu.edu.tw/~cjlin/liblinear/",
  "desc": {
    "en": "LIBLINEAR is a linear classifier for data with millions of instances and features",
    "cn": "Liblinear 是具有数百万个实例和功能的数据的线性分类器"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install -U liblinear-official",
      "reference": "https://www.checkov.io/1.Welcome/Quick%20Start.html#install-checkov-from-pypi"
    },
    "darwin/brew": {
      "cmd": "brew install liblinear",
      "reference": "https://formulae.brew.sh/formula/liblinear#default"
    }
  }
}
