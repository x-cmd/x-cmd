{
  "lang": "c",
  "homepage": "https://www.capstone-engine.org/",
  "desc": {
    "cn": "Capstone 是一个轻量级的多平台、多体系结构的拆卸框架",
    "en": "Capstone is a lightweight multi-platform, multi-architecture disassembly framework"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install capstone",
      "reference": "https://formulae.brew.sh/formula/capstone#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install ecapstone",
      "reference": "https://scoop.sh/#/apps?q=capstone"
    }
  },
  "dsnap": {
    "brew": "capstone"
  }
}
