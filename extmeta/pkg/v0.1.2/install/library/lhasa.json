{
  "lang": "C",
  "homepage": "https://fragglet.github.io/lhasa/",
  "desc": {
    "cn": "Lhasa 是一个用于解析 LHA（.lzh）档案的库，也是 Unix LHA 工具的免费替代品。",
    "en": "Lhasa is a library for parsing LHA (.lzh) archives and a free replacement for the Unix LHA tool."
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install lhasa",
      "reference": "https://fragglet.github.io/lhasa/",
      "dsnap": "lhasa"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install lhasa",
      "reference": "https://fragglet.github.io/lhasa/",
      "dsnap": "lhasa"
    },
    "freebsd/pkg": {
      "cmd": "pkg install archivers/lhasa",
      "reference": "https://fragglet.github.io/lhasa/"
    },
    "darwin/brew": {
      "cmd": "brew install lhasa",
      "reference": "https://fragglet.github.io/lhasa/"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install lhasa",
      "reference": "https://fragglet.github.io/lhasa/"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install lhasa",
      "reference": "https://fragglet.github.io/lhasa/"
    },
    "void/xbps-install": {
      "cmd": "sudo xbps-install lhasa",
      "reference": "https://fragglet.github.io/lhasa/"
    }
  },
  "dsnap": {
    "brew": "lhasa",
    "/cargo": "lhasa",
    "fedora/dnf": "lhasa",
    "alpine/apk": "lhasa",
    "ubuntu/apt": "lhasa",
    "debian/apt": "lhasa"
  }
}
