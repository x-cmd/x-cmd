{
  "homepage": "https://github.com/Kotlin/kotlin-interactive-shell/",
  "desc": {
    "cn": "Kotlin 语言的交互式 Shell",
    "en": "Kotlin Language Interactive Shell"
  },
  "lang": "Ki",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use ki",
      "reference": "https://github.com/comdotlinux/asdf-ki"
    },
    "/sdk": {
      "cmd": "sdk install ki",
      "reference": "https://github.com/Kotlin/kotlin-interactive-shell#sdkman"
    },
    "darwin/brew": {
      "cmd": "brew install ki",
      "reference": "https://formulae.brew.sh/formula/ki#default"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install ki-shell",
      "reference": "https://github.com/Kotlin/kotlin-interactive-shell#macports"
    },
    "arch/trizen": {
      "cmd": "sudo trizen -S ki-shell-bin",
      "reference": "https://github.com/Kotlin/kotlin-interactive-shell#arch-linux"
    }
  },
  "dsnap": {
    "brew": "ki"
  }
}
