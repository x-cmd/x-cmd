{
  "lang": "java",
  "homepage": "https://asciidoctor.org/",
  "desc": {
    "cn": "Asciidoctorj 是在 JVM 上运行 Asciidoctor 的官方图书馆",
    "en": "AsciidoctorJ is the official library for running Asciidoctor on the JVM"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use asciidoctorj",
      "reference": "https://github.com/gliwka/asdf-asciidoctorj"
    },
    "darwin/brew": {
      "cmd": "brew install asciidoctorj",
      "reference": "https://formulae.brew.sh/formula/asciidoctorj#default"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install asciidoctorj",
      "reference": "https://github.com/asciidoctor/asciidoctorj#windows-installation"
    }
  },
  "dsnap": {
    "asdf": "asciidoctorj"
  }
}
