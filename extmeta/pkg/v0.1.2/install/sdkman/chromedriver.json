{
  "lang": "node",
  "homepage": "https://chromedriver.chromium.org/",
  "desc": {
    "cn": "WebDriver 是用于在许多浏览器中对 WebApp 进行自动测试的开源工具",
    "en": "WebDriver is an open source tool for automated testing of webapps across many browsers"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use chromedriver",
      "reference": "https://github.com/schinckel/asdf-chromedriver"
    },
    "darwin/brew": {
      "cmd": "brew install --cask chromedriver",
      "reference": "https://github.com/SeleniumHQ/selenium/wiki/ChromeDriver/01fde32d0ed245141e24151f83b7c2db31d596a4#quick-installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install chromedriver",
      "reference": "https://scoop.sh/#/apps?q=chromedriver"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install chromedriver",
      "reference": "https://community.chocolatey.org/packages/chromedriver"
    },
    "/apt": {
      "cmd": "sudo apt-get install chromium-driver",
      "reference": "https://github.com/SeleniumHQ/selenium/wiki/ChromeDriver/01fde32d0ed245141e24151f83b7c2db31d596a4#quick-installation"
    },
    "darwin/webi": {
      "cmd": "curl -sS https://webi.sh/chromedriver | sh",
      "reference": "https://webinstall.dev/chromedriver/"
    },
    "linux/webi": {
      "cmd": "curl -sS https://webi.sh/chromedriver | sh",
      "reference": "https://webinstall.dev/chromedriver/"
    },
    "win/webi": {
      "cmd": "curl.exe https://webi.ms/chromedriver | powershell",
      "reference": "https://webinstall.dev/chromedriver/"
    }
  },
  "dsnap": {
    "brew": "chromedriver"
  }
}
