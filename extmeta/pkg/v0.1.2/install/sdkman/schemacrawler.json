{
  "lang": "Java",
  "homepage": "https://github.com/schemacrawler/SchemaCrawler",
  "desc": {
    "cn": "SchemaCrawler 是一款免费的数据库架构发现和理解工具",
    "en": "SchemaCrawler is a free database schema discovery and comprehension tool"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use schemacrawler",
      "reference": "https://github.com/davidecavestro/asdf-schemacrawler"
    },
    "/sdk": {
      "cmd": "sdk install schemacrawler",
      "reference": "https://www.schemacrawler.com/downloads.html"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install schemacrawler",
      "reference": "https://www.schemacrawler.com/downloads.html"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install schemacrawler",
      "reference": "https://www.schemacrawler.com/downloads.html"
    }
  },
  "dsnap": null
}
