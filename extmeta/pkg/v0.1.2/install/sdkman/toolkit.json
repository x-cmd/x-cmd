{
  "lang": "Java",
  "homepage": "https://iot.mushuwei.cn/#/toolkit/",
  "desc": {
    "cn": "toolkit 是一款客户端命令行工具，支持多种物联网协议",
    "en": "toolkit is a client-side command line tool that supports multiple iot protocols"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install iot-technology/tap/toolkit",
      "reference": "https://github.com/IoT-Technology/IoT-Toolkit?tab=readme-ov-file#macos"
    },
    "linux/sdk": {
      "cmd": "sdk install toolkit",
      "reference": "https://github.com/IoT-Technology/IoT-Toolkit?tab=readme-ov-file#linux"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install toolkit",
      "reference": "https://github.com/IoT-Technology/IoT-Toolkit?tab=readme-ov-file#windows"
    }
  },
  "dsnap": {
    "brew": "toolkit"
  }
}
