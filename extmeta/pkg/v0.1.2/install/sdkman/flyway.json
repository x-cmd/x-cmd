{
  "lang": "Java",
  "homepage": "https://github.com/flyway/flyway",
  "desc": {
    "cn": "自动化数据库迁移",
    "en": "Automated database migration"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use flyway",
      "reference": "https://github.com/junminahn/asdf-flyway"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install main/flyway",
      "reference": "https://scoop.sh/#/apps?q=flyway"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install flyway.commandline",
      "reference": "https://community.chocolatey.org/packages/flyway.commandline"
    },
    "darwin/brew": {
      "cmd": "brew install flyway",
      "reference": "https://formulae.brew.sh/formula/flyway#default"
    }
  },
  "dsnap": {
    "brew": "flyway"
  }
}
