{
  "lang": "java",
  "homepage": "https://ant.apache.org/bindownload.cgi",
  "desc": {
    "cn": "Java 构建工具",
    "en": "Java build tool"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use ant",
      "reference": "https://github.com/jackboespflug/asdf-ant"
    },
    "darwin/brew": {
      "cmd": "brew install ant",
      "reference": "https://formulae.brew.sh/formula/ant#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install ant",
      "reference": "https://command-not-found.com/ant"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install ant",
      "reference": "https://command-not-found.com/ant"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S ant",
      "reference": "https://command-not-found.com/ant"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install ant",
      "reference": "https://command-not-found.com/ant"
    },
    "centos/yum": {
      "cmd": "sudo yum install ant",
      "reference": "https://command-not-found.com/ant"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install ant",
      "reference": "https://command-not-found.com/ant"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install ant",
      "reference": "https://command-not-found.com/ant"
    }
  }
}
