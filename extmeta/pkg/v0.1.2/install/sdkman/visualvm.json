{
  "lang": "java",
  "homepage": "https://visualvm.github.io/index.html",
  "desc": {
    "cn": "VisualVM 是集成命令行 JDK 工具和轻量级分析功能的可视化工具",
    "en": "VisualVM is a visual tool integrating commandline JDK tools and lightweight profiling capabilities"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask visualvm",
      "reference": "https://formulae.brew.sh/cask/visualvm#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install visualvm",
      "reference": "https://command-not-found.com/visualvm",
      "dsnap": "visualvm"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install visualvm",
      "reference": "https://command-not-found.com/visualvm",
      "dsnap": "visualvm"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S visualvm",
      "reference": "https://command-not-found.com/visualvm"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install visualvm",
      "reference": "https://command-not-found.com/visualvm"
    }
  },
  "dsnap": {
    "brew": "visualvm",
    "pacman": "visualvm",
    "ubuntu/apt": "visualvm",
    "debian/apt": "visualvm"
  }
}
