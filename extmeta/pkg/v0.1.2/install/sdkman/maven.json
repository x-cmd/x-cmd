{
  "homepage": "https://maven.apache.org/download.cgi",
  "desc": {
    "cn": "基于 java 的项目管理器",
    "en": "Java-based project management"
  },
  "lang": "java",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use maven",
