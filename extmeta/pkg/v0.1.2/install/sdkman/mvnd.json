{
  "homepage": "https://github.com/apache/maven-mvnd/releases/",
  "desc": {
    "cn": "使用 Gradle 和 Takari 已知的技术提供更快的 Maven 构建",
    "en": "providing faster Maven builds using techniques known from Gradle and Takari"
  },
  "lang": "java",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use mvnd",
      "reference": "https://github.com/joschi/asdf-mvnd"
    },
    "/sdk": {
      "cmd": "sdk install mvnd",
      "reference": "https://github.com/apache/maven-mvnd#install-using-sdkman"
    },
    "darwin/brew": {
      "cmd": "brew install mvndaemon/homebrew-mvnd/mvnd",
      "reference": "https://github.com/apache/maven-mvnd#install-using-homebrew"
    }
  },
  "dsnap": {
    "brew": "mvnd"
  }
}
