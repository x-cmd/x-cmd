{
  "lang": "Java",
  "homepage": "https://github.com/mthmulders/mcs",
  "desc": {
    "cn": "Maven Central Search，或简称 mcs，是一个小型 CLI，用于从命令行查询 Maven Central",
    "en": "Maven Central Search, or mcs for short, is a small CLI to query Maven Central from your command line"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install mthmulders/tap/mcs",
      "reference": "https://github.com/mthmulders/mcs?tab=readme-ov-file#installation"
    },
    "/snap": {
      "cmd": "sudo snap install maven-central-search",
      "reference": "https://github.com/mthmulders/mcs?tab=readme-ov-file#installation"
    },
    "/sdk": {
      "cmd": "sdk install mcs",
      "reference": "https://github.com/mthmulders/mcs?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install mcs",
      "reference": "https://github.com/mthmulders/mcs?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "mcs",
    "snap": "maven-central-search",
    "sdk": "mcs"
  }
}
