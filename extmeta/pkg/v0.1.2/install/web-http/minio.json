{
  "lang": "Go",
  "homepage": "https://min.io/",
  "desc": {
    "cn": "MinIO是一个高性能、兼容S3的对象存储",
    "en": "MinIO is a high-performance, S3 compatible object store"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install minio/stable/minio && \\\nMINIO_ROOT_USER=admin MINIO_ROOT_PASSWORD=password minio server /mnt/data --console-address \":9001\"\n",
      "reference": "https://min.io/download?license=agpl&platform=macos"
    },
    "/go": {
      "cmd": "go get github.com/minio/minio-go/v7",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    },
    "/npm": {
      "cmd": "npm install --save minio",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    },
    "/pip": {
      "cmd": "pip install minio",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    },
    "/stack": {
      "cmd": "stack install minio-hs",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    },
    "/vcpkg": {
      "cmd": "vcpkg install minio-cpp",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    },
    "/cargo": {
      "cmd": "cargo add minio",
      "reference": "https://min.io/download?license=agpl&platform=linux"
    }
  }
}
