{
  "lang": "go",
  "homepage": "https://about.gitea.com/",
  "desc": {
    "cn": "无痛自托管一体化软件开发服务，包括 Git 托管、代码审查、团队协作、包注册和 CI/CD",
    "en": "Painless self-hosted all-in-one software development service, including Git hosting, code review, team collaboration, package registry and CI/CD"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gitea",
      "reference": "https://formulae.brew.sh/formula/gitea#default"
    },
    "alpine/apk": {
      "cmd": "sudo apk add gitea",
      "reference": "https://command-not-found.com/gitea",
      "dsnap": "gitea"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S gitea",
      "reference": "https://command-not-found.com/gitea"
    }
  },
  "dsnap": {
    "brew": "gitea",
    "pacman": "gitea",
    "alpine/apk": "gitea"
  }
}
