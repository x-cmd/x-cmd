{
  "lang": "go",
  "homepage": "https://gitea.com/gitea/tea",
  "desc": {
    "cn": "与 Gitea 服务器交互，Gitea 官方 CLI",
    "en": "Interact with Gitea servers, Official CLI for Gitea"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use tea",
      "reference": "https://github.com/mvaldes14/asdf-tea"
    },
    "darwin/brew": {
      "cmd": "brew install tea",
      "reference": "https://formulae.brew.sh/formula/tea#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install tea",
      "reference": "https://command-not-found.com/tea",
      "dsnap": "tea"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install tea",
      "reference": "https://command-not-found.com/tea",
      "dsnap": "tea"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install tea",
      "reference": "https://command-not-found.com/tea"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tea",
      "reference": "https://scoop.sh/#/apps?q=tea"
    },
    "/build": {
      "cmd": "git clone https://gitea.com/gitea/tea.git # or: tea clone gitea.com/gitea/tea ;)\ncd tea\nmake\n",
      "reference": "https://gitea.com/gitea/tea#compilation"
    }
  },
  "dsnap": {
    "brew": "tea",
    "/go": "tea",
    "ubuntu/apt": "tea",
    "debian/apt": "tea"
  }
}
