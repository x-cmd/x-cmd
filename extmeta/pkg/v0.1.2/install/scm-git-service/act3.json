{
  "lang": "Go",
  "homepage": "https://github.com/dhth/act3",
  "desc": {
    "cn": "用于查看 Github Actions 最近 3 次运行结果的命令行工具",
    "en": "A command line tool to view the results of your last three Github Actions runs."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install dhth/tap/act3",
      "reference": "https://github.com/dhth/act3?tab=readme-ov-file#-installation"
    },
    "/go": {
      "cmd": "go install github.com/dhth/act3@latest",
      "reference": "https://github.com/dhth/act3?tab=readme-ov-file#-installation",
      "dsnap": "act3"
    }
  },
  "dsnap": {
    "brew": "act3",
    "/go": "act3"
  }
}
