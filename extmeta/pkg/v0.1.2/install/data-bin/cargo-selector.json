{
  "lang": "Rust",
  "homepage": "https://github.com/lusingander/cargo-selector",
  "desc": {
    "en": "Cargo subcommand to select and execute binary/example targets",
    "cn": "Cargo 子命令用于选择并执行二进制/示例目标"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install cargo-selector",
      "reference": "https://github.com/lusingander/cargo-selector?tab=readme-ov-file#installation",
      "dsnap": "cargo-selector"
    },
    "arch/paru": {
      "cmd": "paru -S cargo-selector",
      "reference": "https://github.com/lusingander/cargo-selector?tab=readme-ov-file#aur"
    }
  },
  "dsnap": {
    "/cargo": "cargo-selector"
  }
}
