{
  "lang": "Go",
  "homepage": "https://github.com/wader/fq",
  "desc": {
    "cn": "用于二进制格式的 jq  - 用于处理二进制和文本格式的工具、语言和解码器",
    "en": "jq for binary formats - tool, language and decoders for working with binary and text formats"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install wader/tap/fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#homebrew-macos"
    },
    "darwin/port": {
      "cmd": "sudo port install fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#macports"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#windows"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#arch-linux"
    },
    "arch/paru": {
      "cmd": "sudo paru -S fq-git",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#arch-linux"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#nix"
    },
    "freebsd/port": {
      "cmd": "",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#freebsd"
    },
    "alpine/apk": {
      "cmd": "apk add -X http://dl-cdn.alpinelinux.org/alpine/edge/testing fq",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#alpine",
      "dsnap": "fq"
    },
    "/go": {
      "cmd": "go install github.com/wader/fq@latest",
      "reference": "https://github.com/wader/fq?tab=readme-ov-file#build-from-source",
      "dsnap": "github.com/wader/fq@latest"
    }
  },
  "dsnap": {
    "brew": "wader/tap/fq",
    "scoop": "fq",
    "port": "fq",
    "pacman": "fq",
    "paru": "fq-git",
    "/go": "github.com/wader/fq@latest",
    "alpine/apk": "fq"
  }
}
