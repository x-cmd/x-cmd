{
  "lang": "rust",
  "homepage": "https://github.com/sharkdp/hexyl",
  "desc": {
    "cn": "命令行十六进制查看器",
    "en": "A command-line hex viewer"
  },
  "rule": {
    "/snap": {
      "cmd": "sudo snap install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#via-snap-package"
    },
    "/cargo": {
      "cmd": "cargo install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#via-cargo",
      "dsnap": "hexyl"
    },
    "darwin/brew": {
      "cmd": "brew install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-macos"
    },
    "/nixpkgs": {
      "cmd": "nix-env -i hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#via-nix"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-debian"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-ubuntu"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-arch-linux"
    },
    "kali/apt": {
      "cmd": "sudo apt install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-ubuntu"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-fedora"
    },
    "raspbian/apt": {
      "cmd": "sudo apt install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-ubuntu"
    },
    "gentoo/emerge": {
      "cmd": "sudo eselect repository enable dm9pZCAq\n&& sudo emerge --sync dm9pZCAq\n&& sudo emerge sys-apps/hexyl::dm9pZCAq\n",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-gentoo-linux"
    },
    "void/xbps": {
      "cmd": "xbps-install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-void-linux"
    },
    "darwin/port": {
      "cmd": "sudo port install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install hexyl",
      "reference": "https://scoop.sh/#/apps?q=hexyl"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install hexyl",
      "reference": "https://community.chocolatey.org/packages/hexyl"
    },
    "freebsd/pkg": {
      "cmd": "pkg install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-freebsd"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-netbsd"
    },
    "openbsd/pkg_add": {
      "cmd": "doas pkg_add hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-openbsd"
    },
    "termux/pkg": {
      "cmd": "pkg install hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#on-termux"
    },
    "guixos/guix": {
      "cmd": "guix package -i hexyl",
      "reference": "https://github.com/sharkdp/hexyl?tab=readme-ov-file#via-guix"
    }
  },
  "dsnap": {
    "/cargo": "hexyl"
  }
}
