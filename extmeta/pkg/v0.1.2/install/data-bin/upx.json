{
  "lang": "C++",
  "homepage": "https://github.com/upx/upx",
  "desc": {
    "cn": "UPX 是一款免费、安全、便携、可扩展、高性能的可执行文件打包工具，适用于多种可执行文件格式。",
    "en": "UPX is a free, secure, portable, extendable, high-performance executable packer for several executable formats."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use upx",
      "reference": "https://github.com/jimmidyson/asdf-upx"
    },
    "alpine/apk": {
      "cmd": "sudo apk add upx",
      "reference": "https://command-not-found.com/zstd",
      "dsnap": "upx"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S upx",
      "reference": "https://command-not-found.com/zstd"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install upx",
      "reference": "https://command-not-found.com/zstd",
      "dsnap": "upx"
    }
  },
  "dsnap": {
    "fedora/dnf": "upx",
    "alpine/apk": "upx"
  }
}
