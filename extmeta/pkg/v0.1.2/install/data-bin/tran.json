{
  "lang": "Go",
  "homepage": "https://github.com/abdfnx/tran",
  "desc": {
    "cn": "一个命令行工具和 TUI，允许在计算机之间安全地传输和发送文件和目录。",
    "en": "A command line tool and TUI that allows secure transfer and sending of files and directories between computers."
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fsSL https://cutt.ly/tran-cli | bash",
      "reference": "https://github.com/abdfnx/tran?tab=readme-ov-file#using-script"
    },
    "win/powershell": {
      "cmd": "x pwsh iwr -useb https://cutt.ly/tran-win | iex",
      "reference": "https://github.com/abdfnx/tran?tab=readme-ov-file#using-script"
    },
    "darwin/brew": {
      "cmd": "brew install abdfnx/tap/tran",
      "reference": "https://github.com/abdfnx/tran?tab=readme-ov-file#homebrew"
    },
    "linux/brew": {
      "cmd": "brew install abdfnx/tap/tran",
      "reference": "https://github.com/abdfnx/tran?tab=readme-ov-file#homebrew"
    }
  },
  "dsnap": {
    "brew": "abdfnx/tap/tran"
  }
}
