{
  "lang": "Zig",
  "homepage": "https://github.com/Arnau478/hevi",
  "desc": {
    "cn": "一个简单的十六进制查看器。",
    "en": "A simple hex viewer."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S hevi",
      "reference": "https://github.com/Arnau478/hevi#installation"
    },
    "/brew": {
      "cmd": "brew install hevi",
      "reference": "https://github.com/Arnau478/hevi#installation"
    },
    "/nix": {
      "cmd": "nix shell github:Arnau478/hevi",
      "reference": "https://github.com/Arnau478/hevi#installation"
    },
    "/eget": {
      "cmd": "eget Arnau478/hevi",
      "reference": "https://github.com/Arnau478/hevi#installation"
    }
  }
}
