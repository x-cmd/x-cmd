{
  "lang": "Rust",
  "homepage": "https://github.com/Etto48/HexPatch",
  "desc": {
    "cn": "使用 Rust 编写的具有终端用户界面（TUI）的二进制修补器和编辑器。",
    "en": "A binary patcher and editor written in Rust with terminal user interface (TUI)."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install hex-patch",
      "reference": "https://github.com/Etto48/HexPatch?tab=readme-ov-file#using-cargo",
      "dsnap": "hex-patch"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install hexpatch",
      "reference": "https://github.com/Etto48/HexPatch?tab=readme-ov-file#netbsd"
    },
    "arch/pacman": {
      "cmd": "pacman -S hexpatch",
      "reference": "https://github.com/Etto48/HexPatch?tab=readme-ov-file#arch-linux"
    }
  },
  "dsnap": {
    "/cargo": "hex-patch"
  }
}
