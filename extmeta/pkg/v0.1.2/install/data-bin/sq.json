{
  "lang": "Go",
  "homepage": "https://github.com/neilotoole/sq",
  "desc": {
    "cn": "将 jq 风格的查询引入到结构化数据源（包括 SQL 数据库和 CSV 或 Excel 等文档格式）的命令行工具。",
    "en": "A command-line tool that brings jq-style querying to structured data sources, including SQL databases and document formats like CSV or Excel."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install sq",
      "reference": "https://github.com/neilotoole/sq?tab=readme-ov-file#macos"
    },
    "/curl": {
      "cmd": "/bin/sh -c \"$(curl -fsSL https://sq.io/install.sh)\"",
      "reference": "https://github.com/neilotoole/sq?tab=readme-ov-file#linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add sq https://github.com/neilotoole/sq && \\\nx scoop install sq\n",
      "reference": "https://github.com/neilotoole/sq?tab=readme-ov-file#windows"
    },
    "/go": {
      "cmd": "go install github.com/neilotoole/sq",
      "reference": "https://github.com/neilotoole/sq?tab=readme-ov-file#go",
      "dsnap": "github.com/neilotoole/sq"
    }
  },
  "dsnap": {
    "brew": "sq",
    "/go": "github.com/neilotoole/sq"
  }
}
