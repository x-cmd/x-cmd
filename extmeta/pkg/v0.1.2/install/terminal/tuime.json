{
  "lang": "Rust",
  "homepage": "https://github.com/nthnd/tuime",
  "desc": {
    "cn": "使用 Rust 编写的简单终端时钟。",
    "en": "A simple clock for your terminal made using Rust."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --git https://github.com/nate-sys/tuime",
      "reference": "https://github.com/nate-sys/tuime?tab=readme-ov-file#install",
      "dsnap": "tuime"
    },
    "/nix": {
      "cmd": "nix profile install github:nate-sys/tuime",
      "reference": "https://github.com/nate-sys/tuime?tab=readme-ov-file#nix"
    }
  },
  "dsnap": {
    "/cargo": "tuime"
  }
}
