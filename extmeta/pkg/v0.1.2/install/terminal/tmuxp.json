{
  "lang": "Python",
  "homepage": "https://tmuxp.git-pull.com/",
  "desc": {
    "cn": "ession manager for tmux, which allows users to save and load tmux sessions through simple configuration files",
    "en": "tmux 的 Session Manager，允许用户通过简单的配置文件保存和加载 tmux 会话"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install --user tmuxp",
      "reference": "https://tmuxp.git-pull.com/#installation",
      "dsnap": "tmuxp"
    },
    "darwin/brew": {
      "cmd": "brew install tmuxp",
      "reference": "https://tmuxp.git-pull.com/#installation"
    },
    "debian/apt": {
      "cmd": "sudo apt install tmuxp",
      "reference": "https://tmuxp.git-pull.com/#installation",
      "dsnap": "tmuxp"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install tmuxp",
      "reference": "https://tmuxp.git-pull.com/#installation",
      "dsnap": "tmuxp"
    },
    "/nix-env": {
      "cmd": "[[ -z $(which tmux) ]] && (nix-env -i tmux && nix-env -i tmuxp) || nix-env -i tmuxp\n",
      "reference": "https://tmuxp.git-pull.com/#installation"
    }
  },
  "dsnap": {
    "brew": "tmuxp",
    "/pip": "tmuxp",
    "ubuntu/apt": "tmuxp",
    "debian/apt": "tmuxp"
  }
}
