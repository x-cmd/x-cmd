{
  "lang": "Go",
  "homepage": "https://github.com/kencx/keyb",
  "desc": {
    "cn": "在终端中创建和查看自定义热键备忘单",
    "en": "Create and view custom hotkey cheatsheets in the terminal"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/kencx/keyb@latest",
      "reference": "https://github.com/kencx/keyb?tab=readme-ov-file#install",
      "dsnap": "kencx/keyb"
    },
    "/make": {
      "cmd": "git clone https://github.com/kencx/keyb && cd keyb && make build",
      "reference": "https://github.com/kencx/keyb?tab=readme-ov-file#build-from-source"
    }
  },
  "dsnap": {
    "/go": "kencx/keyb"
  }
}
