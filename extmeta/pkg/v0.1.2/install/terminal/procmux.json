{
  "lang": "Python",
  "homepage": "https://github.com/napisani/procmux",
  "desc": {
    "cn": "用于进程的终端多路复用器",
    "en": "A terminal multiplexer for processes."
  },
  "rule": {
    "/pip": {
      "cmd": "pip install procmux",
      "reference": "https://github.com/napisani/procmux#installation"
    },
    "/pipx": {
      "cmd": "pipx install procmux",
      "reference": "https://github.com/napisani/procmux#installation"
    },
    "/brew": {
      "cmd": "brew tap napisani/procmux\n&& brew install procmux\n",
      "reference": "https://github.com/napisani/procmux#installation"
    }
  }
}
