{
  "lang": "Go",
  "homepage": "https://github.com/merschformann/gotz",
  "desc": {
    "cn": "一个简单的命令行工具，用于显示终端中的多个时区信息。",
    "en": "A simple CLI timezone info tool."
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/merschformann/gotz@latest",
      "reference": "https://github.com/merschformann/gotz#installation"
    },
    "/brew": {
      "cmd": "brew tap merschformann/gotz\n&& brew install gotz\n",
      "reference": "https://github.com/merschformann/gotz#installation"
    },
    "arch/pacman": {
      "cmd": "pacman -S gotz-bin",
      "reference": "https://github.com/merschformann/gotz#installation"
    },
    "/eget": {
      "cmd": "eget merschformann/gotz",
      "reference": "https://github.com/merschformann/gotz#installation"
    }
  }
}
