{
  "lang": "Python",
  "homepage": "https://guake.github.io/",
  "desc": {
    "cn": "Guake 是为 GNOME 桌面环境制作的基于 Python 的下拉终端",
    "en": "Guake is a python based dropdown terminal made for the GNOME desktop environment"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#debian-ubuntu",
      "dsnap": "guake"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#debian-ubuntu",
      "dsnap": "guake"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#fedora",
      "dsnap": "guake"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#arch-linux"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install guake",
      "reference": "https://command-not-found.com/guake"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install guake",
      "reference": "https://command-not-found.com/guake"
    },
    "manjaro/pacman": {
      "cmd": "sudo pacman -S guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#manjaro"
    },
    "/pipx": {
      "cmd": "pipx install guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#install-from-pypi"
    },
    "/pip": {
      "cmd": "pip install --user guake",
      "reference": "https://guake.readthedocs.io/en/latest/user/installing.html#install-from-pypi",
      "dsnap": "guake"
    }
  },
  "dsnap": {
    "pacman": "guake",
    "/pip": "guake",
    "fedora/dnf": "guake",
    "ubuntu/apt": "guake",
    "debian/apt": "guake"
  }
}
