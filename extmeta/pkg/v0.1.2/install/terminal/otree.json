{
  "lang": "Rust",
  "homepage": "https://github.com/fioncat/otree",
  "desc": {
    "cn": "用于查看 TUI 树小部件中的对象 (JSON/YAML/TOML) 的命令行工具",
    "en": "A command line tool to view objects (JSON/YAML/TOML) in TUI tree widget"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --git https://github.com/fioncat/otree",
      "reference": "https://github.com/fioncat/otree?tab=readme-ov-file#install",
      "dsnap": "otree"
    },
    "arch/paru": {
      "cmd": "paru -S otree",
      "reference": "https://github.com/fioncat/otree?tab=readme-ov-file#arch-linux-aur"
    },
    "darwin/brew": {
      "cmd": "brew install otree",
      "reference": "https://github.com/fioncat/otree?tab=readme-ov-file#macos-and-linux-homebrew"
    },
    "linux/brew": {
      "cmd": "brew install otree",
      "reference": "https://github.com/fioncat/otree?tab=readme-ov-file#macos-and-linux-homebrew"
    }
  },
  "dsnap": {
    "brew": "otree",
    "/cargo": "otree"
  }
}
