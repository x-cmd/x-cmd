{
  "lang": "Python",
  "homepage": "https://github.com/sontek/snowmachine",
  "desc": {
    "cn": "在终端中模拟下雪效果的小工具",
    "en": "A small, fun and easy way to make it snow in your terminal"
  },
  "rule": {
    "nix/nix-shell": {
      "cmd": "nix-shell -p snowmachine --command snowmachine",
      "reference": "https://github.com/sontek/snowmachine#snowmachine"
    },
    "/brew": {
      "cmd": "brew install sontek/snowmachine/snowmachine",
      "reference": "https://github.com/sontek/snowmachine#snowmachine"
    },
    "/pip": {
      "cmd": "pip install snowmachine",
      "reference": "https://github.com/sontek/snowmachine#snowmachine",
      "dsnap": "snowmachine"
    },
    "/pipx": {
      "cmd": "pipx install snowmachine",
      "reference": "https://github.com/sontek/snowmachine#snowmachine"
    }
  },
  "dsnap": {
    "brew": "snowmachine",
    "pipx": "snowmachine",
    "/pip": "snowmachine"
  }
}
