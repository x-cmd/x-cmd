{
  "homepage": "https://github.com/tmux/tmux",
  "desc": {
    "cn": "tmux 是一个终端多路复用器。",
    "en": "tmux is a terminal multiplexer."
  },
  "lang": "C",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use tmux",
      "reference": "https://github.com/aphecetche/asdf-tmux"
    },
    "darwin/brew": {
      "cmd": "brew install tmux",
      "reference": "https://formulae.brew.sh/formula/tmux#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install tmux",
      "reference": "https://command-not-found.com/tmux",
      "dsnap": "tmux"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install tmux",
      "reference": "https://command-not-found.com/tmux",
      "dsnap": "tmux"
    },
    "alpine/apk": {
      "cmd": "sudo apk add tmux",
      "reference": "hthttps://command-not-found.com/tmux",
      "dsnap": "tmux"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S tmux",
      "reference": "https://command-not-found.com/tmux"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install tmux",
      "reference": "https://command-not-found.com/tmux"
    },
    "centos/yum": {
      "cmd": "sudo yum install tmux",
      "reference": "https://command-not-found.com/tmux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tmux-1",
      "reference": "https://command-not-found.com/tmux",
      "dsnap": "tmux"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install tmux",
      "reference": "https://command-not-found.com/tmux"
    }
  },
  "dsnap": {
    "brew": "tmux",
    "pacman": "tmux",
    "yum": "tmux",
    "fedora/dnf": "tmux",
    "alpine/apk": "tmux",
    "ubuntu/apt": "tmux",
    "debian/apt": "tmux"
  }
}
