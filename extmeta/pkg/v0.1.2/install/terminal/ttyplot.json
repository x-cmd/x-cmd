{
  "lang": "C",
  "homepage": "https://github.com/tenox7/ttyplot",
  "desc": {
    "en": "a realtime plotting utility for terminal/console with data input from stdin",
    "cn": "一个用于终端/控制台的实时绘图工具，数据来源于标准输入 (stdin)"
  },
  "rule": {
    "ubuntu/snap": {
      "cmd": "snap install ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    },
    "debian/apt": {
      "cmd": "apt install ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    },
    "gentoo/emerge": {
      "cmd": "emerge -av app-admin/ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    },
    "darwin/brew": {
      "cmd": "brew install ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    },
    "termux/pkg": {
      "cmd": "pkg install ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    },
    "freebsd/pkg": {
      "cmd": "pkg install ttyplot",
      "reference": "https://github.com/tenox7/ttyplot?tab=readme-ov-file#get"
    }
  }
}
