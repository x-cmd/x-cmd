{
  "lang": "Go",
  "homepage": "https://github.com/Code-Hex/Neo-cowsay",
  "desc": {
    "cn": "Neo Cowsay 是用 Go 编写的。这个cowsay 是原始cowsay 的扩展。添加了更多有趣的选项，并且可以用作库",
    "en": "Neo Cowsay is written in Go. This cowsay is extended the original cowsay. added fun more options, and you can be used as a library"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install neo-cowsay",
      "reference": "https://github.com/Code-Hex/Neo-cowsay?tab=readme-ov-file#windows-users-via-scoop"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install neo-cowsay",
      "reference": "https://github.com/Code-Hex/Neo-cowsay?tab=readme-ov-file#windows-users-via-winget"
    },
    "darwin/brew": {
      "cmd": "brew install Code-Hex/tap/neo-cowsay",
      "reference": "https://github.com/Code-Hex/Neo-cowsay?tab=readme-ov-file#mac-and-linux-users-via-homebrew"
    },
    "/go": {
      "cmd": "go install github.com/Code-Hex/Neo-cowsay/cmd/v2/cowsay@latest",
      "reference": "https://github.com/Code-Hex/Neo-cowsay?tab=readme-ov-file#go"
    }
  }
}
