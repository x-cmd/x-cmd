{
  "lang": "Python",
  "homepage": "https://github.com/nickolaj-jepsen/fnug",
  "desc": {
    "cn": "一个在终端中同时运行所有 lint、测试和命令的 多路复用器",
    "en": "A TUI and multiplexer designed for running lints, tests, and other commands concurrently in the terminal."
  },
  "rule": {
    "/pipx": {
      "cmd": "pipx install fnug",
      "reference": "https://github.com/nickolaj-jepsen/fnug#installation"
    },
    "/rye": {
      "cmd": "rye install fnug",
      "reference": "https://github.com/nickolaj-jepsen/fnug#installation"
    },
    "/pip": {
      "cmd": "pip install fnug",
      "reference": "https://github.com/nickolaj-jepsen/fnug#installation"
    }
  }
}
