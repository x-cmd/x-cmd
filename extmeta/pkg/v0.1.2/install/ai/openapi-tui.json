{
  "lang": "Rust",
  "homepage": "https://github.com/zaghaghi/openapi-tui",
  "desc": {
    "cn": "用于列出、浏览和运行 OpenAPI v3.0 和 v3.1 规范定义的 API 的终端 UI",
    "en": "Terminal UI to list, browse and run APIs defined with OpenAPI v3.0 and v3.1 spec"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install openapi-tui",
      "reference": "https://github.com/zaghaghi/openapi-tui?tab=readme-ov-file#installation",
      "dsnap": "openapi-tui"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S openapi-tui",
      "reference": "https://github.com/zaghaghi/openapi-tui?tab=readme-ov-file#arch-linux"
    },
    "nix/": {
      "cmd": "nix profile install github:zaghaghi/openapi-tui",
      "reference": "https://github.com/zaghaghi/openapi-tui?tab=readme-ov-file#nixos"
    }
  },
  "dsnap": {
    "/cargo": "openapi-tui"
  }
}
