{
  "lang": "Go",
  "homepage": "https://github.com/eeeXun/GTT",
  "desc": {
    "cn": "Google Translate TUI（最初版本）目前支持 Apertium、Bing、ChatGPT、DeepL、DeepLX、Google、LibreTranslate 和 Reverso 等翻译服务。",
    "en": "Google Translate TUI (Originally). Currently supports Apertium, Bing, ChatGPT, DeepL, DeepLX, Google, LibreTranslate, Reverso."
  },
  "rule": {
    "arch/yay": {
      "cmd": "yay -S gtt-bin",
      "reference": "https://github.com/eeeXun/GTT?tab=readme-ov-file#arch-linux-aur"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p '(import <nixpkgs-unstable> {}).gtt' --run gtt",
      "reference": "https://github.com/eeeXun/GTT?tab=readme-ov-file#nix-%EF%B8%8F-nixpkgs-unstable"
    },
    "/go": {
      "cmd": "go install -ldflags=\"-s -w\" github.com/eeeXun/gtt@latest",
      "reference": "https://github.com/eeeXun/GTT?tab=readme-ov-file#go-install",
      "dsnap": "github.com/eeeXun/gtt@latest"
    }
  },
  "dsnap": {
    "/go": "github.com/eeeXun/gtt@latest"
  }
}
