{
  "lang": "Python",
  "homepage": "https://huggingface.co/",
  "desc": {
    "cn": "Huggingface Hub 的官方 Python 客户端",
    "en": "The official Python client for the Huggingface Hub"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install huggingface_hub",
      "reference": "https://huggingface.co/docs/huggingface_hub/installation#install-with-pip",
      "dsnap": "huggingface_hub"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge huggingface_hub",
      "reference": "https://huggingface.co/docs/huggingface_hub/installation#install-with-conda"
    }
  },
  "dsnap": {
    "/pip": "huggingface_hub"
  }
}
