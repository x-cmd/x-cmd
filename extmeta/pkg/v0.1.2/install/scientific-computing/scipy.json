{
  "lang": "python",
  "homepage": "https://www.scipy.org/",
  "desc": {
    "cn": "SciPy 是一个开源的 Python 算法库和数学工具包",
    "en": "SciPy is an open-source Python library and math toolkit"
  },
  "rule": {
    "/pip": {
      "cmd": "python -m pip install scipy",
      "reference": "https://scipy.org/install/#installing-with-pip",
      "dsnap": "scipy"
    },
    "/conda": {
      "cmd": "conda install scipy",
      "reference": "https://scipy.org/install/#installing-via-conda"
    },
    "darwin/brew": {
      "cmd": "brew install scipy",
      "reference": "https://scipy.org/install/#macos"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install python3-scipy",
      "reference": "https://scipy.org/install/#ubuntu-and-debian",
      "dsnap": "python3-scipy"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install python3-scipy",
      "reference": "https://scipy.org/install/#ubuntu-and-debian",
      "dsnap": "python3-scipy"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install python3-scipy",
      "reference": "https://scipy.org/install/#fedora",
      "dsnap": "python3-scipy"
    }
  },
  "dsnap": {
    "brew": "scipy",
    "/pip": "scipy",
    "fedora/dnf": "python3-scipy",
    "ubuntu/apt": "python3-scipy",
    "debian/apt": "python3-scipy"
  }
}
