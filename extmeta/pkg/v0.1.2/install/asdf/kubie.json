{
  "lang": "Rust",
  "homepage": "https://github.com/sbstp/kubie",
  "desc": {
    "cn": "Kubie 是 kubectx、 kubens 和 k on 提示修改脚本的替代品",
    "en": "kubie is an alternative to kubectx, kubens and the k on prompt modification script"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install kubie",
      "reference": "https://github.com/sbstp/kubie?tab=readme-ov-file#cargo",
      "dsnap": "kubie"
    },
    "darwin/port": {
      "cmd": "sudo port install kubie",
      "reference": "https://github.com/sbstp/kubie?tab=readme-ov-file#macports"
    },
    "darwin/brew": {
      "cmd": "brew install kubie",
      "reference": "https://github.com/sbstp/kubie?tab=readme-ov-file#homebrew"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S kubie",
      "reference": "https://github.com/sbstp/kubie?tab=readme-ov-file#arch-linux"
    },
    "/asdf": {
      "cmd": "x asdf use kubie",
      "reference": "https://github.com/johnhamelink/asdf-kubie"
    }
  },
  "dsnap": {
    "brew": "kubie",
    "/cargo": "kubie"
  }
}
