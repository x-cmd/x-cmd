{
  "lang": "Go",
  "homepage": "https://www.telepresence.io/",
  "desc": {
    "cn": "Tepresence 是 Kubernetes 应用程序开发人员的开源工具，可让您在将该服务连接到远程 Kubernetes 群集时本地运行单个服务",
    "en": "Telepresence is an open source tool for Kubernetes application developers that lets you run a single service locally while connecting that service to a remote Kubernetes cluster"
  },
  "rule": {
    "linux/curl": {
      "cmd": "sudo curl -fL https://app.getambassador.io/download/tel2oss/releases/download/v2.17.0/telepresence-linux-amd64 -o /usr/local/bin/telepresence \\\n&& sudo chmod a+x /usr/local/bin/telepresence\n",
      "reference": "https://www.telepresence.io/docs/latest/quick-start/?os=gnu-linux"
    },
    "darwin/curl": {
      "cmd": "sudo curl -fL https://app.getambassador.io/download/tel2oss/releases/download/v2.17.0/telepresence-darwin-arm64 -o /usr/local/bin/telepresence \\\n&& sudo chmod a+x /usr/local/bin/telepresence\n",
      "reference": "https://www.telepresence.io/docs/latest/quick-start/?os=macos"
    },
    "/asdf": {
      "cmd": "x asdf use telepresence",
      "reference": "https://github.com/pirackr/asdf-telepresence?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "/go": "telepresence"
  }
}
