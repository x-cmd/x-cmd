{
  "lang": "java",
  "homepage": "https://github.com/kcctl/kcctl",
  "desc": {
    "cn": "Kafka Connect 的现代且直观的命令行客户端",
    "en": "A modern and intuitive command line client for Kafka Connect"
  },
  "rule": {
    "/sdk": {
      "cmd": "sdk install kcctl",
      "reference": "https://github.com/kcctl/kcctl?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install kcctl/tap/kcctl",
      "reference": "https://github.com/kcctl/kcctl?tab=readme-ov-file#installation"
    },
    "/wget": {
      "cmd": "wget https://raw.githubusercontent.com/kcctl/kcctl/main/kcctl_completion \\\n&& . kcctl_completion\n",
      "reference": "https://github.com/kcctl/kcctl?tab=readme-ov-file#installation"
    },
    "/asdf": {
      "cmd": "x asdf use kcctl",
      "reference": "https://github.com/joschi/asdf-kcctl?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "asdf": "kcctl"
  }
}
