{
  "lang": "Go",
  "homepage": "https://www.okteto.com/",
  "desc": {
    "cn": "Okteto 是一个平台，它简化了启动云开发环境的过程，而不需要自己动手的专业知识",
    "en": "Okteto is a platform that simplifies the process of launching cloud development environments without requiring the expertise to do this yourself"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://get.okteto.com -sSfL | sh",
      "reference": "https://www.okteto.com/docs/get-started/install-okteto-cli/#macos--linux"
    },
    "darwin/brew": {
      "cmd": "brew install okteto",
      "reference": "https://www.okteto.com/docs/get-started/install-okteto-cli/#macos--linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x $ scoop install okteto",
      "reference": "https://www.okteto.com/docs/get-started/install-okteto-cli/#windows"
    },
    "/asdf": {
      "cmd": "x asdf use okteto",
      "reference": "https://github.com/BradenM/asdf-okteto?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "/go": "okteto"
  }
}
