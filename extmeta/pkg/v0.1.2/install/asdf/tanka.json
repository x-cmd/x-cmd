{
  "lang": "Go",
  "homepage": "https://tanka.dev/",
  "desc": {
    "cn": "Grafana Tanka 是您的 Kubernetes 群集的强大配置实用程序，由唯一的 JSONNET 语言提供支持",
    "en": "Grafana Tanka is the robust configuration utility for your Kubernetes cluster, powered by the unique Jsonnet language"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tanka",
      "reference": "https://tanka.dev/install#tanka"
    },
    "arch/yay": {
      "cmd": "yay tanka",
      "reference": "https://tanka.dev/install#tanka"
    },
    "linux/curl": {
      "cmd": "sudo curl -Lo /usr/local/bin/tk https://github.com/grafana/tanka/releases/latest/download/tk-linux-amd64 \\\n&& sudo chmod a+x /usr/local/bin/tk\n",
      "reference": "https://tanka.dev/install#tanka"
    },
    "darwin/curl": {
      "cmd": "sudo curl -Lo /usr/local/bin/tk https://github.com/grafana/tanka/releases/latest/download/tk-darwin-amd64 \\\n&& sudo chmod a+x /usr/local/bin/tk\n",
      "reference": "https://tanka.dev/install#tanka"
    },
    "/go": {
      "cmd": "go install github.com/grafana/tanka/cmd/tk@latest",
      "reference": "https://tanka.dev/install#tanka",
      "dsnap": "github.com/grafana/tanka/cmd/tk@latest"
    },
    "/adsf": {
      "cmd": "x asdf use tanka",
      "reference": "https://github.com/trotttrotttrott/asdf-tanka?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "tanka",
    "curl": "tanka",
    "/go": "github.com/grafana/tanka/cmd/tk@latest"
  }
}
