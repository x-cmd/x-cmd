{
  "lang": "Go",
  "homepage": "https://goteleport.com/community/",
  "desc": {
    "cn": "Teleport 社区由使用 Teleport 在工作，家庭或任何需要提供安全的基础设施访问的情况组成",
    "en": "The Teleport community is comprised of people using Teleport at work, home or any situation that needs to provide secure infrastructure access"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://goteleport.com/static/install.sh | bash -s 15.0.0",
      "reference": "https://goteleport.com/docs/#install-teleport"
    },
    "/asdf": {
      "cmd": "x asdf use teleport-community",
      "reference": "https://github.com/MaloPolese/asdf-teleport-community?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "/go": "teleport-community"
  }
}
