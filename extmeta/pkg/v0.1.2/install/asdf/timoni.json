{
  "lang": "Go",
  "homepage": "https://timoni.sh/",
  "desc": {
    "cn": "Timoni 是 Kubernetes 的包管理器，由 CUE 提供支持并受到 Helm 的启发",
    "en": "Timoni is a package manager for Kubernetes, powered by CUE and inspired by Helm"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install stefanprodan/tap/timoni",
      "reference": "https://timoni.sh/install/#__tabbed_1_1"
    },
    "/arkade": {
      "cmd": "arkade get timoni",
      "reference": "https://timoni.sh/install/#__tabbed_1_2"
    },
    "/nix-env": {
      "cmd": "nix-env -i timoni",
      "reference": "https://timoni.sh/install/#__tabbed_1_3"
    },
    "arch/yay": {
      "cmd": "yay -S timoni",
      "reference": "https://timoni.sh/install/#__tabbed_1_4"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install timoni",
      "reference": "https://timoni.sh/install/#__tabbed_1_5"
    },
    "/go": {
      "cmd": "go install github.com/stefanprodan/timoni/cmd/timoni@latest",
      "reference": "https://timoni.sh/install/#__tabbed_1_6"
    },
    "/asdf": {
      "cmd": "x asdf use timoni",
      "reference": "https://github.com/Smana/asdf-timoni?tab=readme-ov-file#install"
    }
  }
}
