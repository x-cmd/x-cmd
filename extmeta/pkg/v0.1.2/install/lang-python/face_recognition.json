{
  "lang": "Python",
  "homepage": "https://github.com/ageitgey/face_recognition",
  "desc": {
    "cn": "世界上最简单的 Python 和命令行面部识别 API",
    "en": "The world's simplest facial recognition api for Python and the command line"
  },
  "rule": {
    "/pip3": {
      "cmd": "pip3 install face_recognition",
      "reference": "https://github.com/ageitgey/face_recognition#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install graphics/py-face_recognition",
      "reference": "https://github.com/ageitgey/face_recognition#installing-on-freebsd"
    }
  },
  "dsnap": {
    "/pip": "face_recognition"
  }
}
