{
  "lang": "Python",
  "homepage": "https://twine.readthedocs.io/",
  "desc": {
    "cn": "Twine 是一个将 Python 包发布到 PyPI 和其他存储库的实用程序",
    "en": "Twine is a utility for publishing Python packages to PyPI and other repositories"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install twine",
      "reference": "https://twine.readthedocs.io/en/stable/#installation",
      "dsnap": "twine"
    },
    "darwin/brew": {
      "cmd": "brew install --cask twine",
      "reference": "https://formulae.brew.sh/cask/twine#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install twine",
      "reference": "https://command-not-found.com/twine"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install twine",
      "reference": "https://command-not-found.com/twine"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S twine",
      "reference": "https://command-not-found.com/twine"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install twine",
      "reference": "https://command-not-found.com/twine"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install twine",
      "reference": "https://command-not-found.com/twine"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install twine",
      "reference": "https://command-not-found.com/twine"
    }
  },
  "dsnap": {
    "/pip": "twine"
  }
}
