{
  "lang": "Python",
  "homepage": "https://pycqa.github.io/isort/",
  "desc": {
    "cn": "用于对导入进行排序的 Python 实用程序/库",
    "en": "isort is a Python utility / library to sort imports alphabetically, and automatically separated into sections and by type"
  },
  "rule": {
    "/pip": {
      "cmd": "pip3 install isort",
      "reference": "https://pycqa.github.io/isort/docs/quick_start/1.-install.html",
      "dsnap": "isort"
    },
    "/poetry": {
      "cmd": "poetry add isort",
      "reference": "https://pycqa.github.io/isort/docs/quick_start/1.-install.html"
    },
    "/pipenv": {
      "cmd": "pipenv install isort",
      "reference": "https://pycqa.github.io/isort/docs/quick_start/1.-install.html"
    },
    "/pipx": {
      "cmd": "pipx install isort",
      "reference": "https://pycqa.github.io/isort/docs/quick_start/1.-install.html"
    }
  },
  "dsnap": {
    "/pip": "isort"
  }
}
