{
  "lang": "shell",
  "homepage": "https://github.com/pyenv/pyenv-installer",
  "desc": {
    "cn": "pyenv 用于安装 pyenv",
    "en": "pyenv is used to install pyenv and friends"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://pyenv.run | bash",
      "reference": "https://github.com/pyenv/pyenv-installer"
    },
    "darwin/brew": {
      "cmd": "brew install pyenv",
      "reference": "https://formulae.brew.sh/formula/pyenv#default"
    }
  },
  "dsnap": {
    "shell": "pyenv"
  }
}
