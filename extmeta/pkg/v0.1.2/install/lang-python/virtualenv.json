{
  "lang": "python",
  "homepage": "https://virtualenv.pypa.io/",
  "desc": {
    "cn": "Viralenv 是创建独立 Python 环境的工具",
    "en": "virtualenv is a tool to create isolated Python environments"
  },
  "rule": {
    "/pipx": {
      "cmd": "pipx install virtualenv",
      "reference": "https://virtualenv.pypa.io/en/latest/installation.html#via-pipx"
    },
    "/pip": {
      "cmd": "python -m pip install --user virtualenv",
      "reference": "https://virtualenv.pypa.io/en/latest/installation.html#via-pip",
      "dsnap": "virtualenv"
    },
    "darwin/brew": {
      "cmd": "brew install virtualenv",
      "reference": "https://formulae.brew.sh/formula/virtualenv#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install virtualenv",
      "reference": "https://command-not-found.com/virtualenv",
      "dsnap": "virtualenv"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install virtualenv",
      "reference": "https://command-not-found.com/virtualenv",
      "dsnap": "virtualenv"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S virtualenv",
      "reference": "https://command-not-found.com/virtualenv"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install virtualenv",
      "reference": "https://command-not-found.com/virtualenv"
    },
    "centos/yum": {
      "cmd": "sudo yum install virtualenv",
      "reference": "https://command-not-found.com/virtualenv"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install virtualenv",
      "reference": "https://command-not-found.com/virtualenv",
      "dsnap": "virtualenv"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install virtualenv",
      "reference": "https://command-not-found.com/virtualenv"
    }
  },
  "dsnap": {
    "brew": "virtualenv",
    "pacman": "virtualenv",
    "yum": "virtualenv",
    "/pip": "virtualenv",
    "fedora/dnf": "virtualenv",
    "ubuntu/apt": "virtualenv",
    "debian/apt": "virtualenv"
  }
}
