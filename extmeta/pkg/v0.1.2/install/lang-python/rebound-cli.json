    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install rebound-cli",
      "reference": "https://github.com/shobrook/rebound/?tab=readme-ov-file#installation"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install rebound-cli",
      "reference": "https://github.com/shobrook/rebound/?tab=readme-ov-file#installation"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install rebound-cli",
      "reference": "https://github.com/shobrook/rebound/?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
