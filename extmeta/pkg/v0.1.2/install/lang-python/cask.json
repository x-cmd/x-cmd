{
  "lang": "Emacs Lisp",
  "homepage": "http://cask.readthedocs.io/",
  "desc": {
    "en": "Project management tool for Emacs",
    "cn": "Emacs 的项目管理工具"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fsSL https://raw.githubusercontent.com/cask/cask/master/go | python",
      "reference": "https://cask.readthedocs.io/en/latest/guide/installation.html#manual-installation"
    },
    "darwin/brew": {
      "cmd": "brew install cask",
      "reference": "https://cask.readthedocs.io/en/latest/guide/installation.html#package-managers"
    }
  },
  "dsnap": {
    "/go": "cask"
  }
}
