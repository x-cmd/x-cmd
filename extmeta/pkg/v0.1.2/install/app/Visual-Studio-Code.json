{
  "lang": "Typescript",
  "homepage": "https://code.visualstudio.com/",
  "desc": {
    "cn": "Visual Studio Code 是一个轻量级但功能强大的源代码编辑器，可以在桌面上运行，适用于 Windows、 macOS 和 Linux",
    "en": "Visual Studio Code is a lightweight but powerful source code editor which runs on your desktop and is available for Windows, macOS and Linux"
  },
  "rule": {
    "/flatpak": {
      "cmd": "flatpak install flathub com.visualstudio.code",
      "reference": "https://flathub.org/apps/com.visualstudio.code"
    },
    "arch/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/arch"
    },
    "centos/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/centos"
    },
    "debian/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/debian"
    },
    "elementary/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/elementary"
    },
    "fedora/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/fedora"
    },
    "neon/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/kde-neon"
    },
    "kubuntu/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/kubuntu"
    },
    "manjaro/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/manjaro"
    },
    "pop/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/pop"
    },
    "opensuse/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/opensuse"
    },
    "rhel/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/rhel"
    },
    "ubuntu/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/ubuntu"
    },
    "kali/snap": {
      "cmd": "sudo snap install code --classic",
      "reference": "https://snapcraft.io/install/code/ubuntu"
    },
    "raspbian/snap": {
      "cmd": "sudo snap-get install snap",
      "reference": "https://snapcraft.io/install/code/raspbian"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S code",
      "reference": "https://command-not-found.com/code"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/vscode\n",
      "reference": "https://scoop.sh/#/apps?q=vscode"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install vscode",
      "reference": "https://community.chocolatey.org/packages/vscode"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Microsoft.VisualStudioCode",
      "reference": "https://winget.run/pkg/Microsoft/VisualStudioCode"
    },
    "darwin/brew": {
      "cmd": "brew install --cask visual-studio-code",
      "reference": "https://formulae.brew.sh/cask/visual-studio-code#default"
    }
  },
  "dsnap": {
    "brew": "visual-studio-code",
    "fedora/dnf": "code",
    "alpine/apk": "code"
  }
}
