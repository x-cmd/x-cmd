{
  "lang": "c++",
  "homepage": "https://im.qq.com/index/",
  "desc": {
    "en": "QQ (Tencent QQ) is an Internet-based instant messaging software developed by Tencent based on ICQ",
    "cn": "QQ（ Tencent QQ）是 腾讯公司 借鉴于 ICQ 开发的一款 基于Internet的 即时通信 软件"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask qq",
      "reference": "https://formulae.brew.sh/cask/qq#default"
    },
    "/flatpak": {
      "cmd": "flatpak install flathub com.qq.QQ",
      "reference": "https://flathub.org/apps/com.qq.QQ"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/qq\n",
      "reference": "https://scoop.sh/#/apps?q=QQ"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install tencentqq",
      "reference": "https://community.chocolatey.org/packages/tencentqq"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget installTencent.QQ",
      "reference": "https://winget.run/pkg/Tencent/QQ"
    }
  }
}
