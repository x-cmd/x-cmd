{
  "lang": "C",
  "homepage": "https://www.darktable.org/",
  "desc": {
    "cn": "darktable 是一个开源的摄影工作流应用程序和 RAW 图像开发工具。",
    "en": "darktable is an open source photography workflow application and raw developer."
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install darktable",
      "reference": "https://software.opensuse.org/download.html?project=graphics:darktable&package=darktable",
      "dsnap": "darktable"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install darktable",
      "reference": "https://software.opensuse.org/download.html?project=graphics:darktable&package=darktable",
      "dsnap": "darktable"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install darktable",
      "reference": "https://software.opensuse.org/download.html?project=graphics:darktable&package=darktable"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install darktable",
      "reference": "https://software.opensuse.org/download.html?project=graphics:darktable&package=darktable",
      "dsnap": "darktable"
    },
    "/flatpak": {
      "cmd": "flatpak install flathub org.darktable.Darktable",
      "reference": "https://flathub.org/apps/org.darktable.Darktable"
    },
    "darwin/brew": {
      "cmd": "brew install --cask darktable",
      "reference": "https://www.darktable.org/install/"
    },
    "darwin/port": {
      "cmd": "sudo port install darktable +quartz",
      "reference": "https://www.darktable.org/install/"
    }
  },
  "dsnap": {
    "brew": "darktable",
    "flatpak": "org.darktable.Darktable",
    "fedora/dnf": "darktable",
    "alpine/apk": "darktable",
    "ubuntu/apt": "darktable",
    "debian/apt": "darktable"
  }
}
