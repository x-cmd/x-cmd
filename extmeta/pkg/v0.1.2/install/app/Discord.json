{
  "lang": "c#",
  "homepage": "https://discord.com/",
  "desc": {
    "en": "Discord is a chat software",
    "cn": "Discord 是一款聊天软件"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask discord",
      "reference": "https://formulae.brew.sh/cask/discord#default"
    },
    "/flatpak": {
      "cmd": "sudo flatpak install flathub com.discordapp.Discord",
      "reference": "https://flathub.org/apps/com.discordapp.Discord"
    },
    "arch/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/arch"
    },
    "centos/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/centos"
    },
    "debian/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/debian"
    },
    "elementary/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/elementary"
    },
    "fedora/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/fedora"
    },
    "neon/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/kde-neon"
    },
    "kubuntu/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/kubuntu"
    },
    "manjaro/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/manjaro"
    },
    "pop/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/pop"
    },
    "opensuse/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/opensuse"
    },
    "rhel/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/rhel"
    },
    "ubuntu/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/ubuntu"
    },
    "kali/snap": {
      "cmd": "sudo snap install discord",
      "reference": "https://snapcraft.io/install/discord/ubuntu"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install extras/discord",
      "reference": "https://scoop.sh/#/apps?q=discord"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install discord",
      "reference": "https://community.chocolatey.org/packages/discord"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Discord.Discord",
      "reference": "https://winget.run/pkg/Discord/Discord"
    }
  },
  "dsnap": {
    "brew": "discord"
  }
}
