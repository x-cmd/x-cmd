{
  "lang": "C++",
  "homepage": "https://github.com/qt-creator/qt-creator",
  "desc": {
    "cn": "Qt Creator 是一个跨平台的集成开发环境（IDE），供应用程序开发人员为多个桌面、嵌入式和移动设备平台创建应用程序",
    "en": "Qt Creator is a cross-platform, integrated development environment (IDE) for application developers to create applications for multiple desktop, embedded, and mobile device platforms"
  },
  "rule": {
    "(win|wsl2)/choco": {
      "cmd": "x choco install qtcreator",
      "reference": "https://doc.qt.io/qtcreator/creator-how-to-install.html#using-package-managers"
    },
    "darwin/brew": {
      "cmd": "brew install --cask qt-creator",
      "reference": "https://doc.qt.io/qtcreator/creator-how-to-install.html#using-package-managers"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install qtcreator",
      "reference": "https://command-not-found.com/qtcreator",
      "dsnap": "qtcreator"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install qtcreator",
      "reference": "https://command-not-found.com/qtcreator",
      "dsnap": "qtcreator"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S qtcreator",
      "reference": "https://command-not-found.com/qtcreator"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install qtcreator",
      "reference": "https://command-not-found.com/qtcreator"
    },
    "fedora/pacman": {
      "cmd": "sudo dnf install qt-creator",
      "reference": "https://command-not-found.com/qtcreator"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install qtcreator",
      "reference": "https://command-not-found.com/qtcreator"
    }
  },
  "dsnap": {
    "brew": "qt-creator",
    "pacman": "qtcreator",
    "fedora/dnf": "qt-creator",
    "ubuntu/apt": "qtcreator",
    "debian/apt": "qtcreator"
  }
}
