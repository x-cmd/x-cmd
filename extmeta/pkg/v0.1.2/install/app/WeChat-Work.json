{
  "lang": "C++",
  "homepage": "https://work.weixin.qq.com/",
  "desc": {
    "cn": "企业微信是由腾讯科技(深圳)有限公司开发的高效企业级办公平台",
    "en": "WeChat Work is an efficient enterprise-grade office management platform developed by Tencent Inc"
  },
  "rule": {
    "(win|wsl2)/choco": {
      "cmd": "x choco install wecom",
      "reference": "https://community.chocolatey.org/packages/wecom"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Tencent.WeCom",
      "reference": "https://winget.run/pkg/Tencent/wechat-work"
    },
    "darwin/brew": {
      "cmd": "brew install --cask wechatwork",
      "reference": "https://formulae.brew.sh/cask/wechatwork#default"
    }
  }
}
