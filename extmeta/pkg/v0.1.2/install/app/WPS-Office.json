{
  "lang": "C++",
  "homepage": "https://www.wps.com/",
  "desc": {
    "en": "WPS Office is a lightweight, feature-rich comprehensive office suite with high compatibility",
    "cn": "WPS Office 是一个轻量级、功能丰富、具有高兼容性的综合办公套件"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/wpsoffice\n",
      "reference": "https://scoop.sh/#/apps?q=wps"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install wps-office-free",
      "reference": "https://community.chocolatey.org/packages/wps-office-free"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Kingsoft.WPSOffice",
      "reference": "https://winget.run/pkg/Kingsoft/WPSOffice"
    },
    "darwin/brew": {
      "cmd": "brew install --cask wpsoffice",
      "reference": "https://formulae.brew.sh/cask/wpsoffice#default"
    }
  },
  "dsnap": {
    "brew": "wpsoffice"
  }
}
