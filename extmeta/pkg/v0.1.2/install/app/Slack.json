{
  "lang": "Go",
  "homepage": "https://slack.com/",
  "desc": {
    "en": "Slack is a productivity management platform that puts no-code automation and AI capabilities in the hands of everyone, seamlessly connects search and knowledge sharing, and keeps teams connected and engaged",
    "cn": "Slack 是一个工作效率管理平台，让每个人都能够使用无代码自动化和 AI 功能，还可以无缝连接搜索和知识共享，并确保团队保持联系和参与"
  },
  "rule": {
    "/flatpak": {
      "cmd": "flatpak run com.slack.Slack",
      "reference": "https://flathub.org/apps/com.slack.Slack"
    },
    "arch/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/arch"
    },
    "centos/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/centos"
    },
    "debian/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/debian"
    },
    "elementary/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/elementary"
    },
    "fedora/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/fedora"
    },
    "neon/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/kde-neon"
    },
    "kubuntu/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/kubuntu"
    },
    "manjaro/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/manjaro"
    },
    "pop/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/pop"
    },
    "opensuse/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/opensuse"
    },
    "rhel/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/rhel"
    },
    "ubuntu/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/ubuntu"
    },
    "kali/snap": {
      "cmd": "sudo snap install slack",
      "reference": "https://snapcraft.io/install/slack/ubuntu"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/slack\n",
      "reference": "https://scoop.sh/#/apps?q=slack"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install slack",
      "reference": "https://community.chocolatey.org/packages/slack"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install SlackTechnologies.Slack",
      "reference": "https://winget.run/pkg/SlackTechnologies/Slack"
    },
    "darwin/brew": {
      "cmd": "brew install --cask slack",
      "reference": "https://formulae.brew.sh/cask/slack#default"
    }
  }
}
