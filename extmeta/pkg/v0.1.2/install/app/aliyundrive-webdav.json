{
  "lang": "rust",
  "homepage": "https://github.com/messense/aliyundrive-webdav",
  "desc": {
    "en": "Alibaba Cloud Disk WebDAV service",
    "cn": "阿里云盘 WebDAV 服务"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install aliyundrive-webdav",
      "reference": "https://github.com/messense/aliyundrive-webdav?tab=readme-ov-file#%E5%AE%89%E8%A3%85",
      "dsnap": "aliyundrive-webdav"
    },
    "/snap": {
      "cmd": "sudo snap install aliyundrive-webdav",
      "reference": "https://github.com/messense/aliyundrive-webdav?tab=readme-ov-file#%E5%AE%89%E8%A3%85"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install main/aliyundrive-webdav",
      "reference": "https://scoop.sh/#/apps?q=aliyun"
    }
  },
  "dsnap": {
    "/pip": "aliyundrive-webdav"
  }
}
