{
  "lang": "c#",
  "homepage": "https://www.aliyundrive.com/",
  "desc": {
    "en": "Alibaba Cloud Disk is a fast, non-intrusive, secure enough, and easy-to-share network disk",
    "cn": "阿里云盘是一款速度快、不打扰、够安全、易于分享的网盘"
  },
  "rule": {
    "(win|wsl2)/winget": {
      "cmd": "x winget install Alibaba.aDrive",
      "reference": "https://winget.run/pkg/Alibaba/aDrive"
    },
    "darwin/brew": {
      "cmd": "brew install --cask adrive",
      "reference": "https://formulae.brew.sh/cask/adrive#default"
    }
  },
  "dsnap": {
    "brew": "adrive"
  }
}
