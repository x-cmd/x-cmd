{
  "lang": "python",
  "homepage": "https://github.com/jeffkowalski/geeknote",
  "desc": {
    "cn": "Geeknote 是 Evernote 的命令行客户端，可以在 Linux、 FreeBSD 和 OSX 上使用",
    "en": "Geeknote is a command line client for Evernote that can be use on Linux, FreeBSD and OS X"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install --upgrade pytest",
      "reference": "https://github.com/jeffkowalski/geeknote?tab=readme-ov-file#testing",
      "dsnap": "pytest"
    },
    "darwin/brew": {
      "cmd": "brew install --HEAD https://raw.githubusercontent.com/jeffkowalski/geeknote/master/geeknote.rb",
      "reference": "https://github.com/jeffkowalski/geeknote?tab=readme-ov-file#homebrew-installation"
    }
  },
  "dsnap": {
    "brew": "geeknote",
    "/pip": "pytest"
  }
}
