{
  "lang": "Raku",
  "homepage": "https://rakudo.org/",
  "desc": {
    "cn": "一种通用的、渐进式类型的编程语言，支持多重编程范式",
    "en": "A general, progressive type of programming language that supports multiple programming paradigms"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use raku",
      "reference": "https://github.com/m-dango/asdf-raku"
    },
    "darwin/brew": {
      "cmd": "brew install rakudo",
      "reference": "https://rakudo.org/downloads"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install rakudo",
      "reference": "https://rakudo.org/downloads"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install rakudo",
      "reference": "https://github.com/nodejs/snap#installation",
      "dsnap": "rakudo"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install rakudo",
      "reference": "https://github.com/nodejs/snap#installation",
      "dsnap": "rakudo"
    },
    "alpine/apk": {
      "cmd": "sudo apk add rakudo",
      "reference": "https://nodejs.org/en/download/package-manager#alpine-linux",
      "dsnap": "rakudo"
    },
    "arch/yay": {
      "cmd": "sudo yay -S rakudo",
      "reference": "https://linux-packages.com/aur/package/rakudo"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install rakudo",
      "reference": "https://github.com/nodejs/snap#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install rakudo",
      "reference": "https://nodejs.org/en/download/package-manager#centos-fedora-and-red-hat-enterprise-linux",
      "dsnap": "rakudo"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install rakudo",
      "reference": "https://github.com/nodejs/snap#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install rakudo",
      "reference": "https://nodejs.org/en/download/package-manager#opensuse-and-sle"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask --verbose dev-lang/rakudo",
      "reference": "https://nodejs.org/en/download/package-manager#gentoo"
    },
    "nix/nixpkgs": {
      "cmd": "sudo nix-env -iA nixos.rakudo",
      "reference": "https://rakudo.org/downloads"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install rakudo-moar",
      "reference": "https://rakudo.org/downloads"
    }
  },
  "dsnap": {
    "fedora/dnf": "rakudo",
    "alpine/apk": "rakudo",
    "ubuntu/apt": "rakudo",
    "debian/apt": "rakudo"
  }
}
