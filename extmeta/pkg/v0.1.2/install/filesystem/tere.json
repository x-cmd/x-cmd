{
  "lang": "Rust",
  "homepage": "https://github.com/mgunyho/tere",
  "desc": {
    "en": "tere is a terminal file explorer. It is a faster alternative to using cd and ls to browse folders in your terminal.",
    "cn": "Tere 是一个终端文件浏览器。它比使用 cd 和 ls 在终端中浏览文件夹更快的替代方案。"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tere",
      "reference": "https://github.com/mgunyho/tere?tab=readme-ov-file#step-1-obtain-a-copy-of-tere"
    },
    "/nix-env": {
      "cmd": "nix-env -i tere",
      "reference": "https://github.com/mgunyho/tere?tab=readme-ov-file#step-1-obtain-a-copy-of-tere"
    },
    "/cargo": {
      "cmd": "cargo install tere",
      "reference": "https://github.com/mgunyho/tere?tab=readme-ov-file#step-1-obtain-a-copy-of-tere",
      "dsnap": "tere"
    },
    "arch/pacman": {
      "cmd": "pacman -S tere",
      "reference": "https://github.com/mgunyho/tere?tab=readme-ov-file#step-1-obtain-a-copy-of-tere"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tere",
      "reference": "https://github.com/mgunyho/tere?tab=readme-ov-file#step-1-obtain-a-copy-of-tere"
    }
  },
  "dsnap": {
    "brew": "tere",
    "/cargo": "tere"
  }
}
