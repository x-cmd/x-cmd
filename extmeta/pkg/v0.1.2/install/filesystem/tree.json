{
  "lang": "C",
  "homepage": "https://oldmanprogrammer.net/source.php?dir=projects/tree",
  "desc": {
    "cn": "以树的形式显示当前目录的内容",
    "en": "Show the contents of the current directory as a tree"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tree",
      "reference": "https://formulae.brew.sh/formula/tree"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "alpine/apk": {
      "cmd": "sudo apk add tree",
      "reference": "https://command-not-found.com/tree"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S tree",
      "reference": "https://command-not-found.com/tree"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "centos/yum": {
      "cmd": "sudo yum install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "rhel/yum": {
      "cmd": "sudo yum install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "rhel/dnf": {
      "cmd": "sudo dnf install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install tree",
      "reference": "https://command-not-found.com/tree"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in tree",
      "reference": "https://command-not-found.com/tree"
    },
    "opensuse/snap": {
      "cmd": "sudo snap install tree",
      "reference": "https://snapcraft.io/install/tree/opensuse"
    }
  }
}
