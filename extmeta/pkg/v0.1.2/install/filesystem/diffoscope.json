{
  "lang": "python",
  "desc": {
    "cn": "文件、档案和目录的深入比较。",
    "en": "In-depth comparison of files, archives, and directories."
  },
  "homepage": "https://diffoscope.org/",
  "rule": {
    "/pip": {
      "cmd": "pip install diffoscope",
      "reference": "https://diffoscope.org",
      "dsnap": "diffoscope"
    },
    "darwin/brew": {
      "cmd": "brew install diffoscope",
      "reference": "https://diffoscope.org"
    },
    "debian/apt": {
      "cmd": "sudo apt install diffoscope",
      "reference": "https://diffoscope.org",
      "dsnap": "diffoscope"
    },
    "/docker": {
      "cmd": "docker run --rm -t -w $(pwd) -v $(pwd):$(pwd):ro registry.salsa.debian.org/reproducible-builds/diffoscope",
      "reference": "https://diffoscope.org"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install diffoscope",
      "reference": "https://diffoscope.org",
      "dsnap": "diffoscope"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in diffoscope",
      "reference": "https://diffoscope.org"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S diffoscope",
      "reference": "https://diffoscope.org"
    },
    "freebsd/pkg": {
      "cmd": "sudo pkg install py38-diffoscope",
      "reference": "https://diffoscope.org"
    },
    "openbsd/pkg_add": {
      "cmd": "sudo pkg_add diffoscope",
      "reference": "https://diffoscope.org"
    },
    "nixos/nix": {
      "cmd": "nix-shell -p diffoscope",
      "reference": "https://diffoscope.org"
    },
    "/git": {
      "cmd": "git clone https://salsa.debian.org/reproducible-builds/diffoscope.git",
      "reference": "https://diffoscope.org"
    }
  },
  "dsnap": {
    "brew": "diffoscope",
    "zypper": "diffoscope",
    "pacman": "diffoscope",
    "pkg": "py38-diffoscope",
    "pkg_add": "diffoscope",
    "/pip": "diffoscope",
    "fedora/dnf": "diffoscope",
    "debian/apt": "diffoscope"
  }
}
