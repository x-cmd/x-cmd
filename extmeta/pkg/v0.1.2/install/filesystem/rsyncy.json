{
  "lang": "Python",
  "homepage": "https://github.com/laktak/rsyncy",
  "desc": {
    "cn": "用于 rsync 的状态/进度条",
    "en": "A status/progress bar for rsync."
  },
  "rule": {
    "/brew": {
      "cmd": "brew install rsyncy",
      "reference": "https://github.com/laktak/rsyncy#installation"
    },
    "/pipx": {
      "cmd": "pipx install rsyncy",
      "reference": "https://github.com/laktak/rsyncy#installation"
    },
    "/pip": {
      "cmd": "pip install rsyncy",
      "reference": "https://github.com/laktak/rsyncy#installation"
    }
  }
}
