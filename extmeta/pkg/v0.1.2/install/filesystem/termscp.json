{
  "lang": "Rust",
  "homepage": "https://github.com/veeso/termscp",
  "desc": {
    "cn": "termscp 是一个功能丰富的终端文件传输和浏览器，支持 SCP/SFTP/FTP/S3",
    "en": "Termscp is a feature rich terminal file transfer and explorer, with support for SCP/SFTP/FTP/S3"
  },
  "rule": {
    "/curl": {
      "cmd": "curl --proto '=https' --tlsv1.2 -sSLf \"https://git.io/JBhDb\" | sh",
      "reference": "https://github.com/veeso/termscp?tab=readme-ov-file#get-started-"
    },
    "(win|wsl2)/choco": {
      "cmd": "choco install termscp",
      "reference": "https://github.com/veeso/termscp?tab=readme-ov-file#get-started-"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install termscp",
      "reference": "https://github.com/veeso/termscp?tab=readme-ov-file#get-started-"
    },
    "arch/pacman": {
      "cmd": "pacman -S termscp",
      "reference": "https://github.com/veeso/termscp?tab=readme-ov-file#get-started-"
    }
  }
}
