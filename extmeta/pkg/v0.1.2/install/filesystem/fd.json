{
  "lang": "rust",
  "homepage": "https://github.com/sharkdp/fd",
  "desc": {
    "cn": "`find` 命令的现代化替代品，用于在文件系统中进行快速查找文件和目录",
    "en": "A modern alternative for the `find` command, used for quickly searching files and directories in the file system"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use fd",
      "reference": "https://gitlab.com/wt0f/asdf-fd"
    },
    "darwin/brew": {
      "cmd": "brew install fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install fd-find",
      "reference": "https://github.com/sharkdp/fd",
      "dsnap": "fd-find"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install fd-find",
      "reference": "https://github.com/sharkdp/fd",
      "dsnap": "fd-find"
    },
    "alpine/apk": {
      "cmd": "sudo apk add fd",
      "reference": "https://github.com/sharkdp/fd",
      "dsnap": "fd"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install fdclone",
      "reference": "https://command-not-found.com/fd"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install fd-find",
      "reference": "https://github.com/sharkdp/fd",
      "dsnap": "fd-find"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install fdclone",
      "reference": "https://command-not-found.com/fd"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge -av fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "freebsd/pkg": {
      "cmd": "pkg install fd-find",
      "reference": "https://github.com/sharkdp/fd"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add bat",
      "reference": "https://github.com/sharkdp/fd"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -i fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "guixos/guix": {
      "cmd": "guix install fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "/npm": {
      "cmd": "npm install -g fd-find",
      "reference": "https://github.com/sharkdp/fd",
      "dsnap": "fd-find"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install sharkdp.fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fd",
      "reference": "https://github.com/sharkdp/fd"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install fd",
      "reference": "https://github.com/sharkdp/fd"
    }
  },
  "dsnap": {
    "brew": "fd",
    "pacman": "fd",
    "zypper": "fd",
    "emerge": "fd",
    "xbps": "fd",
    "pkg": "fd-find",
    "nixpkgs": "fd",
    "guix": "fd",
    "/npm": "fd-find",
    "fedora/dnf": "fd-find",
    "alpine/apk": "fd",
    "ubuntu/apt": "fd-find",
    "debian/apt": "fd-find"
  }
}
