{
  "lang": "rust",
  "homepage": "https://github.com/ajeetdsouza/zoxide",
  "desc": {
    "cn": "更智能的 cd 命令。支持所有主要 shell。",
    "en": "A smarter cd command. Supports all major shells."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use zoxide",
      "reference": "https://github.com/nyrst/asdf-zoxide"
    },
    "/curl": {
      "cmd": "curl -sS https://raw.githubusercontent.com/ajeetdsouza/zoxide/main/install.sh | bash",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install zoxide --locked",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation",
      "dsnap": "zoxide"
    },
    "darwin/brew": {
      "cmd": "brew install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "/nixpkgs": {
      "cmd": "nix-env -iA nixpkgs.zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "alpine/apk": {
      "cmd": "sudo apk add zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "debian/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "kali/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "centos/dnf": {
      "cmd": "sudo dnf copr enable atim/zoxide\n&& x dnf install zoxide\n",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "raspbian/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "gentoo/emerge": {
      "cmd": "sudo eselect repository enable guru\n&& sudo emerge --sync guru\n&& sudo emerge app-shells/zoxide\n",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "manjaro/pacman": {
      "cmd": "sudo pacman -S zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "parrot/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "devuan/apt": {
      "cmd": "sudo apt install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "rhino/pacstall": {
      "cmd": "pacstall -I zoxide-deb",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "void/xbps": {
      "cmd": "xbps-install -S zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install ajeetdsouza.zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "dragonflybsd/pkg": {
      "cmd": "pkg install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    },
    "termux/pkg": {
      "cmd": "sudo pkg install zoxide",
      "reference": "https://github.com/ajeetdsouza/zoxide?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/cargo": "zoxide"
  }
}
