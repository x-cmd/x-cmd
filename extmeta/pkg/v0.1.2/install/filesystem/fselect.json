{
  "lang": "rust",
  "homepage": "https://fselect.rocks/",
  "desc": {
    "cn": "find 命令的现代化替代品，用于在文件系统中进行快速查找文件和目录",
    "en": "A modern replacement for the find command for quickly finding files and directories in a file system"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install fselect",
      "reference": "https://github.com/jhspetersson/fselect?tab=readme-ov-file#mac-via-homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install fselect",
      "reference": "https://github.com/FiloSottile/age?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install fselect",
      "reference": "https://github.com/jhspetersson/fselect?tab=readme-ov-file#windows-via-chocolatey"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fselect",
      "reference": "https://github.com/jhspetersson/fselect?tab=readme-ov-file#windows-via-scoop"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install -e --id fselect.fselect",
      "reference": "https://github.com/jhspetersson/fselect?tab=readme-ov-file#windows-via-winget"
    },
    "/cargo": {
      "cmd": "cargo install fselect",
      "reference": "https://github.com/jhspetersson/fselect?tab=readme-ov-file#latest-release-from-source",
      "dsnap": "fselect"
    }
  },
  "dsnap": {
    "/cargo": "fselect"
  }
}
