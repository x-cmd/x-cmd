{
  "lang": "Go",
  "homepage": "https://github.com/babarot/gomi",
  "desc": {
    "en": "A simple CLI tool written in Go that adds trash can functionality to the command line.",
    "cn": "一个用Go语言编写的简单命令行工具，为命令行添加垃圾桶功能"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fsSL https://gomi.dev/install | bash",
      "reference": "https://github.com/babarot/gomi?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install gomi",
      "reference": "https://github.com/babarot/gomi?tab=readme-ov-file#using-homebrew"
    },
    "(win|wsl2)|scoop": {
      "cmd": "scoop bucket add babarot https://github.com/babarot/scoop-bucket\nscoop install gomi\n",
      "reference": "https://github.com/babarot/gomi?tab=readme-ov-file#using-scoop"
    },
    "arch/yay": {
      "cmd": "yay -S gomi",
      "reference": "https://github.com/babarot/gomi?tab=readme-ov-file#using-aur"
    },
    "arch/paru": {
      "cmd": "paru -S gomi",
      "reference": "https://github.com/babarot/gomi?tab=readme-ov-file#using-aur"
    }
  }
}
