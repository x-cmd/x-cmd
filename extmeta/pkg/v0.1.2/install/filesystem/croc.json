{
  "lang": "go",
  "homepage": "https://github.com/schollz/croc",
  "desc": {
    "cn": "允许任意两台计算机简单安全地传输文件和文件夹的工具",
    "en": "a tool that allows any two computers to simply and securely transfer files and folders"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://getcroc.schollz.com | bash",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew install croc",
      "reference": "https://formulae.brew.sh/formula/croc#default"
    },
    "alpine/apk": {
      "cmd": "sudo apk add bash coreutils\nwget -qO- https://getcroc.schollz.com | bash\n",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "nix/": {
      "cmd": "sudo nix-env -i croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "termux/pkg": {
      "cmd": "sudo pkg install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install schollz.croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "freebsd/pkg": {
      "cmd": "pkg install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "/go": {
      "cmd": "go install github.com/schollz/croc/v9@latest",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install",
      "dsnap": "github.com/schollz/croc/v9"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge net-misc/croc",
      "reference": "https://github.com/schollz/croc?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "/go": "github.com/schollz/croc/v9"
  }
}
