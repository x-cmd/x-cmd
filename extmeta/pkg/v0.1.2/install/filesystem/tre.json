{
  "lang": "Rust",
  "homepage": "https://github.com/dduan/tre",
  "desc": {
    "en": "A modern alternative to the tree command",
    "cn": "树状结构命令的现代替代方案"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    },
    "darwin/port": {
      "cmd": "sudo port install tre-tree",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    },
    "debian/apt": {
      "cmd": "sudo apt install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install",
      "dsnap": "tre-command"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    },
    "/cargo": {
      "cmd": "cargo install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install",
      "dsnap": "tre-command"
    },
    "arch/yay": {
      "cmd": "yay -S tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install tre-command",
      "reference": "https://github.com/dduan/tre?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "tre-command",
    "/cargo": "tre-command",
    "debian/apt": "tre-command"
  }
}
