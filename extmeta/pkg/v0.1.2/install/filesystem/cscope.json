{
  "lang": "c",
  "homepage": "https://cscope.sourceforge.net/",
  "desc": {
    "cn": "Cscope 是一个用于浏览源代码的开发人员工具",
    "en": "Cscope is a developer's tool for browsing source code"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install cscope",
      "reference": "https://formulae.brew.sh/formula/cscope#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install cscope",
      "reference": "https://scoop.sh/#/apps?q=cscope"
    }
  },
  "dsnap": {
    "brew": "cscope"
  }
}
