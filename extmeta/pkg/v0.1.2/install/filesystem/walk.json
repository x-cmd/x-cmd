{
  "lang": "Go",
  "homepage": "https://github.com/antonmedv/walk",
  "desc": {
    "cn": "Walk 是一个基于终端的导航器，可以替代传统的 ls 和 cd 命令",
    "en": "Walk is a terminal-based navigator that serves as an alternative to the traditional ls and cd commands"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install walk",
      "reference": "https://github.com/antonmedv/walk?tab=readme-ov-file#install"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add walk",
      "reference": "https://github.com/antonmedv/walk?tab=readme-ov-file#install"
    },
    "/go": {
      "cmd": "go install github.com/antonmedv/walk@latest",
      "reference": "https://github.com/antonmedv/walk?tab=readme-ov-file#install",
      "dsnap": "github.com/antonmedv/walk"
    }
  },
  "dsnap": {
    "brew": "walk",
    "openbsd": "walk",
    "/go": "github.com/antonmedv/walk"
  }
}
