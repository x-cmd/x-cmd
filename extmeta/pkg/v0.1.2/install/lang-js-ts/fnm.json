{
  "lang": "Rust",
  "homepage": "https://github.com/Schniz/fnm",
  "desc": {
    "cn": "快速简单的 Node.js 版本管理器，用 Rust 构建",
    "en": "Fast and simple Node.js version manager, built in Rust"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -fsSL https://fnm.vercel.app/install | bash",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-a-script-macoslinux"
    },
    "darwin/brew": {
      "cmd": "brew install fnm",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-homebrew-macoslinux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fnm",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-scoop-windows"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install fnm",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-chocolatey-windows"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Schniz.fnm",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-winget-windows"
    },
    "/cargo": {
      "cmd": "cargo install fnm",
      "reference": "https://github.com/Schniz/fnm?tab=readme-ov-file#using-cargo-linuxmacoswindows"
    }
  }
}
