{
  "lang": "Rust",
  "homepage": "https://volta.sh/",
  "desc": {
    "cn": "Volta 是一种管理 JavaScript 命令行工具的简单方法",
    "en": "Volta is a hassle-free way to manage your JavaScript command-line tools"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://get.volta.sh | bash",
      "reference": "https://docs.volta.sh/guide/getting-started"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install volta",
      "reference": "hhttps://scoop.sh/#/apps?q=volta"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install volta",
      "reference": "https://community.chocolatey.org/packages/volta"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Volta.Volta",
      "reference": "https://winget.run/pkg/Volta/Volta"
    },
    "darwin/brew": {
      "cmd": "brew install volta",
      "reference": "https://formulae.brew.sh/formula/volta#default"
    }
  },
  "dsnap": {
    "brew": "volta"
  }
}
