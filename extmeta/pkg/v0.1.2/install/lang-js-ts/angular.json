{
  "lang": "javascript",
  "homepage": "https://angular.io/cli",
  "desc": {
    "cn": "用于直接从命令 shell 初始化、开发、搭建和维护 Angular 应用程序的 CLI",
    "en": "CLI for initializing, developing, building, and maintaining Angular applications directly from the command shell"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g @angular/cli",
      "reference": "https://angular.io/cli",
      "dsnap": "@angular/cli"
    },
    "darwin/brew": {
      "cmd": "brew install angular-cli",
      "reference": "https://formulae.brew.sh/formula/angular-cli#default"
    }
  },
  "dsnap": {
    "brew": "angular-cli",
    "/npm": "@angular/cli"
  }
}
