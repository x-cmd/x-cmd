{
  "lang": "Go",
  "homepage": "https://github.com/jesseduffield/lazynpm",
  "desc": {
    "cn": "一个简单的 npm 命令 TUI",
    "en": "A simple TUI for npm commands."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install jesseduffield/lazynpm/lazynpm",
      "reference": "https://github.com/jesseduffield/lazynpm?tab=readme-ov-file#homebrew"
    },
    "/go": {
      "cmd": "go install github.com/jesseduffield/lazynpm@latest",
      "reference": "https://github.com/jesseduffield/lazynpm?tab=readme-ov-file#go",
      "dsnap": "lazynpm"
    }
  },
  "dsnap": {
    "brew": "lazynpm",
    "/go": "lazynpm"
  }
}
