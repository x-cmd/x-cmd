{
  "lang": "Python",
  "homepage": "https://csvkit.readthedocs.io/",
  "desc": {
    "cn": "csvkit 是一套命令行工具，用于转换和使用 CSV（“表格文件格式之王”）",
    "en": "csvkit is a suite of command-line tools for converting to and working with CSV (\"The king of tabular file formats\")"
  },
  "rule": {
    "/pip": {
      "cmd": "sudo pip install csvkit",
      "reference": "https://csvkit.readthedocs.io/en/latest/tutorial/1_getting_started.html#installing-csvkit",
      "dsnap": "csvkit"
    }
  },
  "dsnap": {
    "/pip": "csvkit"
  },
  "binlist": [
    "csvclean",
    "csvcut",
    "csvformat",
    "csvgrep",
    "csvjoin",
    "csvjson",
    "csvlook",
    "csvpy",
    "csvsort",
    "csvsql",
    "csvstack",
    "csvstat",
    "in2csv",
    "sql2csv"
  ]
}
