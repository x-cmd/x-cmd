{
  "lang": "Python",
  "homepage": "https://www.visidata.org/",
  "desc": {
    "cn": "VisiData 是一个用于探索和整理数据的终端工具，支持多种数据格式。",
    "en": "VisiData is a terminal tool for exploring and arranging data, supporting various data formats."
  },
  "rule": {
    "/pip": {
      "cmd": "pip3 install visidata",
      "reference": "https://www.visidata.org/install/#all-platforms---python-v3.6-and-above"
    },
    "darwin/brew": {
      "cmd": "brew install saulpw/vd/visidata",
      "reference": "https://www.visidata.org/install/#macos"
    },
    "/conda": {
      "cmd": "conda install --channel conda-forge visidata",
      "reference": "https://www.visidata.org/install/#conda"
    },
    "debian/apt": {
      "cmd": "sudo apt install visidata",
      "reference": "https://www.visidata.org/install/#linux"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S visidata",
      "reference": "https://www.visidata.org/install/#linux"
    },
    "nix/nix-env": {
      "cmd": "nix-env -iA nixpkgs.visidata",
      "reference": "https://www.visidata.org/install/#linux"
    },
    "guix/guix": {
      "cmd": "guix install visidata",
      "reference": "https://www.visidata.org/install/#linux"
    }
  }
}
