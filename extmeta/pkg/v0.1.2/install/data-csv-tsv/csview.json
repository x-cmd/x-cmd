{
  "lang": "rust",
  "homepage": "https://github.com/wfxr/csview",
  "desc": {
    "cn": "支持 cjk/emoji 的高性能 csv 查看器",
    "en": "A high performance csv viewer with cjk/emoji support"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install csview",
      "reference": "https://github.com/wfxr/csview?tab=readme-ov-file#on-macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install csview",
      "reference": "https://github.com/wfxr/csview?tab=readme-ov-file#on-windows"
    },
    "/cargo": {
      "cmd": "cargo install --locked csview",
      "reference": "https://github.com/wfxr/csview?tab=readme-ov-file#from-source"
    }
  }
}
