{
  "lang": "go",
  "homepage": "https://github.com/noborus/trdsql",
  "desc": {
    "cn": "trdsql 是一个 CLI 工具，可以对 CSV、 LTSV、 JSON、 YAML 和 TBLN 文件执行 SQL 查询。",
    "en": "trdsql is a CLI tool that can execute SQL queries on CSV, LTSV, JSON, YAML and TBLN files."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use trdsql",
      "reference": "https://github.com/johnlayton/asdf-trdsql"
    },
    "darwin/brew": {
      "cmd": "brew install noborus/tap/trdsql",
      "reference": "https://github.com/noborus/trdsql#install"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && $ sudo port install trdsql",
      "reference": "https://github.com/noborus/trdsql#install"
    },
    "freebsd/pkg": {
      "cmd": "pkg install trdsql",
      "reference": "https://github.com/noborus/trdsql#install"
    }
  }
}
