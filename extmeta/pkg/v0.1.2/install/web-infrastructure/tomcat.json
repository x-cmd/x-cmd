{
  "lang": "java",
  "homepage": "https://tomcat.apache.org/",
  "desc": {
    "cn": "Apache Tomcat",
    "en": "Apache Tomcat"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use tomcat",
      "reference": "https://github.com/mbutov/asdf-tomcat"
    },
    "darwin/brew": {
      "cmd": "brew install tomcat",
      "reference": "https://formulae.brew.sh/formula/tomcat#default"
    },
    "centos/yum": {
      "cmd": "sudo yum install tomcat",
      "reference": "https://command-not-found.com/tomcat"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install tomcat-1",
      "reference": "https://command-not-found.com/tomcat"
    }
  },
  "dsnap": {
    "asdf": "tomcat"
  }
}
