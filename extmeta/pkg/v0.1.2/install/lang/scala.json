{
  "lang": "Scala",
  "homepage": "https://www.scala-lang.org/",
  "desc": {
    "cn": "一种强大的静态类型高级通用编程语言，支持面向对象编程和函数式编程。",
    "en": "a strong statically typed high-level general-purpose programming language that supports both object-oriented programming and functional programming."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use scala",
      "reference": "https://github.com/asdf-community/asdf-scala"
    },
    "darwin/brew": {
      "cmd": "brew install scala",
      "reference": "hhttps://formulae.brew.sh/formula/scala#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install scala",
      "reference": "https://command-not-found.com/scala"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install scala",
      "reference": "https://command-not-found.com/scala"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S scala",
      "reference": "https://command-not-found.com/scala"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install scala",
      "reference": "https://command-not-found.com/scala"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install scala",
      "reference": "https://command-not-found.com/scala"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install scala",
      "reference": "https://command-not-found.com/scala"
    }
  }
}
