{
  "lang": "go",
  "homepage": "https://cuelang.org/",
  "desc": {
    "cn": "CUE 是一门开源语言，有丰富的 API 和 工具链。可以用于定义、生成以及校验各种各样的数据：配置、API、数据库结构、代码等等，应有尽有",
    "en": "CUE is an open source language, with a rich set of APIs and tooling, for defining, generating, and validating all kinds of data: configuration, APIs, database schemas, code, … you name it"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use cue",
      "reference": "https://github.com/asdf-community/asdf-cue"
    },
    "darwin/brew": {
      "cmd": "brew install cue-lang/tap/cue",
      "reference": "https://cuelang.org/docs/install/#install-using-homebrew"
    },
    "/go": {
      "cmd": "go install cuelang.org/go/cmd/cue@latest",
      "reference": "https://cuelang.org/docs/install/#installing-cue",
      "dsnap": "cuelang.org/go/cmd/cue"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install cue",
      "reference": "https://scoop.sh/#/apps?q=cue"
    }
  },
  "dsnap": {
    "brew": "cue-lang/tap/cue",
    "/go": "cuelang.org/go/cmd/cue"
  }
}
