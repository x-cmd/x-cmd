{
  "lang": "c",
  "homepage": "https://www.php.net/",
  "desc": {
    "cn": "PHP 解释器（PHP 是一种流行的通用脚本语言，特别适合 Web 开发。）",
    "en": "A popular general-purpose scripting language that is especially suited to web development"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use php",
      "reference": "https://github.com/asdf-community/asdf-php"
    },
    "darwin/brew": {
      "cmd": "brew install php",
      "reference": "https://formulae.brew.sh/formula/php#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install php",
      "reference": "https://command-not-found.com/php"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install php",
      "reference": "https://command-not-found.com/php"
    },
    "alpine/apk": {
      "cmd": "sudo apk add php",
      "reference": "https://command-not-found.com/php"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S php",
      "reference": "https://command-not-found.com/php"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install php",
      "reference": "https://command-not-found.com/php"
    },
    "centos/yum": {
      "cmd": "sudo yum install php-cli",
      "reference": "https://command-not-found.com/php"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install php-cli",
      "reference": "https://command-not-found.com/php"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install php",
      "reference": "https://command-not-found.com/php"
    }
  }
}
