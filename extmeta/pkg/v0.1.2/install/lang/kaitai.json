{
  "lang": "Shell",
  "homepage": "https://kaitai.io/",
  "desc": {
    "cn": "Kaitai 结构是一种特定领域的语言（DSL），其设计为一个特定任务：处理任意二进制格式",
    "en": "Kaitai Struct is a domain-specific language (DSL) that is designed with one particular task in mind: dealing with arbitrary binary formats"
  },
  "rule": {
    "debian/apt": {
      "cmd": "curl -LO https://github.com/kaitai-io/kaitai_struct_compiler/releases/download/0.10/kaitai-struct-compiler_0.10_all.deb && \\\nsudo apt-get install ./kaitai-struct-compiler_0.10_all.deb\n",
      "reference": "https://kaitai.io/#download"
    },
    "darwin/brew": {
      "cmd": "brew install kaitai-struct-compiler",
      "reference": "https://kaitai.io/#download"
    }
  }
}
