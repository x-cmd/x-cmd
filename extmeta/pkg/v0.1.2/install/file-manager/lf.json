{
  "lang": "go",
  "homepage": "https://github.com/gokcehan/lf",
  "desc": {
    "cn": "终端文件管理器",
    "en": "Terminal file manager"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install lf",
      "reference": "https://formulae.brew.sh/formula/lf#default"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install lf",
      "reference": "https://software.opensuse.org//download.html?project=openSUSE%3AFactory&package=lf"
    },
    "alpine/apk": {
      "cmd": "apk add lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#alpine-linux"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#windows"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#windows"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install gokcehan.lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#windows"
    },
    "solus/eopkg": {
      "cmd": "eopkg install lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#solus"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#void-linux"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add lf",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#openbsd"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable pennbauman/ports && \\\nsudo dnf install lf\n",
      "reference": "https://github.com/gokcehan/lf/wiki/Packages#fedora"
    }
  }
}
