{
  "lang": "Rust",
  "homepage": "https://github.com/chaqchase/lla",
  "desc": {
    "cn": "一个快速的 ls 命令替代品，具有强大的功能。",
    "en": "A blazing fast ls replacement with superpowers."
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sSL https://raw.githubusercontent.com/chaqchase/lla/main/install.sh | bash",
      "reference": "https://github.com/chaqchase/lla?tab=readme-ov-file#using-installation-script"
    },
    "/cargo": {
      "cmd": "cargo install lla",
      "reference": "https://github.com/chaqchase/lla?tab=readme-ov-file#using-package-manager"
    },
    "arch/paru": {
      "cmd": "paru -S lla",
      "reference": "https://github.com/chaqchase/lla?tab=readme-ov-file#using-package-manager"
    },
    "darwin/brew": {
      "cmd": "brew install lla",
      "reference": "https://github.com/chaqchase/lla?tab=readme-ov-file#using-package-manager"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install lla",
      "reference": "https://github.com/chaqchase/lla?tab=readme-ov-file#using-package-manager"
    }
  }
}
