{
  "lang": "Go",
  "homepage": "https://github.com/nore-dev/fman",
  "desc": {
    "cn": "TUI 文件管理器",
    "en": "TUI File Manager"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/nore-dev/fman@latest",
      "reference": "https://github.com/nore-dev/fman?tab=readme-ov-file#go",
      "dsnap": "github.com/nore-dev/fman"
    },
    "/npm": {
      "cmd": "npm i @nore-dev/fman",
      "reference": "https://github.com/nore-dev/fman?tab=readme-ov-file#npm",
      "dsnap": "@nore-dev/fman"
    },
    "darwin/brew": {
      "cmd": "brew install fman/tap/fman",
      "reference": "https://github.com/nore-dev/fman?tab=readme-ov-file#brew"
    }
  },
  "dsnap": {
    "brew": "fman/tap/fman",
    "/npm": "@nore-dev/fman",
    "/go": "github.com/nore-dev/fman"
  }
}
