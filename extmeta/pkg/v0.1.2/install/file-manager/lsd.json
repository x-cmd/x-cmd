{
  "lang": "Rust",
  "homepage": "https://github.com/lsd-rs/lsd",
  "desc": {
    "cn": "下一代 ls 命令",
    "en": "The next gen ls command"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge sys-apps/lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "/nix-env": {
      "cmd": "sudo nix-env -iA nixos.lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install --id lsd-rs.lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "debian/apt": {
      "cmd": "sudo apt install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "solus/eopkg": {
      "cmd": "sudo eopkg it lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install lsd",
      "reference": "https://github.com/lsd-rs/lsd?tab=readme-ov-file#from-source",
      "dsnap": "lsd"
    }
  },
  "dsnap": {
    "/cargo": "lsd"
  }
}
