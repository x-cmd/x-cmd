{
  "lang": "c",
  "desc": {
    "en": "Image Viewer and Cataloguer",
    "cn": "图像查看器和目录管理器"
  },
  "homepage": "https://github.com/derf/feh",
  "rule": {
    "darwin/brew": {
      "cmd": "brew install feh",
      "reference": "https://formulae.brew.sh/formula/feh"
    },
    "/apt": {
      "cmd": "sudo apt install feh",
      "reference": "https://github.com/derf/feh?tab=readme-ov-file#build-process"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S feh",
      "reference": "https://archlinux.org/packages/extra/x86_64/feh/"
    },
    "freebsd/pkg": {
      "cmd": "pkg install feh",
      "reference": "https://www.freshports.org/graphics/feh"
    }
  },
  "dsnap": {
    "brew": "feh",
    "pacman": "feh",
    "pkg": "feh"
  }
}
