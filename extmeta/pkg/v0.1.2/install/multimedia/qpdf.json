{
  "lang": "C++",
  "homepage": "https://github.com/qpdf/qpdf",
  "desc": {
    "cn": "用于转换和检查 PDF 文件的工具",
    "en": "Tools for and transforming and inspecting PDF files"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install qpdf",
      "reference": "https://formulae.brew.sh/formula/qpdf#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install qpdf",
      "reference": "https://scoop.sh/#/apps?q=qpdf"
    }
  },
  "dsnap": {
    "brew": "qpdf"
  }
}
