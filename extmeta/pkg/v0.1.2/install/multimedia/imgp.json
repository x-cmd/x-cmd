{
  "lang": "python",
  "homepage": "https://github.com/jarun/imgp",
  "desc": {
    "cn": "用于 JPEG 和 PNG 图像的命令行图像调整器和旋转器",
    "en": "A command line image resizer and rotator for JPEG and PNG images"
  },
  "rule": {
    "/pip3": {
      "cmd": "sudo pip3 install pillow",
      "reference": "https://github.com/jarun/imgp#installation"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install imgp",
      "reference": "https://command-not-found.com/imgp"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install imgp",
      "reference": "https://command-not-found.com/imgp"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S twine",
      "reference": "https://command-not-found.com/twine"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install imgp",
      "reference": "https://command-not-found.com/imgp"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install imgp",
      "reference": "https://command-not-found.com/imgp"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install imgp",
      "reference": "https://command-not-found.com/imgp"
    }
  },
  "dsnap": {
    "/pip": "pillow"
  }
}
