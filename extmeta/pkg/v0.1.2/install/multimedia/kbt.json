{
  "lang": "Rust",
  "homepage": "https://github.com/bloznelis/kbt",
  "desc": {
    "cn": "终端中的键盘测试器",
    "en": "Keyboard tester in terminal"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S kbt",
      "reference": "https://github.com/bloznelis/kbt?tab=readme-ov-file#installation"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p kbt",
      "reference": "https://github.com/bloznelis/kbt?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install kbt",
      "reference": "https://github.com/bloznelis/kbt?tab=readme-ov-file#installation",
      "dsnap": "kbt"
    },
    "/brew": {
      "cmd": "brew tap bloznelis/tap\nbrew install kbt\n",
      "reference": "https://github.com/bloznelis/kbt?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "kbt",
    "/cargo": "kbt"
  }
}
