{
  "lang": "c++",
  "homepage": "https://sourceforge.net/projects/epsilon-project/",
  "desc": {
    "en": "EPSILON is a powerful Open Source wavelet image compressor",
    "cn": "EPSILON 是一种强大的开源小波图像压缩器"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install epsilon",
      "reference": "https://formulae.brew.sh/formula/epsilon#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install epsilon-bin",
      "reference": "https://command-not-found.com/epsilon",
      "dsnap": "epsilon-bin"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install epsilon-bin",
      "reference": "https://command-not-found.com/epsilon",
      "dsnap": "epsilon-bin"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install epsilon-bin",
      "reference": "https://command-not-found.com/epsilon"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install epsilon-bin",
      "reference": "https://command-not-found.com/epsilon"
    }
  },
  "dsnap": {
    "brew": "epsilon",
    "ubuntu/apt": "epsilon-bin",
    "debian/apt": "epsilon-bin"
  }
}
