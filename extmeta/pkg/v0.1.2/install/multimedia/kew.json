{
  "lang": "C",
  "homepage": "https://github.com/ravachol/kew",
  "desc": {
    "cn": "命令行音乐播放器",
    "en": "Command line music player"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install kew",
      "reference": "https://github.com/ravachol/kew?tab=readme-ov-file#installing-on-debian",
      "dsnap": "kew"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install kew",
      "reference": "https://github.com/ravachol/kew?tab=readme-ov-file#installing-on-debian",
      "dsnap": "kew"
    },
    "arch/yay": {
      "cmd": "sudo yay kew",
      "reference": "https://github.com/ravachol/kew?tab=readme-ov-file#installing-via-aur"
    },
    "darwin/brew": {
      "cmd": "brew install kew",
      "reference": "https://github.com/ravachol/kew?tab=readme-ov-file#installing-via-brew"
    },
    "/curl": {
      "cmd": "sudo bash -c \"curl https://raw.githubusercontent.com/ravachol/kew/main/install.sh | bash\"",
      "reference": "https://github.com/ravachol/kew?tab=readme-ov-file#installing-with-quick-install-script"
    }
  },
  "dsnap": {
    "yay": "kew",
    "brew": "kew",
    "ubuntu/apt": "kew",
    "debian/apt": "kew"
  }
}
