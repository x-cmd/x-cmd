{
  "lang": "TypeScirpt",
  "homepage": "https://mystmd.org/guide",
  "desc": {
    "cn": "Myst 是一个由开源，社区驱动工具组成的生态系统，旨在彻底改变科学交流",
    "en": "MyST is an ecosystem of open-source, community-driven tools designed to revolutionize scientific communication"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install mystmd",
      "reference": "https://mystmd.org/guide/quickstart#installing-the-myst-markdown-cli",
      "dsnap": "mystmd"
    },
    "/conda": {
      "cmd": "conda install mystmd -c conda-forge",
      "reference": "https://mystmd.org/guide/quickstart#installing-the-myst-markdown-cli"
    },
    "/npm": {
      "cmd": "npm install -g mystmd",
      "reference": "https://mystmd.org/guide/quickstart#installing-the-myst-markdown-cli",
      "dsnap": "mystmd"
    }
  },
  "dsnap": {
    "conda": "mystmd",
    "/pip": "mystmd",
    "/npm": "mystmd"
  }
}
