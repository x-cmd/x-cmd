{
  "lang": "Python",
  "homepage": "https://github.com/mikf/gallery-dl",
  "desc": {
    "cn": "用于从多个图像托管站点下载图像库和集合的命令行工具",
    "en": "Command-line program to download image galleries and collections from several image hosting sites"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use gallery-dl",
      "reference": "https://github.com/iul1an/asdf-gallery-dl"
    },
    "/pip": {
      "cmd": "python3 -m pip install -U gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#pip",
      "dsnap": "gallery-dl"
    },
    "/snap": {
      "cmd": "sudo snap install gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#snap"
    },
    "darwin/brew": {
      "cmd": "brew install gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#homebrew"
    },
    "debian/apt": {
      "cmd": "sudo add-apt-repository ppa:tomtomtom/yt-dlp\nsudo apt update\nsudo apt install yt-dlp\n",
      "reference": "https://github.com/yt-dlp/yt-dlp/wiki/Installation#apt"
    },
    "ubuntu/apt": {
      "cmd": "sudo add-apt-repository ppa:tomtomtom/yt-dlp\nsudo apt update\nsudo apt install yt-dlp\n",
      "reference": "https://github.com/yt-dlp/yt-dlp/wiki/Installation#apt"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -Syu yt-dlp",
      "reference": "https://github.com/yt-dlp/yt-dlp/wiki/Installation#pacman"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#macports"
    },
    "alpine/apk": {
      "cmd": "doas apk -U add yt-dlp",
      "reference": "https://github.com/yt-dlp/yt-dlp/wiki/Installation#alpine-linux"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install yt-dlp",
      "reference": "https://github.com/yt-dlp/yt-dlp/wiki/Installation#winget"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#scoop"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install gallery-dl",
      "reference": "https://github.com/mikf/gallery-dl#chocolatey"
    }
  },
  "dsnap": {
    "/pip": "gallery-dl"
  }
}
