{
  "lang": "Go",
  "homepage": "https://github.com/Achno/gowall",
  "desc": {
    "cn": "一个可以将图像转换为你喜欢的任何配色方案/调色板的工具。",
    "en": "A tool to convert an image to any color-scheme / pallete you like."
  },
  "rule": {
    "arch/yay": {
      "cmd": "yay -S gowall",
      "reference": "https://github.com/Achno/gowall?tab=readme-ov-file#arch-linux---aur"
    },
    "darwin/brew": {
      "cmd": "brew install gowall",
      "reference": "https://github.com/Achno/gowall?tab=readme-ov-file#homebrew---maintainer--homebrew-core"
    },
    "linux/brew": {
      "cmd": "brew install gowall",
      "reference": "https://github.com/Achno/gowall?tab=readme-ov-file#homebrew---maintainer--homebrew-core"
    },
    "/go": {
      "cmd": "go install github.com/Achno/gowall@latest",
      "reference": "https://github.com/Achno/gowall?tab=readme-ov-file#build-from-source",
      "dsnap": "github.com/Achno/gowall@latest"
    }
  },
  "dsnap": {
    "brew": "gowall",
    "/go": "github.com/Achno/gowall@latest"
  }
}
