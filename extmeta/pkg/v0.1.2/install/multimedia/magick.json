{
  "lang": "c",
  "homepage": "https://imagemagick.org/",
  "desc": {
    "cn": "magick 用于显示、转换和编辑光栅图像和矢量图像文件",
    "en": "magick is used to display, convert, and edit raster and vector image files"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use imagemagick",
      "reference": "https://github.com/mangalakader/asdf-imagemagick"
    },
    "darwin/brew": {
      "cmd": "brew install imagemagick",
      "reference": "https://formulae.brew.sh/formula/imagemagick#default"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S ImageMagick",
      "reference": "https://command-not-found.com/magick"
    },
    "centos/yum": {
      "cmd": "sudo yum install ImageMagick",
      "reference": "https://command-not-found.com/socat"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install ImageMagick",
      "reference": "https://command-not-found.com/magick",
      "dsnap": "imagemagick"
    }
  },
  "dsnap": {
    "brew": "imagemagick",
    "pacman": "imagemagick",
    "yum": "imagemagick",
    "fedora/dnf": "imagemagick"
  }
}
