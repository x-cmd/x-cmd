    "/yay": {
      "cmd": "yay -S icloudpd-bin",
      "reference": "https://github.com/icloud-photos-downloader/icloud_photos_downloader/blob/master/README_AUR.md"
    }
  },
  "dsnap": {
    "/pip": "icloudpd"
  }
}
