{
  "lang": "Rust",
  "homepage": "https://github.com/Rigellute/spotify-tui",
  "desc": {
    "cn": "一个用 Rust 编写的终端版 Spotify 客户端。",
    "en": "A Spotify client for the terminal written in Rust."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#homebrew"
    },
    "/snap": {
      "cmd": "sudo snap install spt",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#snap"
    },
    "arch/yay": {
      "cmd": "yay -S spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#aur"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#nix"
    },
    "void/xbps-install": {
      "cmd": "sudo xbps-install -Su spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#void-linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable atim/spotify-tui -y && sudo dnf install spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#fedoracentos"
    },
    "centos/dnf": {
      "cmd": "sudo dnf copr enable atim/spotify-tui -y && sudo dnf install spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#fedoracentos"
    },
    "/cargo": {
      "cmd": "cargo install spotify-tui",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#cargo",
      "dsnap": "spotify-tui"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add scoop-bucket https://github.com/Rigellute/scoop-bucket && \\\nx scoop install spotify-tui\n",
      "reference": "https://github.com/Rigellute/spotify-tui?tab=readme-ov-file#scoop-installer"
    }
  },
  "dsnap": {
    "brew": "spotify-tui",
    "/cargo": "spotify-tui"
  }
}
