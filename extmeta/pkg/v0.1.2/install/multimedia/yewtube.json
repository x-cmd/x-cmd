{
  "lang": "Python",
  "homepage": "https://github.com/mps-youtube/yewtube",
  "desc": {
    "cn": "yewtube 是一个基于终端的 YouTube 播放器和下载器。",
    "en": "yewtube is a Terminal based YouTube player and downloader."
  },
  "rule": {
    "/pip": {
      "cmd": "pip install yewtube",
      "reference": "https://github.com/mps-youtube/yewtube?tab=readme-ov-file#using-pip"
    },
    "/pipx": {
      "cmd": "pipx install git+https://github.com/mps-youtube/yewtube.git",
      "reference": "https://github.com/mps-youtube/yewtube?tab=readme-ov-file#using-pipx"
    },
    "darwin/brew": {
      "cmd": "brew install yewtube",
      "reference": "https://formulae.brew.sh/formula/yewtube#default"
    }
  }
}
