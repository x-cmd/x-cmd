{
  "lang": "Go",
  "homepage": "https://github.com/qustavo/httplab",
  "desc": {
    "cn": "用于检查 HTTP 请求并在终端中模拟其响应的交互式 Web 服务器。",
    "en": "An interactive web server for inspecting HTTP requests and mocking their responses in the terminal."
  },
  "rule": {
    "arch/yaourt": {
      "cmd": "yaourt httplab",
      "reference": "https://github.com/qustavo/httplab#install"
    },
    "/eget": {
      "cmd": "eget qustavo/httplab",
      "reference": "https://github.com/qustavo/httplab#install"
    },
    "/go": {
      "cmd": "go install github.com/gchaincl/httplab/cmd/httplab@latest",
      "reference": "https://github.com/qustavo/httplab#install"
    }
  }
}
