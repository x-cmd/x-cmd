{
  "lang": "go",
  "homepage": "https://rclone.org/",
  "desc": {
    "cn": "一个用于管理云存储文件的命令行工具",
    "en": "A command-line tool for managing cloud storage files"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use rclone",
      "reference": "https://github.com/johnlayton/asdf-rclone"
    },
    "/curl": {
      "cmd": "sudo -v ; curl https://rclone.org/install.sh | sudo bash",
      "reference": "https://rclone.org/install/"
    },
    "darwin/brew": {
      "cmd": "brew install rclone",
      "reference": "https://rclone.org/install/"
    },
    "freebsd/curl": {
      "cmd": "sudo -v ; curl https://rclone.org/install.sh | sudo bash",
      "reference": "https://rclone.org/install/"
    },
    "openbsd/curl": {
      "cmd": "sudo -v ; curl https://rclone.org/install.sh | sudo bash",
      "reference": "https://rclone.org/install/"
    },
    "/snap": {
      "cmd": "sudo snap install rclone",
      "reference": "https://rclone.org/install/"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install rclone",
      "reference": "https://rclone.org/install/"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Rclone.Rclone",
      "reference": "https://rclone.org/install/"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install rclone",
      "reference": "https://rclone.org/install/"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install rclone",
      "reference": "https://rclone.org/install/"
    }
  }
}
