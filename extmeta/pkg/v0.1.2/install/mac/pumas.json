{
  "lang": "Rust",
  "homepage": "https://github.com/graelo/pumas",
  "desc": {
    "cn": "用于监控 Apple Silicon Mac 的 TUI 电源使用情况监控工具",
    "en": "A TUI power usage monitoring tool for Apple Silicon Macs"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install pumas",
      "reference": "https://github.com/graelo/pumas#installation",
      "dsnap": "pumas"
    },
    "darwin/brew": {
      "cmd": "brew install graelo/tap/pumas",
      "reference": "https://github.com/graelo/pumas#installation"
    },
    "/eget": {
      "cmd": "eget graelo/pumas",
      "reference": "https://github.com/graelo/pumas#installation"
    }
  },
  "dsnap": {
    "brew": "pumas",
    "/cargo": "pumas"
  }
}
