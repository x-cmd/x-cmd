{
  "lang": "rust",
  "homepage": "https://github.com/vi/websocat",
  "desc": {
    "cn": "用于 WebSockets 的 Netcat、 curl 和 socat",
    "en": "Netcat, curl and socat for WebSockets"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use websocat",
      "reference": "https://github.com/bdellegrazie/asdf-websocat"
    },
    "darwin/brew": {
      "cmd": "brew install websocat",
      "reference": "https://formulae.brew.sh/formula/websocat#default"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install websocat",
      "reference": "https://github.com/vi/websocat#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable atim/websocat -y \\\n&& sudo dnf install websocat\n",
      "reference": "https://command-not-found.com/nmap"
    },
    "freebsd/pkg": {
      "cmd": "pkg install websocat",
      "reference": "https://github.com/vi/websocat#installation"
    }
  },
  "dsnap": {
    "asdf": "websocat"
  }
}
