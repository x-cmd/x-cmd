{
  "lang": "python",
  "homepage": "https://you-get.org/",
  "desc": {
    "cn": "小型命令行工具，用于从网络下载媒体内容（视频、音频、图像）",
    "en": "You-Get is a tiny command-line utility to download media contents (videos, audios, images) from the Web"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install you-get",
      "reference": "https://pypi.org/project/you-get/"
    },
    "/pip3": {
      "cmd": "pip3 install you-get",
      "reference": "https://github.com/nvbn/thefuck#installation"
    },
    "darwin/brew": {
      "cmd": "brew install you-get",
      "reference": "https://github.com/soimort/you-get#option-5-homebrew-mac-only"
    },
    "alpine/apk": {
      "cmd": "sudo apk add you-get",
      "reference": "https://command-not-found.com/you-get"
    },
    "freebsd/pkg": {
      "cmd": "pkg install you-get",
      "reference": "https://github.com/soimort/you-get#option-6-pkg-freebsd-only"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S you-get",
      "reference": "https://command-not-found.com/you-get"
    }
  }
}
