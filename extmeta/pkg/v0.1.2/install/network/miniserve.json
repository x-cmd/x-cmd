{
  "lang": "rust",
  "desc": {
    "cn": "一个通过 HTTP 提供文件和目录的 CLI 工具",
    "en": "A CLI tool to serve files and directories over HTTP"
  },
  "homepage": "https://github.com/svenstaro/miniserve",
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S miniserve",
      "reference": "https://github.com/svenstaro/miniserve?tab=readme-ov-file#how-to-install"
    },
    "termux/pkg": {
      "cmd": "pkg install miniserve",
      "reference": "https://github.com/svenstaro/miniserve?tab=readme-ov-file#how-to-install"
    },
    "darwin/brew": {
      "cmd": "brew install miniserve",
      "reference": "https://github.com/svenstaro/miniserve?tab=readme-ov-file#how-to-install"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install miniserve",
      "reference": "https://github.com/svenstaro/miniserve?tab=readme-ov-file#how-to-install"
    },
    "/cargo": {
      "cmd": "cargo install --locked miniserve",
      "reference": "https://github.com/svenstaro/miniserve?tab=readme-ov-file#how-to-install",
      "dsnap": "miniserve"
    }
  },
  "dsnap": {
    "/cargo": "miniserve"
  }
}
