{
  "lang": "python",
  "homepage": "https://mitmproxy.org/",
  "desc": {
    "cn": "mitmproxy 是一个免费开源的交互式 HTTPS 代理",
    "en": "mitmproxy is a free and open source interactive HTTPS proxy"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use mitmproxy",
      "reference": "https://github.com/NeoHsu/asdf-mitmproxy"
    },
    "/pipx": {
      "cmd": "pipx install mitmproxy",
      "reference": "https://docs.mitmproxy.org/stable/overview-installation/#installation-from-the-python-package-index-pypi"
    },
    "darwin/brew": {
      "cmd": "brew install mitmproxy",
      "reference": "https://formulae.brew.sh/cask/mitmproxy#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install mitmproxy",
      "reference": "https://command-not-found.com/mitmproxy"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install mitmproxy",
      "reference": "https://command-not-found.com/mitmproxy"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mitmproxy",
      "reference": "https://command-not-found.com/mitmproxy"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install mitmproxy",
      "reference": "https://command-not-found.com/mitmproxy"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mitmproxy",
      "reference": "https://docs.mitmproxy.org/stable/overview-installation/#linux"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install mitmproxy",
      "reference": "https://docs.mitmproxy.org/stable/overview-installation/#linux"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install mitmproxy",
      "reference": "https://command-not-found.com/mitmproxy"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install mitmproxy.mitmproxy",
      "reference": "https://winget.run/pkg/mitmproxy/mitmproxy"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/mitmproxy",
      "reference": "https://scoop.sh/#/apps?q=mitmproxy"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install mitmproxy",
      "reference": "https://community.chocolatey.org/packages?q=mitmproxy"
    }
  }
}
