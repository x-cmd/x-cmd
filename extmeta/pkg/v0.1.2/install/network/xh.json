{
  "lang": "rust",
  "homepage": "https://github.com/ducaale/xh",
  "desc": {
    "cn": "xh 是用于发送 HTTP 请求的友好而快速的工具",
    "en": "xh is a friendly and fast tool for sending HTTP requests"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use xh",
      "reference": "https://github.com/NeoHsu/asdf-xh"
    },
    "/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/ducaale/xh/master/install.sh | sh",
      "reference": "https://github.com/ducaale/xh?tab=readme-ov-file#via-curl-linux--macos"
    },
    "win/powershell": {
      "cmd": "x pwsh iwr -useb https://raw.githubusercontent.com/ducaale/xh/master/install.ps1 | iex",
      "reference": "https://github.com/ducaale/xh?tab=readme-ov-file#via-powershell-windows"
    }
  },
  "dsnap": {
    "/cargo": "xh"
  }
}
