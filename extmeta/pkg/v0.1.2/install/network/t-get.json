{
  "lang": "javascript",
  "homepage": "https://github.com/jeffjose/tget",
  "desc": {
    "cn": "tget 是用于种子下载的 wget",
    "en": "tget is wget for torrents"
  },
  "rule": {
    "/yarn": {
      "cmd": "yarn global add t-get",
      "reference": "https://github.com/jeffjose/tget?tab=readme-ov-file#tget"
    },
    "/npm": {
      "cmd": "npm install -g t-get",
      "reference": "https://github.com/jeffjose/tget?tab=readme-ov-file#tget"
    },
    "/yay": {
      "cmd": "yay -S nodejs-tget",
      "reference": "https://github.com/jeffjose/tget?tab=readme-ov-file#tget"
    }
  }
}
