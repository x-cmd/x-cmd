{
  "lang": "Rust",
  "homepage": "https://github.com/mike-engel/jwt-cli",
  "desc": {
    "cn": "jwt-cli 是一个命令行工具，可以帮助您使用 JSON Web Tokens (JWT)",
    "en": "jwt-cli is a command line tool to help you work with JSON Web Tokens (JWTs)"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install jwt-cli",
      "reference": "https://github.com/mike-engel/jwt-cli?tab=readme-ov-file#cargo",
      "dsnap": "jwt-cli"
    },
    "darwin/brew": {
      "cmd": "brew install mike-engel/jwt-cli/jwt-cli",
      "reference": "https://github.com/mike-engel/jwt-cli?tab=readme-ov-file#homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port install jwt-cli",
      "reference": "https://github.com/mike-engel/jwt-cli?tab=readme-ov-file#macports"
    }
  },
  "dsnap": {
    "brew": "mike-engel/jwt-cli/jwt-cli",
    "/cargo": "jwt-cli"
  }
}
