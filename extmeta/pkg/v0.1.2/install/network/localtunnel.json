{
  "lang": "Javascript",
  "homepage": "https://theboroer.github.io/localtunnel-www/",
  "desc": {
    "cn": "Localtunnel 允许您轻松地在本地开发机器上共享 Web 服务，而无需改变 DNS 和防火墙设置",
    "en": "Localtunnel allows you to easily share a web service on your local development machine without messing with DNS and firewall settings"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g localtunnel",
      "reference": "https://github.com/localtunnel/localtunnel?tab=readme-ov-file#globally",
      "dsnap": "localtunnel"
    },
    "/yarn": {
      "cmd": "yarn add localtunnel",
      "reference": "https://github.com/localtunnel/localtunnel?tab=readme-ov-file#as-a-dependency-in-your-project"
    },
    "darwin/brew": {
      "cmd": "brew install localtunnel",
      "reference": "https://formulae.brew.sh/formula/localtunnel#default"
    }
  },
  "dsnap": {
    "brew": "localtunnel",
    "/npm": "localtunnel"
  }
}
