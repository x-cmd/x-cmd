{
  "lang": "rust",
  "homepage": "https://github.com/ekzhang/bore",
  "desc": {
    "cn": "一个简单的 CLI 工具，用于建立到本地主机的隧道",
    "en": "A simple CLI tool for establishing a tunnel to localhost"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install bore-cli",
      "reference": "https://github.com/ekzhang/bore?tab=readme-ov-file#installation",
      "dsnap": "bore-cli"
    },
    "darwin/brew": {
      "cmd": "brew install bore-cli",
      "reference": "https://github.com/ekzhang/bore?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "bore-cli",
    "/cargo": "bore-cli"
  }
}
