{
  "lang": "rust",
  "homepage": "https://github.com/ekzhang/bore",
  "desc": {
    "cn": "Rust 编写的 TCP 隧道工具",
    "en": "A modern, simple TCP tunnel in Rust"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install bore-cli",
      "reference": "https://github.com/ekzhang/bore/?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install bore-cli",
      "reference": "https://github.com/ekzhang/bore/?tab=readme-ov-file#bore",
      "dsnap": "bore-cli"
    }
  },
  "dsnap": {
    "brew": "bore-cli",
    "/cargo": "bore-cli"
  }
}
