{
  "lang": "Go",
  "homepage": "https://github.com/tsenart/vegeta",
  "desc": {
    "cn": "Vegeta 是一款多功能 HTTP 负载测试工具，旨在满足以恒定请求率钻探 HTTP 服务的需求",
    "en": "Vegeta is a versatile HTTP load testing tool built out of a need to drill HTTP services with a constant request rate"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew update && brew install vegeta",
      "reference": "https://github.com/tsenart/vegeta?tab=readme-ov-file#macos"
    },
    "darwin/port": {
      "cmd": "sudo port install vegeta",
      "reference": "https://github.com/tsenart/vegeta?tab=readme-ov-file#macos"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S vegeta",
      "reference": "https://github.com/tsenart/vegeta?tab=readme-ov-file#arch-linux"
    },
    "freebsd/pkg": {
      "cmd": "pkg install vegeta",
      "reference": "https://github.com/tsenart/vegeta?tab=readme-ov-file#freebsd"
    }
  }
}
