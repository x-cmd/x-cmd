{
  "lang": "Rust",
  "homepage": "https://github.com/Chleba/netscanner",
  "desc": {
    "en": "Network scanning & diagnostic tool",
    "cn": "网络扫描和诊断工具"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S netscanner",
      "reference": "https://github.com/Chleba/netscanner?tab=readme-ov-file#install-on-arch-linux"
    },
    "/cargo": {
      "cmd": "cargo install netscanner",
      "reference": "https://github.com/Chleba/netscanner?tab=readme-ov-file#install-with-cargo",
      "dsnap": "netscanner"
    },
    "alpine/apk": {
      "cmd": "apk add netscanner --repository=http://dl-cdn.alpinelinux.org/alpine/edge/testing/",
      "reference": "https://github.com/Chleba/netscanner?tab=readme-ov-file#install-on-alpineedge-linux"
    }
  },
  "dsnap": {
    "/cargo": "netscanner"
  }
}
