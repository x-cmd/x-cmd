{
  "lang": "Rust",
  "homepage": "https://github.com/mihaigalos/aim",
  "desc": {
    "cn": "一个带有恢复功能的命令行下载/上传工具。",
    "en": "A command line download/upload tool with resume."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install aim",
      "reference": "https://github.com/mihaigalos/aim#%EF%B8%8E-installation",
      "dsnap": "aim"
    },
    "arch/paru": {
      "cmd": "paru -S aim",
      "reference": "https://github.com/mihaigalos/aim#%EF%B8%8E-installation"
    },
    "/brew": {
      "cmd": "brew install aim",
      "reference": "https://github.com/mihaigalos/aim#%EF%B8%8E-installation"
    }
  },
  "dsnap": {
    "/cargo": "aim"
  }
}
