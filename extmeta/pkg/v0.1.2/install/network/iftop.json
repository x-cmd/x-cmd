      "dsnap": "iftop"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S iftop",
      "reference": "https://command-not-found.com/iftop"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install iftop",
      "reference": "https://command-not-found.com/iftop"
    },
