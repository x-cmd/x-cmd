{
  "lang": "Go",
  "desc": {
    "cn": "用于 HTTP 检查的交互式 cli 工具",
    "en": "An interactive cli tool for HTTP inspection"
  },
  "homepage": "https://github.com/asciimoo/wuzz",
  "rule": {
    "/go": {
      "cmd": "go get github.com/asciimoo/wuzz",
      "reference": "https://github.com/asciimoo/wuzz?tab=readme-ov-file#installation-and-usage",
      "dsnap": "github.com/asciimoo/wuzz"
    },
    "ubuntu/apt": {
      "cmd": "apt install wuzz",
      "reference": "https://packages.ubuntu.com/oracular/wuzz"
    },
    "alpine/apk": {
      "cmd": "apk add wuzz",
      "reference": "https://pkgs.alpinelinux.org/package/edge/community/x86_64/wuzz"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install main/wuzz",
      "reference": "https://scoop.sh/#/apps?q=wuzz"
    },
    "/nix-shell": {
      "cmd": "nix-shell -p wuzz",
      "reference": "https://search.nixos.org/packages?type=packages&query=wuzz"
    }
  },
  "dsnap": {
    "/go": "github.com/asciimoo/wuzz"
  }
}
