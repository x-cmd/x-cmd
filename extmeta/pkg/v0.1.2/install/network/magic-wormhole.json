{
  "lang": "python",
  "homepage": "https://github.com/magic-wormhole/magic-wormhole",
  "desc": {
    "cn": "Wormhole 使得可以将任意大小的文件和目录（或短文本片段）从一台计算机传输到另一台",
    "en": "wormhole makes it possible to get arbitrary-sized files and directories (or short pieces of text) from one computer to another"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install --user magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#install-from-source"
    },
    "darwin/brew": {
      "cmd": "brew install magic-wormhole",
      "reference": "https://formulae.brew.sh/formula/magic-wormhole#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#linux-debian-ubuntu"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#linux-debian-ubuntu"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install magic-wormhole",
      "reference": "https://command-not-found.com/wormhole"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#linux-fedora"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install python-magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#linux-opensuse"
    },
    "/snap": {
      "cmd": "sudo snap install wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#linux-snap-package"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install magic-wormhole",
      "reference": "https://command-not-found.com/wormhole"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install magic-wormhole",
      "reference": "https://magic-wormhole.readthedocs.io/en/latest/welcome.html#chocolatey"
    }
  }
}
