{
  "lang": "Shell",
  "homepage": "https://github.com/BeyondCodeBootcamp/DuckDNS.sh",
  "desc": {
    "cn": "用于跟踪您的 IP 地址（在家中或在设备上）的 Posix Shell 脚本（与 Bash 兼容）",
    "en": "A Posix Shell Script (Bash-compatible) for tracking your IP address (at home, or on devices)"
  },
  "rule": {
    "/webi": {
      "cmd": "curl -sS https://webi.sh/duckdns.sh@v1 | sh \\\n&& source ~/.config/envman/PATH.env\n",
      "reference": "https://github.com/BeyondCodeBootcamp/DuckDNS.sh?tab=readme-ov-file#via-webi"
    },
    "/git": {
      "cmd": "my_branch=\"v1\" \\\n&& mkdir -p ~/bin/ \\\n&& curl -fsSL -o ~/bin/duckdns.sh \"https://raw.githubusercontent.com/BeyondCodeBootcamp/DuckDNS.sh/${my_branch}/duckdns.sh\" \\\n&& chmod a+x ~/bin/duckdns.sh \\\n",
      "reference": "https://github.com/BeyondCodeBootcamp/DuckDNS.sh?tab=readme-ov-file#via-git-assets"
    }
  }
}
