{
  "lang": "rust",
  "homepage": "https://github.com/orf/gping",
  "desc": {
    "cn": "Ping，但有图表",
    "en": "Ping, but with graphs"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "darwin/port": {
      "cmd": "sudo port install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable atim/gping -y && sudo dnf install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "/cargo": {
      "cmd": "cargo install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd",
      "dsnap": "gping"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "alpine/apk": {
      "cmd": "sudo apk add gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "debian/apt": {
      "cmd": "sudo apt install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "gentoo/emerge": {
      "cmd": "sudo eselect repository enable dm9pZCAq \\\n&& sudo emerge --sync dm9pZCAq \\\n&& sudo emerge net-misc/gping::dm9pZCAq\n",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "freebsd/pkg": {
      "cmd": "pkg install gping",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    },
    "freebsd/ports": {
      "cmd": "cd /usr/ports/net-mgmt/gping && make install clean",
      "reference": "https://github.com/orf/gping?tab=readme-ov-file#install-cd"
    }
  },
  "dsnap": {
    "/cargo": "gping"
  }
}
