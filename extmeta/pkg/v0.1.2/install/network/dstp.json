{
  "lang": "Go",
  "homepage": "https://github.com/ycd/dstp",
  "desc": {
    "cn": "针对您的站点运行常见的网络测试",
    "en": "run common networking tests against your site"
  },
  "rule": {
    "/brew": {
      "cmd": "brew install ycd/tap/dstp",
      "reference": "https://github.com/ycd/dstp?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/ycd/dstp/cmd/dstp@latest",
      "reference": "https://github.com/ycd/dstp?tab=readme-ov-file#installation"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p dstp",
      "reference": "https://search.nixos.org/packages?query=dstp"
    }
  }
}
