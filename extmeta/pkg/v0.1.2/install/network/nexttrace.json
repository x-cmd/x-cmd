{
  "lang": "Go",
  "homepage": "https://github.com/nxtrace/NTrace-core",
  "desc": {
    "cn": "NextTrace - 开源可视化路线跟踪 CLI 工具",
    "en": "NextTrace, an open source visual route tracking CLI tool"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/nxtrace/NTrace-core@latest",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#manual-install",
      "dsnap": "github.com/nxtrace/NTrace-core"
    },
    "linux/curl": {
      "cmd": "curl nxtrace.org/nt |bash",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#automated-install"
    },
    "arch/yay": {
      "cmd": "yay -S nexttrace-bin",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#automated-install"
    },
    "(ubuntu|debian)/apt": {
      "cmd": "apt install nexttrace",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#automated-install"
    },
    "/brew": {
      "cmd": "brew install nexttrace",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#automated-install"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop bucket add extras && scoop install extras/nexttrace",
      "reference": "https://github.com/nxtrace/NTrace-core?tab=readme-ov-file#automated-install"
    }
  },
  "dsnap": {
    "brew": "nexttrace",
    "/go": "github.com/nxtrace/NTrace-core"
  }
}
