{
  "lang": "Rust",
  "homepage": "https://shadowsocks.org/",
  "desc": {
    "cn": "Shadowsocks 是一个快速隧道代理，可以帮助您绕过防火墙",
    "en": "shadowsocks is a fast tunnel proxy that helps you bypass firewalls"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install shadowsocks-rust",
      "reference": "https://github.com/shadowsocks/shadowsocks-rust?tab=readme-ov-file#cratesio",
      "dsnap": "shadowsocks-rust"
    },
    "darwin/brew": {
      "cmd": "brew install shadowsocks-rust",
      "reference": "https://github.com/shadowsocks/shadowsocks-rust?tab=readme-ov-file#install-using-homebrew"
    },
    "/snap": {
      "cmd": "snap install shadowsocks-rust",
      "reference": "https://github.com/shadowsocks/shadowsocks-rust?tab=readme-ov-file#install-using-snap"
    }
  },
  "dsnap": {
    "brew": "shadowsocks-rust",
    "/cargo": "shadowsocks-rust"
  }
}
