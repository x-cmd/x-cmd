{
  "lang": "Rust",
  "homepage": "https://github.com/mrjackwills/havn",
  "desc": {
    "cn": "一个快速且可配置的端口扫描器，具有合理的默认值。",
    "en": "A fast configurable port scanner with reasonable defaults."
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/mrjackwills/havn/main/install.sh | bash",
      "reference": "https://github.com/mrjackwills/havn?tab=readme-ov-file#download--install"
    },
    "/cargo": {
      "cmd": "cargo install havn",
      "reference": "https://github.com/mrjackwills/havn?tab=readme-ov-file#download--install",
      "dsnap": "havn"
    },
    "arch/pacman": {
      "cmd": "pacman -S havn",
      "reference": "https://github.com/mrjackwills/havn?tab=readme-ov-file#download--install"
    }
  },
  "dsnap": {
    "/cargo": "havn"
  }
}
