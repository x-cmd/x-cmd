{
  "lang": "C",
  "homepage": "https://github.com/unfrl/dug",
  "desc": {
    "cn": "一个基于 CLI 的 DNS 传播检查器，具有美观的输出。",
    "en": "A CLI based DNS propagation checker with attractive output."
  },
  "rule": {
    "/snap": {
      "cmd": "sudo snap install unfrl-dug",
      "reference": "https://github.com/mehrdad-f/dug#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S dug",
      "reference": "https://github.com/mehrdad-f/dug#installation"
    },
    "/brew": {
      "cmd": "brew install dug",
      "reference": "https://github.com/mehrdad-f/dug#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install dug",
      "reference": "https://github.com/mehrdad-f/dug#installation"
    }
  },
  "dsnap": {
    "brew": "dug"
  }
}
