{
  "lang": "Rust",
  "homepage": "https://github.com/curlpipe/ox",
  "desc": {
    "en": "Ox is a text editor that can be used to write everything from text to code",
    "cn": "Ox 是一款文本编辑器，可以用来编写从文本到代码的各种内容。"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install ox",
      "reference": "https://github.com/curlpipe/ox?tab=readme-ov-file#macos"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate\nsudo port install ox\n",
      "reference": "https://github.com/curlpipe/ox?tab=readme-ov-file#macos"
    },
    "/cargo": {
      "cmd": "cargo install --git https://github.com/curlpipe/ox",
      "reference": "https://github.com/curlpipe/ox?tab=readme-ov-file#manual"
    }
  }
}
