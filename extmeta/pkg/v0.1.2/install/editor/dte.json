{
  "lang": "C",
  "homepage": "https://gitlab.com/craigbarnes/dte",
  "desc": {
    "en": "A small, configurable terminal text editor",
    "cn": "一个小型、可配置的终端文本编辑器"
  },
  "rule": {
    "debian/apt-get": {
      "cmd": "apt-get install dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    },
    "ubuntu/apt-get": {
      "cmd": "apt-get install dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    },
    "arch/pacman": {
      "cmd": "pacman -S dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    },
    "freebsd/pkg": {
      "cmd": "pkg install dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    },
    "termux/pkg": {
      "cmd": "pkg install dte",
      "reference": "https://gitlab.com/craigbarnes/dte#installing"
    }
  }
}
