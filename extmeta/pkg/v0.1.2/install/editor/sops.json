{
  "lang": "Go",
  "homepage": "https://github.com/mozilla/sops",
  "desc": {
    "cn": "加密文件编辑器",
    "en": "Encrypted file editor"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use sops",
      "reference": "https://github.com/feniix/asdf-sops"
    },
    "/go": {
      "cmd": "mkdir -p $GOPATH/src/github.com/getsops/sops/ \\\n&& git clone https://github.com/getsops/sops.git $GOPATH/src/github.com/getsops/sops/  \\\n&& cd $GOPATH/src/github.com/getsops/sops/  \\\n&& make install\n",
      "reference": "https://github.com/getsops/sops?tab=readme-ov-file#12development-branch",
      "dsnap": "github.com/getsops/sops"
    }
  },
  "dsnap": {
    "/go": "github.com/getsops/sops"
  }
}
