{
  "lang": "Rust",
  "homepage": "https://github.com/jmacdonald/amp",
  "desc": {
    "en": "A complete text editor for your terminal",
    "cn": "为你的终端提供的完整文本编辑器"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install amp",
      "reference": "https://amp.rs/docs/installation/"
    },
    "/cargo": {
      "cmd": "cargo install amp",
      "reference": "https://amp.rs/docs/installation/",
      "dsnap": "amp"
    }
  },
  "dsnap": {
    "brew": "amp",
    "/cargo": "amp"
  }
}
