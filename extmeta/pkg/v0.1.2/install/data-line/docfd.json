{
  "lang": "OCaml",
  "homepage": "https://github.com/darrenldl/docfd",
  "desc": {
    "en": "TUI multiline fuzzy document finder",
    "cn": "TUI 多行模糊文档查找器"
  },
  "rule": {
    "/opam": {
      "cmd": "opam install docfd",
      "reference": "https://ocaml.org/p/docfd/latest"
    },
    "arch/pacman": {
      "cmd": "pacman -S docfd",
      "reference": "https://github.com/darrenldl/docfd?tab=readme-ov-file#installation"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p docfd",
      "reference": "https://search.nixos.org/packages?query=docfd"
    }
  }
}
