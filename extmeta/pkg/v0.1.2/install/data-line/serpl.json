{
  "lang": "Rust",
  "homepage": "https://github.com/yassinebridi/serpl",
  "desc": {
    "cn": "serpl 是一个终端用户界面 (TUI) 应用程序，允许用户搜索和替换整个文件夹中的关键字，类似于 VS Code 中提供的功能",
    "en": "serpl is a terminal user interface (TUI) application that allows users to search and replace keywords in an entire folder, similar to the functionality available in VS Code"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install serpl",
      "reference": "https://github.com/yassinebridi/serpl?tab=readme-ov-file#prerequisites",
      "dsnap": "serpl"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S serpl",
      "reference": "https://github.com/yassinebridi/serpl?tab=readme-ov-file#arch-linux"
    },
    "nix/nixpkgs": {
      "cmd": "nix profile install nixpkgs#serpl",
      "reference": "https://github.com/yassinebridi/serpl?tab=readme-ov-file#nixnixos"
    }
  },
  "dsnap": {
    "/cargo": "serpl"
  }
}
