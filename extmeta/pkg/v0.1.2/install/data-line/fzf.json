{
  "lang": "go",
  "homepage": "https://github.com/junegunn/fzf",
  "desc": {
    "cn": "通用命令行模糊查找器",
    "en": "general-purpose command-line fuzzy finder"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use fzf",
      "reference": "https://github.com/kompiro/asdf-fzf"
    },
    "darwin/brew": {
      "cmd": "brew install fzf",
      "reference": "https://formulae.brew.sh/formula/fzf#default"
    },
    "/git": {
      "cmd": "git clone --depth 1 https://github.com/junegunn/fzf.git ~/.fzf && source ~/.fzf/install"
    },
    "debian/apt": {
      "cmd": "sudo apt install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "alpine/apk": {
      "cmd": "sudo apk add fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "kali/apt": {
      "cmd": "sudo apt install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -iA nixpkgs.fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask app-shells/fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install fzf",
      "reference": "https://github.com/junegunn/fzf#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "choco install fzf",
      "reference": "https://github.com/junegunn/fzf?tab=readme-ov-file#windows"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install fzf",
      "reference": "https://github.com/junegunn/fzf?tab=readme-ov-file#windows"
    },
    "(win|wsl2)/winget": {
      "cmd": "winget install fzf",
      "reference": "https://github.com/junegunn/fzf?tab=readme-ov-file#windows"
    }
  }
}
