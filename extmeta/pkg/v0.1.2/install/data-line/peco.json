{
  "lang": "go",
  "desc": {
    "cn": "简单的交互式过滤工具",
    "en": "Simplistic interactive filtering tool"
  },
  "homepage": "https://github.com/peco/peco",
  "rule": {
    "darwin/brew": {
      "cmd": "brew install peco",
      "reference": "https://github.com/peco/peco#installation"
    },
    "/scarf": {
      "cmd": "scarf install peco",
      "reference": "https://github.com/peco/peco#installation"
    },
    "/apt": {
      "cmd": "apt install peco",
      "reference": "https://github.com/peco/peco#installation"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S peco",
      "reference": "https://github.com/peco/peco#installation"
    },
    "arch/pacman": {
      "cmd": "pacman -Syu peco",
      "reference": "https://github.com/peco/peco#installation"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install peco",
      "reference": "https://github.com/peco/peco#installation"
    }
  },
  "dsnap": {
    "brew": "peco",
    "/cargo": "peco",
    "/go": "peco"
  }
}
