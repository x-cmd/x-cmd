{
  "lang": "Python",
  "homepage": "https://mercurial-scm.org/",
  "desc": {
    "cn": "Mercurial 是一款免费的分布式源代码控制管理工具",
    "en": "Mercurial is a free, distributed source control management tool"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt-get install mercurial",
      "reference": "https://mercurial-scm.org/downloads",
      "dsnap": "mercurial"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install mercurial",
      "reference": "https://mercurial-scm.org/downloads",
      "dsnap": "mercurial"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mercurial",
      "reference": "https://mercurial-scm.org/downloads",
      "dsnap": "mercurial"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge mercurial",
      "reference": "https://mercurial-scm.org/downloads"
    },
    "darwin/brew": {
      "cmd": "sudo brew install mercurial",
      "reference": "https://mercurial-scm.org/downloads"
    },
    "freebsd/apt": {
      "cmd": "cd /usr/ports/devel/mercurial && \\\nmake install\n",
      "reference": "https://mercurial-scm.org/downloads"
    },
    "solaris/pkg": {
      "cmd": "sudo pkg install SUNWmercurial",
      "reference": "https://mercurial-scm.org/downloads"
    },
    "/pip": {
      "cmd": "pip install mercurial",
      "reference": "https://pypi.org/project/mercurial/",
      "dsnap": "mercurial"
    }
  },
  "dsnap": {
    "brew": "mercurial",
    "/pip": "mercurial",
    "fedora/dnf": "mercurial",
    "ubuntu/apt": "mercurial",
    "debian/apt": "mercurial"
  }
}
