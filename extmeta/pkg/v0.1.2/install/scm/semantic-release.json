{
  "lang": "javascript",
  "homepage": "https://semantic-release.gitbook.io/semantic-release/",
  "desc": {
    "cn": "semantic-release 自动化了整个包发布工作流程，包括：确定下一个版本号，生成发行说明，以及发布包",
    "en": "semantic-release automates the whole package release workflow including: determining the next version number, generating the release notes, and publishing the package"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install --save-dev semantic-release",
      "reference": "https://github.com/semantic-release/semantic-release/blob/master/docs/usage/installation.md#local-installation",
      "dsnap": "semantic-release"
    },
    "/npx": {
      "cmd": "npx semantic-release",
      "reference": "https://github.com/semantic-release/semantic-release/blob/master/docs/usage/installation.md#global-installation"
    }
  },
  "dsnap": {
    "/npm": "semantic-release"
  }
}
