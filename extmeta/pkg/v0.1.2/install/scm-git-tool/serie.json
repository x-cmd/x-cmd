{
  "lang": "Rust",
  "homepage": "https://github.com/lusingander/serie",
  "desc": {
    "cn": "用于在终端中渲染丰富的 git 提交图的命令行工具",
    "en": "A command line tool to render rich git commit graphs in your terminal"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S serie",
      "reference": "https://github.com/lusingander/serie#installation"
    },
    "darwin/brew": {
      "cmd": "brew install lusingander/tap/serie",
      "reference": "https://github.com/lusingander/serie#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install serie",
      "reference": "https://github.com/lusingander/serie#installation"
    },
    "/cargo": {
      "cmd": "cargo install --locked serie",
      "reference": "https://github.com/lusingander/serie#installation"
    },
    "/eget": {
      "cmd": "eget lusingander/serie",
      "reference": "https://github.com/lusingander/serie#installation"
    }
  }
}
