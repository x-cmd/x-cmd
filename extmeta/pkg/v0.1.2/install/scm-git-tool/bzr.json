{
  "lang": "Python",
  "homepage": "https://bzrhub.com/stewartsmith/bzr",
  "desc": {
    "cn": "Bazaar (bzr) 是一个分散的修订控制系统，旨在易于开发人员和最终用户",
    "en": "Bazaar (bzr) is a decentralized revision control system, designed to be easy for developers and end users alike"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install bzr",
      "reference": "https://pypi.org/project/bzr/"
    },
    "darwin/bzr": {
      "cmd": "sudo port install bzr",
      "reference": "https://ports.macports.org/port/bzr/"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "alpine/apk": {
      "cmd": "sudo apk add bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "centos/yum": {
      "cmd": "sudo yum install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install bzr",
      "reference": "https://command-not-found.com/bzr.bzr"
    }
  }
}
