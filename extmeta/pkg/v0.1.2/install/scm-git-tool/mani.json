{
  "lang": "Go",
  "homepage": "https://manicli.com/",
  "desc": {
    "cn": "mani 是一个 CLI 工具，用于管理多个仓库，方便用户在微服务、多项目系统、多个库或任何仓库集合中进行集中式拉取和跨仓库命令执行。",
    "en": "mani is a CLI tool for managing multiple repositories, allowing users to perform centralized pulls and execute cross-repository commands in microservices, multi-project systems, multiple repositories, or any collection of repositories."
  },
  "rule": {
    "linux/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/alajmo/mani/main/install.sh | sh",
      "reference": "https://manicli.com/installation"
    },
    "darwin/curl": {
      "cmd": "curl -sfL https://raw.githubusercontent.com/alajmo/mani/main/install.sh | sh",
      "reference": "https://manicli.com/installation"
    },
    "linux/brew": {
      "cmd": "brew tap alajmo/mani && brew install mani",
      "reference": "https://manicli.com/installation"
    },
    "darwin/brew": {
      "cmd": "brew tap alajmo/mani && brew install mani",
      "reference": "https://manicli.com/installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mani",
      "reference": "https://manicli.com/installation"
    },
    "/nix-env": {
      "cmd": "sudo nix-env -iA nixos.mani",
      "reference": "https://manicli.com/installation"
    },
    "/go": {
      "cmd": "go get -u github.com/alajmo/mani",
      "reference": "https://manicli.com/installation",
      "dsnap": "github.com/alajmo/mani"
    }
  },
  "dsnap": {
    "brew": "mani",
    "/go": "github.com/alajmo/mani"
  }
}
