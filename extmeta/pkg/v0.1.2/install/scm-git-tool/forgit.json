{
  "lang": "Shell",
  "homepage": "https://github.com/wfxr/forgit",
  "desc": {
    "cn": "用于使用 Git 交互的命令行工具，由 fzf 提供支持",
    "en": "A utility tool powered by fzf for using Git interactively."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install forgit",
      "reference": "https://github.com/wfxr/forgit#homebrew"
    },
    "/zplug": {
      "cmd": "zplug 'wfxr/forgit'",
      "reference": "https://github.com/wfxr/forgit#shell-package-managers"
    },
    "/zgen": {
      "cmd": "zgen load 'wfxr/forgit'",
      "reference": "https://github.com/wfxr/forgit#shell-package-managers"
    },
    "/antigen": {
      "cmd": "antigen bundle 'wfxr/forgit'",
      "reference": "https://github.com/wfxr/forgit#shell-package-managers"
    },
    "/fisher": {
      "cmd": "fisher install wfxr/forgit",
      "reference": "https://github.com/wfxr/forgit#shell-package-managers"
    },
    "/omf": {
      "cmd": "omf install https://github.com/wfxr/forgit",
      "reference": "https://github.com/wfxr/forgit#shell-package-managers"
    },
    "/zinit": {
      "cmd": "zinit load wfxr/forgit",
      "reference": "https://github.com/wfxr/forgit##shell-package-managers"
    }
  },
  "dsnap": {
    "brew": "forgit"
  }
}
