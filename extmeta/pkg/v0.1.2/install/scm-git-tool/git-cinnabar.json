{
  "lang": "rust",
  "homepage": "https://github.com/glandium/git-cinnabar",
  "desc": {
    "cn": "Git-cinnabar 是一个与仓库交互的 git 远程助手",
    "en": "Git-cinnabar is a git remote helper to interact with mercurial repositories"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install git-cinnabar",
      "reference": "https://formulae.brew.sh/formula/git-cinnabar#default"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install git-cinnabar",
      "reference": "https://command-not-found.com/git-cinnabar"
    },
    "/cargo": {
      "cmd": "cargo install git-cinnabar",
      "reference": "https://github.com/glandium/git-cinnabar?tab=readme-ov-file#cargo",
      "dsnap": "git-cinnabar"
    }
  },
  "dsnap": {
    "/cargo": "git-cinnabar"
  }
}
