{
  "lang": "Go",
  "homepage": "https://github.com/dhth/prs",
  "desc": {
    "en": "prs lets you stay updated on pull requests from your terminal.",
    "cn": "prs 让你可以通过终端随时了解 pull requests 的最新动态"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install dhth/tap/prs",
      "reference": "https://github.com/dhth/prs?tab=readme-ov-file#-installation"
    },
    "/go": {
      "cmd": "go install github.com/dhth/prs@latest",
      "reference": "https://github.com/dhth/prs?tab=readme-ov-file#-installation",
      "dsnap": "github.com/dhth/prs"
    }
  },
  "dsnap": {
    "brew": "dhth/tap/prs",
    "/go": "github.com/dhth/prs"
  }
}
