{
  "lang": "rust",
  "homepage": "https://convco.github.io/",
  "desc": {
    "cn": "传统的提交、更改日志、版本控制、验证",
    "en": "Conventional commits, changelog, versioning, validation"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install convco",
      "reference": "https://github.com/convco/convco?tab=readme-ov-file#installation",
      "dsnap": "convco"
    },
    "darwin/brew": {
      "cmd": "brew install convco",
      "reference": "https://convco.github.io/"
    }
  },
  "dsnap": {
    "brew": "convco",
    "/cargo": "convco"
  }
}
