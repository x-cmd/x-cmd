{
  "lang": "rust",
  "homepage": "https://github.com/drewwyatt/git-tidy",
  "desc": {
    "cn": "整理陈旧的 git 分支",
    "en": "Tidy up stale git branches"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap drewwyatt/tap\n&& brew install git-tidy\n",
      "reference": "https://github.com/drewwyatt/git-tidy?tab=readme-ov-file#homebrew"
    },
    "/cargo": {
      "cmd": "cargo install git-tidy",
      "reference": "https://github.com/drewwyatt/git-tidy?tab=readme-ov-file#cargo",
      "dsnap": "git-tidy"
    }
  },
  "dsnap": {
    "brew": "git-tidy",
    "/cargo": "git-tidy"
  }
}
