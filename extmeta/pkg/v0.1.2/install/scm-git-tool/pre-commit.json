{
  "lang": "python",
  "homepage": "https://github.com/pre-commit/pre-commit",
  "desc": {
    "cn": "用于管理和维护多语言预提交挂钩的框架",
    "en": "A framework for managing and maintaining multi-language pre-commit hooks."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use pre-commit",
      "reference": "https://github.com/jonathanmorley/asdf-pre-commit"
    },
    "/pip": {
      "cmd": "pip install pre-commit",
      "reference": "https://pre-commit.com/#installation",
      "dsnap": "pre-commit"
    },
    "darwin/brew": {
      "cmd": "brew install pre-commit",
      "reference": "https://formulae.brew.sh/formula/pre-commit#default"
    }
  },
  "dsnap": {
    "brew": "pre-commit",
    "/pip": "pre-commit"
  }
}
