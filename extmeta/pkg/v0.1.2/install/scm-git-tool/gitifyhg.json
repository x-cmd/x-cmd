{
  "lang": "shell",
  "homepage": "https://github.com/jdalrymple/gitifyhg",
  "desc": {
    "cn": "用于将 git 作为汞存储库的客户端的工具",
    "en": "Tools for using git as a client to mercurial repositories"
  },
  "rule": {
    "fedora/dnf": {
      "cmd": "sudo dnf install gitifyhg",
      "reference": "https://command-not-found.com/git-remote-gitifyhg"
    },
    "/pip": {
      "cmd": "pip install gitifyhg",
      "reference": "https://github.com/jdalrymple/gitifyhg?tab=readme-ov-file#install"
    },
    "/easy_install": {
      "cmd": "easy_install gitifyhg",
      "reference": "https://github.com/jdalrymple/gitifyhg?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew install --cask gitify",
      "reference": "https://formulae.brew.sh/cask/gitify#default"
    }
  }
}
