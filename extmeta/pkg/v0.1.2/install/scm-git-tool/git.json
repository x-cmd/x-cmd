{
  "lang": "C",
  "homepage": "https://git-scm.com/",
  "desc": {
    "cn": "Git 是一个免费的开源分散式版本控制，旨在以快速高效的方式处理从小到大的各种项目",
    "en": "Git is a free and open source distributed version control system designed to handle everything from small to very large projects with speed and efficiency"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install git",
      "reference": "https://formulae.brew.sh/formula/git#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install git",
      "reference": "https://command-not-found.com/git"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install git",
      "reference": "https://command-not-found.com/git"
    },
    "alpine/apk": {
      "cmd": "sudo apk add git",
      "reference": "https://command-not-found.com/git"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S git",
      "reference": "https://command-not-found.com/git"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install git",
      "reference": "https://command-not-found.com/git"
    },
    "centos/yum": {
      "cmd": "sudo yum install git",
      "reference": "https://command-not-found.com/git"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install git",
      "reference": "https://command-not-found.com/git"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install git",
      "reference": "https://command-not-found.com/git"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask --verbose dev-vcs/git",
      "reference": "https://git-scm.com/download/linux"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install git",
      "reference": "https://git-scm.com/download/linux"
    },
    "opensuse-tumbleweed/zypper": {
      "cmd": "sudo zypper install git-core",
      "reference": "https://git-scm.com/download/linux"
    },
    "mageia/urpmi": {
      "cmd": "sudo urpmi git",
      "reference": "https://git-scm.com/download/linux"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -i git",
      "reference": "https://git-scm.com/download/linux"
    },
    "freebsd/pkg": {
      "cmd": "pkg install git",
      "reference": "https://git-scm.com/download/linux"
    },
    "opencsw/pkgutil": {
      "cmd": "pkgutil -i git",
      "reference": "https://git-scm.com/download/linux"
    },
    "solaris/pkg": {
      "cmd": "pkg install developer/versioning/git",
      "reference": "https://git-scm.com/download/linux"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add git",
      "reference": "https://git-scm.com/download/linux"
    },
    "slitaz/tazpkg": {
      "cmd": "tazpkg get-install git",
      "reference": "https://git-scm.com/download/linux"
    }
  }
}
