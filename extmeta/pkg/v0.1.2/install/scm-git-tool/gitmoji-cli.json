{
  "lang": "javascript",
  "homepage": "https://github.com/carloscuesta/gitmoji-cli#readme",
  "desc": {
    "cn": "gitmoji 交互式命令行工具，用于在提交时使用表情符号。",
    "en": "A gitmoji client for using emojis on commit messages"
  },
  "rule": {
    "/npm": {
      "cmd": "npm i -g gitmoji-cli",
      "reference": "https://github.com/carloscuesta/gitmoji-cli?tab=readme-ov-file#npm"
    },
    "darwin/brew": {
      "cmd": "brew install gitmoji",
      "reference": "https://github.com/carloscuesta/gitmoji-cli?tab=readme-ov-file#brew"
    }
  }
}
