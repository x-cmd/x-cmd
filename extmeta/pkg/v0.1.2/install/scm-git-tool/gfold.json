{
  "lang": "Rust",
  "homepage": "https://github.com/nickgerace/gfold",
  "desc": {
    "cn": "用于跟踪目录中多个 Git 存储库的命令行工具。",
    "en": "A CLI tool that helps you keep track of multiple Git repositories."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    },
    "alpine/apk": {
      "cmd": "apk add gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    },
    "nix/nix-env": {
      "cmd": "nix-env --install gfold",
      "reference": "https://github.com/nickgerace/gfold#nix-and-nixos"
    },
    "darwin/brew": {
      "cmd": "brew install gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    },
    "darwin/port": {
      "cmd": "sudo port install gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install devel/gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    },
    "/cargo": {
      "cmd": "cargo install gfold",
      "reference": "https://github.com/nickgerace/gfold#installation",
      "dsnap": "gfold"
    },
    "/eget": {
      "cmd": "eget nickgerace/gfold",
      "reference": "https://github.com/nickgerace/gfold#installation"
    }
  },
  "dsnap": {
    "/cargo": "gfold"
  }
}
