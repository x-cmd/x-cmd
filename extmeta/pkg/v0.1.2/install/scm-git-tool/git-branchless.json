{
  "lang": "rust",
  "homepage": "https://github.com/arxanas/git-branchless",
  "desc": {
    "cn": "Git-branchless 是一套在各方面增强 Git 的工具",
    "en": "git-branchless is a suite of tools which enhances Git in several ways"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install git-branchless",
      "reference": "https://formulae.brew.sh/formula/git-branchless#default"
    },
    "/cargo": {
      "cmd": "cargo install git-branchless",
      "reference": "https://github.com/arxanas/git-branchless/wiki/Installation#installation",
      "dsnap": "git-branchless"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install git-branchless",
      "reference": "https://scoop.sh/#/apps?q=git-branchless"
    }
  },
  "dsnap": {
    "/cargo": "git-branchless"
  }
}
