{
  "lang": "Rust",
  "homepage": "https://github.com/reemus-dev/gitnr",
  "desc": {
    "en": "A cross-platform CLI utility to create .gitignore files using templates.",
    "cn": "一个跨平台 CLI 实用程序，使用模板创建 .gitignore 文件"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -s https://raw.githubusercontent.com/reemus-dev/gitnr/main/scripts/install.sh | bash -s -- -u",
      "reference": "https://github.com/reemus-dev/gitnr?tab=readme-ov-file#install--update"
    },
    "win/powershell": {
      "cmd": "x pwsh 'Set-ExecutionPolicy Unrestricted -Scope Process; iex (iwr \"https://raw.githubusercontent.com/reemus-dev/gitnr/main/scripts/install.ps1\").Content'",
      "reference": "https://github.com/reemus-dev/gitnr?tab=readme-ov-file#windows"
    },
    "/cargo": {
      "cmd": "cargo install gitnr",
      "reference": "https://github.com/reemus-dev/gitnr?tab=readme-ov-file#cargo"
    },
    "/cargo-binstall": {
      "cmd": "cargo-binstall gitnr",
      "reference": "https://github.com/reemus-dev/gitnr?tab=readme-ov-file#cargo-binstall"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install gitnr",
      "reference": "https://github.com/reemus-dev/gitnr?tab=readme-ov-file#netbsd"
    }
  }
}
