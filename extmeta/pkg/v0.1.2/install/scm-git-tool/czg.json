{
  "lang": "typescript",
  "homepage": "https://cz-git.qbb.sh/",
  "desc": {
    "cn": "生成标准化 git 提交消息的交互式 CLI",
    "en": "Interactive CLI that generate standardized git commit messages"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g czg",
      "reference": "https://www.npmjs.com/package/czg#installation"
    },
    "darwin/brew": {
      "cmd": "brew install czg",
      "reference": "https://formulae.brew.sh/formula/czg#default"
    }
  }
}
