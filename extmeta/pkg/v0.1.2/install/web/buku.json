{
  "lang": "python",
  "homepage": "https://github.com/jarun/buku",
  "desc": {
    "cn": "buku 是一个书签管理器和个人文本迷你网络",
    "en": "buku is a powerful bookmark manager and a personal textual mini-web"
  },
  "rule": {
    "/pip3": {
      "cmd": "pip3 install buku",
      "reference": "https://github.com/jarun/buku?tab=readme-ov-file#from-a-package-manager"
    },
    "/pip": {
      "cmd": "pip install buku",
      "reference": "https://pypi.org/project/buku/"
    },
    "darwin/brew": {
      "cmd": "brew install buku",
      "reference": "https://formulae.brew.sh/formula/buku#default"
    }
  }
}
