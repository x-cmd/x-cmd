{
  "lang": "python",
  "homepage": "https://trafilatura.readthedocs.io/en/latest/installation.html#command-line-tool",
  "desc": {
    "cn": "用于收集 Web 上文本的 Python 包和命令行工具",
    "en": "A Python package & command-line tool to gather text on the Web"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install trafilatura",
      "reference": "https://trafilatura.readthedocs.io/en/latest/installation.html#basics",
      "dsnap": "trafilatura"
    },
    "darwin/brew": {
      "cmd": "brew install trafilatura",
      "reference": "https://formulae.brew.sh/formula/trafilatura#default"
    }
  },
  "dsnap": {
    "brew": "trafilatura",
    "/pip": "trafilatura"
  }
}
