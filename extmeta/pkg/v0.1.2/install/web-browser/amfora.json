{
  "lang": "Go",
  "homepage": "https://github.com/makew0rld/amfora",
  "desc": {
    "cn": "一个用于 Gemini 协议的漂亮终端浏览器。",
    "en": "A fancy terminal browser for the Gemini protocol."
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#macos-homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#macos-macports"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install amfora",
      "reference": "https://scoop.sh/#/apps?q=amfora"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "mint/apt": {
      "cmd": "sudo apt-get install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "manjaro/pacman": {
      "cmd": "sudo pacman -S amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "alpine/apk": {
