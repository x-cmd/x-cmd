    },
    "freebsd/pkg": {
      "cmd": "pkg install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge amfora",
      "reference": "https://github.com/makew0rld/amfora?tab=readme-ov-file#installation"
    }
  }
}
