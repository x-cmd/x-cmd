{
  "lang": "c",
  "homepage": "https://gcc.gnu.org/",
  "desc": {
    "cn": "GCC 包括 C、 C + + 、 Objective-C、 Fortran、 Ada、 Go 和 d 的前端，以及这些语言的库(libstdc + + ，...)",
    "en": "The GNU Compiler Collection includes front ends for C, C++, Objective-C, Fortran, Ada, Go, and D, as well as libraries for these languages (libstdc++,...)"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gcc",
      "reference": "https://formulae.brew.sh/formula/gcc#default"
    },
    "/apt": {
      "cmd": "sudo apt-get install gcc",
      "reference": "https://command-not-found.com/gcc"
    },
    "alpine/apk": {
      "cmd": "sudo apk add gcc",
      "reference": "https://command-not-found.com/gcc"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S gcc",
      "reference": "https://command-not-found.com/gcc"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install gcc",
      "reference": "https://command-not-found.com/gcc"
    },
    "centos/yum": {
      "cmd": "sudo yum install gcc",
      "reference": "https://command-not-found.com/gcc"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install gcc",
      "reference": "https://command-not-found.com/gcc"
    }
  }
}
