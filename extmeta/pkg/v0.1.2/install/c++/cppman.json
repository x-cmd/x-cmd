{
  "lang": "Python",
  "homepage": "https://github.com/aitjcize/cppman",
  "desc": {
    "cn": "适用于 Linux 的 C++ 98/11/14/17/20 手册页",
    "en": "C++ 98/11/14/17/20 man page for Linux"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation",
      "dsnap": "cppman"
    },
    "arch/trizen": {
      "cmd": "sudo trizen -S cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation",
      "dsnap": "cppman"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation",
      "dsnap": "cppman"
    },
    "darwin/brew": {
      "cmd": "brew install cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port install cppman",
      "reference": "https://github.com/aitjcize/cppman?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "cppman",
    "port": "cppman",
    "/pip": "cppman",
    "ubuntu/apt": "cppman",
    "debian/apt": "cppman"
  }
}
