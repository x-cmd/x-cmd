{
  "lang": "c++",
  "homepage": "https://github.com/doxygen/doxygen",
  "desc": {
    "cn": "对于从带注释的 c + + 源代码中生成文档，doxy是一个行业标准的工具",
    "en": "Doxygen is the de facto standard tool for generating documentation from annotated C++ sources"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install doxygen",
      "reference": "https://formulae.brew.sh/formula/doxygen#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install doxygen",
      "reference": "https://scoop.sh/#/apps?q=Doxygen"
    }
  }
}
