{
  "lang": "Go",
  "homepage": "https://github.com/nakabonne/ali",
  "desc": {
    "cn": "用于执行实时分析的负载测试工具",
    "en": "A load testing tool capable of performing real-time analysis."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S ali",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "/nix": {
      "cmd": "nix-env -iA nixos.ali",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=ali&from=0&size=50&sort=relevance&type=packages&query=ali"
    },
    "darwin/brew": {
      "cmd": "brew install nakabonne/ali/ali",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install ali",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "debian/apt": {
      "cmd": "wget https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.deb && \\\nsudo apt install ./ali_0.7.3_linux_amd64.deb\n",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "ubuntu/apt": {
      "cmd": "wget https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.deb && \\\nsudo apt install ./ali_0.7.3_linux_amd64.deb\n",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "mint/apt": {
      "cmd": "wget https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.deb && \\\nsudo apt install ./ali_0.7.3_linux_amd64.deb\n",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "kali/apt": {
      "cmd": "wget https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.deb && \\\nsudo apt install ./ali_0.7.3_linux_amd64.deb\n",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "raspbian/apt": {
      "cmd": "wget https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.deb && \\\nsudo apt install ./ali_0.7.3_linux_amd64.deb\n",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "fedora/rpm": {
      "cmd": "rpm -ivh https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.rpm",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "centos/rpm": {
      "cmd": "rpm -ivh https://github.com/nakabonne/ali/releases/download/v0.7.3/ali_0.7.3_linux_amd64.rpm",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "alpine/apk": {
      "cmd": "apk add ali",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install benchmarks/ali",
      "reference": "https://github.com/nakabonne/ali#installation"
    },
    "/go": {
      "cmd": "go install github.com/nakabonne/ali@latest",
      "reference": "https://github.com/nakabonne/ali?tab=readme-ov-file#installation"
    }
  }
}
