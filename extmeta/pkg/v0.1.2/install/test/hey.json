{
  "lang": "go",
  "homepage": "https://github.com/rakyll/hey",
  "desc": {
    "cn": "HTTP 负载生成器，ApacheBench (ab) 的替代工具",
    "en": "HTTP load generator, ApacheBench (ab) replacement"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use hey",
      "reference": "https://github.com/raimon49/asdf-hey"
    },
    "darwin/brew": {
      "cmd": "brew install hey",
      "reference": "https://formulae.brew.sh/formula/hey#default"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install hey",
      "reference": "https://github.com/rakyll/hey",
      "dsnap": "hey"
    }
  },
  "dsnap": {
    "brew": "hey",
    "/go": "hey",
    "ubuntu/apt": "hey"
  }
}
