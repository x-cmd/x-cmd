{
  "lang": "c",
  "homepage": "https://github.com/wg/wrk",
  "desc": {
    "cn": "HTTP 基准测试工具",
    "en": "Modern HTTP benchmarking tool"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use wrk",
      "reference": "https://github.com/ivanvc/asdf-wrk"
    },
    "darwin/brew": {
      "cmd": "brew install wrk",
      "reference": "https://formulae.brew.sh/formula/wrk#default"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install wrk",
      "reference": "https://github.com/wg/wrk",
      "dsnap": "wrk"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install wrk",
      "reference": "https://github.com/wg/wrk",
      "dsnap": "wrk"
    }
  },
  "dsnap": {
    "brew": "wrk",
    "ubuntu/apt": "wrk",
    "debian/apt": "wrk"
  }
}
