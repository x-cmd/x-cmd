{
  "lang": "rust",
  "homepage": "https://github.com/sharkdp/hyperfine",
  "desc": {
    "cn": "命令行基准测试工具。",
    "en": "A command-line benchmarking tool."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use hyperfine",
      "reference": "https://github.com/volf52/asdf-hyperfine"
    },
    "darwin/brew": {
      "cmd": "brew install hyperfine",
      "reference": "https://formulae.brew.sh/formula/hyperfine#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "ubuntu/wget": {
      "cmd": "wget https://github.com/sharkdp/hyperfine/releases/download/v1.16.1/hyperfine_1.16.1_amd64.deb\n&& sudo dpkg -i hyperfine_1.16.1_amd64.deb\n",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "alpine/apk": {
      "cmd": "sudo apk add hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "funtoo/emerge": {
      "cmd": "sudo emerge app-benchmarks/hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "freebsd/pkg": {
      "cmd": "pkg install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "openbsd/pkg_add": {
      "cmd": "doas pkg_add hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "nix/nixpkgs": {
      "cmd": "nix-env -i hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install hyperfine",
      "reference": "https://github.com/sharkdp/hyperfine"
    }
  }
}
