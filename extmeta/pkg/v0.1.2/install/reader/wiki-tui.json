{
  "lang": "Rust",
  "homepage": "https://github.com/Builditluc/wiki-tui",
  "desc": {
    "cn": "wiki-tui 是一个简单易用的 Rust 编写的维基百科文本用户界面。",
    "en": "wiki-tui is a simple and easy-to-use Wikipedia text user interface written in rust"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install wiki-tui",
      "reference": "https://wiki-tui.net/latest/getting-started/installation/",
      "dsnap": "wiki-tui"
    },
    "void/xbps": {
      "cmd": "xbps-install -S wiki-tui",
      "reference": "https://wiki-tui.net/latest/getting-started/installation/"
    },
    "nix": {
      "cmd": "nix-env -iA wiki-tui",
      "reference": "https://wiki-tui.net/latest/getting-started/installation/"
    },
    "freebsd/pkg": {
      "cmd": "pkg install wiki-tui",
      "reference": "https://wiki-tui.net/latest/getting-started/installation/"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install wiki-tui",
      "reference": "https://wiki-tui.net/latest/getting-started/installation/"
    }
  },
  "dsnap": {
    "/cargo": "wiki-tui"
  }
}
