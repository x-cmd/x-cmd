{
  "lang": "Python",
  "homepage": "https://github.com/donnemartin/haxor-news",
  "desc": {
    "cn": "在命令行查看和过滤 Hacker News 的帖子、评论和链接的 Web 内容",
    "en": "View and filter Hacker News from the command line: Posts, comments, and linked web content."
  },
  "rule": {
    "/pip": {
      "cmd": "pip install haxor-news",
      "reference": "https://github.com/donnemartin/haxor-news#pip-installation"
    },
    "darwin/pip": {
      "cmd": "sudo pip install haxor-news --upgrade --ignore-installed six",
      "reference": "https://github.com/donnemartin/haxor-news#mac-os-x-1011-el-capitan-users"
    }
  }
}
