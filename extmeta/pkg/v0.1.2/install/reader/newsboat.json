{
  "lang": "C++",
  "homepage": "https://newsboat.org/",
  "desc": {
    "en": "An RSS/Atom feed reader for text terminals",
    "cn": "一个 terminal 上的 RSS/Atom feed 阅读器"
  },
  "rule": {
    "/snap": {
      "cmd": "sudo snap install newsboat",
      "reference": "https://newsboat.org/releases/2.34/docs/newsboat.html"
    },
    "darwin/brew": {
      "cmd": "brew install newsboat",
      "reference": "https://formulae.brew.sh/formula/newsboat"
    }
  },
  "dsnap": {
    "brew": "newsboat"
  }
}
