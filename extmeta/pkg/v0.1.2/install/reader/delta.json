{
  "lang": "rust",
  "desc": {
    "cn": "用于 git、diff 和 grep 输出的语法高亮分页器",
    "en": "A syntax-highlighting pager for git, diff, and grep output"
  },
  "homepage": "https://github.com/dandavison/delta",
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "/cargo": {
      "cmd": "cargo install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "freebsd/pkg": {
      "cmd": "pkg install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "gentoo/emerge": {
      "cmd": "emerge dev-util/git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "darwin/brew": {
      "cmd": "brew install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "darwin/port": {
      "cmd": "sudo port install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install git-delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install dandavison.delta",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "/conda": {
      "cmd": "conda install git-delta -c conda-forge",
      "reference": "https://dandavison.github.io/delta/installation.html"
    },
    "/mamba": {
      "cmd": "mamba install git-delta -c conda-forge",
      "reference": "https://dandavison.github.io/delta/installation.html"
    }
  }
}
