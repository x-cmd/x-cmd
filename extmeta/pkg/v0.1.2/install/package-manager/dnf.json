{
  "lang": "python",
  "homepage": "https://rpm-software-management.github.io/",
  "desc": {
    "cn": "Dandied YUM (DNF)是 YUM 的下一个主要版本",
    "en": "Dandified YUM (DNF) is the next upcoming major version of YUM"
  },
  "rule": {
    "centos/yum": {
      "cmd": "sudo yum install dnf",
      "reference": "https://command-not-found.com/dnf"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install dnf",
      "reference": "https://command-not-found.com/dnf",
      "dsnap": "dnf"
    }
  },
  "dsnap": {
    "fedora/dnf": "dnf"
  }
}
