{
  "lang": "Go",
  "homepage": "https://github.com/zyedidia/eget",
  "desc": {
    "cn": "用于从 GitHub release 轻松安装预构建二进制文件的命令行工具。",
    "en": "A command-line tool for easily fetching and extracting pre-built binaries from GitHub releases."
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://zyedidia.github.io/eget.sh | sh",
      "reference": "https://github.com/zyedidia/eget#quick-install-script"
    },
    "darwin/brew": {
      "cmd": "brew install eget",
      "reference": "https://github.com/zyedidia/eget#homebrew"
    },
    "arch/pacman": {
      "cmd": "pacman -S eget-bin",
      "reference": "https://github.com/zyedidia/eget#quick-install-script"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install eget",
      "reference": "https://github.com/zyedidia/eget#how-to-get-eget"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install eget",
      "reference": "https://github.com/zyedidia/eget#chocolatey"
    },
    "/go": {
      "cmd": "go install github.com/zyedidia/eget@latest",
      "reference": "https://github.com/zyedidia/eget#how-to-get-eget",
      "dsnap": "github.com/zyedidia/eget@latest"
    }
  },
  "dsnap": {
    "/go": "github.com/zyedidia/eget@latest"
  }
}
