{
  "lang": "python",
  "homepage": "https://github.com/pypa/pipenv",
  "desc": {
    "cn": "Pipenv 是一个 Python virtualenv 管理工具，支持多种系统，并很好地弥合了 pip、python（使用系统 python、pyenv 或 asdf）和 virtualenv 之间的差距",
    "en": "Pipenv is a Python virtualenv management tool that supports a multitude of systems and nicely bridges the gaps between pip, python (using system python, pyenv or asdf) and virtualenv"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install --user pipenv",
      "reference": "https://pipenv.pypa.io/en/latest/#install-pipenv-today",
      "dsnap": "pipenv"
    },
    "darwin/brew": {
      "cmd": "brew install pipenv",
      "reference": "https://formulae.brew.sh/formula/pipenv#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install pipenv",
      "reference": "https://command-not-found.com/pipenv",
      "dsnap": "pipenv"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install pipenv",
      "reference": "https://command-not-found.com/pipenv",
      "dsnap": "pipenv"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S pipenv",
      "reference": "https://command-not-found.com/pipenv"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install pipenv",
      "reference": "https://command-not-found.com/pipenv"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install pipenv",
      "reference": "https://pypi.org/project/pipenv/#installation",
      "dsnap": "pipenv"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge pipenv",
      "reference": "https://pypi.org/project/pipenv/#installation"
    },
    "freebsd/pkg": {
      "cmd": "pkg install py39-pipenv",
      "reference": "https://pypi.org/project/pipenv/#installation"
    }
  },
  "dsnap": {
    "brew": "pipenv",
    "pacman": "pipenv",
    "emerge": "pipenv",
    "pkg": "py39-pipenv",
    "/pip": "pipenv",
    "fedora/dnf": "pipenv",
    "ubuntu/apt": "pipenv",
    "debian/apt": "pipenv"
  }
}
