{
  "lang": "Go",
  "homepage": "https://github.com/orhun/pkgtop",
  "desc": {
    "cn": "用于 GNU/Linux 系统的交互式包管理器和资源监视器",
    "en": "An interactive package manager and resource monitor for GNU/Linux systems"
  },
  "rule": {
    "/nix-env": {
      "cmd": "nix-env -iA nixos.pkgtop",
      "reference": "https://github.com/orhun/pkgtop#installation"
    },
    "/go": {
      "cmd": "go install github.com/orhun/pkgtop/cmd@latest",
      "reference": "https://github.com/orhun/pkgtop#installation"
    }
  }
}
