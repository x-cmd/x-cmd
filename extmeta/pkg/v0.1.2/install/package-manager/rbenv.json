{
  "lang": "shell",
  "homepage": "https://github.com/rbenv/rbenv",
  "desc": {
    "cn": "rbenv 是类 Unix 系统上 Ruby 编程语言的版本管理器工具",
    "en": "rbenv is a version manager tool for the Ruby programming language on Unix-like systems"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install rbenv",
      "reference": "https://formulae.brew.sh/formula/rbenv"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install gitapt-get install rbenv",
      "reference": "https://command-not-found.com/rbenv",
      "dsnap": "rbenv"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install rbenv",
      "reference": "https://command-not-found.com/rbenv",
      "dsnap": "rbenv"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install rbenv",
      "reference": "https://command-not-found.com/rbenv"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install rbenv",
      "reference": "https://command-not-found.com/rbenv"
    }
  },
  "dsnap": {
    "brew": "rbenv",
    "ubuntu/apt": "rbenv",
    "debian/apt": "rbenv"
  }
}
