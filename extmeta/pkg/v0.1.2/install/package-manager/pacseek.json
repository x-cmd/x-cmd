{
  "lang": "Go",
  "homepage": "https://github.com/moson-mo/pacseek",
  "desc": {
    "cn": "用于搜索和安装 Arch Linux 软件包的 TUI。",
    "en": "A TUI for searching and installing Arch Linux packages."
  },
  "rule": {
    "arch/manjaro": {
      "cmd": "pacman -S pacseek",
      "reference": "https://github.com/moson-mo/pacseek#how-to-build--run--install"
    },
    "/eget": {
      "cmd": "eget moson-mo/pacseek",
      "reference": "https://github.com/moson-mo/pacseek#how-to-build--run--install"
    }
  },
  "dsnap": {
    "pacman": "pacseek"
  }
}
