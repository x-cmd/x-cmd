{
  "lang": "Rust",
  "homepage": "https://github.com/devmatteini/dra",
  "desc": {
    "cn": "用于从 GitHub 下载 release 资源的命令行工具。",
    "en": "A command line tool to download release assets from GitHub."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S dra",
      "reference": "https://github.com/devmatteini/dra#installation"
    },
    "/brew": {
      "cmd": "brew install dra",
      "reference": "https://github.com/devmatteini/dra#installation"
    },
    "/eget": {
      "cmd": "eget devmatteini/dra",
      "reference": "https://github.com/devmatteini/dra#installation"
    }
  }
}
