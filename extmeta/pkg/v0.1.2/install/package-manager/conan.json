{
  "lang": "Python",
  "homepage": "https://conan.io/",
  "desc": {
    "cn": "Conan 是 C 和 C++ 语言的依赖项和包管理器",
    "en": "Conan is a dependency and package manager for C and C++ languages"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install conan",
      "reference": "https://docs.conan.io/2/installation.html",
      "dsnap": "conan"
    },
    "/pipx": {
      "cmd": "pipx install conan",
      "reference": "https://docs.conan.io/2/installation.html"
    }
  },
  "dsnap": {
    "/pip": "conan"
  }
}
