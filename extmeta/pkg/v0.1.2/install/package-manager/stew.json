{
  "lang": "Go",
  "homepage": "https://github.com/marwanhawari/stew",
  "desc": {
    "cn": "一个用于已编译二进制文件的独立包管理器。",
    "en": "An independent package manager for compiled binaries."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S stew",
      "reference": "https://github.com/marwanhawari/stew"
    },
    "darwin/brew": {
      "cmd": "brew install marwanhawari/tap/stew",
      "reference": "https://github.com/marwanhawari/stew"
    },
    "/go": {
      "cmd": "go install github.com/marwanhawari/stew@latest",
      "reference": "https://github.com/marwanhawari/stew",
      "dsnap": "github.com/marwanhawari/stew"
    }
  },
  "dsnap": {
    "brew": "marwanhawari/tap/stew",
    "/go": "github.com/marwanhawari/stew"
  }
}
