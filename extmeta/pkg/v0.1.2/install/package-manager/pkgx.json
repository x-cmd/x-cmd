{
  "lang": "TypeScript",
  "homepage": "https://pkgx.sh/",
  "desc": {
    "cn": "Pkgx 是一个快速、独立、跨平台的二进制程序，可以运行任何程序",
    "en": "Pkgx is a blazingly fast, standalone, cross‐platform binary that runs anything"
  },
  "rule": {
    "(linux|darwin|win|wsl2)/": {
      "cmd": "x pkgx --install",
      "reference": "https://x-cmd.com/mod/pkgx"
    },
    "/curl": {
      "cmd": "curl -Ssf https://pkgx.sh | sh",
      "reference": "https://pkgx.sh/?via=curl"
    },
    "darwin/brew": {
      "cmd": "brew install pkgxdev/made/pkgx",
      "reference": "https://pkgx.sh/?via=brew"
    }
  },
  "dsnap": {
    "/go": "pkgx"
  }
}
