{
  "lang": "Rust",
  "homepage": "https://doc.rust-lang.org/stable/cargo/",
  "desc": {
    "cn": "Cargo 是 Rust 包管理器",
    "en": "Cargo is the Rust package manager"
  },
  "rule": {
    "linux/curl": {
      "cmd": "curl https://sh.rustup.rs -sSf | sh",
      "reference": "https://doc.rust-lang.org/stable/cargo/getting-started/installation.html#install-rust-and-cargo"
    },
    "darwin/curl": {
      "cmd": "curl https://sh.rustup.rs -sSf | sh",
      "reference": "https://doc.rust-lang.org/stable/cargo/getting-started/installation.html#install-rust-and-cargo"
    }
  },
  "dsnap": {
    "/cargo": "rustup"
  }
}
