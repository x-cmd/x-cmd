{
  "lang": "Go",
  "homepage": "https://www.datree.io/",
  "desc": {
    "cn": "Datree 通过阻止配置错误的资源的部署来保护您的 Kubernetes",
    "en": "Datree secures your Kubernetes by blocking the deployment of misconfigured resources"
  },
  "rule": {
    "/helm": {
      "cmd": "helm repo add datree-webhook https://datreeio.github.io/admission-webhook-datree",
      "reference": "https://hub.datree.io/#install-datree-using-the-tool-of-your-choice"
    },
    "/asdf": {
      "cmd": "x asdf use datree",
      "reference": "https://github.com/lukeab/asdf-datree?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "helm": "datree-webhook"
  }
}
