{
  "lang": "Go",
  "homepage": "https://github.com/doitintl/kube-no-trouble",
  "desc": {
    "cn": "轻松检查集群是否使用了已弃用的 API",
    "en": "Easily check your clusters for use of deprecated APIs"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use kubent",
      "reference": "https://github.com/virtualstaticvoid/asdf-kubent?tab=readme-ov-file#install"
    },
    "/curl": {
      "cmd": "sh -c \"$(curl -sSL https://git.io/install-kubent)\"",
      "reference": "https://github.com/doitintl/kube-no-trouble?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew install kubent",
      "reference": "https://github.com/doitintl/kube-no-trouble?tab=readme-ov-file#homebrew"
    }
  }
}
