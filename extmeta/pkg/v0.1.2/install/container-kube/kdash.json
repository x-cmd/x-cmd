{
  "lang": "Rust",
  "homepage": "https://kdash.cli.rs/",
  "desc": {
    "cn": "使用 Rust 构建的 Kubernetes 简单终端仪表板",
    "en": "A simple terminal dashboard for Kubernetes built with Rust"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap kdash-rs/kdash && \\\nbrew install kdash\n",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#homebrew-mac--linux"
    },
    "linux/brew": {
      "cmd": "brew tap kdash-rs/kdash && \\\nbrew install kdash\n",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#homebrew-mac--linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add kdash-bucket https://github.com/kdash-rs/scoop-kdash && \\\nx scoop install kdash\n",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#scoop-windows---recommended-way"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install kdash",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#chocolatey-windows"
    },
    "/cargo": {
      "cmd": "cargo install kdash",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#cargo"
    },
    "nix/run": {
      "cmd": "nix run nixpkgs#kdash",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#nix-maintained-by-third-party"
    },
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/kdash-rs/kdash/main/deployment/getLatest.sh | bash",
      "reference": "https://github.com/kdash-rs/kdash?tab=readme-ov-file#install-script"
    }
  }
}
