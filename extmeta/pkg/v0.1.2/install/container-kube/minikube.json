{
  "lang": "go",
  "homepage": "https://github.com/kubernetes/minikube",
  "desc": {
    "cn": "minikube 是一个用于在本地计算机上运行单节点 Kubernetes 集群的工具",
    "en": "minikube is a tool for running single-node Kubernetes clusters on your local machine"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use minikube",
      "reference": "https://github.com/alvarobp/asdf-minikube"
    },
    "darwin/brew": {
      "cmd": "brew install minikube",
      "reference": "https://formulae.brew.sh/formula/minikube#default"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install minikube",
      "reference": "https://minikube.sigs.k8s.io/docs/start/"
    },
    "win/winget": {
      "cmd": "x winget install minikube",
      "reference": "https://minikube.sigs.k8s.io/docs/start/"
    }
  },
  "dsnap": {
    "/go": "minikube"
  }
}
