{
  "lang": "Go",
  "homepage": "https://github.com/hidetatz/kubecolor",
  "desc": {
    "cn": "KubeColor会着色您的Kubectl命令输出，而无需做任何其他事情",
    "en": "kubecolor colorizes your kubectl command output and does nothing else"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install hidetatz/tap/kubecolor",
      "reference": "https://github.com/hidetatz/kubecolor?tab=readme-ov-file#mac-and-linux-users-via-homebrew"
    },
    "/go": {
      "cmd": "go install github.com/hidetatz/kubecolor/cmd/kubecolor@latest",
      "reference": "https://github.com/hidetatz/kubecolor?tab=readme-ov-file#manually-via-go-command",
      "dsnap": "kubecolor"
    },
    "/asdf": {
      "cmd": "x asdf use kubecolor",
      "reference": "https://github.com/dex4er/asdf-kubecolor?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "kubecolor",
    "/go": "kubecolor"
  }
}
