{
  "lang": "Go",
  "homepage": "https://github.com/ryodocx/kube-credential-cache",
  "desc": {
    "cn": "kubernetes 访问的加速器缓存",
    "en": "Accelerator cache for kubernetes access"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/ryodocx/kube-credential-cache/cmd/kcc-cache@latest \\\n&& go install github.com/ryodocx/kube-credential-cache/cmd/kcc-injector@latest\n",
      "reference": "https://github.com/ryodocx/kube-credential-cache?tab=readme-ov-file#installation",
      "dsnap": "kcc-cache,kcc-injector"
    },
    "/asdf": {
      "cmd": "x asdf use kube-credential-cache",
      "reference": "https://github.com/ryodocx/kube-credential-cache?tab=readme-ov-file#installation"
    },
    "/aqua": {
      "cmd": "aqua g -i ryodocx/kube-credential-cache",
      "reference": "https://github.com/ryodocx/kube-credential-cache?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/go": "kcc-cache,kcc-injector"
  }
}
