{
  "lang": "Go",
  "homepage": "https://github.com/zegl/kube-score",
  "desc": {
    "cn": "kube-score 是一个对 Kubernetes 对象定义执行静态代码分析的工具",
    "en": "kube-score is a tool that performs static code analysis of your Kubernetes object definitions"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use kube-score",
      "reference": "https://github.com/bageljp/asdf-kube-score"
    },
    "darwin/brew": {
      "cmd": "brew install kube-score",
      "reference": "https://formulae.brew.sh/formula/kube-score#default"
    },
    "/krew": {
      "cmd": "kubectl krew install score",
      "reference": "https://github.com/zegl/kube-score?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/go": "kube-score"
  }
}
