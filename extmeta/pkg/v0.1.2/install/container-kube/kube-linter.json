{
  "lang": "go",
  "homepage": "https://github.com/stackrox/kube-linter",
  "desc": {
    "cn": "Kubernetes YAML 文件和 Helm charts 的静态分析工具",
    "en": "Static analysis tool for Kubernetes YAML files and Helm charts"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use kube-linter",
      "reference": "https://github.com/devlincashman/asdf-kube-linter"
    },
    "darwin/brew": {
      "cmd": "brew install kube-linter",
      "reference": "https://formulae.brew.sh/formula/kube-linter#default"
    }
  },
  "dsnap": {
    "brew": "kube-linter"
  }
}
