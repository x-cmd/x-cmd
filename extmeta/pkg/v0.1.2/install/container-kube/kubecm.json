{
  "lang": "Go",
  "homepage": "https://kubecm.cloud/",
  "desc": {
    "cn": "列表，切换，添加，删除和更多的交互式操作来管理 KubeConfig",
    "en": "List, switch, add, delete and more interactive operations to manage kubeconfig"
  },
  "rule": {
    "/krew": {
      "cmd": "kubectl krew install kc",
      "reference": "https://kubecm.cloud/en-us/introduction?id=install"
    },
    "darwin/brew": {
      "cmd": "brew install kubecm",
      "reference": "https://kubecm.cloud/en-us/introduction?id=install"
    },
    "/asdf": {
      "cmd": "x asdf use kubecm",
      "reference": "https://github.com/samhvw8/asdf-kubecm?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "kubecm"
  }
}
