{
  "lang": "Go",
  "homepage": "https://k9scli.io/",
  "desc": {
    "cn": "K9s 是一个基于终端的 UI，用于与 Kubernetes 集群交互",
    "en": "K9s is a terminal based UI to interact with your Kubernetes clusters"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install derailed/k9s/k9s",
      "reference": "https://k9scli.io/topics/install/"
    },
    "darwin/port": {
      "cmd": "sudo port install k9s",
      "reference": "https://k9scli.io/topics/install/"
    },
    "/pacman": {
      "cmd": "sudo pacman -S k9s",
      "reference": "https://k9scli.io/topics/install/"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install k9s",
      "reference": "https://k9scli.io/topics/install/"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install k9s",
      "reference": "https://k9scli.io/topics/install/"
    },
    "/asdf": {
      "cmd": "x asdf use k9s",
      "reference": "https://github.com/looztra/asdf-k9s?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "k9s"
  }
}
