{
  "lang": "go",
  "homepage": "https://eksctl.io/",
  "desc": {
    "cn": "Eksctl 是一个简单的 CLI 工具，用于在 EKS 上创建集群——亚马逊为 EC2提供的新的托管 Kubernetes 服务",
    "en": "eksctl is a simple CLI tool for creating clusters on EKS - Amazon's new managed Kubernetes service for EC2"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use eksctl",
      "reference": "https://github.com/elementalvoid/asdf-eksctl?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew tap weaveworks/tap\n&& brew install weaveworks/tap/eksctl\n",
      "reference": "https://eksctl.io/installation/#for-macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install eksctl",
      "reference": "https://eksctl.io/installation/#for-windows_1"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install eksctl",
      "reference": "https://eksctl.io/installation/#for-windows_1"
    }
  },
  "dsnap": {
    "brew": "weaveworks/tap/eksctl"
  }
}
