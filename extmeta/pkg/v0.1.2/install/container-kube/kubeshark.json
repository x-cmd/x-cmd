{
  "lang": "Go",
  "homepage": "https://www.kubeshark.co/",
  "desc": {
    "cn": "Kubernetes API 流量分析器",
    "en": "The Kubernetes API Traffic Analyzer"
  },
  "rule": {
    "/curl": {
      "cmd": "sh <(curl -Ls https://kubeshark.co/install)",
      "reference": "https://www.kubeshark.co/"
    },
    "/helm": {
      "cmd": "helm repo add kubeshark https://helm.kubeshark.co && helm install kubeshark kubeshark/kubeshark\n",
      "reference": "https://www.kubeshark.co/"
    },
    "darwin/brew": {
      "cmd": "brew tap kubeshark/kubeshark && brew install kubeshark",
      "reference": "https://www.kubeshark.co/"
    },
    "/build": {
      "cmd": "git clone https://github.com/kubeshark/kubeshark.git \\\n&& cd kubeshark \\\n&& make \\\n&& ./bin/kubeshark__ tap\n",
      "reference": "https://www.kubeshark.co/"
    },
    "/asdf": {
      "cmd": "x asdf use kubeshark",
      "reference": "https://github.com/carnei-ro/asdf-kubeshark?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "brew": "kubeshark"
  }
}
