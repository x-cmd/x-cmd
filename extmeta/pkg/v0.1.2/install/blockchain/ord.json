{
  "lang": "Rust",
  "homepage": "https://docs.ordinals.com",
  "desc": {
    "cn": "ord 是一个索引、区块浏览器和命令行钱包",
    "en": "ord is an index, block explorer, and command-line wallet"
  },
  "rule": {
    "/curl": {
      "cmd": "curl --proto '=https' --tlsv1.2 -fsLS https://ordinals.com/install.sh | bash -s",
      "reference": "https://github.com/ordinals/ord?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install ord",
      "reference": "https://github.com/ordinals/ord?tab=readme-ov-file#homebrew"
    }
  },
  "dsnap": {
    "/cargo": "ord"
  }
}
