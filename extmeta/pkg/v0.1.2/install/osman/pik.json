{
  "lang": "Rust",
  "homepage": "https://github.com/jacek-kurlit/pik",
  "desc": {
    "cn": "Process Interactive Kill 是一个命令行工具，用于查找并终止进程",
    "en": "Process Interactive Kill is a command line tool that helps to find and kill process"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S pik",
      "reference": "https://github.com/jacek-kurlit/pik?tab=readme-ov-file#installation"
    },
    "fedora/dnf": {
      "cmd": "dnf copr enable rusty-jack/pik\ndnf install pik\n",
      "reference": "https://github.com/jacek-kurlit/pik?tab=readme-ov-file#installation"
    },
    "opensuse-tumbleweed/zypper": {
      "cmd": "zypper install pik",
      "reference": "https://github.com/jacek-kurlit/pik?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install pik",
      "reference": "https://github.com/jacek-kurlit/pik?tab=readme-ov-file#installation",
      "dsnap": "pik"
    }
  },
  "dsnap": {
    "/cargo": "pik"
  }
}
