{
  "lang": "Rust",
  "homepage": "https://github.com/pvolok/mprocs",
  "desc": {
    "cn": "Mprocs 并行运行多个命令，并分别显示每个命令的输出",
    "en": "Mprocs runs multiple commands in parallel and shows output of each command separately"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#cargo-all-platforms",
      "dsnap": "mprocs"
    },
    "darwin/brew": {
      "cmd": "brew install mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#homebrew-macos-linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#scoop-windows"
    },
    "/npm": {
      "cmd": "npm install -g mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#npm-linux-macos-windows",
      "dsnap": "mprocs"
    },
    "/yarn": {
      "cmd": "yarn global add mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#npm-linux-macos-windows"
    },
    "arch/yay": {
      "cmd": "sudo yay mprocs",
      "reference": "https://github.com/pvolok/mprocs?tab=readme-ov-file#aur-arch-linux"
    }
  },
  "dsnap": {
    "brew": "mprocs",
    "yarn": "mprocs",
    "yay": "mprocs",
    "/npm": "mprocs",
    "/cargo": "mprocs"
  }
}
