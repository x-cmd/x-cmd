{
  "lang": "Rust",
  "homepage": "https://github.com/ankddev/envfetch",
  "desc": {
    "en": "Lightweight cross-platform CLI tool for working with environment variables.",
    "cn": "用于处理环境变量的轻量级跨平台命令行工具"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/ankddev/envfetch/main/install.sh | sh",
      "reference": "https://github.com/ankddev/envfetch/wiki/2.-Installation"
    },
    "(win|wsl2)/powershell": {
      "cmd": "x pwsh 'Invoke-RestMethod \"https://raw.githubusercontent.com/ankddev/envfetch/main/install.ps1\" | Invoke-Expression'",
      "reference": "https://github.com/ankddev/envfetch/wiki/2.-Installation"
    },
    "arch/paru": {
      "cmd": "paru -S envfetch",
      "reference": "https://github.com/ankddev/envfetch/wiki/2.-Installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install https://gist.githubusercontent.com/ankddev/f6314b552aa021f676fc999ec697f833/raw/envfetch.json",
      "reference": "https://github.com/ankddev/envfetch/wiki/2.-Installation"
    },
    "/cargo": {
      "cmd": "cargo install envfetch",
      "reference": "https://github.com/ankddev/envfetch/wiki/2.-Installation"
    }
  }
}
