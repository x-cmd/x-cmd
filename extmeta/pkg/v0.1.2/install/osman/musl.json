{
  "lang": "Makefile",
  "homepage": "https://musl.libc.org/",
  "desc": {
    "cn": "musl 是建立在 C 标准库之上的实现 Linux 系统调用 API，包括 base 中定义的接口 语言标准、POSIX 和广泛认可的扩展",
    "en": "musl is an implementation of the C standard library built on top of the Linux system call API, including interfaces defined in the base language standard, POSIX, and widely agreed-upon extensions"
  },
  "rule": {
    "/apt": {
      "cmd": "sudo apt-get install musl-tools",
      "reference": "https://command-not-found.com/musl-gcc"
    },
    "alpine/apk": {
      "cmd": "sudo apk add musl",
      "reference": "https://command-not-found.com/musl-gcc",
      "dsnap": "musl"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S musl",
      "reference": "https://command-not-found.com/musl-gcc"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install musl-tools",
      "reference": "https://command-not-found.com/musl-gcc"
    }
  },
  "dsnap": {
    "alpine/apk": "musl"
  }
}
