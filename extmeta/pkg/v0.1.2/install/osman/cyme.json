{
  "lang": "Go",
  "homepage": "https://github.com/tuna-f1sh/cyme",
  "desc": {
    "cn": "用于列出系统 USB 总线和设备的跨平台 CLI 工具",
    "en": "A cross-platform CLI tool designed to list system USB buses and devices."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S cyme",
      "reference": "https://github.com/tuna-f1sh/cyme#install"
    },
    "/nix-env": {
      "cmd": "nix-env -iA cyme",
      "reference": "https://github.com/tuna-f1sh/cyme#install"
    },
    "/brew": {
      "cmd": "brew install cyme",
      "reference": "https://github.com/tuna-f1sh/cyme#install"
    },
    "/eget": {
      "cmd": "eget tuna-f1sh/cyme",
      "reference": "https://github.com/tuna-f1sh/cyme#install"
    }
  }
}
