{
  "lang": "Shell",
  "homepage": "https://github.com/KittyKatt/screenFetch",
  "desc": {
    "cn": "screenFetch, 用于在 Terminal 中快速收集并显示系统信息，类似于 [fastfetch](https://x-cmd.com/pkg/fastfetch), [neofetch](https://x-cmd.com/install/neofetch).",
    "en": "screenFetch, similar to [fastfetch](https://x-cmd.com/pkg/fastfetch) and [neofetch](https://x-cmd.com/install/neofetch), is a tool that swiftly gathers and displays system information right in your terminal."
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#ubuntu-and-debian",
      "dsnap": "screenfetch"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#ubuntu-and-debian",
      "dsnap": "screenfetch"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#arch-linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install screenfetchh",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#fedora",
      "dsnap": "screenfetchh"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge app-misc/screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#gentoo-or-sabayon"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#opensuse"
    },
    "darwin/brew": {
      "cmd": "brew install screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#mac"
    },
    "msys2/pacman": {
      "cmd": "pacman -S screenfetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#msys2"
    },
    "freebsd/pkg": {
      "cmd": "pkg install screenFetch",
      "reference": "https://github.com/KittyKatt/screenFetch/wiki/Installation#freebsd"
    }
  },
  "dsnap": {
    "brew": "screenfetch",
    "pacman": "screenfetch",
    "pkg": "screenFetch",
    "fedora/dnf": "screenfetchh",
    "ubuntu/apt": "screenfetch",
    "debian/apt": "screenfetch"
  }
}
