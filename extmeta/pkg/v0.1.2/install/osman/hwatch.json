{
  "lang": "rust",
  "homepage": "https://github.com/blacknon/hwatch",
  "desc": {
    "cn": "watch命令的替代品，可以记录命令执行的结果，并可以显示它的历史记录和差异",
    "en": "A modern alternative to the watch command, records the differences in execution results and can check this differences at after."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use hwatch",
      "reference": "https://github.com/chessmango/asdf-hwatch"
    },
    "darwin/brew": {
      "cmd": "brew install hwatch",
      "reference": "https://github.com/blacknon/hwatch?tab=readme-ov-file#macos-brew"
    },
    "darwin/port": {
      "cmd": "sudo port install hwatch",
      "reference": "https://github.com/blacknon/hwatch?tab=readme-ov-file#macos-macports"
    },
    "arch/paru": {
      "cmd": "sudo paru -S hwatch",
      "reference": "https://github.com/blacknon/hwatch?tab=readme-ov-file#arch-linux-aur"
    },
    "/cargo": {
      "cmd": "cargo install hwatch",
      "reference": "https://github.com/blacknon/hwatch?tab=readme-ov-file#cargo-install",
      "dsnap": "hwatch"
    }
  },
  "dsnap": {
    "/cargo": "hwatch"
  }
}
