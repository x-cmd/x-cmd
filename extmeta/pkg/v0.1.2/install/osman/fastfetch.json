{
  "lang": "C",
  "homepage": "https://github.com/fastfetch-cli/fastfetch",
  "desc": {
    "cn": "Fastfetch 是一个类似 neofetch 的工具，用于获取系统信息并以漂亮的方式显示它们",
    "en": "Fastfetch is a neofetch-like tool for fetching system information and displaying them in a pretty way"
  },
  "rule": {
    "debian/apt": {
      "cmd": "sudo apt install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#installation",
      "dsnap": "fastfetch"
    },
    "ubuntu/apt": {
      "cmd": "sudo add-apt-repository ppa:zhangsongcui3371/fastfetch && \\\nsudo apt update && \\\nsudo apt install fastfetch\n",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#installation",
      "dsnap": "fastfetch"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux",
      "dsnap": "fastfetch"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask app-misc/fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux"
    },
    "alpine/apk": {
      "cmd": "apk add --upgrade fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux",
      "dsnap": "fastfetch"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#linux"
    },
    "darwin/brew": {
      "cmd": "brew install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#macos"
    },
    "darwin/port": {
      "cmd": "sudo port install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#windows"
    },
    "(win|wsl2)/choco": {
      "cmd": "x winget install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#windows"
    },
    "msys2/pacman": {
      "cmd": "pacman -S mingw-w64-<subsystem>-<arch>-fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#windows"
    },
    "freebsd/pkg": {
      "cmd": "pkg install fastfetch",
      "reference": "https://github.com/fastfetch-cli/fastfetch?tab=readme-ov-file#freebsd"
    }
  },
  "dsnap": {
    "brew": "fastfetch",
    "fedora/dnf": "fastfetch",
    "alpine/apk": "fastfetch",
    "ubuntu/apt": "fastfetch",
    "debian/apt": "fastfetch"
  }
}
