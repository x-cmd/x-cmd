{
  "lang": "Rust",
  "homepage": "https://github.com/kxxt/tracexec",
  "desc": {
    "en": "A small utility for tracing execve{,at} and pre-exec behavior.",
    "cn": "这是一个用于跟踪 execve{,at} 及 pre-exec 行为的小型工具。"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S tracexec",
      "reference": "https://github.com/kxxt/tracexec/blob/main/INSTALL.md#install-via-package-manager"
    },
    "/cargo": {
      "cmd": "cargo install tracexec --bin tracexec",
      "reference": "https://github.com/kxxt/tracexec/blob/main/INSTALL.md#install-via-cargo"
    }
  }
}
