{
  "lang": "rust",
  "homepage": "https://github.com/oberblastmeister/trashy",
  "desc": {
    "cn": "trashy 是一个用 Rust 写的简单，快速，特色的替代 rm 和 trash-cli",
    "en": "trashy is a simple, fast, and featureful alternative to rm and trash-cli written in rust"
  },
  "rule": {
    "/paru": {
      "cmd": "sudo paru -S trashy",
      "reference": "https://github.com/oberblastmeister/trashy?tab=readme-ov-file#from-the-aur"
    },
    "/cargo": {
      "cmd": "cargo install trashy",
      "reference": "https://github.com/oberblastmeister/trashy?tab=readme-ov-file#using-cargo",
      "dsnap": "trashy"
    },
    "/niv-env": {
      "cmd": "niv-env -i trashy",
      "reference": "https://github.com/oberblastmeister/trashy?tab=readme-ov-file#using-nix"
    },
    "/nix": {
      "cmd": "nix profile install nixpkgs#trashy",
      "reference": "https://github.com/oberblastmeister/trashy?tab=readme-ov-file#using-nix"
    }
  },
  "dsnap": {
    "/cargo": "trashy"
  }
}
