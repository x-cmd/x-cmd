{
  "lang": "Rust",
  "homepage": "https://github.com/vladkens/macmon",
  "desc": {
    "en": "Sudoless performance monitoring CLI tool for Apple Silicon processors",
    "cn": "适用于苹果硅处理器的无声音性能监测命令行工具"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install macmon",
      "reference": "https://github.com/vladkens/macmon?tab=readme-ov-file#-install-via-homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port install macmon",
      "reference": "https://github.com/vladkens/macmon?tab=readme-ov-file#%EF%B8%8F-install-via-macports"
    }
  },
  "dsnap": {
    "brew": "macmon"
  }
}
