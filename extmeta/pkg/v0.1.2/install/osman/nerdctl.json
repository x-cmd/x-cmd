{
  "lang": "Go",
  "homepage": "https://github.com/containerd/nerdctl",
  "desc": {
    "cn": "适用于 Containerd 的 Docker 兼容 CLI",
    "en": "Docker-compatible CLI for containerd"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use nerdctl",
      "reference": "https://github.com/dmpe/asdf-nerdctl"
    },
    "darwin/brew": {
      "cmd": "brew install nerdctl",
      "reference": "https://github.com/containerd/nerdctl?tab=readme-ov-file#brew"
    }
  },
  "dsnap": {
    "brew": "nerdctl"
  }
}
