{
  "lang": "rust",
  "homepage": "https://mirrord.dev/",
  "desc": {
    "cn": "用于连接本地进程和云环境，以在云条件下运行本地代码",
    "en": "Connect your local process and your cloud environment, and run local code in cloud conditions"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install metalbear-co/mirrord/mirrord",
      "reference": "https://github.com/metalbear-co/mirrord#installation-2"
    },
    "/curl": {
      "cmd": "curl -fsSL https://raw.githubusercontent.com/metalbear-co/mirrord/main/scripts/install.sh | bash",
      "reference": "https://github.com/metalbear-co/mirrord#installation-2"
    }
  },
  "dsnap": {
    "brew": "mirrord"
  }
}
