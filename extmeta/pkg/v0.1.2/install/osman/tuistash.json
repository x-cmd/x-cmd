{
  "lang": "Rust",
  "homepage": "https://github.com/edmocosta/tuistash",
  "desc": {
    "cn": "用于查看 Logstash 的终端用户界面",
    "en": "A terminal user interface for Logstash."
  },
  "rule": {
    "arch/paru": {
      "cmd": "paru -S tuistash",
      "reference": "https://github.com/edmocosta/tuistash?tab=readme-ov-file#arch-linux"
    },
    "/brew": {
      "cmd": "brew install edmocosta/homebrew-tap/tuistash",
      "reference": "https://github.com/edmocosta/tuistash?tab=readme-ov-file#homebrew"
    },
    "/cargo": {
      "cmd": "curl https://sh.rustup.rs -sSf | sh && \\\ngit clone https://github.com/edmocosta/tuistash.git && \\\ncd tuistash && \\\ncargo build --release\n",
      "reference": "https://github.com/edmocosta/tuistash?tab=readme-ov-file#manual",
      "dsnap": "tuistash"
    }
  },
  "dsnap": {
    "brew": "edmocosta/homebrew-tap/tuistash",
    "/cargo": "tuistash"
  }
}
