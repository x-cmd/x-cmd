{
  "lang": "Rust",
  "homepage": "https://github.com/jhheider/semverator",
  "desc": {
    "cn": "用于命令行使用的 texyz/lib: semver.ts 的纯 Rust 实现",
    "en": "Pure rust implementation of teaxyz/lib:semver.ts for command-line usage"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install semverator",
      "reference": "https://github.com/jhheider/semverator?tab=readme-ov-file#install",
