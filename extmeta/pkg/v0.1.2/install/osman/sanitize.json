{
  "lang": "Rust",
  "homepage": "https://github.com/jhheider/sanitize",
  "desc": {
    "cn": "用 Rust 编写的简单命令行工具将目录清除到特定的白名单",
    "en": "Simple command-line tool to sanitize a directory to a specific whitelist, written in Rust"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install sanitize",
      "reference": "https://github.com/jhheider/sanitize?tab=readme-ov-file#install",
      "dsnap": "sanitize"
    },
    "/tea": {
      "cmd": "tea +crates.io/sanitize true",
      "reference": "https://github.com/jhheider/sanitize?tab=readme-ov-file#install"
    }
  },
  "dsnap": {
    "/cargo": "sanitize"
  }
}
