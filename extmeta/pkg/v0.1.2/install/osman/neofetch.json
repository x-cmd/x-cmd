{
  "lang": "Shell",
  "homepage": "https://github.com/dylanaraps/neofetch",
  "desc": {
    "cn": "一个用 bash 3.2+ 编写的美化命令行系统信息（操作系统、软件和硬件的信息）显示的工具",
    "en": "A command-line system information tool written in bash 3.2+"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#macos-homebrew"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#debian",
      "dsnap": "neofetch"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#ubuntu",
      "dsnap": "neofetch"
    },
    "alpine/apk": {
      "cmd": "sudo apk add neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#alpine-linux",
      "dsnap": "neofetch"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#arch"
    },
    "kali/apt": {
      "cmd": "sudo apt install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#ubuntu"
    },
    "centos/dnf": {
      "cmd": "sudo dnf install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#fedora--rhel--centos--mageia--openmandriva"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#fedora--rhel--centos--mageia--openmandriva",
      "dsnap": "neofetch"
    },
    "rhel/dnf": {
      "cmd": "sudo dnf install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#fedora--rhel--centos--mageia--openmandriva"
    },
    "raspbian/apt": {
      "cmd": "sudo apt install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#ubuntu"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge app-misc/neofetch",
      "reference": "hhttps://github.com/dylanaraps/neofetch/wiki/Installation#gentoo--funtoo"
    },
    "funtoo/emerge": {
      "cmd": "sudo emerge app-misc/neofetch",
      "reference": "hhttps://github.com/dylanaraps/neofetch/wiki/Installation#gentoo--funtoo"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#opensuse-leap-or-tumbleweed"
    },
    "mageia/dnf": {
      "cmd": "sudo dnf install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#fedora--rhel--centos--mageia--openmandriva"
    },
    "nix/": {
      "cmd": "sudo nix-env -i neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#nixos"
    },
    "freebsd/pkg": {
      "cmd": "sudo pkg install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#freebsd"
    },
    "termux/pkg": {
      "cmd": "sudo pkg install neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#android-termux"
    },
    "solus/eopkg": {
      "cmd": "sudo eopkg it neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#solus"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install -S neofetch",
      "reference": "https://github.com/dylanaraps/neofetch/wiki/Installation#void-linux"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install neofetch",
      "reference": "https://scoop.sh/#/apps?q=neofetch"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install nepnep.neofetch-win",
      "reference": "https://winget.run/pkg/nepnep/neofetch-win"
    }
  },
  "dsnap": {
    "brew": "neofetch",
    "pacman": "neofetch",
    "emerge": "app-misc/neofetch",
    "zypper": "neofetch",
    "pkg": "neofetch",
    "nix": "neofetch",
    "fedora/dnf": "neofetch",
    "alpine/apk": "neofetch",
    "ubuntu/apt": "neofetch",
    "debian/apt": "neofetch"
  }
}
