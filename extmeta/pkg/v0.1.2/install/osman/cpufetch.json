{
  "lang": "C",
  "homepage": "https://github.com/Dr-Noob/cpufetch",
  "desc": {
    "cn": "用于获取和显示您的计算机或服务器的 CPU 架构信息的命令行工具",
    "en": "A command-line (CLI) tool designed to fetch and display your computer or server's CPU architecture information in the terminal."
  },
  "rule": {
    "ubuntu/apt": {
      "cmd": "sudo apt-get install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch",
      "dsnap": "cpufetch"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch",
      "dsnap": "cpufetch"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch",
      "dsnap": "cpufetch"
    },
    "centos/yum": {
      "cmd": "sudo yum install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "opensuse/zypper": {
      "cmd": "zypper install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "/snap": {
      "cmd": "sudo snap install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "alpine/apk": {
      "cmd": "apk add cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch",
      "dsnap": "cpufetch"
    },
    "/nix-env": {
      "cmd": "nix-env -iA cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "gentoo/emerge": {
      "cmd": "emerge app-misc/cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "darwin/brew": {
      "cmd": "brew install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "freebsd/pkg": {
      "cmd": "pkg install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    },
    "/eget": {
      "cmd": "eget Dr-Noob/cpufetch",
      "reference": "https://github.com/Dr-Noob/cpufetch"
    }
  },
  "dsnap": {
    "brew": "cpufetch",
    "fedora/dnf": "cpufetch",
    "alpine/apk": "cpufetch",
    "ubuntu/apt": "cpufetch",
    "debian/apt": "cpufetch"
  }
}
