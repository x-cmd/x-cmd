{
  "lang": "Rust",
  "homepage": "https://github.com/pythops/impala",
  "desc": {
    "cn": "用于在 Linux 上管理 wifi 的 TUI。",
    "en": "TUI for managing wifi on Linux."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install impala",
      "reference": "https://github.com/pythops/impala#-installation"
    },
    "arch/pacman": {
      "cmd": "pacman -S impala",
      "reference": "https://github.com/pythops/impala#-installation"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.impala",
      "reference": "https://github.com/pythops/impala#-installation"
    }
  }
}
