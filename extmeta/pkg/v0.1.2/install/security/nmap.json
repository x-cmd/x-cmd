{
  "lang": "C",
  "homepage": "https://nmap.org/",
  "desc": {
    "cn": "一款用于网络探测和安全审计的工具",
    "en": "A tool for network detection and security auditing"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install nmap",
      "reference": "https://formulae.brew.sh/formula/nmap#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install nmap",
      "reference": "https://command-not-found.com/nmap",
      "dsnap": "nmap"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install nmap",
      "reference": "https://command-not-found.com/nmap",
      "dsnap": "nmap"
    },
    "alpine/apk": {
      "cmd": "sudo apk add nmap",
      "reference": "https://command-not-found.com/nmap",
      "dsnap": "nmap"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S nmap",
      "reference": "https://command-not-found.com/nmap"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install nmap",
      "reference": "https://command-not-found.com/nmap"
    },
    "centos/yum": {
      "cmd": "sudo yum install nmap",
      "reference": "https://command-not-found.com/nmap"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install nmap",
      "reference": "https://command-not-found.com/nmap",
      "dsnap": "nmap"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install nmap",
      "reference": "https://command-not-found.com/nmap"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add main && x scoop install main/nmap",
      "reference": "https://scoop.sh/#/apps?q=nmap"
    }
  },
  "dsnap": {
    "brew": "nmap",
    "yum": "nmap",
    "fedora/dnf": "nmap",
    "alpine/apk": "nmap",
    "ubuntu/apt": "nmap",
    "debian/apt": "nmap"
  }
}
