    "/wget": {
      "cmd": "wget -O -  https://get.acme.sh | sh -s email=my@example.com",
      "reference": "https://github.com/acmesh-official/acme.sh?tab=readme-ov-file#1-install-online"
    }
  },
  "dsnap": null
}
