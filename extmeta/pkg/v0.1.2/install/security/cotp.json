{
  "lang": "Rust",
  "homepage": "https://github.com/replydev/cotp",
  "desc": {
    "cn": "受信任的加密 TOTP/HOTP 身份验证器，具有导入功能。",
    "en": "trusted, encrypted, TOTP/HOTP authenticator with import functionality."
  },
  "rule": {
    "/brew": {
      "cmd": "brew install cotp",
      "reference": "https://github.com/replydev/cotp#macos"
    },
    "/cargo": {
      "cmd": "cargo install cotp",
      "reference": "https://github.com/replydev/cotp#use-the-cratesio-repository",
      "dsnap": "cotp"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S cotp",
      "reference": "https://repology.org/project/cotp/versions"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p cotp",
      "reference": "https://github.com/replydev/cotp#installation"
    }
  },
  "dsnap": {
    "brew": "cotp",
    "/cargo": "cotp"
  }
}
