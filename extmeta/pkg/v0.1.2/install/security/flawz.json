{
  "lang": "Rust",
  "homepage": "https://github.com/orhun/flawz",
  "desc": {
    "cn": "flawz 是一个终端用户界面（TUI），用于浏览安全漏洞（也称为CVE）。",
    "en": "flawz is a Terminal User Interface (TUI) for browsing the security vulnerabilities (also known as CVEs)"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#cargo"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#arch-linux"
    },
    "alpine/apk": {
      "cmd": "apk add flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#alpine-linux"
    },
    "darwin/brew": {
      "cmd": "brew install flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#homebrew"
    },
    "/nixpkgs": {
      "cmd": "nix-channel --add https://nixos.org/channels/nixpkgs-unstable && \\\nnix-channel --update nixpkgs && \\\nnix-env -iA nixpkgs.flawz\n",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#nixpkgs"
    },
    "/nix-env": {
      "cmd": "nix-channel --add https://nixos.org/channels/nixos-unstable && \\\nnix-channel --update nixos && \\\nnix-env -iA nixos.flawz\n",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#nixpkgs"
    },
    "nix/run": {
      "cmd": "nix run nixpkgs#flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#nixpkgs"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install flawz",
      "reference": "https://github.com/orhun/flawz?tab=readme-ov-file#netbsd"
    }
  }
}
