{
  "lang": "Go",
  "homepage": "https://github.com/edoardottt/pwdsafety",
  "desc": {
    "cn": "用于检查密码安全性的命令行工具",
    "en": "A CLI tool for checking password safety."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "pacman -S pwdsafety",
      "reference": "https://github.com/edoardottt/pwdsafety#get-started-"
    },
    "/snap": {
      "cmd": "sudo snap install pwdsafety",
      "reference": "https://github.com/edoardottt/pwdsafety#get-started-"
    },
    "/go": {
      "cmd": "go install -v github.com/edoardottt/pwdsafety@latest",
      "reference": "https://github.com/edoardottt/pwdsafety#get-started-",
      "dsnap": "pwdsafety"
    }
  },
  "dsnap": {
    "/go": "pwdsafety"
  }
}
