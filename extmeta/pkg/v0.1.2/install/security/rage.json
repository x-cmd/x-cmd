{
  "lang": "rust",
  "homepage": "https://github.com/str4d/rage",
  "desc": {
    "cn": "rage 是一种简单、现代且安全的文件加密工具，使用 Age 格式。",
    "en": "rage is a simple, modern, and secure file encryption tool"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap str4d.xyz/rage https://str4d.xyz/rage \\\n&& brew install rage\n",
      "reference": "https://github.com/str4d/rage?tab=readme-ov-file#installation"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install rage-encryption",
      "reference": "https://github.com/str4d/rage?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install rage",
      "reference": "https://github.com/str4d/rage?tab=readme-ov-file#installation"
    }
  }
}
