{
  "lang": "node",
  "homepage": "https://github.com/snyk/snyk#readme",
  "desc": {
    "cn": "Snyk 是开发人员优先的云本地安全工具",
    "en": "Snyk is a developer-first cloud-native security tool"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use snyk",
      "reference": "https://github.com/nirfuchs/asdf-snyk"
    },
    "darwin/brew": {
      "cmd": "brew tap snyk/tap\n&& brew install snyk\n",
      "reference": "https://github.com/snyk/cli?tab=readme-ov-file#homebrew"
    },
    "/npm": {
      "cmd": "npm install snyk@latest -g",
      "reference": "https://github.com/snyk/cli?tab=readme-ov-file#install-with-npm-or-yarn",
      "dsnap": "snyk"
    },
    "/yarn": {
      "cmd": "yarn global add snyk",
      "reference": "https://github.com/snyk/cli?tab=readme-ov-file#install-with-npm-or-yarn"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add snyk https://github.com/snyk/scoop-snyk\n&& x scoop install snyk\n",
      "reference": "https://scoop.sh/#/apps?q=snyk&o=false"
    }
  },
  "dsnap": {
    "/npm": "snyk"
  }
}
