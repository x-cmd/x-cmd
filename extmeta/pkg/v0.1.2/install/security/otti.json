{
  "lang": "Rust",
  "homepage": "https://github.com/dnaka91/otti",
  "desc": {
    "cn": "基于 TUI 的一次性密码管理器",
    "en": "A one-time TUI password manager for the terminal."
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --git https://github.com/dnaka91/otti.git",
      "reference": "https://github.com/dnaka91/otti#installation",
      "dsnap": "otti"
    },
    "arch/paru": {
      "cmd": "paru -S otti",
      "reference": "https://github.com/dnaka91/otti#installation"
    }
  },
  "dsnap": {
    "/cargo": "otti"
  }
}
