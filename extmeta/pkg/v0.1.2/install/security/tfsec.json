{
  "lang": "Go",
  "homepage": "https://aquasecurity.github.io/tfsec/latest/",
  "desc": {
    "cn": "tfsec 是一款针对 Terraform 代码的静态分析安全扫描器",
    "en": "tfsec is a static analysis security scanner for your Terraform code"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install tfsec",
      "reference": "https://aquasecurity.github.io/tfsec/latest/guides/installation/"
    },
    "linux/brew": {
      "cmd": "brew install tfsec",
      "reference": "https://aquasecurity.github.io/tfsec/latest/guides/installation/"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install tfsec",
      "reference": "https://aquasecurity.github.io/tfsec/latest/guides/installation/"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install tfsec",
      "reference": "https://aquasecurity.github.io/tfsec/latest/guides/installation/"
    },
    "/go": {
      "cmd": "go install github.com/aquasecurity/tfsec/cmd/tfsec@latest",
      "reference": "https://aquasecurity.github.io/tfsec/latest/guides/installation/",
      "dsnap": "github.com/aquasecurity/tfsec/cmd/tfsec@latest"
    }
  },
  "dsnap": {
    "brew": "tfsec",
    "/go": "github.com/aquasecurity/tfsec/cmd/tfsec@latest"
  }
}
