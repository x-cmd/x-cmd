{
  "lang": "Rust",
  "homepage": "https://github.com/orhun/gpg-tui",
  "desc": {
    "en": "gpg-tui is a Terminal User Interface for GnuPG",
    "cn": "gpg-tui 是 GnuPG 的终端用户界面"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install gpg-tui",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#cargo"
    },
    "arch/pacman": {
      "cmd": "pacman -S gpg-tui",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#arch-linux"
    },
    "arch/paru": {
      "cmd": "paru -S gpg-tui-git",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#aur"
    },
    "alpine/apk": {
      "cmd": "apk add gpg-tui",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#alpine-linux"
    },
    "darwin/brew": {
      "cmd": "brew install gpg-tui",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#homebrew"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install gpg-tui",
      "reference": "https://github.com/orhun/gpg-tui?tab=readme-ov-file#netbsd"
    }
  }
}
