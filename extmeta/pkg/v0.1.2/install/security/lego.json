{
  "lang": "Go",
  "homepage": "https://go-acme.github.io/lego/",
  "desc": {
    "cn": "用 Go 编写的 Let's Encrypt/ACME 客户端",
    "en": "A Let's Encrypt/ACME client written in Go"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "arch/yay": {
      "cmd": "yay -S lego-bin",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "freebsd/pkg": {
      "cmd": "pkg install lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "/snap": {
      "cmd": "sudo snap install lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge app-crypt/lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "darwin/brew": {
      "cmd": "brew install lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add lego",
      "reference": "https://go-acme.github.io/lego/installation/index.html"
    },
    "/go": {
      "cmd": "go install github.com/go-acme/lego/v4/cmd/lego@latest",
      "reference": "https://go-acme.github.io/lego/installation/index.html",
      "dsnap": "github.com/go-acme/lego/v4/cmd/lego@latest"
    }
  },
  "dsnap": {
    "brew": "lego",
    "/cargo": "lego",
    "/go": "github.com/go-acme/lego/v4/cmd/lego@latest",
    "fedora/dnf": "lego",
    "alpine/apk": "lego"
  }
}
