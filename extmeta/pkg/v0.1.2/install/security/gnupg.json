{
  "lang": "C",
  "homepage": "https://gnupg.org/index.html",
  "desc": {
    "cn": "GnuPG 是 RFC4880 定义的 OpenPGP 标准的完整且免费的实现（也称为 PGP）",
    "en": "GnuPG is a complete and free implementation of the OpenPGP standard as defined by RFC4880 (also known as PGP)"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gnupg",
      "reference": "https://formulae.brew.sh/formula/gnupg"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install gnupg",
      "reference": "https://packages.ubuntu.com/search?suite=default&section=all&arch=any&keywords=gnupg&searchon=names",
      "dsnap": "gnupg"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install gnupg",
      "reference": "https://packages.fedoraproject.org/pkgs/gnupg2/gnupg2/",
      "dsnap": "gnupg"
    },
    "alpine/apk": {
      "cmd": "sudo apk add gnupg",
      "reference": "https://pkgs.alpinelinux.org/package/edge/main/x86_64/gnupg",
      "dsnap": "gnupg"
    }
  },
  "dsnap": {
    "brew": "gnupg",
    "fedora/dnf": "gnupg",
    "alpine/apk": "gnupg",
    "ubuntu/apt": "gnupg"
  }
}
