{
  "lang": "Go",
  "homepage": "https://github.com/google/osv-scanner",
  "desc": {
    "cn": "用 Go 编写的漏洞扫描器，使用 https://osv.dev 提供的数据",
    "en": "Vulnerability scanner written in Go which uses the data provided by"
  },
  "rule": {
    "(linux|darwin|win|wsl2)/": {
      "cmd": "x env use osv-scanner",
      "reference": "https://x-cmd.com/mod/env"
    },
    "/go": {
      "cmd": "go install github.com/google/osv-scanner/cmd/osv-scanner@v1",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "darwin/brew": {
      "cmd": "brew install osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "/pacman": {
      "cmd": "pacman -S osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "/apk": {
      "cmd": "apk add osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "/pkg": {
      "cmd": "pkg install osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    },
    "/pkg_add": {
      "cmd": "pkg_add osv-scanner",
      "reference": "https://google.github.io/osv-scanner/installation/"
    }
  }
}
