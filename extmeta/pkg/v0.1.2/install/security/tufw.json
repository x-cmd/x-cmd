{
  "lang": "Go",
  "homepage": "https://github.com/peltho/tufw",
  "desc": {
    "en": "Terminal UI for ufw",
    "cn": "ufw的终端用户界面"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/peltho/tufw/cmd/tufw@latest",
      "reference": "https://pkg.go.dev/github.com/peltho/tufw/cmd/tufw",
      "dsnap": "github.com/peltho/tufw/cmd/tufw"
    },
    "arch/yaourt": {
      "cmd": "yaourt -S tufw-git",
      "reference": "https://github.com/peltho/tufw?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/go": "github.com/peltho/tufw/cmd/tufw"
  }
}
