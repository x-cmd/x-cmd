{
  "lang": "Go",
  "homepage": "https://github.com/70sh1/eddy",
  "desc": {
    "cn": "简单、快速的 CLI 文件加密工具。",
    "en": "Simple, fast CLI file encryption tool."
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/70sh1/eddy@latest",
      "reference": "https://github.com/70sh1/eddyinstallation"
    },
    "/eget": {
      "cmd": "eget 70sh1/eddy",
      "reference": "https://github.com/70sh1/eddyinstallation"
    },
    "arch/yay": {
      "cmd": "yay -S eddy",
      "reference": "https://github.com/70sh1/eddyinstallation"
    }
  }
}
