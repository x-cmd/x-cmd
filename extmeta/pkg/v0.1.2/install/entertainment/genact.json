{
  "lang": "Rust",
  "homepage": "https://github.com/svenstaro/genact",
  "desc": {
    "cn": "一个无意义的活动发生器",
    "en": "A nonsense activity generator"
  },
  "rule": {
    "freebsd/pkg": {
      "cmd": "pkg install genact",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install genact",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "/port": {
      "cmd": "sudo port install genact",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install genact",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install svenstaro.genact",
      "reference": "powershell run cmd: winget find genact"
    },
    "/cargo": {
      "cmd": "cargo install genact",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "win/powershell": {
      "cmd": "x pwsh Invoke-WebRequest -Uri hhttps://github.com/svenstaro/genact/releases/download/v1.3.0/genact-1.3.0-x86_64-pc-windows-msvc.exe -OutFile genact-1.3.0-x86_64-pc-windows-msvc.exe",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "linux/curl": {
      "cmd": "curl -L https://github.com/svenstaro/genact/releases/download/v1.3.0/genact-1.3.0-x86_64-unknown-linux-gnu -o genact-linux && \\\nchmod +x genact-linux\n",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "linux/wget": {
      "cmd": "wget https://github.com/svenstaro/genact/releases/download/v1.3.0/genact-1.3.0-x86_64-unknown-linux-gnu -o genact-linux && \\\ncbmod +x genact-linux\n",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    },
    "darwin/curl": {
      "cmd": "curl -L https://github.com/svenstaro/genact/releases/download/v1.3.0/genact-1.3.0-x86_64-apple-darwin -o genact-osx && \\\nchmod +x genact-osx\n",
      "reference": "https://github.com/svenstaro/genact?tab=readme-ov-file#installation"
    }
  }
}
