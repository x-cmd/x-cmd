{
  "lang": "Rust",
  "homepage": "https://github.com/ckaznable/poketex",
  "desc": {
    "cn": "基于 TUI（终端用户界面）的简单图鉴",
    "en": "Simple Pokedex based on TUI(Terminal User Interface)"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install poketex --locked",
      "reference": "https://github.com/ckaznable/poketex?tab=readme-ov-file#install-from-cratesio",
      "dsnap": "poketex"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S poketex",
      "reference": "https://github.com/ckaznable/poketex?tab=readme-ov-file#install-from-the-aur"
    }
  },
  "dsnap": {
    "/cargo": "poketex"
  }
}
