{
  "lang": "Go",
  "homepage": "https://github.com/bloznelis/typioca",
  "desc": {
    "cn": "终端中舒适的打字速度测试器。",
    "en": "Cozy typing speed tester in terminal."
  },
  "rule": {
    "/yay": {
      "cmd": "yay -S typioca-git",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#aur"
    },
    "nix/nix-shell": {
      "cmd": "nix-shell -p typioca",
      "reference": "https://github.com/bloznelis/typioca"
    },
    "/brew": {
      "cmd": "brew install typioca",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#homebrew"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install typioca",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#opensuse-tumbleweed"
    },
    "(win|wsl2)/scoop": {
      "cmd": "scoop install typioca",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#scoop"
    },
    "(win|wsl2)/winget": {
      "cmd": "winget install bloznelis.typioca",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#winget"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install typioca",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#void-linux"
    },
    "/go": {
      "cmd": "go install github.com/bloznelis/typioca@latest",
      "reference": "https://github.com/bloznelis/typioca?tab=readme-ov-file#go",
      "dsnap": "github.com/bloznelis/typioca@latest"
    }
  },
  "dsnap": {
    "/go": "github.com/bloznelis/typioca@latest"
  }
}
