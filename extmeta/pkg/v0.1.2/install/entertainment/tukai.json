{
  "lang": "Rust",
  "homepage": "https://github.com/hlsxx/tukai",
  "desc": {
    "cn": "基于终端的打字练习应用程序",
    "en": "Terminal based touch typing application."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S tukai",
      "reference": "https://github.com/hlsxx/tukai?tab=readme-ov-file#arch-linux"
    },
    "/cargo": {
      "cmd": "cargo install tukai",
      "reference": "https://github.com/hlsxx/tukai?tab=readme-ov-file#installation-from-the-cargo",
      "dsnap": "tukai"
    }
  },
  "dsnap": {
    "/cargo": "tukai"
  }
}
