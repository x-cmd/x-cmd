{
  "lang": "Rust",
  "homepage": "https://github.com/max-niederman/ttyper",
  "desc": {
    "cn": "ttyper 是一个使用 Rust 和 Ratatui 构建的基于终端的打字测试",
    "en": "ttyper is a terminal-based typing test built with Rust and Ratatui"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install ttyper",
      "reference": "https://github.com/max-niederman/ttyper?tab=readme-ov-file#cargo"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S ttyper",
      "reference": "https://github.com/max-niederman/ttyper?tab=readme-ov-file#arch-linux"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixpkgs.ttyper",
      "reference": "https://github.com/max-niederman/ttyper?tab=readme-ov-file#nix"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install ttyper",
      "reference": "https://github.com/max-niederman/ttyper?tab=readme-ov-file#scoop"
    }
  }
}
