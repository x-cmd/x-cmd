{
  "lang": "rust",
  "homepage": "https://jless.io/",
  "desc": {
    "cn": "Jless 是一个命令行 JSON 查看器，用于读取、探索和搜索 JSON 数据",
    "en": "jless is a command-line JSON viewer designed for reading, exploring, and searching through JSON data"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use jless",
      "reference": "https://github.com/jc00ke/asdf-jless"
    },
    "darwin/brew": {
      "cmd": "brew install jless",
      "reference": "https://formulae.brew.sh/formula/jless"
    },
    "darwin/port": {
      "cmd": "sudo port install jless",
      "reference": "https://ports.macports.org/port/jless/"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S jless",
      "reference": "https://jless.io/"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install jless",
      "reference": "https://jless.io/"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install jless",
      "reference": "https://pkgsrc.se/textproc/jless/"
    },
    "freebsd/pkg": {
      "cmd": "pkg install jless",
      "reference": "https://jless.io/"
    },
    "/cargo": {
      "cmd": "cargo install jless",
      "reference": "https://jless.io/",
      "dsnap": "jless"
    }
  },
  "dsnap": {
    "/cargo": "jless"
  }
}
