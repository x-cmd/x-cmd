{
  "lang": "Rust",
  "homepage": "https://github.com/jwt-rs/jwt-ui",
  "desc": {
    "en": "A Terminal UI for decoding/encoding JSON Web Tokens",
    "cn": "用于解码/编码JSON Web Token的终端用户界面"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap jwt-rs/jwt-ui\nbrew install jwt-ui\n",
      "reference": "https://github.com/jwt-rs/jwt-ui?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add jwt-ui-bucket https://github.com/jwt-rs/scoop-jwt-ui\nx scoop install jwt-ui\n",
      "reference": "https://github.com/jwt-rs/jwt-ui?tab=readme-ov-file#installation"
    },
    "/cargo": {
      "cmd": "cargo install jwt-ui --locked",
      "reference": "https://github.com/jwt-rs/jwt-ui?tab=readme-ov-file#installation",
      "dsnap": "jwt-ui"
    },
    "arch/pacman": {
      "cmd": "pacman -S jwt-ui",
      "reference": "https://github.com/jwt-rs/jwt-ui?tab=readme-ov-file#installation"
    },
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/jwt-rs/jwt-ui/main/deployment/getLatest.sh | bash",
      "reference": "https://github.com/jwt-rs/jwt-ui?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "jwt-ui",
    "/cargo": "jwt-ui"
  }
}
