{
  "lang": "go",
  "homepage": "https://fx.wtf/",
  "desc": {
    "cn": "Fx 是为 JSON 量身定制的双重用途命令行工具，它提供了基于终端的 JSON 查看器和 JSON 处理实用程序",
    "en": "Fx is a dual-purpose command-line tool tailored for JSON, providing both a terminal-based JSON viewer and a JSON processing utility"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use fx",
      "reference": "https://gitlab.com/wt0f/asdf-fx"
    },
    "darwin/brew": {
      "cmd": "brew install fx",
      "reference": "https://fx.wtf/install"
    },
    "/snap": {
      "cmd": "snap install fx",
      "reference": "https://fx.wtf/install"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install fx",
      "reference": "https://fx.wtf/install"
    },
    "/pacman": {
      "cmd": "sudo pacman -S fx",
      "reference": "https://fx.wtf/install"
    },
    "/pkg": {
      "cmd": "pkg install fx",
      "reference": "https://fx.wtf/install"
    },
    "/go": {
      "cmd": "go install github.com/antonmedv/fx@latest",
      "reference": "https://fx.wtf/install"
    }
  }
}
