{
  "lang": "Go",
  "homepage": "https://github.com/JFryy/qq",
  "desc": {
    "cn": "jq，但具有许多互操作配置格式转换和交互式查询。",
    "en": "jq, but with many interoperable configuration format transcodings and interactive querying."
  },
  "rule": {
    "arch/yay": {
      "cmd": "yay qq-git",
      "reference": "https://github.com/JFryy/qq/?tab=readme-ov-file#installation"
    },
    "/brew": {
      "cmd": "brew install jfryy/tap/qq",
      "reference": "https://github.com/JFryy/qq/?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/JFryy/qq/cli@latest",
      "reference": "https://github.com/JFryy/qq/?tab=readme-ov-file#installation"
    }
  }
}
