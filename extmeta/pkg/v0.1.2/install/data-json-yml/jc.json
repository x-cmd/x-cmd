{
  "lang": "python",
  "desc": {
    "en": "Converts the output of common command-line tools, file types, and strings into JSON, YAML, or dictionaries",
    "cn": "将常见命令行工具、文件类型和字符串的输出 转换为 JSON、YAML 或 dictionaries"
  },
  "homepage": "https://github.com/kellyjonbrazil/jc",
  "rule": {
    "/pip": {
      "cmd": "pip3 install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation",
      "dsnap": "jc"
    },
    "/apt": {
      "cmd": "sudo apt-get install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation",
      "dsnap": "jc"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "/nix": {
      "cmd": "nix-env -iA nixpkgs.jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "guixos/guix": {
      "cmd": "guix install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "gentoo/emerge": {
      "cmd": "emerge dev-python/jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "photon/tdnf": {
      "cmd": "tdnf install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install jc",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    },
    "freebsd/port": {
      "cmd": "portsnap fetch update && cd /usr/ports/textproc/py-jc && make install clean",
      "reference": "https://github.com/kellyjonbrazil/jc?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "brew": "jc",
    "/pip": "jc",
    "fedora/dnf": "jc",
    "alpine/apk": "jc"
  }
}
