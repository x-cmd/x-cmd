{
  "lang": "Typescript",
  "homepage": "https://cspell.org/",
  "desc": {
    "cn": "CSpell 代码拼写检查器",
    "en": "CSpell a spell checker for code"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g cspell@latest",
      "reference": "https://cspell.org/docs/installation/#npm-global",
      "dsnap": "cspell"
    }
  },
  "dsnap": {
    "/npm": "cspell"
  }
}
