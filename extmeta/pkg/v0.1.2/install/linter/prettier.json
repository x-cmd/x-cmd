{
  "homepage": "https://prettier.io",
  "lang": "node",
  "desc": {
    "cn": "Prettier 是一个固执, 严格的代码格式化工具",
    "en": "Prettier is an opinionated code formatter"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g prettier",
      "reference": "https://prettier.io/docs/en/install",
      "dsnap": "prettier"
    },
    "darwin/brew": {
      "cmd": "brew install prettier",
      "reference": "https://formulae.brew.sh/formula/prettier#default"
    }
  },
  "dsnap": {
    "brew": "prettier",
    "/npm": "prettier"
  }
}
