{
  "lang": "node",
  "homepage": "https://github.com/lint-staged",
  "desc": {
    "cn": "在 git 暂存文件上运行 linters",
    "en": "Run linters on git staged files"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install --save-dev lint-staged",
      "reference": "https://github.com/lint-staged/lint-staged?tab=readme-ov-file#installation-and-setup",
      "dsnap": "lint-staged"
    }
  },
  "dsnap": {
    "/npm": "lint-staged"
  }
}
