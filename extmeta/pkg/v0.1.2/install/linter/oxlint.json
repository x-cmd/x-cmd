{
  "lang": "Rust",
  "homepage": "https://oxc-project.github.io/",
  "desc": {
    "cn": "Oxlint 旨在捕获错误或无用的代码，默认情况下不需要任何配置",
    "en": "Oxlint is designed to catch erroneous or useless code without requiring any configurations by default"
  },
  "rule": {
    "/npm": {
      "cmd": "npm add -g oxlint",
      "reference": "https://oxc-project.github.io/docs/guide/usage/linter.html#installation",
      "dsnap": "oxlint"
    },
    "/npx": {
      "cmd": "npx oxlint@latest",
      "reference": "https://oxc-project.github.io/docs/guide/usage/linter.html#installation"
    }
  },
  "dsnap": {
    "/npm": "oxlint"
  }
}
