{
  "lang": "Rust",
  "homepage": "https://rustwasm.github.io/wasm-pack/",
  "desc": {
    "en": "wasm-pack is a tool for building and using Rust-generated WebAssembly that can be easily integrated with JavaScript.",
    "cn": "wasm-pack 是一个用于构建和使用由 Rust 生成的 WebAssembly 的工具，可以轻松地与 JavaScript 集成。"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh",
      "reference": "https://rustwasm.github.io/wasm-pack/installer/"
    },
    "darwin/brew": {
      "cmd": "brew install wasm-pack",
      "reference": "https://formulae.brew.sh/formula/wasm-pack#default"
    },
    "/npm": {
      "cmd": "npm install -g wasm-pack",
      "reference": "https://rustwasm.github.io/wasm-pack/installer/"
    },
    "/cargo": {
      "cmd": "cargo install wasm-pack",
      "reference": "https://rustwasm.github.io/wasm-pack/installer/"
    },
    "/yarn": {
      "cmd": "yarn global add wasm-pack",
      "reference": "https://rustwasm.github.io/wasm-pack/installer/"
    }
  }
}
