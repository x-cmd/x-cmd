{
  "homepage": "https://wasmer.io/",
  "desc": {
    "cn": "Wasmer 是一个快速和安全的 WebAssembly 运行时",
    "en": "Wasmer is a blazing fast and secure WebAssembly runtime"
  },
  "lang": "rust",
  "rule": {
    "/asdf": {
      "cmd": "x asdf use wasmer",
      "reference": "https://github.com/tachyonicbytes/asdf-wasmer"
    },
    "/curl": {
      "cmd": "curl https://get.wasmer.io -sSfL | sh",
      "reference": "https://wasmer.io/"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install wasmer",
      "reference": "https://github.com/wasmerio/wasmer-install?tab=readme-ov-file#install-via-package-manager"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install wasmer",
      "reference": "https://github.com/wasmerio/wasmer-install?tab=readme-ov-file#install-via-package-manager"
    },
    "/cargo": {
      "cmd": "cargo install wasmer-cli --features singlepass,cranelift",
      "reference": "https://github.com/wasmerio/wasmer-install?tab=readme-ov-file#install-via-package-manager",
      "dsnap": "wasmer-cli"
    },
    "darwin/brew": {
      "cmd": "brew install wasmer",
      "reference": "https://formulae.brew.sh/formula/wasmer#default"
    }
  },
  "dsnap": {
    "/cargo": "wasmer-cli"
  }
}
