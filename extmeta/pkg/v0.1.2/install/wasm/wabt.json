{
  "lang": "C++",
  "homepage": "https://github.com/WebAssembly/wabt",
  "desc": {
    "cn": "WABT（我们发音为“wabbit”）是一套用于 WebAssembly 的工具",
    "en": "WABT (we pronounce it \"wabbit\") is a suite of tools for WebAssembly"
  },
  "rule": {
    "darwin/wabt": {
      "cmd": "brew install wabt",
      "reference": "https://formulae.brew.sh/formula/wabt#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/wabt",
      "reference": "https://scoop.sh/#/apps?q=wabt"
    },
    "/nix-shell": {
      "cmd": "nix-shell -p wabt",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=wabt&from=0&size=50&sort=relevance&type=packages&query=wabt"
    },
    "/nix-env": {
      "cmd": "nix-env -iA nixos.wabt",
      "reference": "https://search.nixos.org/packages?channel=unstable&show=wabt&from=0&size=50&sort=relevance&type=packages&query=wabt"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install wabt",
      "reference": "https://installati.one/install-wabt-ubuntu-22-04/#install-wabt-using-apt-get"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install wabt",
      "reference": "https://installati.one/install-wabt-ubuntu-22-04/#install-wabt-using-apt-get"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install wabt",
      "reference": "https://installati.one/install-wabt-ubuntu-22-04/#install-wabt-using-apt-get"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install wabt",
      "reference": "https://installati.one/install-wabt-ubuntu-22-04/#install-wabt-using-apt-get"
    },
    "/aptitude": {
      "cmd": "sudo aptitude -y install wabt",
      "reference": "https://installati.one/install-wabt-ubuntu-22-04/#install-wabt-using-aptitude"
    }
  }
}
