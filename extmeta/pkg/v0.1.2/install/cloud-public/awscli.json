{
  "lang": "python",
  "homepage": "https://github.com/aws/aws-cli",
  "desc": {
    "cn": "Amazon Web Services 的通用命令行界面",
    "en": "Universal Command Line Interface for Amazon Web Services"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use awscli",
      "reference": "https://github.com/MetricMike/asdf-awscli"
    },
    "/pip": {
      "cmd": "pip3 install awscli",
      "reference": "https://docs.aws.amazon.com/cli/v1/userguide/install-linux.html",
      "dsnap": "awscli"
    },
    "darwin/brew": {
      "cmd": "brew install awscli",
      "reference": "https://formulae.brew.sh/formula/awscli#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install aws",
      "reference": "https://scoop.sh/#/apps?q=aws"
    }
  },
  "dsnap": {
    "/pip": "awscli"
  }
}
