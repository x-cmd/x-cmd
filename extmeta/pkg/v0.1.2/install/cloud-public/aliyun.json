{
  "lang": "go",
  "homepage": "https://github.com/aliyun/aliyun-cli",
  "desc": {
    "cn": "阿里云 CLI 是一种通过命令行界面管理和使用阿里云资源的工具",
    "en": "The Alibaba Cloud CLI is a tool to manage and use Alibaba Cloud resources through a command line interface"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install aliyun-cli",
      "reference": "https://formulae.brew.sh/formula/aliyun-cli#default"
    },
    "/curl": {
      "cmd": "/bin/bash -c \"$(curl -fsSL https://raw.githubusercontent.com/aliyun/aliyun-cli/HEAD/install.sh)\"",
      "reference": "https://github.com/aliyun/aliyun-cli#installation"
    }
  }
}
