{
  "lang": "python",
  "homepage": "https://github.com/ovh/ovh-cli",
  "desc": {
    "cn": "OVH命令行界面",
    "en": "OVH command line interface"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install ovhcli",
      "reference": "https://github.com/ovh/ovh-cli?tab=readme-ov-file#installation",
      "dsnap": "ovhcli"
    },
    "/git": {
      "cmd": "pip install -e git+https://github.com/ovh/ovh-cli.git#egg=ovh-cli",
      "reference": "https://github.com/ovh/ovh-cli?tab=readme-ov-file#installation"
    }
  },
  "dsnap": {
    "/pip": "ovhcli"
  }
}
