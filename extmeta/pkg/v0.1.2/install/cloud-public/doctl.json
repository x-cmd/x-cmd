{
  "lang": "go",
  "homepage": "https://github.com/digitalocean/doctl",
  "desc": {
    "cn": "DigitalOcean API 的官方命令行工具。",
    "en": "The official command line interface for the DigitalOcean API."
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use doctl",
      "reference": "https://github.com/maristgeek/asdf-doctl"
    },
    "darwin/brew": {
      "cmd": "brew install doctl",
      "reference": "https://github.com/digitalocean/doctl?tab=readme-ov-file#macos"
    },
    "darwin/port": {
      "cmd": "sudo port install doctl",
      "reference": "https://ports.macports.org/port/doctl/"
    },
    "/snap": {
      "cmd": "sudo snap install doctl",
      "reference": "https://github.com/digitalocean/doctl?tab=readme-ov-file#snap-supported-os"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S doctl",
      "reference": "https://github.com/digitalocean/doctl?tab=readme-ov-file#arch-linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install doctl",
      "reference": "https://github.com/digitalocean/doctl?tab=readme-ov-file#fedora"
    },
    "nix/": {
      "cmd": "nix-shell -p doctl",
      "reference": "https://search.nixos.org/packages?channel=23.11&show=doctl&from=0&size=50&sort=relevance&type=packages&query=doctl"
    }
  },
  "dsnap": {
    "/go": "doctl"
  }
}
