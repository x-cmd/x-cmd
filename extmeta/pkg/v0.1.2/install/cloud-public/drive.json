{
  "lang": "go",
  "desc": {
    "cn": "用于命令行的 Google Drive 客户端",
    "en": "Google Drive client for command line"
  },
  "homepage": "https://github.com/odeke-em/drive",
  "rule": {
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable vaughan/drive-google\n&& sudo dnf install drive-google\n",
      "reference": "https://github.com/odeke-em/drive/blob/master/platform_packages.md#fedora-distro"
    },
    "arch/yay": {
      "cmd": "yay -S drive-bin",
      "reference": "https://github.com/odeke-em/drive/blob/master/platform_packages.md#arch-linux-or-arch-based-distros"
    }
  }
}
