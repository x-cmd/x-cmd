{
  "lang": "typescript",
  "homepage": "https://aws.amazon.com/cn/cdk/",
  "desc": {
    "cn": "AWS 云开发工具包是一个用于在代码中定义云基础设施的框架",
    "en": "The AWS Cloud Development Kit is a framework for defining cloud infrastructure in code"
  },
  "rule": {
    "/npm": {
      "cmd": "npm i -g aws-cdk",
      "reference": "https://github.com/aws/aws-cdk?tab=readme-ov-file#at-a-glance",
      "dsnap": "aws-cdk"
    },
    "darwin/brew": {
      "cmd": "brew install aws-cdk",
      "reference": "https://formulae.brew.sh/formula/aws-cdk#default"
    },
    "/pip": {
      "cmd": "pip install aws-cdk-lib",
      "reference": "https://pypi.org/project/aws-cdk-lib/",
      "dsnap": "aws-cdk-lib"
    }
  },
  "dsnap": {
    "brew": "aws-cdk",
    "/pip": "aws-cdk-lib",
    "/npm": "aws-cdk"
  }
}
