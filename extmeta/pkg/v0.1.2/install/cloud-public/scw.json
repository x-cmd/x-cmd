{
  "lang": "go",
  "desc": {
    "cn": "Scaleway CLI 是一款帮助您直接从终端试用 Scaleway 基础设施的工具",
    "en": "Scaleway CLI is a tool to help you pilot your Scaleway infrastructure directly from your terminal"
  },
  "homepage": "https://github.com/scaleway/scaleway-cli",
  "rule": {
    "darwin/brew": {
      "cmd": "brew install scw",
      "reference": "https://github.com/scaleway/scaleway-cli?tab=readme-ov-file#homebrew"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S scaleway-cli",
      "reference": "https://github.com/scaleway/scaleway-cli?tab=readme-ov-file#arch-linux"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install scaleway-cli",
      "reference": "https://github.com/scaleway/scaleway-cli?tab=readme-ov-file#chocolatey"
    },
    "/curl": {
      "cmd": "curl -s https://raw.githubusercontent.com/scaleway/scaleway-cli/master/scripts/get.sh | sh",
      "reference": "https://github.com/scaleway/scaleway-cli?tab=readme-ov-file#linux"
    }
  },
  "dsnap": {
    "brew": "scw"
  }
}
