{	"homepage"	:	"https://github.com/metalbear-co/mirrord"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/mirrord/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/mirrord"	,	"https://npmmirror.com/package/@x-cmd-pkg/mirrord"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/mirrord/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"xbin"	:	{	"mirrord"	:	"bin/mirrord%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v3.84.1"	}	,	"latest/*"	:	{	"version"	:	"v3.84.1"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	,	"v0.0.1/*/*"	:	{	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"gh_url"	:	"https://github.com/x-cmd-build/mirrord/releases/download/%{version}"	,	"zipname"	:	"mirrord_%{os}_%{arch}.tar.xz"	}	,	"v3.84.1/*/*"	:	{	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/mirrord/-/mirrord-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/mirrord/-/mirrord-0.0.0-%{version}-%{tree}.tgz"	}	}	}	}	