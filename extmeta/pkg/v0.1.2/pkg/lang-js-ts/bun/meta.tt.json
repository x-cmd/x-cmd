{	"homepage"	:	"https://github.com/oven-sh/bun"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/bun/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/bun"	,	"https://npmmirror.com/package/@x-cmd-pkg/bun"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/bun/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/bun/-/bun-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/bun/-/bun-0.0.0-%{version}-%{tree}.tgz"	}	,	"xbin"	:	{	"bun"	:	"shim_bin/bun%{bin_suffix}"	,	"bunx"	:	"shim_bin/bunx%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	,	"boot"	:	"boot.sh"	,	"unboot"	:	"unboot.sh"	}	,	"sociality"	:	"intrusive"	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"env"	:	{	"path"	:	{	"unshift"	:	[	"%{HOME}/.bun/bin"	]	}	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v1.2.2"	}	,	"latest/*"	:	{	"version"	:	"v1.2.2"	}	,	"*/linux/x64"	:	null	,	"*/linux/arm64"	:	null	,	"*/darwin/x64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/win/x64"	:	{	"bin_suffix"	:	".exe"	}	}	}	