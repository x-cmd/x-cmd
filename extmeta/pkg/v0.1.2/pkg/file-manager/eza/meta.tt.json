{	"homepage"	:	"https://github.com/eza-community/eza"	,	"license"	:	"MIT"	,	"source"	:	{	"repack-official"	:	[	"https://github.com/x-cmd-build/eza/releases"	,	"https://npmjs.com/package/@x-cmd-pkg/eza"	,	"https://npmmirror.com/package/@x-cmd-pkg/eza"	]	}	,	"gh_url"	:	"https://github.com/x-cmd-build/eza/releases/download/%{semantic_version}"	,	"download_file_ext"	:	"tar.xz"	,	"zipname"	:	"%{tree}.%{download_file_ext}"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"%{gh_url}/%{zipname}"	,	"cn"	:	"%{gh_url}/%{zipname}"	}	,	"npm"	:	"yes"	,	"npmurl"	:	{	"internet"	:	"https://registry.npmjs.org/@x-cmd-pkg/eza/-/eza-0.0.0-%{version}-%{tree}.tgz"	,	"cn"	:	"https://registry.npmmirror.com/@x-cmd-pkg/eza/-/eza-0.0.0-%{version}-%{tree}.tgz"	}	,	"xbin"	:	{	"eza"	:	"bin/eza%{bin_suffix}"	}	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v0.21.0"	}	,	"latest/*"	:	{	"version"	:	"v0.21.0"	}	,	"*/linux/x64"	:	{	"_build"	:	"musl"	}	,	"*/linux/arm64"	:	{	"_build"	:	"glib"	}	,	"*/darwin/x64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/win/x64"	:	{	"_build"	:	"glib"	,	"bin_suffix"	:	".exe"	}	,	"(v0.19.4|v0.20.10|v0.21.0)/darwin/*"	:	{	"tree"	:	"%{os}.%{arch}.%{_build}.build"	}	}	}	