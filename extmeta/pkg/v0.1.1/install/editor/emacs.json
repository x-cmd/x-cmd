{
  "lang": "Emacs Lisp",
  "homepage": "https://www.gnu.org/software/emacs/",
  "desc": {
    "cn": "GNU Emacs 是一个可扩展、可定制、免费的文本编辑器",
    "en": "GNU Emacs is an extensible, customizable, free/libre text editor"
  },
  "rule": {
    "/guix": {
      "cmd": "guix package -i emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "fefora/dnf": {
      "cmd": "sudo dnf install emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#gnu-linux"
    },
    "darwin/brew": {
      "cmd": "brew install --cask emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#nonfree"
    },
    "darwin/port": {
      "cmd": "sudo port install emacs-app",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#nonfree"
    },
    "win-x64/pacman": {
      "cmd": "pacman -S mingw-w64-x86_64-emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#nonfree"
    },
    "win-i686/pacman": {
      "cmd": "pacman -S mingw-w64-i686-emacs",
      "reference": "https://www.gnu.org/savannah-checkouts/gnu/emacs/download.html#nonfree"
    },
    "alpine/apk": {
      "cmd": "sudo apk add emacs",
      "reference": "https://command-not-found.com/emacs"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install emacs",
      "reference": "https://command-not-found.com/emacs"
    },
    "centos/yum": {
      "cmd": "sudo yum install emacs-1",
      "reference": "https://command-not-found.com/emacs"
    },
    "fedora/dnf": {
      "cmd": "sudo yum install emacs-1",
      "reference": "https://command-not-found.com/emacs"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install emacs",
      "reference": "https://command-not-found.com/emacs"
    }
  }
}
