{
  "lang": "Go",
  "homepage": "https://budimanjojo.github.io/talhelper/latest/",
  "desc": {
    "cn": "talhelper 是一种工具，可以声明地创建 Talos 配置文件",
    "en": "talhelper is a tool to help creating Talos configuration files declaratively\\"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use talhelper",
      "reference": "https://github.com/bjw-s/asdf-talhelper?tab=readme-ov-file#install"
    },
    "darwin/brew": {
      "cmd": "brew install talhelper",
      "reference": "https://budimanjojo.github.io/talhelper/latest/installation/#using-homebrew"
    },
    "/pacman": {
      "cmd": "sudo pacman -S talhelper",
      "reference": "https://budimanjojo.github.io/talhelper/latest/installation/#using-pacman"
    },
    "/yay": {
      "cmd": "yay -S talhelper-bin",
      "reference": "https://budimanjojo.github.io/talhelper/latest/installation/#using-aur"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add budimanjojo https://github.com/budimanjojo/talhelper.git \\\n&& x scoop install talhelper\n",
      "reference": "https://budimanjojo.github.io/talhelper/latest/installation/#using-scoop"
    },
    "/curl": {
      "cmd": "curl https://i.jpillora.com/budimanjojo/talhelper! | sudo bash",
      "reference": "https://budimanjojo.github.io/talhelper/latest/installation/#using-one-liner-with-jpillora"
    }
  }
}
