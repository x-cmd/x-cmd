{
  "lang": "Go",
  "homepage": "https://github.com/stern/stern",
  "desc": {
    "cn": "STERN 使您可以在吊舱内的 Kubernetes 和多个容器上尾部多个 POD",
    "en": "Stern allows you to tail multiple pods on Kubernetes and multiple containers within the pod"
  },
  "rule": {
    "/go": {
      "cmd": "go install github.com/stern/stern@latest",
      "reference": "https://github.com/stern/stern?tab=readme-ov-file#build-from-source"
    },
    "darwin/brew": {
      "cmd": "brew install stern",
      "reference": "https://github.com/stern/stern?tab=readme-ov-file#homebrew-linuxmacos"
    },
    "/krew": {
      "cmd": "kubectl krew install stern",
      "reference": "https://github.com/stern/stern?tab=readme-ov-file#krew-linuxmacoswindows"
    },
    "/asdf": {
      "cmd": "x asdf use stern",
      "reference": "https://github.com/looztra/asdf-stern?tab=readme-ov-file#install"
    }
  }
}
