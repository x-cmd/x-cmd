{
  "lang": "Go",
  "homepage": "https://1panel.cn/",
  "desc": {
    "cn": "1Panel 是新一代的 Linux 服务器运维管理面板",
    "en": "1Panel is a modern and Open-Source linux server operation and management panel"
  },
  "rule": {
    "rhel/curl": {
      "cmd": "curl -sSL https://resource.fit2cloud.com/1panel/package/quick_start.sh -o quick_start.sh && sh quick_start.sh",
      "reference": "https://1panel.cn/docs/installation/online_installation/#2"
    },
    "ubuntu/curl": {
      "cmd": "curl -sSL https://resource.fit2cloud.com/1panel/package/quick_start.sh -o quick_start.sh && sudo bash quick_start.sh",
      "reference": "https://1panel.cn/docs/installation/online_installation/#2"
    },
    "debian/curl": {
      "cmd": "curl -sSL https://resource.fit2cloud.com/1panel/package/quick_start.sh -o quick_start.sh && bash quick_start.sh",
      "reference": "https://1panel.cn/docs/installation/online_installation/#2"
    }
  }
}
