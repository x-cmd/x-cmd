{
  "lang": "JavaScript",
  "homepage": "https://github.com/postmanlabs/newman",
  "desc": {
    "cn": "Newman 是 Postman 的命令行收集运行程序",
    "en": "Newman is a command-line collection runner for Postman"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g newman",
      "reference": "https://github.com/postmanlabs/newman?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install newman",
      "reference": "https://github.com/postmanlabs/newman?tab=readme-ov-file#using-homebrew"
    }
  }
}
