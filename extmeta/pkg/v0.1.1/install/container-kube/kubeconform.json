{
  "lang": "Go",
  "homepage": "https://github.com/yannh/kubeconform",
  "desc": {
    "cn": "KubeConform 是 Kubernetes 清单验证工具",
    "en": "Kubeconform is a Kubernetes manifest validation tool"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install kubeconform",
      "reference": "https://github.com/yannh/kubeconform?tab=readme-ov-file#installation"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install YannHamon.kubeconform",
      "reference": "https://github.com/yannh/kubeconform?tab=readme-ov-file#installation"
    },
    "/go": {
      "cmd": "go install github.com/yannh/kubeconform/cmd/kubeconform@latest",
      "reference": "https://github.com/yannh/kubeconform?tab=readme-ov-file#installation"
    },
    "/asdf": {
      "cmd": "x asdf use kubeconform",
      "reference": "https://github.com/lirlia/asdf-kubeconform?tab=readme-ov-file#usage"
    }
  }
}
