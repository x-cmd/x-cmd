{
  "lang": "Go",
  "homepage": "https://github.com/Ladicle/kubectl-rolesum",
  "desc": {
    "cn": "总结指定主题的RBAC角色（ServiceAccount，用户和组）",
    "en": "Summarize RBAC roles for the specified subject (ServiceAccount, User and Group)"
  },
  "rule": {
    "/krew": {
      "cmd": "kubectl krew install rolesum",
      "reference": "https://github.com/Ladicle/kubectl-rolesum?tab=readme-ov-file#krew"
    },
    "darwin/brew": {
      "cmd": "brew install Ladicle/kubectl-bindrole/kubectl-rolesum",
      "reference": "https://github.com/Ladicle/kubectl-rolesum?tab=readme-ov-file#homebrew-for-macos"
    },
    "/asdf": {
      "cmd": "x sdf use kubectl-bindrole",
      "reference": "https://github.com/looztra/asdf-kubectl-bindrole"
    }
  }
}
