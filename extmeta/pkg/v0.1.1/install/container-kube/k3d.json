{
  "lang": "go",
  "homepage": "https://k3d.io/v5.6.0/",
  "desc": {
    "cn": "k3d 是一个轻量级封装工具，用于在 Docker 中运行 k3s（Rancher Lab 的精简 Kubernetes 发行版）",
    "en": "k3d is a lightweight wrapper to run k3s (Rancher Lab’s minimal Kubernetes distribution) in docker"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use k3d",
      "reference": "https://github.com/spencergilbert/asdf-k3d"
    },
    "darwin/brew": {
      "cmd": "brew install k3d",
      "reference": "https://formulae.brew.sh/formula/k3d#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install k3d",
      "reference": "https://command-not-found.com/k3d"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install k3d",
      "reference": "https://command-not-found.com/k3d"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install k3d",
      "reference": "https://command-not-found.com/k3d"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install k3d",
      "reference": "https://command-not-found.com/k3d"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install k3d",
      "reference": "https://command-not-found.com/k3d"
    }
  }
}
