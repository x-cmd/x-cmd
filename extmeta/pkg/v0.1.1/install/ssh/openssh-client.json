{
  "lang": "c",
  "homepage": "https://www.openssh.com/",
  "desc": {
    "cn": "openssh-server 是 OpenSSH 连接工具的客户端",
    "en": "openssh-server is a client for the OpenSSH connection tool"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install openssh",
      "reference": "https://formulae.brew.sh/formula/openssh#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install openssh-client",
      "reference": "https://www.cyberciti.biz/faq/how-to-install-ssh-on-ubuntu-linux-using-apt-get/"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install openssh-client",
      "reference": "https://www.cyberciti.biz/faq/how-to-install-ssh-on-ubuntu-linux-using-apt-get/"
    },
    "alpine/apk": {
      "cmd": "sudo apk add openssh",
      "reference": "https://www.cyberciti.biz/faq/how-to-install-openssh-server-on-alpine-linux-including-docker/"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -Sy openssh",
      "reference": "https://www.unixmen.com/how-to-install-openssh-in-archinux/"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install openssh-client",
      "reference": "https://www.kali.org/tools/openssh/"
    },
    "centos/yum": {
      "cmd": "sudo yum -y install openssh-server openssh-clients",
      "reference": "https://phoenixnap.com/kb/how-to-enable-ssh-centos-7"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf -y install openssh",
      "reference": "https://installati.one/fedora/34/openssh/"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install openssh",
      "reference": "https://linuxways.net/opensuse/how-to-enable-ssh-on-opensuse/"
    }
  }
}
