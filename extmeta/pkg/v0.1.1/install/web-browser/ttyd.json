{
  "lang": "c",
  "homepage": "https://github.com/tsl0922/ttyd",
  "desc": {
    "cn": "ttyd 是一个简单的命令行工具，用于通过 Web 共享终端",
    "en": "ttyd is a simple command-line tool for sharing terminal over the web"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use ttyd",
      "reference": "https://github.com/ivanvc/asdf-ttyd"
    },
    "darwin/brew": {
      "cmd": "brew install ttyd",
      "reference": "https://formulae.brew.sh/formula/ttyd#default"
    },
    "alpine/apk": {
      "cmd": "sudo apk add ttyd",
      "reference": "https://command-not-found.com/ttyd"
    }
  }
}
