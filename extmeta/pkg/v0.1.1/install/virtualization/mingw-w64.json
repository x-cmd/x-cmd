{
  "lang": "CSS",
  "homepage": "https://www.mingw-w64.org/",
  "desc": {
    "cn": "Mingw-w64 是原始 mingw.org 项目的进步，旨在支持 Windows 系统上的 GCC 编译器",
    "en": "Mingw-w64 is an advancement of the original mingw.org project, created to support the GCC compiler on Windows systems"
  },
  "rule": {
    "darwin/port": {
      "cmd": "sudo port install mingw-w64",
      "reference": "https://www.mingw-w64.org/downloads/#macports"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S mingw-w64",
      "reference": "https://www.mingw-w64.org/downloads/#arch-linux"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install mingw-w64",
      "reference": "https://www.mingw-w64.org/downloads/#ubuntu"
    },
    "debian/apt": {
      "cmd": "sudo apt install mingw-w64",
      "reference": "https://www.mingw-w64.org/downloads/#debian"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install mingw-w64",
      "reference": "https://www.mingw-w64.org/downloads/#fedora"
    },
    "win/powershell": {
      "cmd": "x pwsh Invoke-WebRequest -Uri https://github.com/msys2/msys2-installer/releases/download/2024-01-13/msys2-x86_64-20240113.exe -OutFile msys2-x86_64-20240113.exe",
      "reference": "https://www.msys2.org/#installation"
    }
  }
}
