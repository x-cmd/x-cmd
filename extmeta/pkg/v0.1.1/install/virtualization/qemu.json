{
  "lang": "c",
  "homepage": "https://www.qemu.org/",
  "desc": {
    "en": "A generic and open source machine emulator and virtualizer",
    "cn": "一个通用的开源机器模拟器和虚拟化器"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install qemu",
      "reference": "https://formulae.brew.sh/formula/qemu#default"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port install qemu",
      "reference": "https://www.qemu.org/download/#macos"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S qemu",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "centos/yum": {
      "cmd": "sudo yum install qemu-kvm",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install qemu-system",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install qemu-system",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install @virtualization",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge --ask app-emulation/qemu",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install qemu",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install qemu-system",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "rhel/yum": {
      "cmd": "sudo yum install qemu-kvm",
      "reference": "https://www.qemu.org/download/#linux"
    },
    "win/msys2": {
      "cmd": "sudo pacman -S mingw-w64-x86_64-qemu",
      "reference": "https://www.qemu.org/download/#windows"
    }
  }
}
