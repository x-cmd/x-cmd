{
  "lang": "java",
  "homepage": "https://github.com/manikmagar/mulefd",
  "desc": {
    "cn": "Mule 流程调用图和图表",
    "en": "Mule flow call graphs and diagrams"
  },
  "rule": {
    "/sdk": {
      "cmd": "sdk install mulefd",
      "reference": "https://github.com/manikmagar/mulefd#sdkman-linux-apple"
    },
    "darwin/brew": {
      "cmd": "brew install manikmagar/tap/mulefd",
      "reference": "https://github.com/manikmagar/mulefd#homebrew-apple"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add manikmagar https://github.com/manikmagar/scoop-bucket\n&& x scoop install mulefd\n",
      "reference": "https://github.com/manikmagar/mulefd#scoop-windows"
    }
  }
}
