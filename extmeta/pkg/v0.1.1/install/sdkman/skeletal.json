{
  "lang": "Groovy",
  "homepage": "https://github.com/cbmarcum/skeletal",
  "desc": {
    "cn": "一个使用打包模板的简单项目创建工具",
    "en": "A simple project creation tool that uses packaged templates"
  },
  "rule": {
    "/sdk": {
      "cmd": "sdk install skeletal 0.16.0",
      "reference": "https://cbmarcum.github.io/skeletal/application-users-guide.html#_installation"
    },
    "/curl": {
      "cmd": "curl -o skeletal-0.16.0.zip -L https://github.com/cbmarcum/skeletal/releases/download/v0.16.0/skeletal-0.16.0.zip",
      "reference": "https://cbmarcum.github.io/skeletal/application-users-guide.html#_installation"
    }
  }
}
