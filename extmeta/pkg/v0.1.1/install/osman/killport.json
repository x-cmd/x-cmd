{
  "lang": "rust",
  "homepage": "https://github.com/jkfran/killport",
  "desc": {
    "cn": "killport 是一个命令行实用程序，用于终止监听特定端口的进程",
    "en": "killport is a command-line utility for killing processes listening on specific ports"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install killport",
      "reference": "https://github.com/jkfran/killport?tab=readme-ov-file#using-cargo"
    },
    "darwin/brew": {
      "cmd": "brew install killport",
      "reference": "https://github.com/jkfran/killport?tab=readme-ov-file#using-homebrew"
    },
    "/curl": {
      "cmd": "curl -sL https://bit.ly/killport | sh",
      "reference": "https://github.com/jkfran/killport?tab=readme-ov-file#using-installsh"
    }
  }
}
