{
  "lang": "rust",
  "homepage": "https://github.com/stepchowfun/toast",
  "desc": {
    "cn": "Toast 是一种在容器中工作的工具,将您的开发和持续集成环境集成在一起",
    "en": "Toast is a tool for doing work in containers,containerize your development and continuous integration environments"
  },
  "rule": {
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/stepchowfun/toast/main/install.sh -LSfs | sh",
      "reference": "https://github.com/stepchowfun/toast?tab=readme-ov-file#installation-on-macos-or-linux-aarch64-or-x86-64"
    },
    "darwin/brew": {
      "cmd": "brew install toast",
      "reference": "https://formulae.brew.sh/formula/toast#default"
    }
  }
}
