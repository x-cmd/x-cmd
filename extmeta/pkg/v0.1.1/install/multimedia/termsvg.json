{
  "lang": "go",
  "homepage": "https://github.com/MrMarble/termsvg",
  "desc": {
    "cn": "记录，共享和导出您的终端作为一个动画 SVG 图像",
    "en": "Record, share and export your terminal as a animated SVG image"
  },
  "rule": {
    "/curl": {
      "cmd": "curl -sL https://raw.githubusercontent.com/MrMarble/termsvg/master/scripts/install-termsvg.sh | sudo -E bash -",
      "reference": "https://github.com/MrMarble/termsvg#installation"
    },
    "/wget": {
      "cmd": "wget -O - https://raw.githubusercontent.com/MrMarble/termsvg/master/scripts/install-termsvg.sh | sudo -E bash -",
      "reference": "https://github.com/MrMarble/termsvg#installation"
    }
  }
}
