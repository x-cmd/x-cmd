{
  "lang": "Rust",
  "homepage": "https://github.com/YS-L/csvlens",
  "desc": {
    "cn": "csvlens 是一个命令行 CSV 文件查看器",
    "en": "csvlens is a command line CSV file viewer"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#homebrew"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#arch-linux"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install --id YS-L.csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#windows"
    },
    "freebsd/pkg": {
      "cmd": "pkg install csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#freebsd"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#netbsd"
    },
    "/cargo": {
      "cmd": "cargo install csvlens",
      "reference": "https://github.com/YS-L/csvlens?tab=readme-ov-file#cargo"
    }
  }
}
