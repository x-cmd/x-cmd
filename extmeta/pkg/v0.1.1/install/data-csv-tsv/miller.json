{
  "lang": "Go",
  "homepage": "https://miller.readthedocs.io/",
  "desc": {
    "cn": "Miller 是一个命令行工具，用于查询、调整和重新格式化各种格式的数据文件，包括 CSV、TSV、JSON 和 JSON Lines",
    "en": "Miller is a command-line tool for querying, shaping, and reformatting data files in various formats including CSV, TSV, JSON, and JSON Lines"
  },
  "rule": {
    "/yum": {
      "cmd": "sudo yum install miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "/apt": {
      "cmd": "sudo apt-get install miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "darwin/brew": {
      "cmd": "brew install miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "darwin/port": {
      "cmd": "sudo port install miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Miller.Miller",
      "reference": "https://github.com/johnkerl/miller?tab=readme-ov-file#installing"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add main && x scoop install main/miller",
      "reference": "https://scoop.sh/#/apps?q=miller"
    }
  }
}
