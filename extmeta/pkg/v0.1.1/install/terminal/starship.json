{
  "lang": "rust",
  "desc": {
    "cn": "适用于任何 shell 的最小、速度极快且可无限定制的提示",
    "en": "The minimal, blazing-fast, and infinitely customizable prompt for any shell"
  },
  "homepage": "https://github.com/starship/starship",
  "rule": {
    "/cargo": {
      "cmd": "cargo install starship --locked",
      "reference": "https://github.com/starship/starship"
    },
    "termux/pkg": {
      "cmd": "pkg install starship",
      "reference": "https://github.com/starship/starship"
    },
    "freebsd/pkg": {
      "cmd": "pkg install starship",
      "reference": "https://github.com/starship/starship"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install starship",
      "reference": "https://github.com/starship/starship"
    },
    "/curl": {
      "cmd": "curl -sS https://starship.rs/install.sh | sh",
      "reference": "https://github.com/starship/starship"
    },
    "/conda": {
      "cmd": "conda install -c conda-forge starship",
      "reference": "https://github.com/starship/starship"
    },
    "linux/brew": {
      "cmd": "brew install starship",
      "reference": "https://github.com/starship/starship"
    },
    "alpine/apk": {
      "cmd": "apk add starship",
      "reference": "https://github.com/starship/starship"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S starship",
      "reference": "https://github.com/starship/starship"
    },
    "centos/dnf": {
      "cmd": "sudo dnf copr enable atim/starship\nsudo dnf install starship\n",
      "reference": "https://github.com/starship/starship"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge app-shells/starship",
      "reference": "https://github.com/starship/starship"
    },
    "manjaro/pacman": {
      "cmd": "sudo pacman -S starship",
      "reference": "https://github.com/starship/starship"
    },
    "nixos/nix": {
      "cmd": "nix-env -iA nixpkgs.starship",
      "reference": "https://github.com/starship/starship"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper in starship",
      "reference": "https://github.com/starship/starship"
    },
    "void/xbps": {
      "cmd": "xbps-install -S starship",
      "reference": "https://github.com/starship/starship"
    },
    "darwin/brew": {
      "cmd": "brew install starship",
      "reference": "https://github.com/starship/starship"
    },
    "darwin/port": {
      "cmd": "sudo port install starship",
      "reference": "https://github.com/starship/starship"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install starship",
      "reference": "https://github.com/starship/starship"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install starship",
      "reference": "https://github.com/starship/starship"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install starship",
      "reference": "https://github.com/starship/starship"
    }
  }
}
