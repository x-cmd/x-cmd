{
  "lang": "python",
  "homepage": "http://s3tools.org",
  "desc": {
    "cn": "适用于 Linux 和 Mac 的命令行 S3 客户端和备份工具",
    "en": "Command Line S3 Client and Backup for Linux and Mac"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install s3cmd",
      "reference": "https://github.com/s3tools/s3cmd/blob/master/INSTALL.md#installation-from-pypa-python-package-authority"
    },
    "darwin/brew": {
      "cmd": "brew install s3cmd",
      "reference": "https://github.com/s3tools/s3cmd/blob/master/INSTALL.md#installation-of-brew-package"
    },
    "debian/apt": {
      "cmd": "sudo apt install s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    },
    "kali/apt": {
      "cmd": "sudo apt install s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install s3cmd",
      "reference": "https://command-not-found.com/s3cmd"
    }
  }
}
