{
  "lang": "Go",
  "homepage": "https://obsproject.com/",
  "desc": {
    "cn": "免费和开源的视频录制和现场流媒体软件",
    "en": "Free and open source software for video recording and live streaming"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install --cask obs",
      "reference": "https://formulae.brew.sh/cask/obs#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add extras && x scoop install extras/obs-studio\n",
      "reference": "https://scoop.sh/#/apps?q=obs"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install obs-studio",
      "reference": "https://community.chocolatey.org/packages/obs-studio"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install OBSProject.OBSStudio",
      "reference": "https://winget.run/pkg/OBSProject/OBSStudio"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install obs-studio",
      "reference": "https://command-not-found.com/obs"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install obs-studio",
      "reference": "https://command-not-found.com/obs"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S obs-studio",
      "reference": "https://command-not-found.com/obs"
    },
    "manjaro/pacman": {
      "cmd": "sudo pacman -S obs-studio",
      "reference": "https://command-not-found.com/obs"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install obs-studio",
      "reference": "https://command-not-found.com/obs"
    },
    "/flatpak": {
      "cmd": "sudo flatpak install flathub com.obsproject.Studio",
      "reference": "https://flathub.org/apps/com.obsproject.Studio"
    }
  }
}
