{
  "lang": "c++",
  "homepage": "https://7-zip.org",
  "desc": {
    "en": "7-Zip is a file archiver with a high compression ratio",
    "cn": "7-Zip 是一款拥有极高压缩比的开源压缩软件"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install 7zip",
      "reference": "https://scoop.sh/#/apps?q=7zip"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install 7zip",
      "reference": "https://community.chocolatey.org/packages/7zip"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install 7zip.7zip",
      "reference": "https://winget.run/pkg/7zip/7zip"
    },
    "darwin/brew": {
      "cmd": "brew install sevenzip",
      "reference": "https://formulae.brew.sh/formula/sevenzip#default"
    }
  }
}
