{
  "lang": "javascript",
  "homepage": "https://www.google.com/chrome/",
  "desc": {
    "en": "Google Chrome is a web browser developed by Google Inc",
    "cn": "Google Chrome 是一款由 Google 公司开发的网页浏览器"
  },
  "rule": {
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install googlechrome",
      "reference": "https://scoop.sh/#/apps?q=Google+Chrome"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install googlechrome",
      "reference": "https://community.chocolatey.org/packages/GoogleChrome"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install Google.Chrome",
      "reference": "https://winget.run/pkg/Google/Chrome"
    },
    "darwin/brew": {
      "cmd": "brew install --cask google-chrome",
      "reference": "https://formulae.brew.sh/cask/google-chrome#default"
    },
    "/flatpak": {
      "cmd": "sudo flatpak install flathub com.google.Chrome",
      "reference": "https://flathub.org/apps/com.google.Chrome"
    },
    "ubuntu/wget": {
      "cmd": "wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb && \\\nsudo dpkg -i google-chrome-stable_current_amd64.deb\n",
      "reference": "https://linuxtldr.com/installing-google-chrome/"
    },
    "debian/wget": {
      "cmd": "wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb && \\\nsudo dpkg -i google-chrome-stable_current_amd64.deb\n",
      "reference": "https://linuxtldr.com/installing-google-chrome/"
    },
    "/yay": {
      "cmd": "yay -S google-chrome",
      "reference": "https://linuxtldr.com/installing-google-chrome/"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install google-chrome-stable",
      "reference": "https://linuxtldr.com/installing-google-chrome/"
    }
  }
}
