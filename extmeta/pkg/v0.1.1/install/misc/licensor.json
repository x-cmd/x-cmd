{
  "lang": "rust",
  "homepage": "https://github.com/raftario/licensor",
  "desc": {
    "cn": "根据标准输出的 SPDX ID 编写一个许可证",
    "en": "Write a license to standard output given its SPDX ID"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install licensor",
      "reference": "https://github.com/raftario/licensor?tab=readme-ov-file#cratesio"
    },
    "darwin/brew": {
      "cmd": "brew install licensor",
      "reference": "https://github.com/raftario/licensor?tab=readme-ov-file#homebrew"
    }
  }
}
