{
  "lang": "Go",
  "homepage": "https://github.com/hairyhenderson/gomplate",
  "desc": {
    "cn": "gomplate 是一个用于模板渲染的命令行工具，支持许多本地和远程数据源",
    "en": "gomplate is a flexible commandline tool for template rendering. Supports lots of local and remote datasources"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gomplate",
      "reference": "https://docs.gomplate.ca/installing/#macoslinux-with-homebrew"
    },
    "linux/brew": {
      "cmd": "brew install gomplate",
      "reference": "https://docs.gomplate.ca/installing/"
    },
    "darwin/port": {
      "cmd": "sudo port install gomplate",
      "reference": "https://docs.gomplate.ca/installing/#macos-with-macports"
    },
    "/asdf": {
      "cmd": "x asdf use gomplate",
      "reference": "https://github.com/sneakybeaky/asdf-gomplate"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install gomplate",
      "reference": "https://docs.gomplate.ca/installing/#windows-with-chocolatey"
    },
    "alpine/apk": {
      "cmd": "sudo apk add --no-cache gomplate",
      "reference": "https://docs.gomplate.ca/installing/#alpine-linux"
    },
    "/go": {
      "cmd": "go install github.com/hairyhenderson/gomplate/v4/cmd/gomplate@latest",
      "reference": "https://docs.gomplate.ca/installing/#install-with-go-install"
    },
    "/npm": {
      "cmd": "npm install -g gomplate",
      "reference": "https://docs.gomplate.ca/installing/#install-with-npm"
    },
    "/tea.xyz": {
      "cmd": "sh <(curl https://tea.xyz) +gomplate.ca sh",
      "reference": "https://docs.gomplate.ca/installing/#install-with-teaxyz"
    }
  }
}
