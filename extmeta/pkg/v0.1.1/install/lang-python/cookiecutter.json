{
  "lang": "Python",
  "homepage": "https://github.com/cookiecutter/cookiecutter",
  "desc": {
    "cn": "一个命令行实用程序，可从 cookiecutter（项目模板）创建项目",
    "en": "Create projects swiftly from cookiecutters (project templates) with this command-line utility"
  },
  "rule": {
    "/pipx": {
      "cmd": "pipx install cookiecutter",
      "reference": "https://github.com/cookiecutter/cookiecutter?tab=readme-ov-file#installation"
    },
    "/pip": {
      "cmd": "python -m pip install --user cookiecutter",
      "reference": "https://github.com/cookiecutter/cookiecutter?tab=readme-ov-file#installation"
    }
  }
}
