{
  "lang": "python",
  "homepage": "https://www.riverbankcomputing.com/software/sip/intro",
  "desc": {
    "cn": "SIP 是一个非常容易为 C 和 C + + 库创建 Python 绑定的工具",
    "en": "SIP is a tool that makes it very easy to create Python bindings for C and C++ libraries"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install sip",
      "reference": "https://formulae.brew.sh/formula/sip#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install sip-devel",
      "reference": "https://command-not-found.com/sip"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install sip-devel",
      "reference": "https://command-not-found.com/sip"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S sip",
      "reference": "https://command-not-found.com/sip"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install sip-devel",
      "reference": "https://command-not-found.com/sip"
    },
    "centos/yum": {
      "cmd": "sudo yum install sip-devel",
      "reference": "https://command-not-found.com/sip"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install sip",
      "reference": "https://command-not-found.com/sip"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install sip-devel",
      "reference": "https://command-not-found.com/sip"
    }
  }
}
