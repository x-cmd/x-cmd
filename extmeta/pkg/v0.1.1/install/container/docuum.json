{
  "lang": "rust",
  "homepage": "https://github.com/stepchowfun/docuum",
  "desc": {
    "cn": "对于持续集成(CI)工作人员、开发人员工作站或任何其他 Docker 映像随时间累积在磁盘上的环境等用例来说，Docum 是理想的",
    "en": "Docuum is ideal for use cases such as continuous integration (CI) workers, developer workstations, or any other environment in which Docker images accumulate on disk over time"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use docuum",
      "reference": "https://github.com/bradym/asdf-docuum"
    },
    "darwin/brew": {
      "cmd": "brew install docuum",
      "reference": "https://github.com/stepchowfun/docuum?tab=readme-ov-file#installation-with-homebrew"
    },
    "/curl": {
      "cmd": "curl https://raw.githubusercontent.com/stepchowfun/docuum/main/install.sh -LSfs | sh",
      "reference": "https://github.com/stepchowfun/docuum?tab=readme-ov-file#installation-on-macos-or-linux-aarch64-or-x86-64"
    },
    "/cargo": {
      "cmd": "cargo install docuum",
      "reference": "https://github.com/stepchowfun/docuum?tab=readme-ov-file#installation-with-cargo"
    }
  }
}
