{
  "lang": "go",
  "homepage": "https://github.com/ayn2op/discordo",
  "desc": {
    "cn": "Discordo 是一个轻量级的、安全的、功能丰富的 Discord 终端客户端",
    "en": "Discordo is a lightweight, secure, and feature-rich Discord terminal client. Heavily work-in-progress, expect breaking changes"
  },
  "rule": {
    "arch/yay": {
      "cmd": "sudo yay -S discordo-git",
      "reference": "https://github.com/ayn2op/discordo?tab=readme-ov-file#package-managers"
    },
    "freebsd/pkg": {
      "cmd": "pkg install discordo",
      "reference": "https://github.com/ayn2op/discordo?tab=readme-ov-file#package-managers"
    },
    "nix/": {
      "cmd": "nix-shell -p discordo",
      "reference": "https://github.com/ayn2op/discordo?tab=readme-ov-file#package-managers"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop bucket add vvxrtues https://github.com/vvirtues/bucket && \\\nx scoop install discordo\n",
      "reference": "https://github.com/ayn2op/discordo?tab=readme-ov-file#package-managers"
    },
    "darwin/brew": {
      "cmd": "brew install --cask discord",
      "reference": "https://formulae.brew.sh/cask/discord#default"
    }
  }
}
