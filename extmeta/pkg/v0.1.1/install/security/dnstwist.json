{
  "lang": "python",
  "homepage": "https://github.com/elceef/dnstwist",
  "desc": {
    "cn": "用于检测同形异义网络钓鱼攻击、拼写错误抢注和品牌假冒的域名排列引擎",
    "en": "Domain name permutation engine for detecting homograph phishing attacks, typo squatting, and brand impersonation"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install dnstwist",
      "reference": "https://github.com/elceef/dnstwist#installation"
    },
    "darwin/brew": {
      "cmd": "brew install dnstwist",
      "reference": "https://formulae.brew.sh/formula/dnstwist#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install dnstwist",
      "reference": "https://github.com/elceef/dnstwist#installation"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install dnstwist",
      "reference": "https://github.com/elceef/dnstwist#installation"
    },
    "kali/apt": {
      "cmd": "sudo apt install dnstwist",
      "reference": "https://github.com/elceef/dnstwist#installation"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install dnstwist",
      "reference": "https://github.com/elceef/dnstwist#installation"
    }
  }
}
