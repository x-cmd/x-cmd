{
  "lang": "Rust",
  "homepage": "https://github.com/GyulyVGC/sniffnet",
  "desc": {
    "cn": "用于轻松监控您的互联网流量的应用程序",
    "en": "Application to comfortably monitor your Internet traffic"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install sniffnet --locked",
      "reference": "https://github.com/GyulyVGC/sniffnet?tab=readme-ov-file#download"
    },
    "darwin/brew": {
      "cmd": "brew install sniffnet",
      "reference": "https://github.com/GyulyVGC/sniffnet?tab=readme-ov-file#download"
    },
    "linux/pacman": {
      "cmd": "pacman -S sniffnet",
      "reference": "https://github.com/GyulyVGC/sniffnet?tab=readme-ov-file#download"
    },
    "linux/pkg": {
      "cmd": "pkg install sniffnet",
      "reference": "https://github.com/GyulyVGC/sniffnet?tab=readme-ov-file#download"
    }
  }
}
