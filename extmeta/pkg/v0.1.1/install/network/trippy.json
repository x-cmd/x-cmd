{
  "lang": "Rust",
  "homepage": "https://trippy.cli.rs/",
  "desc": {
    "cn": "Trippy 结合了 traceroute 和 ping 的功能，旨在协助分析网络问题",
    "en": "Trippy combines the functionality of traceroute and ping and is designed to assist with the analysis of networking issues"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install trippy",
      "reference": "https://trippy.cli.rs/#cargo"
    },
    "/snap": {
      "cmd": "sudo snap install trippy",
      "reference": "https://trippy.cli.rs/#snap-linux"
    },
    "darwin/brew": {
      "cmd": "brew install trippy",
      "reference": "https://trippy.cli.rs/#homebrew-macos"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install trippy",
      "reference": "https://trippy.cli.rs/#scoop-windows"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install trippy",
      "reference": "https://trippy.cli.rs/#winget-windows"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install trippy",
      "reference": "https://trippy.cli.rs/#netbsd"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add trippy",
      "reference": "https://trippy.cli.rs/#openbsd"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S trippy",
      "reference": "https://trippy.cli.rs/#pacman-arch-linux"
    },
    "nix/nix-env": {
      "cmd": "sudo nix-env -iA trippy",
      "reference": "https://trippy.cli.rs/#nix"
    }
  }
}
