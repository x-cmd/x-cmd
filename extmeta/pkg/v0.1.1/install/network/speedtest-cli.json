{
  "lang": "Python",
  "homepage": "https://github.com/sivel/speedtest-cli",
  "desc": {
    "cn": "使用 speedtest.net 测试互联网带宽的命令行界面",
    "en": "Command line interface for testing internet bandwidth using speedtest.net"
  },
  "rule": {
    "/pip": {
      "cmd": "pip install speedtest-cli",
      "reference": "https://speedtest-cli.readthedocs.io/en/latest/welcome.html#install-from-source"
    },
    "darwin/brew": {
      "cmd": "brew install speedtest-cli",
      "reference": "https://formulae.brew.sh/formula/speedtest-cli#default"
    },
    "debian/apt": {
      "cmd": "sudo apt install speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "alpine/apk": {
      "cmd": "sudo apk add speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install speedtest-cli",
      "reference": "https://command-not-found.com/speedtest-cli"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install speedtest-cli",
      "reference": "https://speedtest-cli.readthedocs.io/en/latest/welcome.html#chocolatey"
    },
    "/easy_install": {
      "cmd": "easy_install speedtest-cli",
      "reference": "https://github.com/sivel/speedtest-cli#installation"
    },
    "/wget": {
      "cmd": "wget -O speedtest-cli https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest.py\n&& chmod +x speedtest-cli\n",
      "reference": "https://github.com/sivel/speedtest-cli#just-download-like-the-way-it-used-to-be"
    },
    "/curl": {
      "cmd": "curl -Lo speedtest-cli https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest.py\n&& chmod +x speedtest-cli\n",
      "reference": "https://github.com/sivel/speedtest-cli#just-download-like-the-way-it-used-to-be"
    }
  }
}
