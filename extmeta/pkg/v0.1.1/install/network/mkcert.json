{
  "lang": "go",
  "homepage": "https://github.com/FiloSottile/mkcert",
  "desc": {
    "cn": "mkcert 是一个用于制作本地信任的开发证书的简单工具",
    "en": "mkcert is a simple tool for making locally-trusted development certificates"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use mkcert",
      "reference": "https://github.com/salasrod/asdf-mkcert"
    },
    "darwin/brew": {
      "cmd": "brew install mkcert",
      "reference": "https://github.com/FiloSottile/mkcert?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port selfupdate && sudo port mkcert",
      "reference": "https://github.com/FiloSottile/mkcert?tab=readme-ov-file#installation"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -Syu mkcert",
      "reference": "https://github.com/FiloSottile/mkcert?tab=readme-ov-file#installation"
    }
  }
}
