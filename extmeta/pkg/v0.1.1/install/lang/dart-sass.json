{
  "lang": "dart",
  "homepage": "https://sass-lang.com/dart-sass/",
  "desc": {
    "en": "Compile Sass to CSS",
    "cn": "将 Sass 编译为 CSS"
  },
  "rule": {
    "/npm": {
      "cmd": "npm install -g sass",
      "reference": "https://www.npmjs.com/package/sass"
    },
    "darwin/brew": {
      "cmd": "brew install node-sass",
      "reference": "https://formulae.brew.sh/formula/node-sass"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install sass",
      "reference": "https://scoop.sh/#/apps?q=sass"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install sass",
      "reference": "https://community.chocolatey.org/packages/sass"
    }
  }
}
