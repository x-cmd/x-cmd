{
  "lang": "rust",
  "homepage": "https://www.rust-lang.org/",
  "desc": {
    "cn": "一门赋予每个人构建可靠且高效软件能力的语言",
    "en": "A language empowering everyone to build reliable and efficient software"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use rust",
      "reference": "https://github.com/code-lever/asdf-rust"
    },
    "/curl": {
      "cmd": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh",
      "reference": "https://www.rust-lang.org/tools/install"
    },
    "darwin/brew": {
      "cmd": "brew install rust",
      "reference": "https://formulae.brew.sh/formula/rust#default"
    }
  }
}
