{
  "lang": "dart",
  "homepage": "https://dart.dev/",
  "desc": {
    "cn": "Dart 是一种客户端优化的语言，用于在任何平台上开发快速应用程序",
    "en": "Dart is a client-optimized language for developing fast apps on any platform"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install dart-sdk",
      "reference": "https://formulae.brew.sh/formula/dart-sdk#default"
    },
    "(win|wsl2)/choco": {
      "cmd": "x choco install dart-sdk",
      "reference": "https://dart.dev/get-dart#install"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install dart",
      "reference": "https://scoop.sh/#/apps?q=dart"
    }
  }
}
