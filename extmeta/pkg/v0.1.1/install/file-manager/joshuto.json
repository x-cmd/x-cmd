{
  "lang": "Rust",
  "homepage": "https://github.com/kamiyaa/joshuto",
  "desc": {
    "cn": "受 Ranger 启发的终端文件管理器",
    "en": "Terminal file manager inspired by ranger"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --git https://github.com/kamiyaa/joshuto.git --force",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#for-single-user-with-cargo"
    },
    "/curl": {
      "cmd": "bash <(curl -s https://raw.githubusercontent.com/kamiyaa/joshuto/master/utils/install.sh)",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#from-pre-compiled-binary"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf copr enable atim/joshuto -y && \\\nsudo dnf install joshuto\n",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#fedora-copr"
    },
    "arch/yay": {
      "cmd": "yay -S joshuto",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#arch-aur"
    },
    "arch/paru": {
      "cmd": "paru -S joshuto",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#arch-aur"
    },
    "gentoo/emerge": {
      "cmd": "sudo eselect repository enable gentoo-zh && \\\nsudo emerge -av app-misc/joshuto\n",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#gentoo-gentoo-zh"
    },
    "darwin/prot": {
      "cmd": "sudo port install joshuto",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#macos-macports"
    },
    "darwin/brew": {
      "cmd": "brew install joshuto",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#macoslinux-homebrew"
    },
    "linux/brew": {
      "cmd": "brew install joshuto",
      "reference": "https://github.com/kamiyaa/joshuto?tab=readme-ov-file#macoslinux-homebrew"
    }
  }
}
