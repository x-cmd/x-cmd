{
  "lang": "python",
  "desc": {
    "cn": "受 VIM 启发的用于控制台的文件管理器",
    "en": "A VIM-inspired filemanager for the console"
  },
  "homepage": "https://github.com/ranger/ranger",
  "rule": {
    "/pip": {
      "cmd": "pip install ranger-fm",
      "reference": "https://github.com/ranger/ranger?tab=readme-ov-file#installing"
    },
    "/pipx": {
      "cmd": "pipx run --spec ranger-fm ranger",
      "reference": "https://github.com/ranger/ranger?tab=readme-ov-file#installing"
    },
    "/build": {
      "cmd": "git clone https://github.com/ranger/ranger.git\n&& cd ranger\n&& sudo make install\n",
      "reference": "https://github.com/ranger/ranger?tab=readme-ov-file#installing"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "alpine/apk": {
      "cmd": "sudo apk add ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "centos/yum": {
      "cmd": "sudo yum install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "fedora/yum": {
      "cmd": "sudo yum install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "rhel/yum": {
      "cmd": "sudo yum install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "rocky/yum": {
      "cmd": "sudo yum install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "alma/yum": {
      "cmd": "sudo yum install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install ranger",
      "reference": "https://command-not-found.com/ranger"
    },
    "gentoo/emerge": {
      "cmd": "sudo emerge -a sys-apps/ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    },
    "opensuse/zypper": {
      "cmd": "sudo zypper install ranger",
      "reference": "https://www.tecmint.com/linux-terminal-file-managers/"
    }
  }
}
