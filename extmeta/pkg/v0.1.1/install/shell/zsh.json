{
  "lang": "C",
  "homepage": "https://github.com/romkatv/zsh-bin",
  "desc": {
    "cn": "一款可用作交互式登录的 shell 及脚本编写的命令解释器",
    "en": "The Z shell (Zsh) is a Unix shell that can be used as an interactive login shell and as a command interpreter for shell scripting"
  },
  "rule": {
    "/curl": {
      "cmd": "sh -c \"$(curl -fsSL https://raw.githubusercontent.com/romkatv/zsh-bin/master/install)\"",
      "reference": "https://github.com/romkatv/zsh-bin?tab=readme-ov-file#installation"
    },
    "/wget": {
      "cmd": "sh -c \"$(wget -O- https://raw.githubusercontent.com/romkatv/zsh-bin/master/install)\"",
      "reference": "https://github.com/romkatv/zsh-bin?tab=readme-ov-file#installation"
    },
    "darwin/brew": {
      "cmd": "brew install zsh",
      "reference": "https://formulae.brew.sh/formula/zsh#default"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "alpine/apk": {
      "cmd": "sudo apk add zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "centos/yum": {
      "cmd": "sudo yum install zsh",
      "reference": "https://command-not-found.com/zsh"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install zsh",
      "reference": "https://command-not-found.com/zsh"
    }
  }
}
