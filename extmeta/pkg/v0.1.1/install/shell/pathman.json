{
  "lang": "Go",
  "homepage": "https://git.rootprojects.org/root/pathman",
  "desc": {
    "cn": "PATH 管理工具，用于管理 Windows 10、Mac 和 Linux 上的 PATH",
    "en": "PATH management tool for managing PATH on Windows 10, Mac and Linux"
  },
  "rule": {
    "darwin/curl": {
      "cmd": "curl -s https://webinstall.dev/pathman | bash",
      "reference": "https://git.rootprojects.org/root/pathman#install"
    },
    "linux/curl": {
      "cmd": "curl -s https://webinstall.dev/pathman | bash",
      "reference": "https://git.rootprojects.org/root/pathman#install"
    },
    "win/curl": {
      "cmd": "curl.exe -sA \"MS\" https://webinstall.dev/pathman | powershell",
      "reference": "https://git.rootprojects.org/root/pathman#install"
    },
    "/npm": {
      "cmd": "npm install -g pathman",
      "reference": "https://git.rootprojects.org/root/pathman#manual-install"
    }
  }
}
