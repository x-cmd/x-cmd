{
  "lang": "Rust",
  "homepage": "https://github.com/konradsz/igrep",
  "desc": {
    "cn": "交互式 Grep",
    "en": "Interactive Grep"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap konradsz/igrep https://github.com/konradsz/igrep.git && \\\nbrew install igrep\n",
      "reference": "https://github.com/konradsz/igrep?tab=readme-ov-file#homebrew"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install igrep",
      "reference": "https://github.com/konradsz/igrep?tab=readme-ov-file#scoop"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S igrep",
      "reference": "https://github.com/konradsz/igrep?tab=readme-ov-file#arch-linux"
    },
    "alpine/apk": {
      "cmd": "apk add igrep",
      "reference": "https://github.com/konradsz/igrep?tab=readme-ov-file#alpine-linux"
    }
  }
}
