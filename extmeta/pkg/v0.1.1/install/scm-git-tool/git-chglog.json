{
  "lang": "Go",
  "homepage": "https://github.com/git-chglog/git-chglog",
  "desc": {
    "cn": "用 Go (Golang) 实现的 CHANGELOG 生成器",
    "en": "CHANGELOG generator implemented in Go (Golang)"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew tap git-chglog/git-chglog \\\n&& brew install git-chglog\n",
      "reference": "https://github.com/git-chglog/git-chglog?tab=readme-ov-file#homebrew-for-macos-users"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install git-chglog",
      "reference": "https://github.com/git-chglog/git-chglog?tab=readme-ov-file#scoop-for-windows-users"
    },
    "/asdf": {
      "cmd": "x asdf use git-chglog",
      "reference": "https://github.com/git-chglog/git-chglog?tab=readme-ov-file#asdf"
    },
    "/go": {
      "cmd": "go install github.com/git-chglog/git-chglog/cmd/git-chglog@latest",
      "reference": "https://github.com/git-chglog/git-chglog?tab=readme-ov-file#go-users"
    }
  }
}
