{
  "lang": "Rust",
  "homepage": "https://github.com/Byron/gitoxide",
  "desc": {
    "cn": "一个惯用、精简、快速和安全的纯 Rust 实现的 Git",
    "en": "An idiomatic, lean, fast & safe pure Rust implementation of Git"
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S gitoxide",
      "reference": "https://github.com/Byron/gitoxide?tab=readme-ov-file#download-from-arch-repository"
    },
    "/cargo": {
      "cmd": "cargo install gitoxide",
      "reference": "https://github.com/Byron/gitoxide?tab=readme-ov-file#from-source-via-cargo"
    }
  }
}
