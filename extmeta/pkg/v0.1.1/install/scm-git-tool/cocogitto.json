{
  "lang": "rust",
  "homepage": "https://docs.cocogitto.io/",
  "desc": {
    "cn": "Cocogitto 是用于常规 Commit 和 Semver 规范的 CLI 和 GitOps 工具箱",
    "en": "Cocogitto is a CLI and GitOps toolbox for the Conventional Commits and Semver specifications"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install cocogitto",
      "reference": "https://github.com/cocogitto/cocogitto?tab=readme-ov-file#macos"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S cocogitto",
      "reference": "https://github.com/eza-community/eza/blob/main/INSTALL.md#arch-linux"
    },
    "nix/": {
      "cmd": "nix-env -iA cocogitto",
      "reference": "https://github.com/cocogitto/cocogitto?tab=readme-ov-file#nixos"
    },
    "void/xbps": {
      "cmd": "sudo xbps-install cocogitto",
      "reference": "https://github.com/cocogitto/cocogitto?tab=readme-ov-file#void-linux"
    },
    "/cargo": {
      "cmd": "cargo install --locked cocogitto",
      "reference": "https://github.com/cocogitto/cocogitto?tab=readme-ov-file#cargo"
    }
  }
}
