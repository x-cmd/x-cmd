{
  "lang": "rust",
  "homepage": "https://amrdeveloper.github.io/GQL/",
  "desc": {
    "cn": "Git Query 语言是一种类似于语言，可以在.git文件上执行查询，并支持大多数SQL功能，例如分组，订购和聚合功能",
    "en": "Git Query language is a SQL like language to perform queries on .git files with supports of most of SQL features such as grouping, ordering and aggregations functions"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install gitql",
      "reference": "https://github.com/AmrDeveloper/GQL/blob/master/docs/setup.md#cargoio"
    },
    "darwin/brew": {
      "cmd": "brew install gitql",
      "reference": "https://formulae.brew.sh/formula/gitql#default"
    },
    "(win|wsl2)/winget": {
      "cmd": "x winget install gitql",
      "reference": "https://github.com/AmrDeveloper/GQL/blob/master/docs/setup.md#winget-on-windows"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install gitql",
      "reference": "https://github.com/AmrDeveloper/GQL/blob/master/docs/setup.md#scoop-on-windows"
    }
  }
}
