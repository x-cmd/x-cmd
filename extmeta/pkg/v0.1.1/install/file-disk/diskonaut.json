{
  "lang": "rust",
  "homepage": "https://github.com/imsnif/diskonaut",
  "desc": {
    "cn": "终端磁盘空间导航器",
    "en": "Terminal disk space navigator"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install diskonaut",
      "reference": "https://github.com/imsnif/diskonaut?tab=readme-ov-file#with-cargo-linux--macos--windows"
    },
    "darwin/brew": {
      "cmd": "brew install diskonaut",
      "reference": "https://github.com/imsnif/diskonaut?tab=readme-ov-file#macos"
    },
    "nix/": {
      "cmd": "nix-env --install diskonaut",
      "reference": "https://github.com/hisbaan/didyoumean?tab=readme-ov-file#nixos"
    },
    "centos/dnf": {
      "cmd": "sudo dnf install diskonaut",
      "reference": "https://github.com/imsnif/diskonaut?tab=readme-ov-file#fedoracentos"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install diskonaut",
      "reference": "https://github.com/imsnif/diskonaut?tab=readme-ov-file#fedoracentos"
    },
    "arch/yay": {
      "cmd": "sudo yay diskonaut",
      "reference": "https://github.com/imsnif/diskonaut?tab=readme-ov-file#arch-linux"
    }
  }
}
