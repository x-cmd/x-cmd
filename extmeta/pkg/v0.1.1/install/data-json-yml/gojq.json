{
  "lang": "Go",
  "homepage": "https://github.com/itchyny/gojq",
  "desc": {
    "cn": "纯用 Go 实现的 jq",
    "en": "Pure Go implementation of jq"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install gojq",
      "reference": "https://github.com/itchyny/gojq?tab=readme-ov-file#homebrew"
    },
    "/0install": {
      "cmd": "0install add gojq https://apps.0install.net/utils/gojq.xml",
      "reference": "https://github.com/itchyny/gojq?tab=readme-ov-file#zero-install"
    },
    "/go": {
      "cmd": "go install github.com/itchyny/gojq/cmd/gojq@latest",
      "reference": "https://github.com/itchyny/gojq?tab=readme-ov-file#build-from-source"
    },
    "/docker": {
      "cmd": "docker run -i --rm ghcr.io/itchyny/gojq",
      "reference": "https://github.com/itchyny/gojq?tab=readme-ov-file#docker"
    }
  }
}
