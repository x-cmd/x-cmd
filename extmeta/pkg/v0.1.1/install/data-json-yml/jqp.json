{
  "lang": "go",
  "homepage": "https://github.com/noahgorstein/jqp",
  "desc": {
    "cn": "用于试验 jq 的 TUI",
    "en": "a TUI playground for exploring jq"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use jqp",
      "reference": "https://gitlab.com/wt0f/asdf-jqp"
    },
    "darwin/brew": {
      "cmd": "brew install noahgorstein/tap/jqp",
      "reference": "https://github.com/noahgorstein/jqp?tab=readme-ov-file#installation"
    },
    "darwin/port": {
      "cmd": "sudo port install jqp",
      "reference": "https://github.com/noahgorstein/jqp?tab=readme-ov-file#installation"
    },
    "arch/yay": {
      "cmd": "sudo yay -S jqp-bin",
      "reference": "https://github.com/noahgorstein/jqp?tab=readme-ov-file#installation"
    }
  }
}
