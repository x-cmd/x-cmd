{
  "lang": "Rust",
  "homepage": "https://github.com/ynqa/jnv",
  "desc": {
    "cn": "jnv 专为浏览 JSON 而设计，提供了一个交互式的 JSON 查看器和 jq 过滤器编辑器",
    "en": "jnv is designed for navigating JSON, offering an interactive JSON viewer and jq filter editor"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install ynqa/tap/jnv",
      "reference": "https://github.com/ynqa/jnv?tab=readme-ov-file#homebrew"
    },
    "darwin/prot": {
      "cmd": "sudo port install jnv",
      "reference": "https://github.com/ynqa/jnv?tab=readme-ov-file#macports"
    },
    "/cargo": {
      "cmd": "cargo install jnv",
      "reference": "https://github.com/ynqa/jnv?tab=readme-ov-file#cargo"
    }
  }
}
