{
  "lang": "shell",
  "homepage": "https://github.com/processone/iconv",
  "desc": {
    "cn": "Erlang / Elixir 的快速编码转换库",
    "en": "Fast encoding conversion library for Erlang / Elixir"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install plan9port",
      "reference": "https://command-not-found.com/iconv"
    },
    "debian/apt": {
      "cmd": "sudo apt-get install libc-bin",
      "reference": "https://command-not-found.com/iconv"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt-get install libc-bin",
      "reference": "https://command-not-found.com/iconv"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S plan9port",
      "reference": "https://command-not-found.com/iconv"
    },
    "kali/apt": {
      "cmd": "sudo apt-get install libc-bin",
      "reference": "https://command-not-found.com/iconv"
    },
    "centos/yum": {
      "cmd": "sudo yum install glibc-common",
      "reference": "https://command-not-found.com/iconv"
    },
    "fedora/yum": {
      "cmd": "sudo dnf install glibc-common",
      "reference": "https://command-not-found.com/iconv"
    },
    "raspbian/apt": {
      "cmd": "sudo apt-get install libc-bin",
      "reference": "https://command-not-found.com/iconv"
    }
  }
}
