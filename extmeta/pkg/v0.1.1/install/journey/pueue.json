{
  "lang": "Rust",
  "homepage": "https://github.com/Nukesor/pueue",
  "desc": {
    "cn": "Pueue 是一个命令行任务管理工具，用于顺序执行和并行执行长时间运行的任务",
    "en": "Pueue is a command-line task management tool for sequential and parallel execution of long-running tasks"
  },
  "rule": {
    "/cargo": {
      "cmd": "cargo install --locked pueue",
      "reference": "https://github.com/Nukesor/pueue?tab=readme-ov-file#via-cargo"
    },
    "darwin/brew": {
      "cmd": "brew install pueue",
      "reference": "https://formulae.brew.sh/formula/pueue#default"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install pueue",
      "reference": "https://scoop.sh/#/apps?q=pueue"
    }
  }
}
