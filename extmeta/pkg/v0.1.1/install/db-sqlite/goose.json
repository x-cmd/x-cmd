{
  "lang": "Go",
  "homepage": "https://pressly.github.io/goose/",
  "desc": {
    "cn": "goose 是一个数据库迁移工具。通过创建增量 SQL 更改和/或 Go 函数来管理数据库架构",
    "en": "goose is a database migration tool. Manage your database schema by creating incremental SQL changes and/or Go functions"
  },
  "rule": {
    "darwin/brew": {
      "cmd": "brew install goose",
      "reference": "https://pressly.github.io/goose/installation/"
    },
    "liux/curl": {
      "cmd": "curl -fsSL https://raw.githubusercontent.com/pressly/goose/master/install.sh | sh",
      "reference": "https://pressly.github.io/goose/installation/"
    },
    "/go": {
      "cmd": "go install github.com/pressly/goose/v3/cmd/goose@latest",
      "reference": "https://pressly.github.io/goose/installation/"
    }
  }
}
