{
  "lang": "go",
  "homepage": "https://cloud.google.com/",
  "desc": {
    "cn": "Google Cloud 命令行工具，用于管理 Google Cloud 上托管的资源和应用",
    "en": "Google Cloud command-line tools to manage resources and apps hosted on Google Cloud"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use gcloud",
      "reference": "https://github.com/jthegedus/asdf-gcloud"
    },
    "darwin/brew": {
      "cmd": "brew install --cask google-cloud-sdk",
      "reference": "https://formulae.brew.sh/cask/google-cloud-sdk#default"
    },
    "ubuntu/snap": {
      "cmd": "sudo snap install google-cloud-cli --classic",
      "reference": "https://cloud.google.com/sdk/docs/downloads-snap"
    },
    "win/powershell": {
      "cmd": "x pwsh (New-Object Net.WebClient).DownloadFile(\"https://dl.google.com/dl/cloudsdk/channels/rapid/GoogleCloudSDKInstaller.exe\", \"$env:Temp\\GoogleCloudSDKInstaller.exe\")\n& $env:Temp\\GoogleCloudSDKInstaller.exe\n",
      "reference": "https://cloud.google.com/sdk/docs/install#windows"
    }
  }
}
