{
  "lang": "go",
  "homepage": "https://github.com/vultr/vultr-cli",
  "desc": {
    "cn": "Vultr 服务的官方命令行工具",
    "en": "Official command line tool for Vultr services"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use vultr-cli",
      "reference": "https://github.com/ikuradon/asdf-vultr-cli"
    },
    "darwin/brew": {
      "cmd": "brew install vultr/vultr-cli/vultr-cli",
      "reference": "https://formulae.brew.sh/formula/vultr#default"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S vultr-cli",
      "reference": "https://github.com/vultr/vultr-cli?tab=readme-ov-file#installing-on-arch-linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install vultr-cli",
      "reference": "https://github.com/vultr/vultr-cli?tab=readme-ov-file#installing-on-fedora"
    },
    "openbsd/pkg_add": {
      "cmd": "pkg_add vultr-cli",
      "reference": "https://github.com/vultr/vultr-cli?tab=readme-ov-file#installing-on-openbsd"
    }
  }
}
