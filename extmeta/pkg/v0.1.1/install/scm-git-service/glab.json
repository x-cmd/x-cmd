{
  "lang": "Go",
  "homepage": "https://gitlab.com/gitlab-org/cli",
  "desc": {
    "cn": "GitLab CLI工具将GitLab带到您的命令行",
    "en": "A GitLab CLI tool bringing GitLab to your command line"
  },
  "rule": {
    "/asdf": {
      "cmd": "x asdf use glab",
      "reference": "https://github.com/particledecay/asdf-glab"
    },
    "darwin/brew": {
      "cmd": "brew install glab",
      "reference": "https://gitlab.com/gitlab-org/cli#homebrew"
    },
    "darwin/port": {
      "cmd": "sudo port install glab",
      "reference": "https://ports.macports.org/port/glab/details/"
    },
    "/curl": {
      "cmd": "curl -s \"https://gitlab.com/gitlab-org/cli/-/raw/main/scripts/install.sh\" | sudo sh",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md"
    },
    "(win|wsl2)/scoop": {
      "cmd": "x scoop install glab",
      "reference": "https://scoop.sh/#/apps?q=glab"
    },
    "(win|wsl2)/winget": {
      "cmd": "x choco install glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#windows"
    },
    "(win|wsl2)/choco": {
      "cmd": "x winget install glab.glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#windows"
    },
    "arch/pacman": {
      "cmd": "sudo pacman -S glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#arch-linux"
    },
    "alpine/apk": {
      "cmd": "sudo apk add --no-cache glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#alpine-linux"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#fedora"
    },
    "nix/": {
      "cmd": "nix-env -iA nixos.glab",
      "reference": "https://gitlab.com/gitlab-org/cli/-/blob/main/docs/installation_options.md#nixnixos"
    }
  }
}
