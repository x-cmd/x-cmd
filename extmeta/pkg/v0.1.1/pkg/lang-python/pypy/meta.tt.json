{	"homepage"	:	"https://www.pypy.org/"	,	"license"	:	"MIT"	,	"source"	:	{	"official"	:	[	"https://downloads.python.org/pypy/"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"tar.bz2"	,	"unpack"	:	"folder"	,	"url"	:	{	"internet"	:	"https://downloads.python.org/pypy/%{_version}-%{_os}.%{download_file_ext}"	,	"cn"	:	"https://downloads.python.org/pypy/%{_version}-%{_os}.%{download_file_ext}"	}	,	"sociality"	:	"intrusive"	,	"hook"	:	{	"populate"	:	"populate.sh"	,	"boot"	:	"boot.sh"	,	"unboot"	:	"unboot.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"env"	:	{	"path"	:	{	"append"	:	[	"%{HOME}/.local/bin"	]	,	"unshift"	:	[	"%{___X_CMD_PKG___META_TGT}/bin"	]	}	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v3.10.0+v7.3.15"	}	,	"latest/*"	:	{	"version"	:	"v3.10.0+v7.3.15"	}	,	"v3.10.0[+]v7.3.15/*/*"	:	{	"_version"	:	"pypy3.10-v7.3.15"	}	,	"v3.9.0[+]v7.3.15/*/*"	:	{	"_version"	:	"pypy3.9-v7.3.15"	}	,	"v3.8.0[+]v7.3.11/*/*"	:	{	"_version"	:	"pypy3.8-v7.3.11"	}	,	"v3.7.0[+]v7.3.9/*/*"	:	{	"_version"	:	"pypy3.7-v7.3.9"	}	,	"v3.6.0[+]v7.3.3/*/*"	:	{	"_version"	:	"pypy3.6-v7.3.3"	}	,	"v3.5.0[+]v7.0.0/*/*"	:	{	"_version"	:	"pypy3.5-v7.0.0"	}	,	"*/darwin/x64"	:	{	"_os"	:	"macos_x86_64"	}	,	"*/darwin/arm64"	:	{	"_os"	:	"macos_arm64"	}	,	"*/linux/x64"	:	{	"_os"	:	"linux64"	}	,	"*/linux/arm64"	:	{	"_os"	:	"aarch64"	}	,	"*/win/x64"	:	{	"_os"	:	"win64"	,	"bin_suffix"	:	".exe"	,	"download_file_ext"	:	"zip"	,	"env"	:	{	"path"	:	{	"append"	:	[	"%{HOME}/.local/bin"	]	,	"unshift"	:	[	"%{___X_CMD_PKG___META_TGT}/Scripts"	]	}	}	}	}	}	