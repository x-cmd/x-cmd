{	"homepage"	:	"https://spark.apache.org/"	,	"license"	:	"Apache License"	,	"source"	:	{	"repack-thirdparty"	:	[	"https://dlcdn.apache.org/spark"	]	}	,	"_os"	:	"%{os}"	,	"_arch"	:	"%{arch}"	,	"download_file_ext"	:	"tgz"	,	"url"	:	{	"internet"	:	"https://dlcdn.apache.org/spark/spark-%{semantic_version}/spark-%{semantic_version}-bin-hadoop3.tgz"	,	"cn"	:	"https://dlcdn.apache.org/spark/spark-%{semantic_version}/spark-%{semantic_version}-bin-hadoop3.tgz"	}	,	"unpack"	:	"folder"	,	"hook"	:	{	"populate"	:	"populate.sh"	}	,	"path"	:	{	"bin"	:	"shim_bin"	}	,	"_build"	:	"0"	,	"tree"	:	"%{os}.%{arch}.%{_build}"	,	"rule"	:	{	"/*"	:	{	"version"	:	"stable"	}	,	"stable/*"	:	{	"version"	:	"v3.5.0"	}	,	"latest/*"	:	{	"version"	:	"v3.4.1"	}	,	"*/linux/arm64"	:	null	,	"*/darwin/arm64"	:	null	,	"*/linux/x64"	:	null	,	"*/darwin/x64"	:	null	}	}	