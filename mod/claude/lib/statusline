# shellcheck shell=dash

___x_cmd_claude_statusline(){
    [ $# -gt 0 ] || set -- --help
    local X_help_cmd=;  X_help_cmd='x help -m claude statusline' help:arg:parse
    local op="$1"
    case "$op" in
        setup)
            shift; ___x_cmd_claude_statusline_"$op" "$@" ;;
        *)  N=claude M="Not support such subcmd ==> $op" log:ret:64 ;;
    esac
}

___x_cmd_claude_statusline_setup(){
    local X_help_cmd=;  X_help_cmd='x help -m claude statusline setup' help:arg:parse
    ___x_cmd theme statusline setup "$@"
}
