# shellcheck shell=dash

xrc:mod:lib     claude      usage/daily  usage/monthly  usage/projects  usage/util

___x_cmd_claude_usage(){
    [ "$#" -gt 0 ] ||   set -- daily

    local op="$1";      shift
    case "$op" in
        daily|monthly|projects)
                            ___x_cmd_claude_usage_"$op"   "$@" ;;
        -h|--help)          ___x_cmd help -m claude usage "$@" ;       return 0 ;;
        *)                  N=claude M="usage not such [subcmd=$op]" log:ret:64 ;;
    esac
}
