
___x_cmd_claude_router___runmain(){
    ___x_cmd_claude_router___runcmd "$@"
}

___x_cmd_claude_router___runcmd(){
    if ___x_cmd_hascmd ccr; then
        ___x_cmd_claude_router___runcmd(){
            ___x_cmd_cmds ccr "$@"
        }
    else
        ___x_cmd_claude_router___runcmd(){
            ___x_cmd snap --run ccr "$@"
        }
    fi && ___x_cmd_claude_router___runcmd "$@"
}

