
xrc:mod:lib     claude  mcp/use

___x_cmd_claude_mcp(){
    [ $# -gt 0 ] ||     set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m claude mcp         "$@" ;;

        use|unuse)      ___x_cmd_claude_mcp_"$op"           "$@" ;;
        *)              ___x_cmd_claude___runcmd    "$op"   "$@" ;;
    esac
}

___x_cmd_claude_mcp_unuse(){
    :
}
