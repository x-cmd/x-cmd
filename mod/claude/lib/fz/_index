
___x_cmd_claude_fz(){
    [ "$#" -gt 0 ] ||       set -- --help

    while [ $# -gt 0 ]; do
        case "$1" in
            -h|--help)      ___x_cmd help -m claude fz "$@"; return 0 ;;

            *)              break ;;
        esac
    done
}

___x_cmd_claude_fz___folder(){
    :
}

___x_cmd_claude_fz___history(){
    :
}

___x_cmd_claude_fz___flat(){
    :
}
