
___x_cmd_agent___runmain(){
    ___x_cmd_agent___runcmd "$@"
}

___x_cmd_agent___runcmd(){
    if ___x_cmd_hascmd agent; then
        ___x_cmd_agent___runcmd(){  ___x_cmd_cmds agent "$@"; }
    else
        ___x_cmd_agent___install || return
        ___x_cmd_agent___runcmd(){ "$___X_CMD_agent_BIN_PATH/$___X_CMD_agent_BIN" "$@" ; }
    fi || return
    ___x_cmd_agent___runcmd "$@"
}
