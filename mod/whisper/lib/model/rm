
___x_cmd_whisper_model_rm(){
    local modelName="${1}"
    local x_=""
    [ -n "$modelName" ] || {
        ___x_cmd_whisper___dictate___decidemodel_ || return
        modelName="${x_}"
    }
    local modelDir="$___X_CMD_ROOT_DATA/model/${modelName}.bin"
    [ -f "${modelDir}" ]    || N=whisper M="${modelName} model does not exist locally" log:ret:1
    x rm "${modelDir}"
}

