
___x_cmd_ccal___runmain(){
    ___x_cmd_ccal___runcmd "$@"
}

___x_cmd_ccal___runcmd(){
    if ___x_cmd_hascmd ccal; then
        ___x_cmd_ccal___runcmd(){  ___x_cmd_cmds ccal "$@"; }
    else
        ___x_cmd_ccal___install || return
        ___x_cmd_ccal___runcmd(){ "$___X_CMD_ccal_BIN_PATH/$___X_CMD_ccal_BIN" "$@" ; }
    fi || return
    ___x_cmd_ccal___runcmd "$@"
}
