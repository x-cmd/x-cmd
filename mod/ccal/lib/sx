
___x_cmd_ccal_sx(){
    case "$1" in
        -h|--help)  ___x_cmd help -m ccal sx "$@"; return 0 ;;
    esac

    local IFS=' '
    ___x_cmd ccal lunar cat -a | {
        DATA="$*" ___x_cmd_cmds awk -v FS='\t' \
            -f "$___X_CMD_ROOT_MOD/ccal/lib/awk/lunar.awk"  \
            -f "$___X_CMD_ROOT_MOD/ccal/lib/awk/sx.awk"
    }
}

