# shellcheck shell=dash

___x_cmd log init crush
xrc:mod:lib     crush   __runcmd

___x_cmd_crush___main(){
    [ "$#" -gt 0 ] ||   set -- --runmain

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m crush            "$@" ;      return 0 ;;

        --|--runmain)   ___x_cmd_crush___runmain        "$@" ;;
        --runcmd)       ___x_cmd_crush___runcmd         "$@" ;;
        *)              ___x_cmd_crush___runmain "$op"  "$@" ;;
    esac
}
