xrc:mod:lib     tap             main

___x_cmd_tap(){
    case "$1" in
        -h|--help)
            x advise gen_help "$___X_CMD_ROOT_MOD/tap/res/advise.jso" 1>&2
            return 1 ;;
        *)                             ___x_cmd_tap___main "$@"
    esac
}

xrc setmain ___x_cmd_tap
