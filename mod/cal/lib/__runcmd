
___x_cmd_cal___runmain(){
    ___x_cmd_cal___runcmd "$@"
}

___x_cmd_cal___runcmd(){
    if ___x_cmd_hascmd cal; then
        ___x_cmd_cal___runcmd(){  ___x_cmd_cmds cal "$@"; }
    else
        ___x_cmd_cal___install || return
        ___x_cmd_cal___runcmd(){ "$___X_CMD_cal_BIN_PATH/$___X_CMD_cal_BIN" "$@" ; }
    fi || return
    ___x_cmd_cal___runcmd "$@"
}
