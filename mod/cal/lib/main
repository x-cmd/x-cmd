# shellcheck shell=dash

___x_cmd log init cal
xrc:mod:lib     cal   __runcmd zodiac

___x_cmd_cal___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m cal "$@";    return 0 ;;

        z|zodiac)       ___x_cmd_cal_zodiac "$@" ;;

        *)              N=cal M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}
