# shellcheck shell=dash

xrc:mod:lib     cal   __runcmd

___x_cmd_cal___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m cal "$@";    return 0 ;;

        *)              N=cal M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}
