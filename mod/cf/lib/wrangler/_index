xrc:mod:lib cf     wrangler/__runcmd

___x_cmd_cf_wrangler(){
    [ "$#" -gt 0 ] ||   set -- --runmain

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m cf wrangler "$@";    return 0 ;;

        --|--runmain)   ___x_cmd_cf_wrangler___runmain       "$@" ;;
        --runcmd)       ___x_cmd_cf_wrangler___runcmd        "$@" ;;
        *)              ___x_cmd_cf_wrangler___runmain "$op" "$@" ;;
    esac
}
