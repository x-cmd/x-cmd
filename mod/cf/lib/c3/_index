xrc:mod:lib cf     c3/__runcmd

___x_cmd_cf_c3(){
    [ "$#" -gt 0 ] ||   set -- --runmain

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m cf "$@";    return 0 ;;

        --|--runmain)   ___x_cmd_cf_c3___runmain       "$@" ;;
        --runcmd)       ___x_cmd_cf_c3___runcmd        "$@" ;;
        *)              ___x_cmd_cf_c3___runmain "$op" "$@" ;;
    esac
}
