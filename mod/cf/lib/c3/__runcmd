___x_cmd_cf_c3___runmain(){
    ___x_cmd_cf_c3___runcmd "$@"
}

___x_cmd_cf_c3___runcmd(){
    if ___x_cmd_hascmd create-cloudflare; then
        ___x_cmd_cf_c3___runcmd(){
            ___x_cmd_cmds create-cloudflare "$@"
        }
    else
        ___x_cmd env use create-cloudflare || return $?
        ___x_cmd_cf_c3___runcmd(){
            ___x_cmd snap --run create-cloudflare "$@"
        }
    fi && ___x_cmd_cf_c3___runcmd "$@"
}
