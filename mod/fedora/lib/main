# shellcheck shell=dash

xrc:mod:lib     fedora   __runcmd

___x_cmd_fedora___main(){
    [ "$#" -gt 0 ] ||   set -- bash

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m fedora "$@";    return 0 ;;

        --|snap)        ___x_cmd_fedora_snap            "$@" ;;
        *)              ___x_cmd_fedora_snap "$op"      "$@" ;;
    esac
}


___x_cmd_fedora_snap(){
    ___x_cmd docker snap --imagetype fedora "$@"
}

