
___x_cmd_fedora___runmain(){
    ___x_cmd_fedora___runcmd "$@"
}

___x_cmd_fedora___runcmd(){
    if ___x_cmd_hascmd fedora; then
        ___x_cmd_fedora___runcmd(){  ___x_cmd_cmds fedora "$@"; }
    else
        ___x_cmd_fedora___install || return
        ___x_cmd_fedora___runcmd(){ "$___X_CMD_fedora_BIN_PATH/$___X_CMD_fedora_BIN" "$@" ; }
    fi || return
    ___x_cmd_fedora___runcmd "$@"
}
