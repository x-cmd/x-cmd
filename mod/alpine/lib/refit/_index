# shellcheck shell=dash


# xal refit --apk 'openjdk21 python3' python3

___x_cmd_alpine_refit(){
    local x_=""
    ___x_cmd alpine image prepare_
    case "$?" in
        130)        return 130 ;;
    esac
    ___x_cmd docker refit --from "$x_" "$@"
}

