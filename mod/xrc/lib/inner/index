# shellcheck shell=dash

___X_CMD_INNER_PATH="$PATH"

if [ -n "$ZSH_VERSION" ]; then

___x_cmd_inner_cd(){
    builtin cd "$@" || return
}

else

___x_cmd_inner_cd(){
    command cd "$@" || return
}

fi

___x_cmd_inner_mv(){
    PATH="$___X_CMD_INNER_PATH" command mv "$@"
}

___x_cmd_inner_awk(){
    PATH="$___X_CMD_INNER_PATH" command awk "$@"
}
