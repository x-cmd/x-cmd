# shellcheck shell=dash

___x_cmd log init minimax

___X_CMD_MINIMAX_DEFAULT_ENDPOINT="https://api.minimaxi.com/anthropic"
___X_CMD_MINIMAX_DEFAULT_FIRST_MODEL="MiniMax-M2.5"

xrc:mod:lib     minimax   cfg   cpstatus

___x_cmd_minimax___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m minimax    "$@";    return 0 ;;

        cfg|--cfg)      ___x_cmd_minimax_cfg        "$@" ;;
        --cur)          ___x_cmd_minimax_cur        "$@" ;;

        cp|cpstatus)    ___x_cmd_minimax_cpstatus   "$@" ;;
        init|--init)    ___x_cmd_minimax_init       "$@" ;;

        *)              N=minimax M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}
