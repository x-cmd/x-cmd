
___x_cmd_gcal___runmain(){
    ___x_cmd_gcal___runcmd "$@"
}

___x_cmd_gcal___runcmd(){
    if ___x_cmd_hascmd gcal; then
        ___x_cmd_gcal___runcmd(){  ___x_cmd_cmds gcal "$@"; }
    else
        ___x_cmd_gcal___runcmd(){ ___x_cmd pixi exec gcal "$@" ; }
    fi || return $?
    ___x_cmd_gcal___runcmd "$@"
}
