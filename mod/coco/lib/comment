# shellcheck shell=dash

# x coco convert java python,c,cpp

___x_cmd_coco_comment(){
    local file_path="$1"
    [ -f "$file_path" ] ||  N=coco M="Please provide a valid [filepath=$file_path]" log:ret:64
    ___x_cmd_cmds_cat "$file_path" |  ___x_cmd_coco___request comment --pipe "FILE-CONTENT"
}

