# shellcheck shell=dash
# generate shell command

___x_cmd_coco_exec(){
    case "$1" in
        -h|--help) ___x_cmd help -m coco --exec; return 0 ;;
    esac

    local LOCALSHELL=
    local OS=

    local x_=""
    x os name_

    LOCALSHELL="$___X_CMD_SHELL" OS="$x_" ___x_cmd chat --send --type coco,shell --without-repl --minion "$___X_CMD_ROOT_MOD/coco/lib/minion/exec.yml" --no-empty-question "$@"
}

