
___x_cmd_coco_gg(){
    local question=""
    arg:init:x  coco
    while [ $# -gt 0 ]; do
        case "$1" in
            --question)     question="$2";       arg:2:shift ;;
            *)              break ;;
        esac
    done

    [ -n "$question" ] || N=coco M="Please provide the question" log:ret:64
    coco:info "Searching web using Gemini Google Search"
    ___x_cmd gemini gg --raw --no-source "$question"
}
