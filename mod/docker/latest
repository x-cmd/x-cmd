xrc:mod:lib     docker          main
[ -z "$ZSH_VERSION$BASH_VERSION" ] || xrc:mod:lib     docker     advise

___x_cmd_docker(){
    ___x_cmd_docker___main "$@"
}

xrc setmain ___x_cmd_docker
