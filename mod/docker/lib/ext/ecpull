
___x_cmd_docker_ecpull(){
    [ "$#" -gt 0 ]      || set - --help
    case "$1" in
        -h|--help)   ___x_cmd help -m docker ecpull;  return  0;;
    esac

    local image="$1"
    case "$image" in
        */*:*)      ;;
        *:*)        image="library/${image}" ;;
        *)          image="library/${image}:latest"
    esac

    ___x_cmd docker pull "public.ecr.aws/docker/${image#/}"
}

