
# x sb env try /jq/--no-net/--no-tty/ %yq%--no-net%--no-tty%
# x sb env try /yq/--argfile/:jq/

___x_cmd_sb_env_run(){
    [ $# -gt 0 ]    ||      set -- --help

    local XSB_RULE="${XSB_RULE}"
    while [ $# -gt 0 ]; do
        case "$1" in
            -0|--arg0)      XSB_RULE="" ;;
            --)             shift; break ;;
            *)              XSB_RULE="${XSB_RULE}${1}
" ;;

        esac
        shift
    done

    XSB_RULE="$XSB_RULE" ___x_cmd sb run "$@"
}
