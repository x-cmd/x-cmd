

xrc:mod:lib     mac     app/ps app/appid

___x_cmd_mac_app(){

    [ $# -gt 0 ]    ||  set -- ps

    local op="$1";  shift
    case "$op" in
        id|ps)
            ___x_cmd_mac_app_"$op" "$@"
            ;;
    esac
}

___x_cmd_mac_app_id(){
    ___x_cmd_mac_appid "$@"
}

___x_cmd_mac_aps(){
    ___x_cmd_mac_app_ps "$@"
}

