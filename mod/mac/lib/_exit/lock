
___x_cmd_mac_lock(){
    [ "$#" -gt 0 ] || set -- --run

    case "$1" in
        -h|--help)      ___x_cmd help -m mac lock ;;
        --run)          ___x_cmd_mac_lock___run "$@" ;;
        *)              N=mac M="Unsupport subcmd for lock -> $1"   log:ret:64
    esac
}

___x_cmd_mac_lock___run(){
    ___x_cmd_cmds python3 "$___X_CMD_ROOT_MOD/mac/lib/_exit/lock.py" "$@" || return 1
}
