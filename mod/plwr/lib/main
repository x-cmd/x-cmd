# shellcheck shell=dash

___x_cmd log init plwr
xrc:mod:lib       plwr   init handon htmlsource

___x_cmd_plwr___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m plwr   "$@";    return 0 ;;

        h|handon)       ___x_cmd_plwr_handon    "$@" ;;
        reinit|init|htmlsource)
                        ___x_cmd_plwr_"$op"     "$@"  ;;
        *)              ;;
    esac
}
