


___x_cmd_html_fparse(){
    [ $# -gt 0 ]    ||      set -- -h

    local op="$1";  shift
    case "$op" in
        info)
            ___x_cmd_html_fparse_"$op" "$@" ;;

        *)
            N=html M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}


___x_cmd_html_fparse_info(){
    ___x_cmd_cmds awk -f "$___X_CMD_ROOT_MOD/html/lib/awk/fparse_info.awk"
}
