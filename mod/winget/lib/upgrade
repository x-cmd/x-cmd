# shellcheck shell=dash

___x_cmd_winget_upgrade(){
    ___x_cmd_winget___bin upgrade "$@"
    local err="$?"
    case "$err" in
        0)  ___x_cmd notify "âœ… Successfully winget upgrade $*." ;;
        *)  ___x_cmd notify "ğŸ” Failed to winget upgrade $*. [exitcode=$err]." ;;
    esac
    return $err
}
