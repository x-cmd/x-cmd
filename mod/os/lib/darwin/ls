

___x_cmd_os_ls_cpu(){
    command sysctl -a | command grep machdep.cpu | ___x_cmd_cmds_awk -v FS=: '{
        gsub("machdep.cpu.", "", $1)
        if (NR % 5 == 0) {
            printf("\033[34;1m\033[4m%-30s\t\t\033[32m%-50s\033[0m\n", $1, $2)
        } else {
            printf("\033[34;1m%-30s\t\t\033[32m%-50s\033[0m\n", $1, $2)
        }
    }'
}

___x_cmd_os_ls_mem(){
    memory_pressure
}

___x_cmd_os_ls_net(){
    memory_pressure
}
