

___x_cmd_emacs___runmain(){
    TERM=xterm-256color ___x_cmd_emacs___runcmd emacs "$@"
}

___x_cmd_emacs___runcmd(){
    if ___x_cmd_hascmd emacs; then
        ___x_cmd_emacs___runcmd(){  ___x_cmd_cmds "$@"; }
    else
        ___x_cmd_emacs___install || return
        ___x_cmd_emacs___runcmd(){  ___x_cmd cosmo "$@" ; }
    fi || return
    ___x_cmd_emacs___runcmd "$@"
}
