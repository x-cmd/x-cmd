
___x_cmd_ubuntu___runmain(){
    ___x_cmd_ubuntu___runcmd "$@"
}

___x_cmd_ubuntu___runcmd(){
    if ___x_cmd_hascmd ubuntu; then
        ___x_cmd_ubuntu___runcmd(){  ___x_cmd_cmds ubuntu "$@"; }
    else
        ___x_cmd_ubuntu___install || return
        ___x_cmd_ubuntu___runcmd(){ "$___X_CMD_ubuntu_BIN_PATH/$___X_CMD_ubuntu_BIN" "$@" ; }
    fi || return
    ___x_cmd_ubuntu___runcmd "$@"
}
