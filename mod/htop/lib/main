# shellcheck shell=dash

xrc:mod:lib     htop        __cmd

___x_cmd_htop___main(){
    [ "$#" -gt 0 ] ||   {
        ___x_cmd_htop___cmd
        return
    }

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m htop ;;
        --|--cmd)       ___x_cmd_htop___cmd         "$@" ;;
        *)              ___x_cmd_htop___cmd "$op"   "$@" ;;
    esac
}
