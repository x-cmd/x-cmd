# shellcheck shell=dash

xrc:mod:lib     gh      model/chat model/ls
___x_cmd_gh_model(){
    param:void
    local ___X_CMD_GH_MODEL_DEFAULT_NAME='gpt-4o-mini'
    local op="$1"
    case "$op" in
        ls|chat)    shift; ___x_cmd_gh_model_"$op" "$@" ;;
        -h|--help)  ___x_cmd help -m gh model ;;
        *)          ___x_cmd help -m gh model >&2; return 64 ;;
    esac
}
