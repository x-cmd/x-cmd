___x_cmd_ascii_logo__raspbian_(){
    local c1; local c2
    c1="${e33}[3${1:-1}m"; c2="${e33}[3${2:-2}m"
    x_="
${c1}   .~~.   .~~.
  '. \\ ' ' / .'
${c2}   .~ .~~~..~.
  : .~.'~'.~. :
 ~ (  ) (  ) ~
( ~ '~.~.'~' ~)
(  : '~' :  )
 '~ .~~~. ~'
     '~'
${e33}[0m"
}

