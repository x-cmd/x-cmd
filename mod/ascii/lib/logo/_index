___x_cmd_ascii_logo__centos_(){
    local c1; local c2; local c3; local c4
    c1="${e33}[3${1:-6}m"; c2="${e33}[3${2:-3}m"
    c3="${e33}[3${3:-5}m"; c4="${e33}[3${4:-4}m"
    x_="
${c1} ____${c2}____${c3}____
/    ${c2}\\   ${c3}\\   \\
|  ${c4}(_)${c1}  |${c2}   |${c3}  |
\\    ${c1}_${c2}___${c3}___/
 \\  ${c1}/ ${c4}(_)${c3} /
  \\_____/
${e33}[0m"
}

