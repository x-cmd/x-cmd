# shellcheck shell=dash
___x_cmd_theme_envinit(){
    [ -z "$___X_CMD_THEME_ENVINIT" ] || return
    ___X_CMD_THEME_ENVINIT=1

    ___x_cmd tty update

    local x_
    ___x_cmd_term vendor_
    ___X_CMD_THEME_TERM_VENDOR="$x_"

    ___x_cmd_term color_
    ___X_CMD_THEME_TERM_COLORTYPE="$x_"
}

___x_cmd_theme_envinit
