
___x_cmd_arch___runmain(){
    ___x_cmd_arch___runcmd "$@"
}

___x_cmd_arch___runcmd(){
    if ___x_cmd_hascmd arch; then
        ___x_cmd_arch___runcmd(){  ___x_cmd_cmds arch "$@"; }
    else
        ___x_cmd_arch___install || return
        ___x_cmd_arch___runcmd(){ "$___X_CMD_arch_BIN_PATH/$___X_CMD_arch_BIN" "$@" ; }
    fi || return
    ___x_cmd_arch___runcmd "$@"
}
