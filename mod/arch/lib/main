# shellcheck shell=dash

xrc:mod:lib arch   __exec

___x_cmd_arch___main(){
    [ "$#" -gt 0 ] ||   set -- bash

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m arch "$@";    return 0 ;;

        --|snap)        ___x_cmd_arch_snap            "$@" ;;
        *)              ___x_cmd_arch_snap "$op"      "$@" ;;
    esac
}


___x_cmd_arch_snap(){
    ___x_cmd docker snap --imagetype arch "$@"
}

