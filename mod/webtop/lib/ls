# shellcheck shell=dash

# list all possible

___x_cmd_webtop_la(){
    # consider using fzf for more information
    ___x_cmd_webtop_la___raw
}

___x_cmd_webtop_ls(){
    ___x_cmd_webtop_la___raw
}

___x_cmd_webtop_la___raw(){
    local os
    local wm

    while read -r os; do
        while read -r wm; do
            printf "%s-%s\n" "$os" "$wm"
        done <<A
kde
mate
xfce
i3
A
    done <<A
ubuntu
debian
alpine
arch
fedora
A
}
