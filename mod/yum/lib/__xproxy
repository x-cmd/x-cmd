# shellcheck shell=dash
___x_cmd_yum___xproxy(){
    [ $# -gt 0 ]    ||  set - ls
    local op="$1";      shift

    local O=yum
    case "$op" in
        ls|use|unuse|try|untry|run)
            ___x_cmd proxy wrap "$op"  "$@" ;;

        -h|--help)
            ___x_cmd help -m yum proxy        "$@" ;;
    esac
}

