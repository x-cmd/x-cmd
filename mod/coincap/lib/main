# shellcheck shell=dash

xrc:mod:lib     coincap   ls cfg

___x_cmd_coincap___main(){
    [ "$#" -gt 0 ] ||   set -- ls

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m coincap "$@";    return 0 ;;
        --cfg|--cur|--init|init)
                        ___x_cmd_coincap_"${op#--}" "$@"         ;;
        ls)             ___x_cmd_coincap_"$op" "$@" ;;

        *)              N=coincap M="Unknown subcmd -> $op" log:ret:64 ;;
    esac
}

