# shellcheck shell=dash

# ___x_cmd_tldr_cat_form_xcmd gh repo create
___x_cmd_tldr_cat_form_xcmd(){
    local ___X_CMD_HELP_POSITION_ORDER="tldr"
    local ___X_CMD_LANG=""
    local x_=; ___x_cmd_tldr_lang get_
    case "$x_" in
        zh|zh_*)    ___X_CMD_LANG=zh ;;
        *)          ___X_CMD_LANG=en ;;
    esac

    ___x_cmd help -m "$@"
}
