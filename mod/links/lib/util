
___x_cmd_links_codeurl_(){
    local url="$1"

    case "$url" in
        https://github.com/*)
            ___x_cmd gh codeurl_ "$url"
            ;;
        https://codeberg.com/*)
            ___x_cmd cb codeurl_ "$url"
            ;;
        https://gitee.com/*)
            ___x_cmd gt codeurl_ "$url"
            ;;
        https://gitlab.com/*)
            ___x_cmd gl codeurl_ "$url"
            ;;
    esac
}
