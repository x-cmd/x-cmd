# shellcheck shell=dash

xrc:mod:lib     gop   __runcmd

___x_cmd_gop___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m gop "$@";    return 0  ;;

        --|--runmain)   ___x_cmd_gop___runmain         "$@"    ;;
        *)              ___x_cmd_gop___runmain "$op"   "$@"    ;;
    esac
}
