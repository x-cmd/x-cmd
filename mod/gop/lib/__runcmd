
___x_cmd_gop___runmain(){
    ___x_cmd_gop___runcmd "$@"
}

___x_cmd_gop___runcmd(){
    if ___x_cmd_hascmd gop; then
        ___x_cmd_gop___runcmd(){  ___x_cmd_cmds gop "$@"; }
    else
        ___x_cmd_gop___install || return
        ___x_cmd_gop___runcmd(){ "$___X_CMD_gop_BIN_PATH/$___X_CMD_gop_BIN" "$@" ; }
    fi || return
    ___x_cmd_gop___runcmd "$@"
}
