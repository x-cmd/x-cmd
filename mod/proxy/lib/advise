# shellcheck shell=bash

___x_cmd_proxy___advise_rollback_ls(){
    local IFS="$___X_CMD_UNSEENCHAR_NEWLINE";
    candidate_exec_arr=( $( x proxy $1 rollback ls) ) 
}

___x_cmd_proxy___advise_mirror_ls(){
    local IFS="$___X_CMD_UNSEENCHAR_NEWLINE"

    #TODO: to use shell replace awk
    if [ -n "$ZSH_VERSION" ]; then
        # shellcheck disable=SC2207
        candidate_exec_arr=( $( x proxy "$1" ls | \
        awk 'NR >1 {
            printf("%s:",$1)
            for (i = 3; i <= NF; i++) {
                printf "%s ", $i
            }
            print ""
        }' ) )
    else
        candidate_exec_arr=( $( x proxy "$1" ls | awk 'NR >1 { print $1 }' ) )
    fi
}
