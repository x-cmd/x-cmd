
___x_cmd_skill___advise_list_full(){
    ___x_cmd_skill_ll___id
}

___x_cmd_skill___advise_list_active(){
    local agent="$1"; shift
    [ "$#" -gt 0 ] ||   set -- ""
    local dir=""
    while [ "$#" -gt 0 ]; do
        dir="$1"; shift
        ___x_cmd_skill_ls --agent "$agent" --skill-dir "$dir" --id
    done
}
