# shellcheck shell=dash

___x_cmd_gtb_search(){
    [ "$#" -gt 0 ] ||  N=gtb M="Please provide a value for the query" log:ret:64
    local op="$1";  shift
    case "$op" in
        -h|--help)      ___x_cmd help -m gtb search;    return  ;;
        *)              ___x_cmd_gtb_meta_ls "$@"               ;;
    esac
}
