
___x_cmd_openclaw___runmain(){
    ___x_cmd_openclaw___checkcmd || return $?
    ___x_cmd_cmds openclaw "$@"
}

___x_cmd_openclaw___checkcmd(){
    ___x_cmd_hascmd openclaw || {
        openclaw:info "The openclaw command was not found. Proceeding with installation."
        ___x_cmd_openclaw___install || return $?
    }
}
