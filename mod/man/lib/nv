___x_cmd_man___nv(){
    [ -f "$___X_CMD_MAN_ALL_CACHE" ] || {
        x:info "Wait several seconds for the first time to build all index."
        ___x_cmd_man___all_build
    }

    local no=1  # [1-7n]
    local res
    ___x_cmd_man___all | ___x_cmd_cmds awk -v no="$no" '$1~no' | {
        res="$(x pick --col 6)"
        [ -n "$res" ] || return 1
        eval x man "$res"
    }
}
