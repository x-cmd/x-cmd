$2!="="{
    key = $1
    $1 = ""
    gsub( "(^[ ]+)|([ ]+$)", "", $0 )
    printf( "\033[36m" "%-20s" "\033[0m" "\t" "\033[32m" "%s\n", key, $0 )

    next
}

{
    key = $1
    $1 = $2 = ""
    gsub( "(^[ ]+)|([ ]+$)", "", $0 )
    printf( "\033[36m" "%-20s" "\033[0m" ":\t" "\033[32m" "%s\n", key, $0 )
}


