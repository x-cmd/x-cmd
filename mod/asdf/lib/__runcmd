
___x_cmd_asdf___runmain(){
    ___x_cmd_asdf___runcmd "$@"
}

___x_cmd_asdf___runcmd(){
    if ___x_cmd_hascmd asdf; then
        ___x_cmd_asdf___runcmd(){  ___x_cmd_cmds asdf "$@"; }
    else
        ___x_cmd_asdf___install || return $?
        ___x_cmd_asdf___runcmd(){ "$___X_CMD_ASDF_BIN_PATH/$___X_CMD_ASDF_BIN" "$@" ; }
    fi || return $?
    ___x_cmd_asdf___runcmd "$@"
}
