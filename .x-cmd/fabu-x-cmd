git clone git@github.com:x-cmd/x7 -b main

(
    cd x7
    git clone git@github.com:x-cmd/x-cmd -b main
    xws copy-to-x-cmd x-cmd

    (
        cd x-cmd
        git add .
        git commit -m ...
        git push origin main

        git push origin main
        git push git@gitcode.net:x-cmd/x-cmd main
        git push git@gitee.com:x-cmd/x-cmd main
    )
)
